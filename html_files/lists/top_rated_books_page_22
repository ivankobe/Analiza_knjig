        <a id="SearchLink" class=" tab js-dhtmlTab" data-tab-name="Search" data-tab-set="154470813" href="/list/show/22.Best_Books_of_the_Decade_1950s?tab=Search">Search</a>
<div class="clear"> </div></div><script type="text/javascript" charset="utf-8">
//<![CDATA[

  current_tabs[154470813] = 'My_Books';

  Event.observe(window, 'load', function(e) {
    // Switch to an anchored tab if specified in the URL
    if (location.href.include('#')) {
      var tabID = location.href.split('#')[1];
      if (tabID != '' && $(tabID)) {
        changeTab(tabID)
      }
    }
  })
//]]>
</script>


