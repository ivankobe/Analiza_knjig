              <a id="loading_link_742970" href="#" onclick="new Ajax.Request(&#39;/list/list_book/5223656&#39;, {asynchronous:true, evalScripts:true, onFailure:function(request){Element.hide(&#39;loading_anim_742970&#39;);$(&#39;loading_link_742970&#39;).innerHTML = &#39;&lt;span class=&quot;error&quot;&gt;ERROR&lt;/span&gt;try again&#39;;$(&#39;loading_link_742970&#39;).show();;Element.hide(&#39;loading_anim_742970&#39;);}, onLoading:function(request){;Element.show(&#39;loading_anim_742970&#39;);Element.hide(&#39;loading_link_742970&#39;)}, onSuccess:function(request){Element.hide(&#39;loading_anim_742970&#39;);Element.show(&#39;loading_link_742970&#39;);}, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;t8BNQmb6droo7doZBeeNLoM5xzhbH6KvvnwFgdAb5hTGlpCnwuJ179+BNDik6T0EQ0mp9M0vGo4BU2R0UOkZYw==&#39;)}); return false;">76 people voted</a><img style="display:none" id="loading_anim_742970" class="loading" src="https://s.gr-assets.com/assets/loading-trans-ced157046184c3bc7c180ffbfc6825a4.gif" alt="Loading trans" />
