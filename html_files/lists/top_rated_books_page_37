        <a id="SearchLink" class=" tab js-dhtmlTab" data-tab-name="Search" data-tab-set="381531593" href="/list/show/37.Best_American_Plays?tab=Search">Search</a>
<div class="clear"> </div></div><script type="text/javascript" charset="utf-8">
//<![CDATA[

  current_tabs[381531593] = 'My_Books';

  Event.observe(window, 'load', function(e) {
    // Switch to an anchored tab if specified in the URL
    if (location.href.include('#')) {
      var tabID = location.href.split('#')[1];
      if (tabID != '' && $(tabID)) {
        changeTab(tabID)
      }
    }
  })
//]]>
</script>


