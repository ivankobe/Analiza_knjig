      $gr.components.mentionInserter.setField(field);
      Lightbox.showBoxByID(add_mention_box.id, 380);
  };

  gr.add_reference = function(reference) {
      $gr.components.mentionInserter.insertText(reference);
      Lightbox.hideBox();
  };

