        $('bookCover_7135602').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_7135602').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
