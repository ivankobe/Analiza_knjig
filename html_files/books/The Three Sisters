<div id="review_3741545544" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/3741545544" />
    <a title="Tuti" class="left imgcol" href="/user/show/44793822-tuti"><img alt="Tuti" src="https://images.gr-assets.com/users/1436964894p2/44793822.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/3741545544?book_show_action=true">Jan 04, 2021</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Tuti" class="user" itemprop="url" name="Tuti" href="/user/show/44793822-tuti">Tuti</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was amazing"><span size="15x15" class="staticStar p10">it was amazing</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span></span>

          <span class="greyText">&nbsp;&middot;&nbsp;</span>
          <a class="lightGreyText" title="Paperback 9783150042649" href="/book/show/2000061.Drei_Schwestern">review of another edition</a>


        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/44793822-tuti?shelf=2021">2021</a>, 
              <a class="actionLinkLite" href="/review/list/44793822-tuti?shelf=plays">plays</a>, 
              <a class="actionLinkLite" href="/review/list/44793822-tuti?shelf=theatre">theatre</a>, 
              <a class="actionLinkLite" href="/review/list/44793822-tuti?shelf=german">german</a>, 
              <a class="actionLinkLite" href="/review/list/44793822-tuti?shelf=classics">classics</a>, 
