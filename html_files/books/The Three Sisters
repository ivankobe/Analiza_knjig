      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_325916').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_325916').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_325916').observe('prototip:hidden', function () {
