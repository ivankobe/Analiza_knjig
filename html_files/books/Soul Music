      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_55383501').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_55383501').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_55383501').observe('prototip:hidden', function () {
