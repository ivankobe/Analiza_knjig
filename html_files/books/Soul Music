<div id="review_364873727" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/364873727" />
    <a title="Brooke" class="left imgcol" href="/user/show/126262-brooke"><img alt="Brooke" src="https://images.gr-assets.com/users/1448065679p2/126262.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/364873727?book_show_action=true">Jul 09, 2012</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Brooke" class="user" itemprop="url" name="Brooke" href="/user/show/126262-brooke">Brooke</a>
