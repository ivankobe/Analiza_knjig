        $('bookCover_846854').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_846854').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
