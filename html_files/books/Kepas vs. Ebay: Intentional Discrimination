        $('bookCover_99208').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_99208').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
