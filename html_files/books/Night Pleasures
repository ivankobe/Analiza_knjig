        $('bookCover_84183').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_84183').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_84183').observe('prototip:hidden', function () {
