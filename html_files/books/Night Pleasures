      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_581'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/84150.Night_Pleasures?rating=\">all <span class=\"greyText\">(130269)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/84150.Night_Pleasures?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;W4RVbdmBTC9d7uh99nLPCe7bs84U6COuzYtvGhCxGqaGzf783BnBzRgSBWmi0YgSR8xhllBEIM96nrkMnc7xmA==&#39;)}); return false;\">5 stars <span class=\"greyText\">(35429)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/84150.Night_Pleasures?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;oHcNY7yF0G3VOz1lHmzCeXt/krUiMkewofNVpqd++HN9PqbyuR1dj5DH0HFKz4Vi0mhA7WaeRNEW5oOwKgETTQ==&#39;)}); return false;\">4 stars <span class=\"greyText\">(24479)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  