        $('bookCover_12198984').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_12198984').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
