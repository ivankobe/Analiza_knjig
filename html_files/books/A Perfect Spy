      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_22535415').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_22535415').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_22535415').observe('prototip:hidden', function () {
