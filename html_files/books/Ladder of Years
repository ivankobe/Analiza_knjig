        $('bookCover_2833745').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_2833745').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
