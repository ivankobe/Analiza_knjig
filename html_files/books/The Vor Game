        $('bookCover_96279').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_96279').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
