      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_223'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/16032127-revenge?rating=\">all <span class=\"greyText\">(29414)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/16032127-revenge?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;Bhrrs1say+NZiWsL5zxCbiJ4n1HcV8Z4RqCVdjyl4LUtOFlH1zFKMWY4KeX1laDpij6aZPIn40yeTTh0t5VVNQ==&#39;)}); return false;\">5 stars <span class=\"greyText\">(3118)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/16032127-revenge?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;s4KpTKFytnR+eU1yoI+6dE041bHVj3pMf3lXkAsEIRaYoBu4LVk3pkHID5yyJljz5X7QhPv/X3inlPqSgDSUlg==&#39;)}); return false;\">4 stars <span class=\"greyText\">(4570)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"load