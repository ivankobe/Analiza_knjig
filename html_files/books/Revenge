ers: true });
      $('span_class_gr-hyperlink_more_filters_span_tip_223').observe('prototip:shown', function() {
        if (this.up('#box')) {
          $$('div.prototip').each(function(i){i.setStyle({zIndex: $('box').getStyle('z-index')})});
