        $('bookCover_160988').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_160988').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
