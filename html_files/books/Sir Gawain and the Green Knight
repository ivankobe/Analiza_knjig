        $('bookCover_2696').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_2696').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
