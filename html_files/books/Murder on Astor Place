      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_13505530').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_13505530').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_13505530').observe('prototip:hidden', function () {
