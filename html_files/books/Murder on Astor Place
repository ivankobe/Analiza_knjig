        $('bookCover_512391').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_512391').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
