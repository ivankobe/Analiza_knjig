      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_39507').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_39507').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_39507').observe('prototip:hidden', function () {
