        $('bookCover_414392').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_414392').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
