      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_14058818').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_14058818').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_14058818').observe('prototip:hidden', function () {
