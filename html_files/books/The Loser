        $('bookCover_605684').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_605684').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
