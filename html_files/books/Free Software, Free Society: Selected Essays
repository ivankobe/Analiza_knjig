        $('bookCover_647').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_647').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
