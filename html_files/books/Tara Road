        $('bookCover_5180').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_5180').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
