        $('bookCover_53435584').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_53435584').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
