        $('bookCover_312957').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_312957').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
