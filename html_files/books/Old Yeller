        $('bookCover_598117').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_598117').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
