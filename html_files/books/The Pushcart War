        $('bookCover_55530593').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_55530593').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_55530593').observe('prototip:hidden', function () {
