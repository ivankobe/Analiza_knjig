      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_46000220').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_46000220').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_46000220').observe('prototip:hidden', function () {
