      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_38357345').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_38357345').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_38357345').observe('prototip:hidden', function () {
