<div id="review_2019596246" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2019596246" />
    <a title="namericanwordcat" class="left imgcol" href="/user/show/2556235-namericanwordcat"><img alt="namericanwordcat" src="https://images.gr-assets.com/users/1390432619p2/2556235.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2019596246?book_show_action=true">Jun 04, 2017</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="namericanwordcat" class="user" itemprop="url" name="namericanwordcat" href="/user/show/2556235-namericanwordcat">namericanwordcat</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="really liked it"><span size="15x15" class="staticStar p10">really liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/2556235-namericanwordcat?shelf=single-mother">single-mother</a>, 
              <a class="actionLinkLite" href="/review/list/2556235-namericanwordcat?shelf=working-class-hero">working-class-hero</a>, 
              <a class="actionLinkLite" href="/review/list/2556235-namericanwordcat?shelf=toddler">toddler</a>, 
              <a class="actionLinkLite" href="/review/list/2556235-namericanwordcat?shelf=child-abuse">child-abuse</a>, 
              <a class="actionLinkLite" href="/review/list/2556235-namericanwordcat?shelf=contemporary-romance-series">contemporary-romance-series</a>, 
              <a class="actionLinkLite" href="/review/list/2556235-namericanwordcat?shelf=library-loan">library-loan</a>, 
              <a class="actionLinkLite" href="/review/list/2556235-namericanwordcat?shelf=dancer">dancer</a>, 
              <a class="actionLinkLite" href="/review/list/2556235-namericanwordcat?shelf=maryland">maryland</a>, 
