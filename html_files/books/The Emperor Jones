<div id="review_2581345843" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2581345843" />
    <a title="Illiterate" class="left imgcol" href="/user/show/72945589-illiterate"><img alt="Illiterate" src="https://images.gr-assets.com/users/1539215959p2/72945589.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2581345843?book_show_action=true">Nov 02, 2018</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Illiterate" class="user" itemprop="url" name="Illiterate" href="/user/show/72945589-illiterate">Illiterate</a>
