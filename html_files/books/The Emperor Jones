        $('bookCover_3070605').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_3070605').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
