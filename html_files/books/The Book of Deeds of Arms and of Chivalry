        $('bookCover_52758144').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_52758144').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_52758144').observe('prototip:hidden', function () {
