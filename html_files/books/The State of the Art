          $$('div.prototip').each(function (e) {
            if ($('bookCover_54329757').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_54329757').observe('prototip:hidden', function () {
