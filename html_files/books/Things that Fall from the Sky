        $('bookCover_9007886').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_9007886').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
