        $('bookCover_39333207').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_39333207').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_39333207').observe('prototip:hidden', function () {
