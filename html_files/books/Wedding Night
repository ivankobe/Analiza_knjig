      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_14748').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_14748').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_14748').observe('prototip:hidden', function () {
