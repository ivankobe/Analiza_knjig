        $('bookCover_163362').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_163362').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
