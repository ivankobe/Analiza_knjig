      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_352078').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_352078').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_352078').observe('prototip:hidden', function () {
