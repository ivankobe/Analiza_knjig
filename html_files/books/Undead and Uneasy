      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_30366837').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_30366837').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_30366837').observe('prototip:hidden', function () {
