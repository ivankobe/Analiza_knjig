        $('bookCover_30282').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_30282').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
