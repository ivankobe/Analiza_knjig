        $('bookCover_246245').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_246245').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
