        $('bookCover_258108').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_258108').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
