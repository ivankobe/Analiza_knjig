        $('bookCover_7894340').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_7894340').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
