<div id="review_541447243" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/541447243" />
    <a title="Giuseppe " class="left imgcol" href="/user/show/15655893-giuseppe"><img alt="Giuseppe " src="https://s.gr-assets.com/assets/nophoto/user/m_50x66-82093808bca726cb3249a493fbd3bd0f.png" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/541447243?book_show_action=true">Feb 20, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Giuseppe " class="user" itemprop="url" name="Giuseppe " href="/user/show/15655893-giuseppe">Giuseppe </a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was amazing"><span size="15x15" class="staticStar p10">it was amazing</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span></span>



        

