//<![CDATA[
      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_298'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/791345.Picnic_at_Hanging_Rock?rating=\">all <span class=\"greyText\">(46746)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/791345.Picnic_at_Hanging_Rock?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;5ZPfEjUSUKRQZm+Wu+HkkKaKcV7T59vg1/bvq8/Cn3+3eQ6ISNgBFXezS5ot4vChhAZENf3fi/vRQna5BPxl+A==&#39;)}); return false;\">5 stars <span class=\"greyText\">(3588)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/791345.Picnic_at_Hanging_Rock?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;76ce0gwnMt7X4JlDoAaZTV6t3bqsQ8RdYTJxZ5fJdfu9Tc9Ice1jb/A1vU82BY18fCHo0YJ7lEZnhuh1XPePfA==&#39;)}); return false;\">4 stars <span class=\"greyText\">(6639)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span