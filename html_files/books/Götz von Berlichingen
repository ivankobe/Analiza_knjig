        $('bookCover_965956').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_965956').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
