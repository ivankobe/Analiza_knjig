<div id="review_280299192" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/280299192" />
    <a title="Nick" class="left imgcol" href="/user/show/5548923-nick"><img alt="Nick" src="https://images.gr-assets.com/users/1366411351p2/5548923.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/280299192?book_show_action=true">Feb 19, 2012</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Nick" class="user" itemprop="url" name="Nick" href="/user/show/5548923-nick">Nick</a>
