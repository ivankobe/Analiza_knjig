        $('bookCover_50517752').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_50517752').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
