        $('bookCover_12913059').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_12913059').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
