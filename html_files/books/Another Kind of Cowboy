        $('bookCover_38350847').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_38350847').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
