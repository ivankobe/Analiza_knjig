      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_672620').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_672620').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_672620').observe('prototip:hidden', function () {
