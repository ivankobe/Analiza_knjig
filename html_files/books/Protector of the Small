        $('bookCover_153784').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_153784').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
