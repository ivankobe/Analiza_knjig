        $('bookCover_30324056').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_30324056').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
