          $$('div.prototip').each(function (e) {
            if ($('bookCover_492893').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_492893').observe('prototip:hidden', function () {
