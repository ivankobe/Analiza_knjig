        $('bookCover_14570').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_14570').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
