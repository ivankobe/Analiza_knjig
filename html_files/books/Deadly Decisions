<div id="review_3187198795" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/3187198795" />
    <a title="Jan" class="left imgcol" href="/user/show/22004574-jan"><img alt="Jan" src="https://images.gr-assets.com/users/1614687064p2/22004574.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/3187198795?book_show_action=true">Mar 16, 2020</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Jan" class="user" itemprop="url" name="Jan" href="/user/show/22004574-jan">Jan</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="liked it"><span size="15x15" class="staticStar p10">liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/22004574-jan?shelf=canada">canada</a>, 
              <a class="actionLinkLite" href="/review/list/22004574-jan?shelf=mystery">mystery</a>, 
              <a class="actionLinkLite" href="/review/list/22004574-jan?shelf=crime">crime</a>, 
              <a class="actionLinkLite" href="/review/list/22004574-jan?shelf=thriller">thriller</a>, 
