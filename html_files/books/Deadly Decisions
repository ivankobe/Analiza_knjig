<div id="review_2453172198" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2453172198" />
    <a title="Mike" class="left imgcol" href="/user/show/7291253-mike"><img alt="Mike" src="https://images.gr-assets.com/users/1350595639p2/7291253.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2453172198?book_show_action=true">Jul 18, 2018</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Mike" class="user" itemprop="url" name="Mike" href="/user/show/7291253-mike">Mike</a>
