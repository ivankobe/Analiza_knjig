        $('bookCover_6969591').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_6969591').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
