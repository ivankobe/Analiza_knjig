        $('bookCover_13152').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_13152').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
