        $('bookCover_6693332').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_6693332').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
