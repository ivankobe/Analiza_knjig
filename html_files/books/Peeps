<div id="review_17362022" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/17362022" />
    <a title="Brigid ✩" class="left imgcol" href="/user/show/886144-brigid"><img alt="Brigid ✩" src="https://images.gr-assets.com/users/1581869052p2/886144.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/17362022?book_show_action=true">Mar 09, 2008</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Brigid ✩" class="user" itemprop="url" name="Brigid ✩" href="/user/show/886144-brigid">Brigid ✩</a>
