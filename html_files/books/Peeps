        $('bookCover_25785333').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_25785333').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
