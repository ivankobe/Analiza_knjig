        $('bookCover_18006496').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_18006496').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
