        $('bookCover_116560').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_116560').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
