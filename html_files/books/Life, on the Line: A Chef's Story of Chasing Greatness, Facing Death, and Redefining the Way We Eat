        $('bookCover_164428').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_164428').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
