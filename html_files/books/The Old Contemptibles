<div id="review_2360030194" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2360030194" />
    <a title="Barb" class="left imgcol" href="/user/show/22083484-barb"><img alt="Barb" src="https://images.gr-assets.com/users/1375909608p2/22083484.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2360030194?book_show_action=true">May 21, 2018</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Barb" class="user" itemprop="url" name="Barb" href="/user/show/22083484-barb">Barb</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="liked it"><span size="15x15" class="staticStar p10">liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>



        

