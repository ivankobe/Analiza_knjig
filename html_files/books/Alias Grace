        $('bookCover_57328166').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_57328166').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
