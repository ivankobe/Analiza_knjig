        $('bookCover_516792').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_516792').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
