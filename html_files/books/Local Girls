        $('bookCover_50209372').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_50209372').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
