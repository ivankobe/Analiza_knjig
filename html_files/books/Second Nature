        $('bookCover_30361199').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_30361199').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
