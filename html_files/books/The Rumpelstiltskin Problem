        $('bookCover_47674').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_47674').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
