      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_1136483').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1136483').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_1136483').observe('prototip:hidden', function () {
