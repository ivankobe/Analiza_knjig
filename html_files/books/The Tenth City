        $('bookCover_35808').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_35808').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_35808').observe('prototip:hidden', function () {
