      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_54'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/397844.The_Masque_of_the_Black_Tulip?rating=\">all <span class=\"greyText\">(16568)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/397844.The_Masque_of_the_Black_Tulip?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;ebGum7TVsc37/K3ti87qZ2xhGStoqeRaEWfnXrlml5Cg1DwZi/h/IPFQ2Zcvx33n5KDDiWDZEkgunOVwwVsciQ==&#39;)}); return false;\">5 stars <span class=\"greyText\">(3004)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/397844.The_Masque_of_the_Black_Tulip?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;j3wbD1Zgix5YlIWj8XUOwbxNCcqCb0hOTerNGuVRo9hWGYmNaU1F81I48dlVfJlBNIzTaIofvlxyEc80nWwowQ==&#39;)}); return false;\">4 stars <span class=\"greyText\">(4595)<\/span><\/a>\n                  <span class=\"loadin