<div id="review_2957444960" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2957444960" />
    <a title="David" class="left imgcol" href="/user/show/1385615-david"><img alt="David" src="https://images.gr-assets.com/users/1611291805p2/1385615.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2957444960?book_show_action=true">Aug 29, 2019</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="David" class="user" itemprop="url" name="David" href="/user/show/1385615-david">David</a>
