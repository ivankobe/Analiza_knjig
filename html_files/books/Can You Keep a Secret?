      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_142'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/33724.Can_You_Keep_a_Secret_?rating=\">all <span class=\"greyText\">(652923)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/33724.Can_You_Keep_a_Secret_?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;+RTnid9rSLaSvsbAM/TsT928Ef6tW8d6RkK3LAFWPiqm2gpyZAvq/wxoXNm6OS3bGfu+x06/gzsiEasByU066g==&#39;)}); return false;\">5 stars <span class=\"greyText\">(125763)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/33724.Can_You_Keep_a_Secret_?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;vgT5E+8F7/UDlQGb+tDfYa7nPdTsl+AqL8ZGoO6a2RbhyhToVGVNvJ1Dm4JzHR71aqCS7Q9zpGtLlVqNJoHd1g==&#39;)}); return false;\">4 stars <span class=\"greyText\">(135980)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/sp