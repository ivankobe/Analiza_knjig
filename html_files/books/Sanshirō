        $('bookCover_14029').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_14029').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
