<div id="review_4074217" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/4074217" />
    <a title="Kay" class="left imgcol" href="/user/show/241691-kay"><img alt="Kay" src="https://images.gr-assets.com/users/1541802339p2/241691.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/4074217?book_show_action=true">Aug 04, 2007</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Kay" class="user" itemprop="url" name="Kay" href="/user/show/241691-kay">Kay</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was amazing"><span size="15x15" class="staticStar p10">it was amazing</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/241691-kay?shelf=social-observ">social-observ</a>, 
              <a class="actionLinkLite" href="/review/list/241691-kay?shelf=british_lit">british_lit</a>, 
              <a class="actionLinkLite" href="/review/list/241691-kay?shelf=drama">drama</a>, 
              <a class="actionLinkLite" href="/review/list/241691-kay?shelf=fiction">fiction</a>, 
              <a class="actionLinkLite" href="/review/list/241691-kay?shelf=satire">satire</a>
