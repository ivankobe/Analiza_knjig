      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_93575').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_93575').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_93575').observe('prototip:hidden', function () {
