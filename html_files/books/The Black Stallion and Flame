<div id="review_77617962" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/77617962" />
    <a title="Tania" class="left imgcol" href="/user/show/2938614-tania"><img alt="Tania" src="https://images.gr-assets.com/users/1452215598p2/2938614.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/77617962?book_show_action=true">Nov 12, 2009</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Tania" class="user" itemprop="url" name="Tania" href="/user/show/2938614-tania">Tania</a>
