      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_494'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/123209.The_Black_Stallion_and_Flame?rating=\">all <span class=\"greyText\">(7951)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/123209.The_Black_Stallion_and_Flame?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;0m2vNZSIUDK3q/78OV6GMlIQ2sz/02PC3aFuhkrCKR0g6JfX1c01Bww2WX4Oeehv5lLANNVe1no3K7VEXi5Owg==&#39;)}); return false;\">5 stars <span class=\"greyText\">(2288)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/123209.The_Black_Stallion_and_Flame?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;/SikdhLVyDWsHFZvUCIU5yA5ipcFkWQ+6FeUQAvT7JsPrZyUU5CtABeB8e1nBXq6lHuQby8c0YYC3U+CHz+LRA==&#39;)}); return false;\">4 stars <span class=\"greyText\">(1735)<\/span><\/a>\n                  <span class=\"loadingAn