  <div class="friendReviews elementListBrown" >
    <div class="section">
      

<div id="review_1985545288" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1985545288" />
    <a title="Jess" class="left imgcol" href="/user/show/41763158-jess"><img alt="Jess" src="https://images.gr-assets.com/users/1611747988p2/41763158.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1985545288?book_show_action=true">Apr 29, 2017</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Jess" class="user" itemprop="url" name="Jess" href="/user/show/41763158-jess">Jess</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was amazing"><span size="15x15" class="staticStar p10">it was amazing</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span></span>

          <span class="greyText">&nbsp;&middot;&nbsp;</span>
