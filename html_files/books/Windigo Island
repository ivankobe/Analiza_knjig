        $('bookCover_175828').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_175828').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
