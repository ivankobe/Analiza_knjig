      <a class="uitext" rel="nofollow" id="span_class_gr-hyperlink_more_filters_span_tip_135" style="cursor: pointer; " href="#" onclick="return false"><span class="gr-hyperlink">More filters</span></a><script>
//<![CDATA[
      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_135'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/30312819-at-the-edge-of-the-universe?rating=\">all <span class=\"greyText\">(18486)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/30312819-at-the-edge-of-the-universe?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;tr0TgD38epN36URiVQjSFaDn+e/PLBlUQ+aFN6FaaVxlubQvSuHJJqun1fWS/wjPnM/Lpg1pDLF16M7oWOIheQ==&#39;)}); return false;\">5 stars <span class=\"greyText\">(2108)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/30312819-at-the-edge-of-the-universe?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;Bu3P5GlLWRdTbPZR6+DFfiL20xxOAK0p89a2BgllPQzV6WhLHlbqoo8iZ8YsFx+kHt7hVYxFuMzF2P3Z8N11KQ==&#39;)}); return false;\">4 stars <span class=\"greyText\">(1846)<\/span><\/a>\n                  <span class=\"loadi