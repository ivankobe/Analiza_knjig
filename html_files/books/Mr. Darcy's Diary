<div id="review_1483725598" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1483725598" />
    <a title="Alaina" class="left imgcol" href="/user/show/50620706-alaina"><img alt="Alaina" src="https://images.gr-assets.com/users/1451325817p2/50620706.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1483725598?book_show_action=true">Jan 10, 2018</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Alaina" class="user" itemprop="url" name="Alaina" href="/user/show/50620706-alaina">Alaina</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was ok"><span size="15x15" class="staticStar p10">it was ok</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/50620706-alaina?shelf=chick-lit">chick-lit</a>, 
              <a class="actionLinkLite" href="/review/list/50620706-alaina?shelf=january-challenge-2018">january-challenge-2018</a>, 
              <a class="actionLinkLite" href="/review/list/50620706-alaina?shelf=historical-fiction">historical-fiction</a>, 
              <a class="actionLinkLite" href="/review/list/50620706-alaina?shelf=romance">romance</a>, 
              <a class="actionLinkLite" href="/review/list/50620706-alaina?shelf=fiction">fiction</a>, 
              <a class="actionLinkLite" href="/review/list/50620706-alaina?shelf=classics">classics</a>, 
              <a class="actionLinkLite" href="/review/list/50620706-alaina?shelf=favorites">favorites</a>
          </div>
