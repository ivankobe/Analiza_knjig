<div id="review_352162054" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/352162054" />
    <a title="Amanda" class="left imgcol" href="/user/show/7897151-amanda"><img alt="Amanda" src="https://images.gr-assets.com/users/1461450073p2/7897151.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/352162054?book_show_action=true">Jun 20, 2012</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Amanda" class="user" itemprop="url" name="Amanda" href="/user/show/7897151-amanda">Amanda</a>
