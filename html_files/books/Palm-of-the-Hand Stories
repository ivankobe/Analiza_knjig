  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/721395951?book_show_action=true">Sep 16, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Florencia" class="user" itemprop="url" name="Florencia" href="/user/show/21575078-florencia">Florencia</a>
