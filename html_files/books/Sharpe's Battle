        $('bookCover_90949').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_90949').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
