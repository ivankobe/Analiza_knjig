        $('bookCover_469901').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_469901').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_469901').observe('prototip:hidden', function () {
