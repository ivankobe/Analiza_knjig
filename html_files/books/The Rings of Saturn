        $('bookCover_52011661').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_52011661').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
