<div id="review_2303889213" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2303889213" />
    <a title="Tony" class="left imgcol" href="/user/show/1690088-tony"><img alt="Tony" src="https://images.gr-assets.com/users/1332589331p2/1690088.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2303889213?book_show_action=true">Mar 06, 2018</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Tony" class="user" itemprop="url" name="Tony" href="/user/show/1690088-tony">Tony</a>
