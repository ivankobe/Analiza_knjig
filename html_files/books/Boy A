          $$('div.prototip').each(function (e) {
            if ($('bookCover_56328065').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_56328065').observe('prototip:hidden', function () {
