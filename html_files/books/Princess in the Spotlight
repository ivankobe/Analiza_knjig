      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_49008872').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_49008872').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_49008872').observe('prototip:hidden', function () {
