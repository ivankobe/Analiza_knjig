  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/765112898?book_show_action=true">Nov 14, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Cindy" class="user" itemprop="url" name="Cindy" href="/user/show/7783587-cindy">Cindy</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="did not like it"><span size="15x15" class="staticStar p10">did not like it</span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/7783587-cindy?shelf=regency">regency</a>, 
              <a class="actionLinkLite" href="/review/list/7783587-cindy?shelf=romance">romance</a>, 
              <a class="actionLinkLite" href="/review/list/7783587-cindy?shelf=angst">angst</a>, 
              <a class="actionLinkLite" href="/review/list/7783587-cindy?shelf=historical-romance">historical-romance</a>
          </div>
