        $('bookCover_42201449').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_42201449').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
