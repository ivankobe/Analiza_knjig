        $('bookCover_35607916').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_35607916').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
