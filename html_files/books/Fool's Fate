        $('bookCover_2315892').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_2315892').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
