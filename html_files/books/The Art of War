      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('edition_9397310').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('edition_9397310').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('edition_9397310').observe('prototip:hidden', function () {
