        $('bookCover_29492072').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_29492072').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
