      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_3697927').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_3697927').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_3697927').observe('prototip:hidden', function () {
