<div id="review_2881515338" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2881515338" />
    <a title="Mary" class="left imgcol" href="/user/show/27517527-mary"><img alt="Mary" src="https://images.gr-assets.com/users/1389296411p2/27517527.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2881515338?book_show_action=true">Jul 03, 2019</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Mary" class="user" itemprop="url" name="Mary" href="/user/show/27517527-mary">Mary</a>
