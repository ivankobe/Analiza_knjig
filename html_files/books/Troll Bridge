      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_43621467').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_43621467').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_43621467').observe('prototip:hidden', function () {
