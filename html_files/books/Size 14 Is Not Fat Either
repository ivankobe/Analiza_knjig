      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Anna" class="user" itemprop="url" name="Anna" href="/user/show/314550-anna">Anna</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was amazing"><span size="15x15" class="staticStar p10">it was amazing</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/314550-anna?shelf=fiction">fiction</a>, 
              <a class="actionLinkLite" href="/review/list/314550-anna?shelf=chick-lit">chick-lit</a>, 
              <a class="actionLinkLite" href="/review/list/314550-anna?shelf=mystery">mystery</a>, 
              <a class="actionLinkLite" href="/review/list/314550-anna?shelf=favorites">favorites</a>, 
