        $('bookCover_21409678').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_21409678').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
