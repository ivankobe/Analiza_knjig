        $('bookCover_324735').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_324735').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
