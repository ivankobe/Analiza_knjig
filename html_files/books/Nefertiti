        $('bookCover_20486495').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_20486495').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
