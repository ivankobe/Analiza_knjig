        $('bookCover_41563261').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_41563261').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
