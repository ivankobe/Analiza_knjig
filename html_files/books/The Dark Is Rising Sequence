          $$('div.prototip').each(function (e) {
            if ($('bookCover_492490').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_492490').observe('prototip:hidden', function () {
