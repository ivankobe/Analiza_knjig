        $('bookCover_13923').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_13923').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
