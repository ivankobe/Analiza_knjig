        $('bookCover_218750').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_218750').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
