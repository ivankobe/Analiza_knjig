        $('bookCover_16006132').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_16006132').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_16006132').observe('prototip:hidden', function () {
