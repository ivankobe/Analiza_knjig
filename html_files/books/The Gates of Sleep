          $$('div.prototip').each(function (e) {
            if ($('bookCover_12974280').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_12974280').observe('prototip:hidden', function () {
