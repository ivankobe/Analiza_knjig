        $('bookCover_28452447').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_28452447').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
