      var newTip = new Tip($('rating_details_tip'), "\n        <div id=\"moreBookData\" class=\"bookMeta\" >\n  \n\n  \n\n<table border=\"0\" id=\"rating_distribution\" style=\"width: auto; max-width: 500px;\" cellspacing=\"5\" cellpadding=\"0\">\n      <tr>\n        <th width=\"25\" class=\"right\">5&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 100%;\" title=\"18 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/2229006.The_Sentences_Book_1?rating=5\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'IcK1YmYCBxjGj6yT2aFHFD2JtvZlCUasFVYmr8RvDbU45N+kdrrVukLoQRk/AtyZ2JORO4HRVJqM8lLJ0NxQeA==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          45% (18)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">4&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 88%;\" title=\"16 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/2229006.The_Sentences_Book_1?rating=4\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'3KpsBTTDZFkotQEFESPKZoZqsUNnPftmGCJ3gDrtKOHFjAbDJHu2+6zS7I/3gFHrY3CWjoPl6VCBhgPmLl51LA==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          40% (16)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">3&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 27%;\" title=\"5 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/2229006.The_Sentences_Book_1?rating=3\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'LYYSaIiexYo6nX8ynaA79NLEVcytvkRiuLcbCNoIYuE0oHiumCYXKL76krh7A6B5N95yAUlmVlQhE29uzrs/LA==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          12% (5)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">2&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 0%;\" title=\"0 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/2229006.The_Sentences_Book_1?rating=2\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'mRPJwBWJ1f+9qxgKmnDensdY8oPLhqezlIRJa+t+fc6ANaMGBTEHXTnM9YB800UTIkLVTi9etYUNID0N/80gAw==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          0% (0)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">1&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 5%;\" title=\"1 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/2229006.The_Sentences_Book_1?rating=1\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'gnB9RjgHbfFRhVysJqHSyBtCCKA8Ee6x3MEMAXM1pSqbVheAKL+/U9XisSbAAklF/lgvbdjJ/IdFZXhnZ4b45w==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          2% (1)\n        <\/td>\n      <\/tr>\n\n<\/table>\n\n\n    <span class=\"value\">98<\/span>% of people liked it\n\n  <table>\n    <tr>\n      <td><strong>All editions:<\/strong><\/td>\n      <td>\n        <span class=\"value\">4.25<\/span> average rating,\n        <span class=\"value\">40<\/span> ratings,\n      