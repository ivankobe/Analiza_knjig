        $('bookCover_507868').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_507868').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
