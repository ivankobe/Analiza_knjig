<div id="review_3963804369" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/3963804369" />
    <a title="Luana ☆" class="left imgcol" href="/user/show/113100603-luana"><img alt="Luana ☆" src="https://images.gr-assets.com/users/1612392680p2/113100603.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/3963804369?book_show_action=true">Apr 25, 2021</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Luana ☆" class="user" itemprop="url" name="Luana ☆" href="/user/show/113100603-luana">Luana ☆</a>
