      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_54968308').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_54968308').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_54968308').observe('prototip:hidden', function () {
