      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_22871').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_22871').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_22871').observe('prototip:hidden', function () {
