        $('edition_3615648').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('edition_3615648').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
