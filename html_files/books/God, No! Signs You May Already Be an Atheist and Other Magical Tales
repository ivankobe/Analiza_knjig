<div id="review_529872814" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/529872814" />
    <a title="Eva" class="left imgcol" href="/user/show/5118691-eva"><img alt="Eva" src="https://images.gr-assets.com/users/1299981178p2/5118691.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/529872814?book_show_action=true">Feb 08, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Eva" class="user" itemprop="url" name="Eva" href="/user/show/5118691-eva">Eva</a>
