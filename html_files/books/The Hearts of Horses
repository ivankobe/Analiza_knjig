<div id="review_12224655" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/12224655" />
    <a title="Felicity" class="left imgcol" href="/user/show/26729-felicity"><img alt="Felicity" src="https://images.gr-assets.com/users/1536640933p2/26729.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/12224655?book_show_action=true">Jan 11, 2008</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Felicity" class="user" itemprop="url" name="Felicity" href="/user/show/26729-felicity">Felicity</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was amazing"><span size="15x15" class="staticStar p10">it was amazing</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/26729-felicity?shelf=historical-fiction">historical-fiction</a>, 
              <a class="actionLinkLite" href="/review/list/26729-felicity?shelf=fiction">fiction</a>, 
              <a class="actionLinkLite" href="/review/list/26729-felicity?shelf=novel">novel</a>, 
              <a class="actionLinkLite" href="/review/list/26729-felicity?shelf=pacific-mfa-faculty">pacific-mfa-faculty</a>, 
              <a class="actionLinkLite" href="/review/list/26729-felicity?shelf=grad-school">grad-school</a>, 
              <a class="actionLinkLite" href="/review/list/26729-felicity?shelf=signed">signed</a>, 
