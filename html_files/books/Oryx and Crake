        $('bookCover_17934530').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_17934530').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
