          $$('div.prototip').each(function (e) {
            if ($('bookCover_847735').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_847735').observe('prototip:hidden', function () {
