<div id="review_2229839019" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2229839019" />
    <a title="Melissa" class="left imgcol" href="/user/show/2272883-melissa"><img alt="Melissa" src="https://images.gr-assets.com/users/1520466466p2/2272883.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2229839019?book_show_action=true">Dec 28, 2017</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Melissa" class="user" itemprop="url" name="Melissa" href="/user/show/2272883-melissa">Melissa</a>
