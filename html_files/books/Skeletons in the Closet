        $('bookCover_30171512').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_30171512').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
