        $('bookCover_853180').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_853180').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
