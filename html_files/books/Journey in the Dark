        $('bookCover_39078166').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_39078166').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
