      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_172691').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_172691').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_172691').observe('prototip:hidden', function () {
