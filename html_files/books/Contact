        $('bookCover_112537').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_112537').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
