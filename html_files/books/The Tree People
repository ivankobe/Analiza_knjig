        $('bookCover_30753857').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_30753857').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
