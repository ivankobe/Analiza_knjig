<div id="review_2837148206" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2837148206" />
    <a title="MrsNightRead" class="left imgcol" href="/user/show/42806644-mrsnightread"><img alt="MrsNightRead" src="https://images.gr-assets.com/users/1533241283p2/42806644.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2837148206?book_show_action=true">May 28, 2019</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="MrsNightRead" class="user" itemprop="url" name="MrsNightRead" href="/user/show/42806644-mrsnightread">MrsNightRead</a>
