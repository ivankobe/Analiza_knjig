<div id="review_2121586846" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2121586846" />
    <a title="Rick" class="left imgcol" href="/user/show/4787962-rick"><img alt="Rick" src="https://images.gr-assets.com/users/1316208823p2/4787962.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2121586846?book_show_action=true">Sep 12, 2017</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Rick" class="user" itemprop="url" name="Rick" href="/user/show/4787962-rick">Rick</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="liked it"><span size="15x15" class="staticStar p10">liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/4787962-rick?shelf=history">history</a>, 
              <a class="actionLinkLite" href="/review/list/4787962-rick?shelf=american-civil-war">american-civil-war</a>, 
              <a class="actionLinkLite" href="/review/list/4787962-rick?shelf=non-fiction">non-fiction</a>, 
