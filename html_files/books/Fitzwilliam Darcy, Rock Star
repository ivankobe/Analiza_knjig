        $('bookCover_46016863').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_46016863').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
