<div id="review_295362022" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/295362022" />
    <a title="Maria Fernanda  Gonzalez" class="left imgcol" href="/user/show/5681033-maria-fernanda-gonzalez"><img alt="Maria Fernanda  Gonzalez" src="https://images.gr-assets.com/users/1373729459p2/5681033.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/295362022?book_show_action=true">Mar 16, 2012</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Maria Fernanda  Gonzalez" class="user" itemprop="url" name="Maria Fernanda  Gonzalez" href="/user/show/5681033-maria-fernanda-gonzalez">Maria Fernanda  Gonzalez</a>
