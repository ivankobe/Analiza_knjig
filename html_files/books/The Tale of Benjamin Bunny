        $('bookCover_39936277').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_39936277').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
