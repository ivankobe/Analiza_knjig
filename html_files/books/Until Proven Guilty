        $('bookCover_127754').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_127754').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
