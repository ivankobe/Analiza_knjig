<div id="review_2514178637" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2514178637" />
    <a title="AC" class="left imgcol" href="/user/show/1655437-ac"><img alt="AC" src="https://images.gr-assets.com/users/1228702469p2/1655437.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2514178637?book_show_action=true">Aug 31, 2018</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="AC" class="user" itemprop="url" name="AC" href="/user/show/1655437-ac">AC</a>
