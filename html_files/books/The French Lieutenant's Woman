        $('bookCover_497221').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_497221').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
