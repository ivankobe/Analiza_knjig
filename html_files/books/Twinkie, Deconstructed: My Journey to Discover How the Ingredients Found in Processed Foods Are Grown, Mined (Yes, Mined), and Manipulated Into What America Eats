        $('bookCover_138188').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_138188').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
