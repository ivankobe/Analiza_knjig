        $('bookCover_27774758').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_27774758').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
