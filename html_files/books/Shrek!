        $('bookCover_388307').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_388307').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
