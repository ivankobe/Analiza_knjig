        $('bookCover_40062839').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_40062839').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
