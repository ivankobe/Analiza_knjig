        $('bookCover_28569').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_28569').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_28569').observe('prototip:hidden', function () {
