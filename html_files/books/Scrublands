        $('bookCover_39122892').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_39122892').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
