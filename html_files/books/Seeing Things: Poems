<div id="review_31551147" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/31551147" />
    <a title="Ian" class="left imgcol" href="/user/show/710618-ian"><img alt="Ian" src="https://images.gr-assets.com/users/1319145663p2/710618.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/31551147?book_show_action=true">Aug 29, 2008</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Ian" class="user" itemprop="url" name="Ian" href="/user/show/710618-ian">Ian</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="liked it"><span size="15x15" class="staticStar p10">liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/710618-ian?shelf=own">own</a>, 
              <a class="actionLinkLite" href="/review/list/710618-ian?shelf=poetry">poetry</a>
          </div>
