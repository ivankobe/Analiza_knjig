      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_13085609').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_13085609').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_13085609').observe('prototip:hidden', function () {
