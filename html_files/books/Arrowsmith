        $('bookCover_221327').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_221327').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
