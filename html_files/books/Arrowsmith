        $('bookCover_543095').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_543095').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
