        $('bookCover_20746724').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_20746724').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_20746724').observe('prototip:hidden', function () {
