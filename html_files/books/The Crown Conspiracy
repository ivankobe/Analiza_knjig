      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_15750692').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_15750692').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_15750692').observe('prototip:hidden', function () {
