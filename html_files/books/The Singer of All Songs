        $('bookCover_7124101').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_7124101').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
