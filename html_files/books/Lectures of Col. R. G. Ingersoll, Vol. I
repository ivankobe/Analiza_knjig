        $('bookCover_22323692').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_22323692').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
