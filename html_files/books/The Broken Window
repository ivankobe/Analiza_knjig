      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_911108').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_911108').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_911108').observe('prototip:hidden', function () {
