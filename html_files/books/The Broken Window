        $('bookCover_2373').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_2373').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_2373').observe('prototip:hidden', function () {
