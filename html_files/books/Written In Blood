<div id="review_3329700833" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/3329700833" />
    <a title="Martha" class="left imgcol" href="/user/show/1529237-martha"><img alt="Martha" src="https://images.gr-assets.com/users/1221495937p2/1529237.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/3329700833?book_show_action=true">May 16, 2020</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Martha" class="user" itemprop="url" name="Martha" href="/user/show/1529237-martha">Martha</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="really liked it"><span size="15x15" class="staticStar p10">really liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/1529237-martha?shelf=fiction">fiction</a>, 
              <a class="actionLinkLite" href="/review/list/1529237-martha?shelf=good-characters">good-characters</a>, 
              <a class="actionLinkLite" href="/review/list/1529237-martha?shelf=british">british</a>, 
              <a class="actionLinkLite" href="/review/list/1529237-martha?shelf=funny">funny</a>, 
