        $('bookCover_493124').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_493124').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
