        $('bookCover_50457896').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_50457896').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
