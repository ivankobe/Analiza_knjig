        $('bookCover_13507967').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_13507967').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
