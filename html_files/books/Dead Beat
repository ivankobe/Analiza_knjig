      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_6122181').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_6122181').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_6122181').observe('prototip:hidden', function () {
