        $('bookCover_7086781').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_7086781').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
