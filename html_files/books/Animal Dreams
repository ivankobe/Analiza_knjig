        $('bookCover_662107').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_662107').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
