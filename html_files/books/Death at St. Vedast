        $('bookCover_56635524').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_56635524').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
