        $('bookCover_26112531').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_26112531').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
