        $('bookCover_1000718').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1000718').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
