        $('bookCover_920795').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_920795').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
