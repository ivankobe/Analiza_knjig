        $('bookCover_9588207').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_9588207').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
