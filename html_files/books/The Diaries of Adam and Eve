        $('bookCover_25015830').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_25015830').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
