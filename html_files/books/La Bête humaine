        $('bookCover_92965').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_92965').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
