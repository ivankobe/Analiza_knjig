        $('bookCover_57587848').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_57587848').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
