<div id="review_573469679" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/573469679" />
    <a title="Diane S ☔" class="left imgcol" href="/user/show/4159922-diane-s"><img alt="Diane S ☔" src="https://images.gr-assets.com/users/1619688680p2/4159922.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/573469679?book_show_action=true">Mar 28, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Diane S ☔" class="user" itemprop="url" name="Diane S ☔" href="/user/show/4159922-diane-s">Diane S ☔</a>
