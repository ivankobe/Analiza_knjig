        $('bookCover_402843').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_402843').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
