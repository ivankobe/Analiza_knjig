        $('bookCover_29917182').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_29917182').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
