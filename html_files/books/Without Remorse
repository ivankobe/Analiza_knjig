        $('bookCover_19675').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_19675').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
