        $('bookCover_27272426').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_27272426').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
