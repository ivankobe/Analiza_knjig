        $('bookCover_26054982').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_26054982').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
