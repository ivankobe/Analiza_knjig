        $('bookCover_383137').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_383137').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
