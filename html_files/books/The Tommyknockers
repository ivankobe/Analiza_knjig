      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_604'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/17660.The_Tommyknockers?rating=\">all <span class=\"greyText\">(208874)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/17660.The_Tommyknockers?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;z1R26HYgGvXUTcFcUWweSscL++fzLzv1PgD21YZboQFDk6D2xo9coHswM6zasdqgxvV0nnhNqSA/gkUx4PHkYg==&#39;)}); return false;\">5 stars <span class=\"greyText\">(30666)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/17660.The_Tommyknockers?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;GsU2QvJhHsckgb5UOzaXxC8tQd2Q5bcqnhG0OYN/lJqWAuBcQs5Ykov8TKSw61MuLtPOpBuHJf+fkwfd5dXR+Q==&#39;)}); return false;\">4 stars <span class=\"greyText\">(39041)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n            