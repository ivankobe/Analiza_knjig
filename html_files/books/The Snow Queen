          $$('div.prototip').each(function (e) {
            if ($('bookCover_30191').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_30191').observe('prototip:hidden', function () {
