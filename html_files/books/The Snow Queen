        $('bookCover_372807').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_372807').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
