        $('bookCover_1033755').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1033755').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
