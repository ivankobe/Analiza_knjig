        $('bookCover_5391115').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_5391115').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
