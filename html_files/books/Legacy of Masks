        $('bookCover_51084454').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_51084454').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
