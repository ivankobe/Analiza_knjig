<div id="review_1958454360" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1958454360" />
    <a title="Ana Yar√≠" class="left imgcol" href="/user/show/32191194-ana-yar"><img alt="Ana Yar√≠" src="https://images.gr-assets.com/users/1434155865p2/32191194.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1958454360?book_show_action=true">Mar 31, 2017</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Ana Yar√≠" class="user" itemprop="url" name="Ana Yar√≠" href="/user/show/32191194-ana-yar">Ana Yar√≠</a>
