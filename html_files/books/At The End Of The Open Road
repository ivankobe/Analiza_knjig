        $('bookCover_289631').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_289631').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
