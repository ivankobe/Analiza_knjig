        $('bookCover_55106109').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_55106109').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
