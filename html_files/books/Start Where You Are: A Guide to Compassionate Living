      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_40078926').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_40078926').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_40078926').observe('prototip:hidden', function () {
