        $('bookCover_84748').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_84748').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
