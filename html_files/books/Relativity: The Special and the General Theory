      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_5553').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_5553').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_5553').observe('prototip:hidden', function () {
