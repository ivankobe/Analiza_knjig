        $('bookCover_30474').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_30474').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
