<div id="review_30673508" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/30673508" />
    <a title="Angela" class="left imgcol" href="/user/show/1415821-angela"><img alt="Angela" src="https://images.gr-assets.com/users/1218471369p2/1415821.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/30673508?book_show_action=true">Aug 20, 2008</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Angela" class="user" itemprop="url" name="Angela" href="/user/show/1415821-angela">Angela</a>
