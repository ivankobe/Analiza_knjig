        $('bookCover_6457334').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_6457334').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
