      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_594970').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_594970').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_594970').observe('prototip:hidden', function () {
