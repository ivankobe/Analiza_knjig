        $('bookCover_7924849').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_7924849').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
