        $('bookCover_13063951').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_13063951').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
