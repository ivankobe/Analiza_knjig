      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_51810799').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_51810799').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_51810799').observe('prototip:hidden', function () {
