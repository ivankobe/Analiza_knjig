<div id="review_197852890" class="review notext" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/197852890" />
    <a title="David" class="left imgcol" href="/user/show/6025307-david"><img alt="David" src="https://images.gr-assets.com/users/1345930637p2/6025307.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="David" class="user" itemprop="url" name="David" href="/user/show/6025307-david">David</a>
