<div id="review_1313153588" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1313153588" />
    <a title="Adrian" class="left imgcol" href="/user/show/22809267-adrian"><img alt="Adrian" src="https://images.gr-assets.com/users/1484242635p2/22809267.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1313153588?book_show_action=true">Jul 22, 2018</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Adrian" class="user" itemprop="url" name="Adrian" href="/user/show/22809267-adrian">Adrian</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was amazing"><span size="15x15" class="staticStar p10">it was amazing</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/22809267-adrian?shelf=sci-fi">sci-fi</a>, 
              <a class="actionLinkLite" href="/review/list/22809267-adrian?shelf=sf-time-travel">sf-time-travel</a>, 
              <a class="actionLinkLite" href="/review/list/22809267-adrian?shelf=favorites">favorites</a>, 
              <a class="actionLinkLite" href="/review/list/22809267-adrian?shelf=zz-owned-books">zz-owned-books</a>, 
              <a class="actionLinkLite" href="/review/list/22809267-adrian?shelf=z-goodreads-authors">z-goodreads-authors</a>
          </div>
