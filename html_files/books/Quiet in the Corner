        $('bookCover_50525983').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_50525983').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
