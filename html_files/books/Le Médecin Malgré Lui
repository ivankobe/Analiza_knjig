        $('bookCover_6333785').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_6333785').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
