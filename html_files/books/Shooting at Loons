  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1126278392?book_show_action=true">Dec 07, 2014</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Sandra" class="user" itemprop="url" name="Sandra" href="/user/show/330479-sandra">Sandra</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was amazing"><span size="15x15" class="staticStar p10">it was amazing</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/330479-sandra?shelf=books-read-in-2014">books-read-in-2014</a>, 
              <a class="actionLinkLite" href="/review/list/330479-sandra?shelf=library-books">library-books</a>, 
              <a class="actionLinkLite" href="/review/list/330479-sandra?shelf=3">3</a>, 
              <a class="actionLinkLite" href="/review/list/330479-sandra?shelf=boats">boats</a>, 
