        $('bookCover_165398').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_165398').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
