<div id="review_2614525570" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2614525570" />
    <a title="Tooter" class="left imgcol" href="/user/show/9469263-tooter"><img alt="Tooter" src="https://images.gr-assets.com/users/1544666220p2/9469263.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2614525570?book_show_action=true">Dec 08, 2018</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Tooter" class="user" itemprop="url" name="Tooter" href="/user/show/9469263-tooter">Tooter</a>
