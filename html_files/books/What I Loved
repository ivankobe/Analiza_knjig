<div id="review_1118411499" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1118411499" />
    <a title="Dem" class="left imgcol" href="/user/show/2752994-dem"><img alt="Dem" src="https://images.gr-assets.com/users/1552868566p2/2752994.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1118411499?book_show_action=true">Nov 29, 2014</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Dem" class="user" itemprop="url" name="Dem" href="/user/show/2752994-dem">Dem</a>
