      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_3575676').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_3575676').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_3575676').observe('prototip:hidden', function () {
