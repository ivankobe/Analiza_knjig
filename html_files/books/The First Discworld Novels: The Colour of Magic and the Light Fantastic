      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_34503').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_34503').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_34503').observe('prototip:hidden', function () {
