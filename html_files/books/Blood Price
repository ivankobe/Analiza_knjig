        $('bookCover_34466816').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_34466816').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
