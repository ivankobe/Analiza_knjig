<div id="review_1373694713" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1373694713" />
    <a title="Paige  Bookdragon" class="left imgcol" href="/user/show/15550986-paige-bookdragon"><img alt="Paige  Bookdragon" src="https://images.gr-assets.com/users/1570121828p2/15550986.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1373694713?book_show_action=true">Aug 25, 2015</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Paige  Bookdragon" class="user" itemprop="url" name="Paige  Bookdragon" href="/user/show/15550986-paige-bookdragon">Paige  Bookdragon</a>
