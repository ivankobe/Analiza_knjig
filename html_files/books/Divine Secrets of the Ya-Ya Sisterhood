        $('bookCover_23219').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_23219').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
