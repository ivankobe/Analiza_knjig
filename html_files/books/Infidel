<div id="review_3261895352" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/3261895352" />
    <a title="Rachelle" class="left imgcol" href="/user/show/21507709-rachelle"><img alt="Rachelle" src="https://images.gr-assets.com/users/1371780287p2/21507709.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/3261895352?book_show_action=true">May 30, 2020</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Rachelle" class="user" itemprop="url" name="Rachelle" href="/user/show/21507709-rachelle">Rachelle</a>
      </span>

