        $('bookCover_107783').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_107783').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
