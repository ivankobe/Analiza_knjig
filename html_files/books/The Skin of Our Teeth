        $('bookCover_764270').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_764270').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
