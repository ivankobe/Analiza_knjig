        $('bookCover_107611').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_107611').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
