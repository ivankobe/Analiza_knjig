<div id="review_1915267767" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1915267767" />
    <a title="Barbara" class="left imgcol" href="/user/show/893768-barbara"><img alt="Barbara" src="https://images.gr-assets.com/users/1609828359p2/893768.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1915267767?book_show_action=true">Feb 16, 2017</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Barbara" class="user" itemprop="url" name="Barbara" href="/user/show/893768-barbara">Barbara</a>
