        $('bookCover_5291841').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_5291841').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
