  <div class="friendReviews elementListBrown" >
    <div class="section">
      

<div id="review_650648701" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/650648701" />
    <a title="Anne" class="left imgcol" href="/user/show/1720620-anne"><img alt="Anne" src="https://images.gr-assets.com/users/1415374035p2/1720620.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/650648701?book_show_action=true">Apr 04, 2020</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Anne" class="user" itemprop="url" name="Anne" href="/user/show/1720620-anne">Anne</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was ok"><span size="15x15" class="staticStar p10">it was ok</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>

          <span class="greyText">&nbsp;&middot;&nbsp;</span>
          <a class="lightGreyText" title="Audiobook 9781624068676" href="/book/show/18657819-the-5th-wave">review of another edition</a>


        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/1720620-anne?shelf=read-in-2020">read-in-2020</a>, 
              <a class="actionLinkLite" href="/review/list/1720620-anne?shelf=coronavirus-read">coronavirus-read</a>, 
              <a class="actionLinkLite" href="/review/list/1720620-anne?shelf=young-adult">young-adult</a>, 
              <a class="actionLinkLite" href="/review/list/1720620-anne?shelf=sci-fi">sci-fi</a>, 
              <a class="actionLinkLite" href="/review/list/1720620-anne?shelf=audio">audio</a>, 
