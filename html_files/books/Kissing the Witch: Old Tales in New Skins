      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_58'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/44537.Kissing_the_Witch?rating=\">all <span class=\"greyText\">(25493)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/44537.Kissing_the_Witch?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;GWRK3qNNsN2clKFn15O0bWHgOPPXYq2PrUOffy9IRuQcdeHNG/By4BWKzE6l1Hq9TsswQB1j+noyWMjlsK8gPg==&#39;)}); return false;\">5 stars <span class=\"greyText\">(2356)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/44537.Kissing_the_Witch?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;er9PPm8k0xjZThHqoe9/ppZv2/YKs7C3n7AzK6tGw0N/ruQt15kRJVBQfMPTqLF2uUTTRcCy50IAq2SxNKGlmQ==&#39;)}); return false;\">4 stars <span class=\"greyText\">(2492)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                