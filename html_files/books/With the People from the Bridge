      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_50401507').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_50401507').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_50401507').observe('prototip:hidden', function () {
