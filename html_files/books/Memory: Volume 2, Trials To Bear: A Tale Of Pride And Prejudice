        $('bookCover_31548368').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_31548368').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
