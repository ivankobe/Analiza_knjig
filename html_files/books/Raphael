        $('bookCover_18623052').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_18623052').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
