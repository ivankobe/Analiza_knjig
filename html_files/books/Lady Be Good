        $('bookCover_420739').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_420739').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
