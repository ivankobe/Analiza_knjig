      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_7'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/73090.Lady_Be_Good?rating=\">all <span class=\"greyText\">(24632)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/73090.Lady_Be_Good?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;wAtnD7ZOw53pFoMHX5I6JMKoNgPXWUV1toaToBurKX0Zlv8hFQ/VeBH6jw8cbK0pCaVCkOD3udO3K3qyTCVw5A==&#39;)}); return false;\">5 stars <span class=\"greyText\">(5713)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/73090.Lady_Be_Good?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;Vm9tZILpXNdgEMlw1msSHZBUh5P6gVeiw7Vc085ilc+P8vVKIahKMpj8xXiVlYUQW1nzAM0vqwTCGLXBmezMVg==&#39;)}); return false;\">4 stars <span class=\"greyText\">(6256)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"