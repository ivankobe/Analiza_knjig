        $('bookCover_32689').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_32689').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
