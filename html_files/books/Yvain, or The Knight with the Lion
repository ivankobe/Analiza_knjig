        $('bookCover_28178840').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_28178840').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_28178840').observe('prototip:hidden', function () {
