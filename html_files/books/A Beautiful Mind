        $('bookCover_9742').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_9742').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
