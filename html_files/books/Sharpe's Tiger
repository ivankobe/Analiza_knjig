        $('bookCover_90951').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_90951').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
