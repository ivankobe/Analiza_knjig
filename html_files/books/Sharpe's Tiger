        $('bookCover_56312378').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_56312378').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
