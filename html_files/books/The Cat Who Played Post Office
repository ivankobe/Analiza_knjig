<div id="review_100439277" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/100439277" />
    <a title="Writerlibrarian" class="left imgcol" href="/user/show/116025-writerlibrarian"><img alt="Writerlibrarian" src="https://images.gr-assets.com/users/1180996006p2/116025.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/100439277?book_show_action=true">Apr 28, 2010</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Writerlibrarian" class="user" itemprop="url" name="Writerlibrarian" href="/user/show/116025-writerlibrarian">Writerlibrarian</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was ok"><span size="15x15" class="staticStar p10">it was ok</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/116025-writerlibrarian?shelf=re-read">re-read</a>, 
              <a class="actionLinkLite" href="/review/list/116025-writerlibrarian?shelf=read2010">read2010</a>, 
              <a class="actionLinkLite" href="/review/list/116025-writerlibrarian?shelf=mystery">mystery</a>, 
              <a class="actionLinkLite" href="/review/list/116025-writerlibrarian?shelf=april-pile">april-pile</a>, 
              <a class="actionLinkLite" href="/review/list/116025-writerlibrarian?shelf=readbefore2005">readbefore2005</a>
