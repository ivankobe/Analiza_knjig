        $('bookCover_194642').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_194642').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
