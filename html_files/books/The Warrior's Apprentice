        $('bookCover_1197129').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1197129').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
