      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_45892271').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_45892271').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_45892271').observe('prototip:hidden', function () {
