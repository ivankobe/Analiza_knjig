<div id="review_2726527484" class="review notext nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2726527484" />
    <a title="Amanda" class="left imgcol" href="/user/show/93987666-amanda"><img alt="Amanda" src="https://images.gr-assets.com/users/1550933536p2/93987666.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Amanda" class="user" itemprop="url" name="Amanda" href="/user/show/93987666-amanda">Amanda</a>
