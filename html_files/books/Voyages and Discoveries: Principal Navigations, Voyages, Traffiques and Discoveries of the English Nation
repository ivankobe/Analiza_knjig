        $('bookCover_31566586').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_31566586').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
