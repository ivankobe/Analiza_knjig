        $('bookCover_30282181').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_30282181').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
