        $('bookCover_56331126').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_56331126').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
