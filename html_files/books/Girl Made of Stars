        $('bookCover_49644532').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_49644532').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
