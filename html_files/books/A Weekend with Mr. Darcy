        $('bookCover_56885313').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_56885313').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
