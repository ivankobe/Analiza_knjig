        $('bookCover_50891314').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_50891314').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
