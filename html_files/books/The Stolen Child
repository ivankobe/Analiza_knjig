<div id="review_39851558" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/39851558" />
    <a title="Minakshi" class="left imgcol" href="/user/show/351702-minakshi"><img alt="Minakshi" src="https://images.gr-assets.com/users/1189384162p2/351702.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/39851558?book_show_action=true">Dec 11, 2008</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Minakshi" class="user" itemprop="url" name="Minakshi" href="/user/show/351702-minakshi">Minakshi</a>
