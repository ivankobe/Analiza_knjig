<div id="review_1935673199" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1935673199" />
    <a title="Amber Scaife" class="left imgcol" href="/user/show/64987691-amber-scaife"><img alt="Amber Scaife" src="https://images.gr-assets.com/users/1524508473p2/64987691.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1935673199?book_show_action=true">Mar 19, 2017</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Amber Scaife" class="user" itemprop="url" name="Amber Scaife" href="/user/show/64987691-amber-scaife">Amber Scaife</a>
