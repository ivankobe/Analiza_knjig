        $('bookCover_26079330').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_26079330').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
