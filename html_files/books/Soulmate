        $('bookCover_11262424').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_11262424').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
