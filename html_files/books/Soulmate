<div id="review_2571328635" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2571328635" />
    <a title="melhara" class="left imgcol" href="/user/show/40153615-melhara"><img alt="melhara" src="https://images.gr-assets.com/users/1560348039p2/40153615.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2571328635?book_show_action=true">Oct 23, 2018</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="melhara" class="user" itemprop="url" name="melhara" href="/user/show/40153615-melhara">melhara</a>
