<div id="review_52825566" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/52825566" />
    <a title="Anna" class="left imgcol" href="/user/show/938428-anna"><img alt="Anna" src="https://images.gr-assets.com/users/1263947049p2/938428.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/52825566?book_show_action=true">Apr 15, 2009</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Anna" class="user" itemprop="url" name="Anna" href="/user/show/938428-anna">Anna</a>
      </span>

