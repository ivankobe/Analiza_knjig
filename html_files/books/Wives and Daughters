      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_514761').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_514761').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_514761').observe('prototip:hidden', function () {
