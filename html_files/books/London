        $('bookCover_23476412').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_23476412').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
