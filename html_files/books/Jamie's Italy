        $('bookCover_8185785').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_8185785').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
