      <a class="uitext" rel="nofollow" id="span_class_gr-hyperlink_more_filters_span_tip_956" style="cursor: pointer; " href="#" onclick="return false"><span class="gr-hyperlink">More filters</span></a><script>
//<![CDATA[
      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_956'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/4285707-the-one-minute-manager?rating=\">all <span class=\"greyText\">(204672)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/4285707-the-one-minute-manager?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;fhE7QdURzs6XojaLWGdfOqFuxwwUA8LGZyCjlOzZ9AqEiLrsD1hvVp+XMwJpj/LtJPG5b928hyGB3qFulMZS9Q==&#39;)}); return false;\">5 stars <span class=\"greyText\">(39673)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/4285707-the-one-minute-manager?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;CyldCeELB+50VsVyD3VojWzCkSOY5OcoXcDAL749d7zxsNykO0KmdnxjwPs+ncVa6V3vQFFbos+7PsLVxiLRQw==&#39;)}); return false;\">4 stars <span class=\"greyText\">(34735)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><