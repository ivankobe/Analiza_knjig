        $('bookCover_72695').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_72695').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
