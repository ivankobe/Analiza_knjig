        $('bookCover_17901366').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_17901366').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
