  <div class="friendReviews elementListBrown" >
    <div class="section">
      

<div id="review_2023490610" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2023490610" />
    <a title="Abdullah" class="left imgcol" href="/user/show/7433452-abdullah"><img alt="Abdullah" src="https://images.gr-assets.com/users/1326631366p2/7433452.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2023490610?book_show_action=true">Jun 10, 2017</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Abdullah" class="user" itemprop="url" name="Abdullah" href="/user/show/7433452-abdullah">Abdullah</a>
      </span>

