        $('bookCover_45019183').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_45019183').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
