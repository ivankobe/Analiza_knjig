        $('bookCover_45339077').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_45339077').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
