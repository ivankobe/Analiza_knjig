        $('bookCover_389189').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_389189').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
