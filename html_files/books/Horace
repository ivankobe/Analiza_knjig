        $('bookCover_354364').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_354364').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
