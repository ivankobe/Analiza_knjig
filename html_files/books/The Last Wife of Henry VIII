        $('bookCover_6282683').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_6282683').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
