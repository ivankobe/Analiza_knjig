        $('edition_1243554').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('edition_1243554').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
