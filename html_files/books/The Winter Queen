<div id="review_3071592578" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/3071592578" />
    <a title="BrokenTune" class="left imgcol" href="/user/show/10592271-brokentune"><img alt="BrokenTune" src="https://images.gr-assets.com/users/1391017756p2/10592271.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/3071592578?book_show_action=true">Jan 09, 2020</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="BrokenTune" class="user" itemprop="url" name="BrokenTune" href="/user/show/10592271-brokentune">BrokenTune</a>
