        $('bookCover_359410').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_359410').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
