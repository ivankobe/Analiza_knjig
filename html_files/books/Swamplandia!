<div id="review_259305826" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/259305826" />
    <a title="Jen" class="left imgcol" href="/user/show/967285-jen"><img alt="Jen" src="https://images.gr-assets.com/users/1572299819p2/967285.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/259305826?book_show_action=true">Jan 09, 2012</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Jen" class="user" itemprop="url" name="Jen" href="/user/show/967285-jen">Jen</a>
