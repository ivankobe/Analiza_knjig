      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_998'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/45556.Co_Aytch?rating=\">all <span class=\"greyText\">(6272)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/45556.Co_Aytch?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;/xpWwseyHr+brOYV5qo7JuKWTvqC0UrbpETropSwGlo3RxVcSQZx+Qg0MQHq3uHYrjRNWES8Q7/y892BzzFF3A==&#39;)}); return false;\">5 stars <span class=\"greyText\">(1607)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/45556.Co_Aytch?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;Ot9lDaeJHxNPL/c9n0idYYSnMDSRzNxxCl+iGE+9HMjygiaTKT1wVdy3ICmTPEefyAUzlleh1RVc6JQ7FDxDTg==&#39;)}); return false;\">4 stars <span class=\"greyText\">(1141)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingErro