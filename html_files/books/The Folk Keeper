        $('bookCover_43238534').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_43238534').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
