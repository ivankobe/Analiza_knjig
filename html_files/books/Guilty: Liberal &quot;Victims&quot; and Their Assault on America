<div id="review_43504439" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/43504439" />
    <a title="Annie" class="left imgcol" href="/user/show/1643369-annie"><img alt="Annie" src="https://s.gr-assets.com/assets/nophoto/user/f_50x66-6a03a5c12233c941481992b82eea8d23.png" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/43504439?book_show_action=true">Jan 18, 2009</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Annie" class="user" itemprop="url" name="Annie" href="/user/show/1643369-annie">Annie</a>
      </span>

