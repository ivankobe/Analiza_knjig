      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_76'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/37415.Their_Eyes_Were_Watching_God?rating=\">all <span class=\"greyText\">(591571)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/37415.Their_Eyes_Were_Watching_God?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;5gJO1UoonNRtD3LIqB2mSlzfkGYIm++ylXI5XhTGe3H3bzD1sdWtgTL+OeIiZPCAyDEQzXNO9u+IB0gYX4lFiQ==&#39;)}); return false;\">5 stars <span class=\"greyText\">(100718)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/37415.Their_Eyes_Were_Watching_God?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;vpFQrJgyHQ6EgXO/j4ULrWjPUjxt3huJdVr9CDkDVbSv/C6MY88sW9twOJUF/F1n/CHSlxYLAtRoL4xOckxrTA==&#39;)}); return false;\">4 stars <span class=\"greyText\">(102856)<\/span><\/a>\n                  <span class=\"loading