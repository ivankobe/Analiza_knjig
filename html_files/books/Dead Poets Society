          $$('div.prototip').each(function (e) {
            if ($('bookCover_13912').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_13912').observe('prototip:hidden', function () {
