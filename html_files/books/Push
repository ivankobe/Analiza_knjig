        $('bookCover_640486').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_640486').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
