    <div class="js-dataTooltip" data-use-wtr-tooltip='true'>
        <div class="js-tooltipTrigger tooltipTrigger" data-resource-type='Book' data-resource-id='11297' data-placement='bottom-left'>
          <a href="/book/show/11297.Norwegian_Wood"><img id="more_book_11297" alt="Norwegian Wood" title="Norwegian Wood" width="50" src="https://i.gr-assets.com/images/S/compressed.photo.goodreads.com/books/1386924361l/11297._SX98_.jpg" /></a>
        </div>
        <div class="js-tooltipTrigger tooltipTrigger" data-resource-type='Book' data-resource-id='11275' data-placement='bottom-left'>
          <a href="/book/show/11275.The_Wind_Up_Bird_Chronicle"><img id="more_book_11275" alt="The Wind-Up Bird Chronicle" title="The Wind-Up Bird Chronicle" width="50" src="https://i.gr-assets.com/images/S/compressed.photo.goodreads.com/books/1327872639l/11275._SX98_.jpg" /></a>
        </div>
        <div class="js-tooltipTrigger tooltipTrigger" data-resource-type='Book' data-resource-id='10357575' data-placement='bottom-left'>
