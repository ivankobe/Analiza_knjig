        $('bookCover_486444').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_486444').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
