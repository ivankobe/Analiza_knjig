  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2418495061?book_show_action=true">Jun 10, 2018</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Ailsa" class="user" itemprop="url" name="Ailsa" href="/user/show/6117156-ailsa">Ailsa</a>
