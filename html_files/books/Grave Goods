        $('bookCover_372260').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_372260').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
