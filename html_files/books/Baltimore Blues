      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_902496').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_902496').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_902496').observe('prototip:hidden', function () {
