        $('bookCover_239102').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_239102').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
