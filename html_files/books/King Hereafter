<div id="review_963799914" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/963799914" />
    <a title="Jane" class="left imgcol" href="/user/show/7177008-jane"><img alt="Jane" src="https://images.gr-assets.com/users/1388366904p2/7177008.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/963799914?book_show_action=true">Jun 10, 2014</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Jane" class="user" itemprop="url" name="Jane" href="/user/show/7177008-jane">Jane</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was ok"><span size="15x15" class="staticStar p10">it was ok</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/7177008-jane?shelf=scandinavia">scandinavia</a>, 
              <a class="actionLinkLite" href="/review/list/7177008-jane?shelf=reviewed">reviewed</a>, 
              <a class="actionLinkLite" href="/review/list/7177008-jane?shelf=middle-ages">middle-ages</a>, 
              <a class="actionLinkLite" href="/review/list/7177008-jane?shelf=library">library</a>, 
              <a class="actionLinkLite" href="/review/list/7177008-jane?shelf=scotland">scotland</a>
          </div>
