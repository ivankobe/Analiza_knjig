          $$('div.prototip').each(function (e) {
            if ($('bookCover_509529').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_509529').observe('prototip:hidden', function () {
