        $('bookCover_53484679').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_53484679').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
