        $('bookCover_84156').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_84156').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
