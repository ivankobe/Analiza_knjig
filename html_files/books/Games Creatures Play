          $$('div.prototip').each(function (e) {
            if ($('bookCover_17902184').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_17902184').observe('prototip:hidden', function () {
