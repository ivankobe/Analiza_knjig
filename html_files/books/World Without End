      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_68527').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_68527').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_68527').observe('prototip:hidden', function () {
