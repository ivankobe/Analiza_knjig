      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_55481226').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_55481226').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_55481226').observe('prototip:hidden', function () {
