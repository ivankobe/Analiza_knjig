      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_55285911').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_55285911').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_55285911').observe('prototip:hidden', function () {
