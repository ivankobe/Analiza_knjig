        $('edition_6480875').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('edition_6480875').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
