<div id="review_2264548609" class="review notext nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2264548609" />
    <a title="josh" class="left imgcol" href="/user/show/10616814-josh"><img alt="josh" src="https://images.gr-assets.com/users/1617847130p2/10616814.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="josh" class="user" itemprop="url" name="josh" href="/user/show/10616814-josh">josh</a>
