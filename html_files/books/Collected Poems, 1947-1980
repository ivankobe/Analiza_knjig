        $('bookCover_15869004').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_15869004').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
