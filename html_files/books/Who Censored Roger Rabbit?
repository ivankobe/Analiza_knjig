        $('bookCover_43837995').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_43837995').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
