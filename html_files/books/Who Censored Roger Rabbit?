        $('bookCover_312489').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_312489').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
