        $('bookCover_116918').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_116918').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
