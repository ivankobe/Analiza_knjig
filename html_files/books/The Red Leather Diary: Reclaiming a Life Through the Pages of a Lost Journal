      <a class="uitext" rel="nofollow" id="span_class_gr-hyperlink_more_filters_span_tip_560" style="cursor: pointer; " href="#" onclick="return false"><span class="gr-hyperlink">More filters</span></a><script>
//<![CDATA[
      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_560'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/1704570.The_Red_Leather_Diary?rating=\">all <span class=\"greyText\">(6951)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/1704570.The_Red_Leather_Diary?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;sARI9kWUkptjz5pEGE/RzfFBK//gj8lEnpe2hBhYQVBRHFrk+cZmeVt6nu13snTsoOAIhD8HQ8MDYHioG8qxeA==&#39;)}); return false;\">5 stars <span class=\"greyText\">(603)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/1704570.The_Red_Leather_Diary?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;Y394SRQrQdvksDuykt3Pz/5wh5FqxbBFh94FeLQaHMqCZ2pbqHm1OdwFPxv9IGrur9Gk6rVNOsIaKctUt4js4g==&#39;)}); return false;\">4 stars <span class=\"greyText\">(976)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n