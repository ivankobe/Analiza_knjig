        $('bookCover_13330664').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_13330664').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
