  <div class="friendReviews elementListBrown" >
    <div class="section">
      

<div id="review_3334237947" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/3334237947" />
    <a title="Baba" class="left imgcol" href="/user/show/60866073-baba"><img alt="Baba" src="https://images.gr-assets.com/users/1532961539p2/60866073.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/3334237947?book_show_action=true">May 13, 2020</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Baba" class="user" itemprop="url" name="Baba" href="/user/show/60866073-baba">Baba</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="liked it"><span size="15x15" class="staticStar p10">liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>

