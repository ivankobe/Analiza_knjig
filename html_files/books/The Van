        $('bookCover_8538501').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_8538501').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
