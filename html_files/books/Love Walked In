      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_48721353').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_48721353').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_48721353').observe('prototip:hidden', function () {
