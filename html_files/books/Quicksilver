        $('bookCover_16243767').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_16243767').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
