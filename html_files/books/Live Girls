        $('bookCover_51083453').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_51083453').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
