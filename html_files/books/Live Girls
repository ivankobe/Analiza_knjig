        $('bookCover_33670466').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_33670466').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
