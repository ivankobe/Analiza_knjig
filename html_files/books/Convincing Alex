<div id="review_2890141102" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2890141102" />
    <a title="Jamoz23" class="left imgcol" href="/user/show/30896044-jamoz23"><img alt="Jamoz23" src="https://images.gr-assets.com/users/1556509347p2/30896044.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2890141102?book_show_action=true">Jul 09, 2019</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Jamoz23" class="user" itemprop="url" name="Jamoz23" href="/user/show/30896044-jamoz23">Jamoz23</a>
