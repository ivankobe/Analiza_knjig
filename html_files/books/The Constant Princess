        $('bookCover_11806563').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_11806563').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
