            if ($('bookCover_106198').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_106198').observe('prototip:hidden', function () {
