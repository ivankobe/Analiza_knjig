  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2301905272?book_show_action=true">Mar 25, 2018</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Britany" class="user" itemprop="url" name="Britany" href="/user/show/5550397-britany">Britany</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="really liked it"><span size="15x15" class="staticStar p10">really liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/5550397-britany?shelf=2018">2018</a>, 
              <a class="actionLinkLite" href="/review/list/5550397-britany?shelf=books-to-film">books-to-film</a>, 
              <a class="actionLinkLite" href="/review/list/5550397-britany?shelf=classics">classics</a>, 
              <a class="actionLinkLite" href="/review/list/5550397-britany?shelf=mmd">mmd</a>
          </div>
