        $('bookCover_31503').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_31503').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
