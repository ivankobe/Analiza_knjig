        $('bookCover_62794').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_62794').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
