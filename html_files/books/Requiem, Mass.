        $('bookCover_68520').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_68520').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
