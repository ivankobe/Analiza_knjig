        $j("#reviewControls__ratingDetailsMiniGraph").prependTo("#rating_details_tip");
      });
    //]]>
    </script>
    </span>
    <a id="rating_details_tip" class="gr-hyperlink" style="cursor: pointer; " href="#" onclick="return false"> Rating details</a><script>
//<![CDATA[
      var newTip = new Tip($('rating_details_tip'), "\n        <div id=\"moreBookData\" class=\"bookMeta\" >\n  \n\n  \n\n<table border=\"0\" id=\"rating_distribution\" style=\"width: auto; max-width: 500px;\" cellspacing=\"5\" cellpadding=\"0\">\n      <tr>\n        <th width=\"25\" class=\"right\">5&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 48%;\" title=\"51 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/2234847.Requiem_Mass_?rating=5\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'zF1nlRhaTl7kG94KDGyM8kiX8PPkqO258LqlzqcjMs4XUUu8RebneWe9ehXAJ5pVusESqQKXIloNfj0sfquNNA==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          16% (51)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">4&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 87%;\" title=\"92 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/2234847.Requiem_Mass_?rating=4\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'4BcVoMk53x2lN+ZsM9mJhoZzDiUFn/oWfSacWtMSns87GzmJlIV2OiaRQnP/kp8hdCXsf+OgNfWA4gS4CpohNQ==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          28% (92)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">3&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 100%;\" title=\"105 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/2234847.Requiem_Mass_?rating=3\', {asynchronous:t