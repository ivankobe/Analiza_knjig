      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_39325105').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_39325105').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_39325105').observe('prototip:hidden', function () {
