        $('bookCover_952164').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_952164').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
