        $('bookCover_185012').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_185012').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
