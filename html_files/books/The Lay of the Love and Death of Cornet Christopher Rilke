        $('bookCover_200531').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_200531').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
