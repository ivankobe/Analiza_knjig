        $('bookCover_704043').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_704043').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_704043').observe('prototip:hidden', function () {
