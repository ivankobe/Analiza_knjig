      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('edition_1437990').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('edition_1437990').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('edition_1437990').observe('prototip:hidden', function () {
