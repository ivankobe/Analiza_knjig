<div id="review_1786463760" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1786463760" />
    <a title="Alice Porter" class="left imgcol" href="/user/show/58863158-alice-porter"><img alt="Alice Porter" src="https://images.gr-assets.com/users/1471833972p2/58863158.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1786463760?book_show_action=true">Oct 16, 2016</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Alice Porter" class="user" itemprop="url" name="Alice Porter" href="/user/show/58863158-alice-porter">Alice Porter</a>
