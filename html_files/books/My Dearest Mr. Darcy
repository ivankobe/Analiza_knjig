        $('bookCover_36311136').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_36311136').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_36311136').observe('prototip:hidden', function () {
