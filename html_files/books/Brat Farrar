        $('bookCover_660277').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_660277').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_660277').observe('prototip:hidden', function () {
