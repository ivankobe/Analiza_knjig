        $('bookCover_201548').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_201548').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
