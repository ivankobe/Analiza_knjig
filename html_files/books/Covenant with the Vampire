        $('bookCover_11901').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_11901').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
