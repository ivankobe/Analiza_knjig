        $('bookCover_51186057').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_51186057').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
