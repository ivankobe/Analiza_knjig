        $('bookCover_16130366').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_16130366').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
