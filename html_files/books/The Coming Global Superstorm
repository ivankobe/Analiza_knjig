        $('bookCover_437421').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_437421').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
