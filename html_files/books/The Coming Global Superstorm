        $('bookCover_79').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_79').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
