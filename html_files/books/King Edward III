        $('bookCover_1990').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1990').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
