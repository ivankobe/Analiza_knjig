        $('bookCover_9408479').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_9408479').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
