      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_956'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/134025.The_Complete_English_Poems?rating=\">all <span class=\"greyText\">(16638)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/134025.The_Complete_English_Poems?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;QFG2gCLoGbDfyGIYTWF2CfSQuMvYsKa5CeSGf8ItZzgO977sw34BA5jRD3hzcJ/vnsNiIFQ5frCwDFxlC1mivQ==&#39;)}); return false;\">5 stars <span class=\"greyText\">(4323)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/134025.The_Complete_English_Poems?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;U33ni+NyoyKORWAwMSQA9PbOvNaWg/qGWvNSFlKT2qId2+/nAuS7kclcDVAPNekSnJ1mPRoKIo/jG4gMm+cfJw==&#39;)}); return false;\">4 stars <span class=\"greyText\">(2975)<\/span><\/a>\n                  <span class=\"loadingAnimati