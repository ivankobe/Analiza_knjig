<div id="review_602296523" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/602296523" />
    <a title="Sara" class="left imgcol" href="/user/show/3136341-sara"><img alt="Sara" src="https://images.gr-assets.com/users/1263166097p2/3136341.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/602296523?book_show_action=true">Apr 28, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Sara" class="user" itemprop="url" name="Sara" href="/user/show/3136341-sara">Sara</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="liked it"><span size="15x15" class="staticStar p10">liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>



        

