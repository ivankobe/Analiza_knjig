      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_6259').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_6259').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_6259').observe('prototip:hidden', function () {
