        $('bookCover_249050').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_249050').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
