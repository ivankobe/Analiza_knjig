        $('bookCover_27735009').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_27735009').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
