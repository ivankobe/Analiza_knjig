        $('bookCover_53754459').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_53754459').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
