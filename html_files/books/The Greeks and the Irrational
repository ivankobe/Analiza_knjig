        $('bookCover_752900').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_752900').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
