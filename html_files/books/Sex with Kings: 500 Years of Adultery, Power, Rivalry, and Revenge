        $('bookCover_401495').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_401495').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
