        $('bookCover_15840188').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_15840188').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
