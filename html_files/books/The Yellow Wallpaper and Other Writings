      <a class="uitext" rel="nofollow" id="span_class_gr-hyperlink_more_filters_span_tip_131" style="cursor: pointer; " href="#" onclick="return false"><span class="gr-hyperlink">More filters</span></a><script>
//<![CDATA[
      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_131'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/99302.The_Yellow_Wallpaper_and_Other_Writings?rating=\">all <span class=\"greyText\">(149050)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/99302.The_Yellow_Wallpaper_and_Other_Writings?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;+hz6s9wvc+cTyBlXqV3ALPnLhK+qpLcWmRJDQMtBJWLyKSr0HYK1G5QpyjUqaONHsb4L/wuK5JeswJiRRZp7pg==&#39;)}); return false;\">5 stars <span class=\"greyText\">(31101)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/99302.The_Yellow_Wallpaper_and_Other_Writings?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;KMdT5JeTOFNEiuI9TyAUi9J7hOSq9SgD+HafMnwrwJ4g8oOjVj7+r8NrMV/MFTfgmg4LtAvbe4LNpETj8vCeWg==&#39;)}); return false;\">4 stars <span class=\"greyText\">(30304)<\/span><\/a>\n       