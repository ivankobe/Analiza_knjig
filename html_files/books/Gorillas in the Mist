        $('bookCover_546693').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_546693').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
