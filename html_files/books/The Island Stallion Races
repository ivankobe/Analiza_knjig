        $('bookCover_17461').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_17461').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
