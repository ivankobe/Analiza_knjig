        $('bookCover_9888775').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_9888775').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
