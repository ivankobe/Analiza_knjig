<div id="review_28175078" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/28175078" />
    <a title="Deborah" class="left imgcol" href="/user/show/1343061-deborah"><img alt="Deborah" src="https://images.gr-assets.com/users/1262488582p2/1343061.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/28175078?book_show_action=true">Jul 24, 2008</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Deborah" class="user" itemprop="url" name="Deborah" href="/user/show/1343061-deborah">Deborah</a>
