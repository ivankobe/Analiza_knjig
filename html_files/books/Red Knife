      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Teresa" class="user" itemprop="url" name="Teresa" href="/user/show/7793520-teresa">Teresa</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="really liked it"><span size="15x15" class="staticStar p10">really liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/7793520-teresa?shelf=detective">detective</a>, 
              <a class="actionLinkLite" href="/review/list/7793520-teresa?shelf=drama">drama</a>, 
              <a class="actionLinkLite" href="/review/list/7793520-teresa?shelf=male-pov">male-pov</a>, 
              <a class="actionLinkLite" href="/review/list/7793520-teresa?shelf=dark">dark</a>, 
              <a class="actionLinkLite" href="/review/list/7793520-teresa?shelf=crime">crime</a>, 
              <a class="actionLinkLite" href="/review/list/7793520-teresa?shelf=mystery">mystery</a>
          </div>
