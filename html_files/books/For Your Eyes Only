        $('bookCover_177194').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_177194').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
