        $('bookCover_18698127').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_18698127').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
