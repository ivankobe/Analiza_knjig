        $('bookCover_34550311').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_34550311').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_34550311').observe('prototip:hidden', function () {
