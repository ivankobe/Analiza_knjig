        $('bookCover_374148').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_374148').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
