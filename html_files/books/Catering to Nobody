        $('bookCover_376256').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_376256').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
