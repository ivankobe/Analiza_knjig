        $('bookCover_25621455').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_25621455').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
