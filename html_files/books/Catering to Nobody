        $('bookCover_18811178').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_18811178').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
