        $$('span.elementTwo').each(function (e) {
          if (e.getStyle('display') !== 'none') {
            var lessLink = e.next();
            swapContent(lessLink);
          }
        });
      });

//]]>
</script>

</li>
</ul>
</div>
<div class='navButton navButtonPrev initiallyHidden'>
<div class='navButtonArrow sliderArrowPrev'></div>
</div>
