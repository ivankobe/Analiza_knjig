        $('bookCover_204273').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_204273').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
