          $$('div.prototip').each(function (e) {
            if ($('bookCover_301207').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_301207').observe('prototip:hidden', function () {
