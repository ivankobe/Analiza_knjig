      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_454'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/74389.The_Plains_of_Passage?rating=\">all <span class=\"greyText\">(78310)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/74389.The_Plains_of_Passage?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;UOOQGg6a52iTIFLEHG+0LwwC+LaAblpQFyZb7ZkOPlOxQ7wEPWZB0Sk66noDBK5Bk7EMD5/vglGsLH/uJF3Amg==&#39;)}); return false;\">5 stars <span class=\"greyText\">(15604)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/74389.The_Plains_of_Passage?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;EwE5WDpfKDCOsFIYUVxkufQPtbuGNsuerzHdVgn2ooryoRVGCaOOiTSq6qZON37Xa7xBApm3E58UO/lVtKVcQw==&#39;)}); return false;\">4 stars <span class=\"greyText\">(17476)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n 