      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_297186').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_297186').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_297186').observe('prototip:hidden', function () {
