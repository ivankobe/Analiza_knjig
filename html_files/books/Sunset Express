        $('bookCover_29995903').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_29995903').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
