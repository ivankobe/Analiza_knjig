      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('edition_53403847').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('edition_53403847').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('edition_53403847').observe('prototip:hidden', function () {
