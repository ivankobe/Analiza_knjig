        $('bookCover_52811655').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_52811655').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
