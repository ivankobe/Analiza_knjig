        $('bookCover_18342308').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_18342308').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
