        $('bookCover_15862125').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_15862125').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
