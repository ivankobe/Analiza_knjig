        $('bookCover_507624').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_507624').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
