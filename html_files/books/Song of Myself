        $('bookCover_1760630').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1760630').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
