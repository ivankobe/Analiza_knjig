        $('bookCover_2984286').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_2984286').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
