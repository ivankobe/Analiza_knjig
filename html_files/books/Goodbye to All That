      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_376817').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_376817').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_376817').observe('prototip:hidden', function () {
