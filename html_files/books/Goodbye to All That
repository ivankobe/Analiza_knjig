      $('span_class_gr-hyperlink_more_filters_span_tip_886').observe('prototip:hidden', function () {
        $$('span.elementTwo').each(function (e) {
          if (e.getStyle('display') !== 'none') {
            var lessLink = e.next();
            swapContent(lessLink);
          }
        });
      });

//]]>
</script>    </div>

    <div class="reviewControls--left">
      <span class="greyText">&nbsp;|&nbsp;</span>
    </div>
      <div class="reviewControls--left">
      <a class="uitext" id="span_class_gr-hyperlink_sort_order_span_tip_218" style="cursor: pointer; " href="#" onclick="return false"><span class="gr-hyperlink">Sort order</span></a><script>
