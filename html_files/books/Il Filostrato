        $('bookCover_103159').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_103159').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
