<div id="review_31205167" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/31205167" />
    <a title="Stephen" class="left imgcol" href="/user/show/1413439-stephen"><img alt="Stephen" src="https://images.gr-assets.com/users/1311616811p2/1413439.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/31205167?book_show_action=true">Aug 25, 2008</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Stephen" class="user" itemprop="url" name="Stephen" href="/user/show/1413439-stephen">Stephen</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was amazing"><span size="15x15" class="staticStar p10">it was amazing</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/1413439-stephen?shelf=award-nominee-campbell">award-nominee-campbell</a>, 
              <a class="actionLinkLite" href="/review/list/1413439-stephen?shelf=all-time-favorites">all-time-favorites</a>, 
              <a class="actionLinkLite" href="/review/list/1413439-stephen?shelf=6-star-books">6-star-books</a>, 
              <a class="actionLinkLite" href="/review/list/1413439-stephen?shelf=award-nominee-nebula">award-nominee-nebula</a>, 
              <a class="actionLinkLite" href="/review/list/1413439-stephen?shelf=1980-1989">1980-1989</a>, 
              <a class="actionLinkLite" href="/review/list/1413439-stephen?shelf=easton-press">easton-press</a>, 
              <a class="actionLinkLite" href="/review/list/1413439-stephen?shelf=science-fiction">science-fiction</a>, 
