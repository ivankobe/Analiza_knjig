      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Mo" class="user" itemprop="url" name="Mo" href="/user/show/13030645-mo">Mo</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was ok"><span size="15x15" class="staticStar p10">it was ok</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/13030645-mo?shelf=w-ebooks">w-ebooks</a>, 
              <a class="actionLinkLite" href="/review/list/13030645-mo?shelf=2013">2013</a>, 
              <a class="actionLinkLite" href="/review/list/13030645-mo?shelf=lib-nypl">lib-nypl</a>, 
              <a class="actionLinkLite" href="/review/list/13030645-mo?shelf=contemporary">contemporary</a>, 
              <a class="actionLinkLite" href="/review/list/13030645-mo?shelf=non-fiction">non-fiction</a>
          </div>
