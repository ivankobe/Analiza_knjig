      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_102374').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_102374').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_102374').observe('prototip:hidden', function () {
