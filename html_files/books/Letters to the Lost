        $('bookCover_53863416').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_53863416').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
