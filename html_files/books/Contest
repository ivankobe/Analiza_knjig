        $('bookCover_136640').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_136640').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
