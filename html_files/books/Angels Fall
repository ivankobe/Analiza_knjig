        $('bookCover_3670173').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_3670173').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
