        $('bookCover_17733431').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_17733431').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
