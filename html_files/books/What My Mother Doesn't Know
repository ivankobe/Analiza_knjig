          $$('div.prototip').each(function (e) {
            if ($('bookCover_563321').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_563321').observe('prototip:hidden', function () {
