        $('bookCover_1044355').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1044355').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
