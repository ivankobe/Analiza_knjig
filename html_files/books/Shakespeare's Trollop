        $('bookCover_25489471').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_25489471').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
