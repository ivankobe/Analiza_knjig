        $('bookCover_80176').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_80176').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
