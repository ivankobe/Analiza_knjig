        $('bookCover_32261').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_32261').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
