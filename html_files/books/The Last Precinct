<div id="review_2796738876" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2796738876" />
    <a title="Angie Cody" class="left imgcol" href="/user/show/13442175-angie-cody"><img alt="Angie Cody" src="https://images.gr-assets.com/users/1618843795p2/13442175.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2796738876?book_show_action=true">May 22, 2019</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Angie Cody" class="user" itemprop="url" name="Angie Cody" href="/user/show/13442175-angie-cody">Angie Cody</a>
