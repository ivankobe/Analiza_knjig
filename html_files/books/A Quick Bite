        $('bookCover_50788').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_50788').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
