        $('bookCover_1702668').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1702668').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
