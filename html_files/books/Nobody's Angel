        $('bookCover_8893227').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_8893227').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_8893227').observe('prototip:hidden', function () {
