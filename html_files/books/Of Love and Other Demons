        $('bookCover_53926').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_53926').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
