      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Natalie" class="user" itemprop="url" name="Natalie" href="/user/show/7511965-natalie">Natalie</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was amazing"><span size="15x15" class="staticStar p10">it was amazing</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/7511965-natalie?shelf=fiction">fiction</a>, 
              <a class="actionLinkLite" href="/review/list/7511965-natalie?shelf=american-lit">american-lit</a>, 
              <a class="actionLinkLite" href="/review/list/7511965-natalie?shelf=contemporary-classics">contemporary-classics</a>, 
              <a class="actionLinkLite" href="/review/list/7511965-natalie?shelf=2013">2013</a>, 
              <a class="actionLinkLite" href="/review/list/7511965-natalie?shelf=fairy-tales">fairy-tales</a>, 
              <a class="actionLinkLite" href="/review/list/7511965-natalie?shelf=favorites">favorites</a>, 
              <a class="actionLinkLite" href="/review/list/7511965-natalie?shelf=modern-classics">modern-classics</a>, 
