        $('bookCover_1047347').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1047347').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
