<div id="review_3648116645" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/3648116645" />
    <a title="Tatevik" class="left imgcol" href="/user/show/7377638-tatevik"><img alt="Tatevik" src="https://images.gr-assets.com/users/1617213569p2/7377638.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/3648116645?book_show_action=true">Dec 13, 2020</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Tatevik" class="user" itemprop="url" name="Tatevik" href="/user/show/7377638-tatevik">Tatevik</a>
