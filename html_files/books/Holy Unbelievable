        $('bookCover_35684626').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_35684626').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
