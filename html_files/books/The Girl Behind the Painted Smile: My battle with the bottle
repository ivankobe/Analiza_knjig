        $('bookCover_34513845').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_34513845').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
