        $('bookCover_55428').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_55428').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
