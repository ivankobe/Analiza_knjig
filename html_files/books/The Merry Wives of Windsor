  <div class="friendReviews elementListBrown" >
    <div class="section">
      

<div id="review_3617650933" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/3617650933" />
    <a title="ale ♡" class="left imgcol" href="/user/show/101179363-ale"><img alt="ale ♡" src="https://images.gr-assets.com/users/1627069027p2/101179363.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/3617650933?book_show_action=true">Nov 01, 2020</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="ale ♡" class="user" itemprop="url" name="ale ♡" href="/user/show/101179363-ale">ale ♡</a>
