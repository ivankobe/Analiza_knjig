        $('bookCover_3443389').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_3443389').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
