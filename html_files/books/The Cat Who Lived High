        $('bookCover_15808346').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_15808346').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
