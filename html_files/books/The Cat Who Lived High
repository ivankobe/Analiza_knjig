        $('bookCover_508903').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_508903').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
