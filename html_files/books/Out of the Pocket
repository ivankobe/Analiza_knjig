        $('bookCover_23399267').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_23399267').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
