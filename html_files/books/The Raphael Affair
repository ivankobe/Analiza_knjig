  <div class="friendReviews elementListBrown" >
    <div class="section">
      

<div id="review_630592154" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/630592154" />
    <a title="Alan" class="left imgcol" href="/user/show/5650849-alan"><img alt="Alan" src="https://images.gr-assets.com/users/1310234599p2/5650849.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/630592154?book_show_action=true">Jun 01, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Alan" class="user" itemprop="url" name="Alan" href="/user/show/5650849-alan">Alan</a>
