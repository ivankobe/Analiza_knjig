        $('bookCover_16888').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_16888').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
