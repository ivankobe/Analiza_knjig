        $('bookCover_258978').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_258978').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
