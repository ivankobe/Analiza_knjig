        $('bookCover_216698').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_216698').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
