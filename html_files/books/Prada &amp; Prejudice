        $('bookCover_48505816').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_48505816').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
