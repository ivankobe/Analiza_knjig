      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_527'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/6751.Consider_the_Lobster_and_Other_Essays?rating=\">all <span class=\"greyText\">(106070)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/6751.Consider_the_Lobster_and_Other_Essays?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;To9yOb48mVl3x7pB7xSXVYSqKfOUPxHAgZAadIw4Lrh7ybog89OBo8fhIff/3nlr7CNAIyg79/ehx0Keso3E5A==&#39;)}); return false;\">5 stars <span class=\"greyText\">(20034)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/6751.Consider_the_Lobster_and_Other_Essays?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;kJUxR+oozajahn6b96qmjeLWrecB1SsAct7wLzP8VOyl0/lep8fVUmqg5S3nYEizil/EN73RzTdSiajFDUm+sA==&#39;)}); return false;\">4 stars <span class=\"greyText\">(18017)<\/span><\/a>\n                