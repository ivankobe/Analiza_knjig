        $('bookCover_110171').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_110171').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
