  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1174934539?book_show_action=true">Jan 20, 2015</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Mistery" class="user" itemprop="url" name="Mistery" href="/user/show/20246244-mistery">Mistery</a>
