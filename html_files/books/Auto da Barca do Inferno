        $('bookCover_6480073').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_6480073').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
