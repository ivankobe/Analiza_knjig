        $('bookCover_25063614').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_25063614').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
