        $('bookCover_464049').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_464049').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
