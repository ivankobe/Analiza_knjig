        $('bookCover_98573').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_98573').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
