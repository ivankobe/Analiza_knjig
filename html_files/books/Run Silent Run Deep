    <div class="js-dataTooltip" data-use-wtr-tooltip='true'>
        <div class="js-tooltipTrigger tooltipTrigger" data-resource-type='Book' data-resource-id='377691' data-placement='bottom-left'>
          <a href="/book/show/377691.Submarine_"><img id="more_book_377691" alt="Submarine!" title="Submarine!" width="50" src="https://i.gr-assets.com/images/S/compressed.photo.goodreads.com/books/1348774817l/377691._SX98_.jpg" /></a>
