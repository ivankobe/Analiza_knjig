        $('bookCover_50257876').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_50257876').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
