        $('bookCover_3244500').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_3244500').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
