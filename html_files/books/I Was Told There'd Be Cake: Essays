        $('bookCover_53487334').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_53487334').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
