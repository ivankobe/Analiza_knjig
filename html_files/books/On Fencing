        $('bookCover_118108').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_118108').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
