        $('edition_21760465').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('edition_21760465').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
