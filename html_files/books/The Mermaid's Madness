        $('bookCover_55531784').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_55531784').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
