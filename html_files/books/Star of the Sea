        $('bookCover_222589').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_222589').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
