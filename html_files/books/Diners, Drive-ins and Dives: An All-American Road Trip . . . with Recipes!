        $('bookCover_67655').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_67655').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
