        $('bookCover_4345498').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_4345498').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
