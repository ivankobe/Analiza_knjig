        $('bookCover_25482850').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_25482850').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
