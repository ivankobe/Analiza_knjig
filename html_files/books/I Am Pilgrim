      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_407'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/14781675-i-am-pilgrim?rating=\">all <span class=\"greyText\">(258520)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/14781675-i-am-pilgrim?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;l2/vjgKRvSTvSS6eqZAX8IMbzmyDXcObtzuAco31vvkIHHrrlTY45+/mz0GEAgI69mFpXB1yEEIwo3cjQFdSCQ==&#39;)}); return false;\">5 stars <span class=\"greyText\">(71766)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/14781675-i-am-pilgrim?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;a0u2waxak+D83qHE6U5zo35sHAg8S8fp4he9XHKJ2fT0OCOkO/0WI/xxQBvE3GZpCxa7OKJkFDBlj0oNvys1BA==&#39;)}); return false;\">4 stars <span class=\"greyText\">(41351)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/14781675-i-am-pilgrim?rating=3&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;buVhkry16gzQlydanMuARm8CYFsHFWx7mG9tQrqFsM/xlvT3KxJvz9A4xoWxWZWMGnjHa5k6v6If95oTdydcPw==&#39;)}); return false;\">3 stars <span class=\"greyText\">(15315)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/14781675-i-am-pilgrim?rating=2&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;zUA5/HJvAp5zEbLpS30BPin0Bzb1ZV3mTianxEQVmclSM6yZ5ciHXXO+UzZm7xT0XI6gBmtKjj/JvlCVibd1OQ==&#39;)}); return false;\">2 stars <span class=\"greyText\">(5223)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/14781675-i-am-pilgrim?rating=1&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&