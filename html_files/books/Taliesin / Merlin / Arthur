        $('bookCover_177001').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_177001').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
