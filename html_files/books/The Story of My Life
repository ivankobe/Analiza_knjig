        $('bookCover_112803').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_112803').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
