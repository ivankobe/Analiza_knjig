        $('bookCover_36254').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_36254').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
