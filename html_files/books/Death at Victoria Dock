      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_853995').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_853995').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_853995').observe('prototip:hidden', function () {
