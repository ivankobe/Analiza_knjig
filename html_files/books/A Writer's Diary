        $('bookCover_75060').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_75060').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
