  <div class="friendReviews elementListBrown" >
    <div class="section">
      

<div id="review_2623354395" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2623354395" />
    <a title="Andy" class="left imgcol" href="/user/show/73959126-andy"><img alt="Andy" src="https://images.gr-assets.com/users/1538687553p2/73959126.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2623354395?book_show_action=true">Jan 01, 2019</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Andy" class="user" itemprop="url" name="Andy" href="/user/show/73959126-andy">Andy</a>
