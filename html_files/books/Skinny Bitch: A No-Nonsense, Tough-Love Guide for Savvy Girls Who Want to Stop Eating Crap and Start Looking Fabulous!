      $('span_class_gr-hyperlink_sort_order_span_tip_523').observe('prototip:hidden', function () {
        $$('span.elementTwo').each(function (e) {
          if (e.getStyle('display') !== 'none') {
            var lessLink = e.next();
            swapContent(lessLink);
          }
        });
      });

//]]>
</script>    </div>
</div>






        <div data-react-class="ReactComponents.ReviewCta" data-react-props="{&quot;bookId&quot;:&quot;196613&quot;,&quot;user&quot;:{&quot;viewableName&quot;:&quot;&quot;,&quot;profileImage&quot;:&quot;https://s.gr-assets.com/assets/nophoto/user/u_50x66-632230dc9882b4352d753eedf9396530.png&quot;,&quot;profileUrl&quot;:null},&quot;actionType&quot;:&quot;community_reviews_cta&quot;,&quot;destinationPath&quot;:&quot;/review/edit/196613&quot;,&quot;bookTitle&quot;:&quot;Skinny Bitch: A No-Nonsense, Tough-Love Guide for Savvy Girls Who Want to Stop Eating Crap and Start Looking Fabulous!&quot;,&quot;ratingClickstreamRef&quot;:&quot;cr_rtg_str_bsh&quot;}"><div class="reviewCta" data-reactid=".24u4e6p26cq" data-react-checksum="-2048043121"><div class="reviewCta__left" data-reactid=".24u4e6p26cq.0"><img class="rectangularIcon rectangularIcon--border" src="https://s.gr-assets.com/assets/nophoto/user/u_50x66-632230dc9882b4352d753eedf9396530.png" alt="" data-reactid=".24u4e6p26cq.0.0"/></div><div class="reviewCta__right" data-reactid=".24u4e6p26cq.1"><div class="u-marginLeftXSmall reviewCta__label" data-reactid=".24u4e6p26cq.1.0"><noscript data-reactid=".24u4e6p26cq.1.0.0"></noscript><span data-reactid=".24u4e6p26cq.1.0.1">Start your review of </span><span data-reactid=".24u4e6p26cq.1.0.2">Skinny Bitch: A No-Nonsense, Tough-Love Guide for Savvy Girls Who Want to Stop Eating Crap and Start Looking Fabulous!</span></div><div class="reviewCta__controls" data-reactid=".24u4e6p26cq.1.1"><div class="userRating" data-reactid=".24u4e6p26cq.1.1.0"><div class="userRating__starsWrapper" tabindex="0" role="group" data-reactid=".24u4e6p26cq.1.1.0.1"><button class="userRating__star userRating__star--mediumStars" title="Rate 1 star" data-reactid=".24u4e6p26cq.1.1.0.1.$1"></button><button class="userRating__star userRating__star--mediumStars" title="Rate 2 stars" data-reactid=".24u4e6p26cq.1.1.0.1.$2"></button><button class="userRating__star userRating__star--mediumStars" title="Rate 3 stars"