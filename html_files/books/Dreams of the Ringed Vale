      var newTip = new Tip($('rating_details_tip'), "\n        <div id=\"moreBookData\" class=\"bookMeta\" >\n  \n\n  \n\n<table border=\"0\" id=\"rating_distribution\" style=\"width: auto; max-width: 500px;\" cellspacing=\"5\" cellpadding=\"0\">\n      <tr>\n        <th width=\"25\" class=\"right\">5&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 100%;\" title=\"1492 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/526764.Dreams_of_the_Ringed_Vale?rating=5\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'IglBp2UN/mVUcgIqF5Br0xPxwF7YSBRN50VcJNlEu5E965f9JA390bwdH+qL94qnvSEVx9tE6C1+bOsFpsvhrg==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          30% (1492)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">4&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 92%;\" title=\"1374 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/526764.Dreams_of_the_Ringed_Vale?rating=4\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'K0txF+ghwue714nzZJ44Nyr29beDMcPkElB6cpv7ugA0qadNqSHBU1O4lDP4+dlDhCYgLoA9P4SLec1T5HTgPw==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          28% (1374)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">3&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 65%;\" title=\"974 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/526764.Dreams_of_th