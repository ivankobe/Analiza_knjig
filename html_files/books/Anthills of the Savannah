        $('bookCover_152642').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_152642').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
