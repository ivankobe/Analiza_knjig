<div id="review_1789866065" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1789866065" />
    <a title=" Li&#39;l Owl" class="left imgcol" href="/user/show/48372551-li-l-owl"><img alt=" Li&#39;l Owl" src="https://images.gr-assets.com/users/1616729152p2/48372551.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1789866065?book_show_action=true">Jan 14, 2021</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title=" Li&#39;l Owl" class="user" itemprop="url" name=" Li&#39;l Owl" href="/user/show/48372551-li-l-owl"> Li&#39;l Owl</a>
