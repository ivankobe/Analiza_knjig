<div id="review_3105118139" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/3105118139" />
    <a title="Sam" class="left imgcol" href="/user/show/7814046-sam"><img alt="Sam" src="https://images.gr-assets.com/users/1329615283p2/7814046.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/3105118139?book_show_action=true">Dec 30, 2019</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Sam" class="user" itemprop="url" name="Sam" href="/user/show/7814046-sam">Sam</a>
      </span>

