        $('bookCover_18080597').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_18080597').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_18080597').observe('prototip:hidden', function () {
