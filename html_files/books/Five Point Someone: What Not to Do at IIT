<div id="review_3157713253" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/3157713253" />
    <a title="Tamoghna Biswas" class="left imgcol" href="/user/show/97513431-tamoghna-biswas"><img alt="Tamoghna Biswas" src="https://images.gr-assets.com/users/1580746459p2/97513431.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/3157713253?book_show_action=true">Jan 23, 2020</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Tamoghna Biswas" class="user" itemprop="url" name="Tamoghna Biswas" href="/user/show/97513431-tamoghna-biswas">Tamoghna Biswas</a>
