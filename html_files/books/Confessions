        $('bookCover_168561').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_168561').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
