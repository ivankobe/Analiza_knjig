        $('bookCover_8530').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_8530').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_8530').observe('prototip:hidden', function () {
