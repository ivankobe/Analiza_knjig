        $('bookCover_18416').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_18416').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_18416').observe('prototip:hidden', function () {
