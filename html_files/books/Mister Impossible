        $('bookCover_3039234').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_3039234').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
