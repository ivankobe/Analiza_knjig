      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_9539').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_9539').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_9539').observe('prototip:hidden', function () {
