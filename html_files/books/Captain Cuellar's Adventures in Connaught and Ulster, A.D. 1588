<div id="review_2276673029" class="review notext" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2276673029" />
    <a title="David Mcmillan" class="left imgcol" href="/user/show/39427514-david-mcmillan"><img alt="David Mcmillan" src="https://images.gr-assets.com/users/1421877646p2/39427514.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="David Mcmillan" class="user" itemprop="url" name="David Mcmillan" href="/user/show/39427514-david-mcmillan">David Mcmillan</a>
      </span>

