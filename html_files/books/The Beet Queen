      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_684'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/1136118.The_Beet_Queen?rating=\">all <span class=\"greyText\">(24271)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/1136118.The_Beet_Queen?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;j+ncqaLQ6HxVntmu/rNppoUwjOCcJxm1x2RR4X4Cl+a++ZjLxhXUZz3fwXMuzz/UAkCptSk+RJZMKYubfljekA==&#39;)}); return false;\">5 stars <span class=\"greyText\">(3543)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/1136118.The_Beet_Queen?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;DkQtBdWnUMzbOq/ZaWDOheOfjVG+uCILx1ZUZc7Q2oU/VGlnsWJs17N7twS5HJj3ZO+oBAuhfyhMG44fzoqT8w==&#39;)}); return false;\">4 stars <span class=\"greyText\">(5298)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  