      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_49190').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_49190').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_49190').observe('prototip:hidden', function () {
