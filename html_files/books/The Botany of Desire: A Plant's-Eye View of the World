        $('bookCover_23013818').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_23013818').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
