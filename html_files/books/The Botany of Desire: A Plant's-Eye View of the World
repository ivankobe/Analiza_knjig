  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2427262364?book_show_action=true">Jun 17, 2018</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Mehrsa" class="user" itemprop="url" name="Mehrsa" href="/user/show/1045774-mehrsa">Mehrsa</a>
