        $('bookCover_17230517').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_17230517').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
