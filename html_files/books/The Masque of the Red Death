        $('bookCover_1574006').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1574006').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
