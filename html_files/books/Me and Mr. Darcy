<div id="review_749983038" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/749983038" />
    <a title="Alexandra" class="left imgcol" href="/user/show/24149261-alexandra"><img alt="Alexandra" src="https://images.gr-assets.com/users/1442463521p2/24149261.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/749983038?book_show_action=true">Oct 26, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Alexandra" class="user" itemprop="url" name="Alexandra" href="/user/show/24149261-alexandra">Alexandra</a>
