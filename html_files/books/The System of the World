        $('bookCover_823').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_823').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
