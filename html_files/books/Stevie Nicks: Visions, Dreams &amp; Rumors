<div id="review_2817547679" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2817547679" />
    <a title="Susan" class="left imgcol" href="/user/show/11136793-susan"><img alt="Susan" src="https://images.gr-assets.com/users/1620688085p2/11136793.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2817547679?book_show_action=true">May 11, 2019</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Susan" class="user" itemprop="url" name="Susan" href="/user/show/11136793-susan">Susan</a>
