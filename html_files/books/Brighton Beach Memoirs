        $('bookCover_106123').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_106123').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
