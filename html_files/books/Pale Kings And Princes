      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_50359504').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_50359504').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_50359504').observe('prototip:hidden', function () {
