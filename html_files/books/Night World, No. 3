        $('bookCover_493123').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_493123').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
