        $('bookCover_1536').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1536').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
