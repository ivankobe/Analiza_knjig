        $('bookCover_3658780').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_3658780').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
