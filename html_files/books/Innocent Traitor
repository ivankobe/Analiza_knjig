      var newTip = new Tip($('rating_details_tip'), "\n        <div id=\"moreBookData\" class=\"bookMeta\" >\n  \n\n  \n\n<table border=\"0\" id=\"rating_distribution\" style=\"width: auto; max-width: 500px;\" cellspacing=\"5\" cellpadding=\"0\">\n      <tr>\n        <th width=\"25\" class=\"right\">5&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 79%;\" title=\"10156 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/111218.Innocent_Traitor?rating=5\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'glsT6MNMKhBAihQ7OGCtbNuX2/92ukEunGnFieRtEXZ58l/zd7gTvypmAJkdWR/2QBKqHWff03Lx4YoH86NPnw==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          31% (10156)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">4&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 100%;\" title=\"12790 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/111218.Innocent_Traitor?rating=4\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'KjM8TtmflBNfV5hmHrre1VL4LQrmGiMrQiFwYC9UN7zRmnBVbWutvDW7jMQ7g2xPyX1c6Pd/sXcvqT/uOJppVQ==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          39% (12790)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">3&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 55%;\" title=\"7129 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/111218.Innocent_Traitor?rating=3\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'qDe8hO/OCOF7Vg1aJXBQVN/YqXSj/0U57poQ+hHsSfhTnvCfWzoxThG6GfgASeLORF3YlrKa12WDEl90BiIXEQ==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          22% (7129)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">2&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 11%;\" title=\"1457 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/111218.Innocent_Traitor?rating=2\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'ufBLA/87a4UM8iqJnwy4i+9WCX+vs6hzI7HMJ3UlIYFCWQcYS89SKmYePiu6NQoRdNN4nb7WOi9OOYOpYut/aA==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          4% (1457)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">1&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 4%;\" title=\"584 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/111218.Innocent_Traitor?rating=1\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'/Ad047uIVT1k5bG3G2OxhDlz/EqI8KaOpACfCBj6v1oHrjj4D3xskg4JpRU+WgMeovaNqJmVNNLJiNCGDzThsw==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          1% (584)\n        <\/td>\n      <\/tr>\n\n<\/table>\n\n\n    <span class=\"value\">94<\/span>% of people liked it\n\n  <table>\n    <tr>\n      <td><strong>All editions:<\/strong><\/td>\n      <td>\n        <span class=\"value\">3.95<\/span> average rating,\n        <span class=\"value\">32116<\/span> ratings,\