        $('bookCover_16181').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_16181').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
