<div id="review_270915768" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/270915768" />
    <a title="Sarah Elizabeth" class="left imgcol" href="/user/show/7591340-sarah-elizabeth"><img alt="Sarah Elizabeth" src="https://images.gr-assets.com/users/1328957304p2/7591340.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/270915768?book_show_action=true">Nov 02, 2017</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Sarah Elizabeth" class="user" itemprop="url" name="Sarah Elizabeth" href="/user/show/7591340-sarah-elizabeth">Sarah Elizabeth</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="liked it"><span size="15x15" class="staticStar p10">liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>

          <span class="greyText">&nbsp;&middot;&nbsp;</span>
          <a class="lightGreyText" title="Kindle Edition " href="/book/show/30622126-gone">review of another edition</a>


        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/7591340-sarah-elizabeth?shelf=as">as</a>, 
              <a class="actionLinkLite" href="/review/list/7591340-sarah-elizabeth?shelf=romance">romance</a>, 
              <a class="actionLinkLite" href="/review/list/7591340-sarah-elizabeth?shelf=paranormal">paranormal</a>, 
              <a class="actionLinkLite" href="/review/list/7591340-sarah-elizabeth?shelf=read-2017">read-2017</a>, 
              <a class="actionLinkLite" href="/review/list/7591340-sarah-elizabeth?shelf=read-with-abbie">read-with-abbie</a>, 
              <a class="actionLinkLite" href="/review/list/7591340-sarah-elizabeth?shelf=death">death</a>, 
              <a class="actionLinkLite" href="/review/list/7591340-sarah-elizabeth?shelf=young-adult">young-adult</a>, 
              <a class="actionLinkLite" href="/review/list/7591340-sarah-elizabeth?shelf=dystopian">dystopian</a>, 
              <a class="actionLinkLite" href="/review/list/7591340-sarah-elizabeth?shelf=sci-fi">sci-fi</a>
          </div>
