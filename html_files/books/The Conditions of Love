        $('bookCover_52769189').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_52769189').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
