        $('bookCover_49353950').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_49353950').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
