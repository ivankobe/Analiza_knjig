<div id="review_3824512856" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/3824512856" />
    <a title="Hayli" class="left imgcol" href="/user/show/32159671-hayli"><img alt="Hayli" src="https://images.gr-assets.com/users/1615146050p2/32159671.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/3824512856?book_show_action=true">Feb 07, 2021</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Hayli" class="user" itemprop="url" name="Hayli" href="/user/show/32159671-hayli">Hayli</a>
      </span>

