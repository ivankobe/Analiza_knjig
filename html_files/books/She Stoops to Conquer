            if ($('bookCover_765552').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_765552').observe('prototip:hidden', function () {
