        $('bookCover_129020').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_129020').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
