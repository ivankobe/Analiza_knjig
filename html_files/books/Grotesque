    <div class="js-dataTooltip" data-use-wtr-tooltip='true'>
        <div class="js-tooltipTrigger tooltipTrigger" data-resource-type='Book' data-resource-id='25365' data-placement='bottom-left'>
          <a href="/book/show/25365.Out"><img id="more_book_25365" alt="Out" title="Out" width="50" src="https://i.gr-assets.com/images/S/compressed.photo.goodreads.com/books/1386749063l/25365._SX98_.jpg" /></a>
