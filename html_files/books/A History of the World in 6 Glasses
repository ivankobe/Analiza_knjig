        $('bookCover_5776170').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_5776170').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
