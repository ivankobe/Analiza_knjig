        $('bookCover_56605787').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_56605787').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
