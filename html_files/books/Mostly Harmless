<div id="review_549623440" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/549623440" />
    <a title="Saliha" class="left imgcol" href="/user/show/11793470-saliha"><img alt="Saliha" src="https://s.gr-assets.com/assets/nophoto/user/f_50x66-6a03a5c12233c941481992b82eea8d23.png" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/549623440?book_show_action=true">Mar 01, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Saliha" class="user" itemprop="url" name="Saliha" href="/user/show/11793470-saliha">Saliha</a>
