        $('bookCover_150255').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_150255').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
