        $('bookCover_160969').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_160969').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
