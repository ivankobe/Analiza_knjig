        $('bookCover_96081').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_96081').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
