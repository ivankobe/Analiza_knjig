      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_6513551').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_6513551').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_6513551').observe('prototip:hidden', function () {
