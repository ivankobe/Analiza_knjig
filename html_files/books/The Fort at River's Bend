        $('bookCover_79665').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_79665').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
