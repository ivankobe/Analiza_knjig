        $('bookCover_437414').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_437414').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_437414').observe('prototip:hidden', function () {
