        $('bookCover_11389').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_11389').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
