      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_267115').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_267115').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_267115').observe('prototip:hidden', function () {
