        $('bookCover_50698585').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_50698585').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
