          $$('div.prototip').each(function (e) {
            if ($('bookCover_8058309').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_8058309').observe('prototip:hidden', function () {
