        $('bookCover_8830').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_8830').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
