        $('bookCover_10507293').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_10507293').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
