        $('bookCover_171547').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_171547').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
