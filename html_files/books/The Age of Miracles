        $('bookCover_57027834').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_57027834').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
