        $('bookCover_52239837').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_52239837').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
