      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_861326').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_861326').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_861326').observe('prototip:hidden', function () {
