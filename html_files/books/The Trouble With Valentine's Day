        $('bookCover_60223').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_60223').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
