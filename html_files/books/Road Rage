        $('bookCover_748989').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_748989').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
