      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_357664').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_357664').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_357664').observe('prototip:hidden', function () {
