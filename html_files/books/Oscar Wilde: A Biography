        $('bookCover_40977850').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_40977850').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
