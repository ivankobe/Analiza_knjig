        $('bookCover_207897').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_207897').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
