        $('bookCover_616089').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_616089').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
