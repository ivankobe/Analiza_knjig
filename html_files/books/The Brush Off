        $('bookCover_880').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_880').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
