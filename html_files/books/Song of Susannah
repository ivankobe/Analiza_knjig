      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_11607').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_11607').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_11607').observe('prototip:hidden', function () {
