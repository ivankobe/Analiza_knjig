        $('bookCover_6215832').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_6215832').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
