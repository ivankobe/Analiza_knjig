        $('bookCover_4261483').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_4261483').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
