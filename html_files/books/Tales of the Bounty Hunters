        $('bookCover_9650960').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_9650960').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
