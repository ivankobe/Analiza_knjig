      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_28863').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_28863').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_28863').observe('prototip:hidden', function () {
