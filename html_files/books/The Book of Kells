<div id="review_2397696112" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2397696112" />
    <a title="Nancy" class="left imgcol" href="/user/show/7453733-nancy"><img alt="Nancy" src="https://images.gr-assets.com/users/1326816970p2/7453733.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2397696112?book_show_action=true">May 22, 2018</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Nancy" class="user" itemprop="url" name="Nancy" href="/user/show/7453733-nancy">Nancy</a>
