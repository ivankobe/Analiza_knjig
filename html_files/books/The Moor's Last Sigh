      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_970684').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_970684').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_970684').observe('prototip:hidden', function () {
