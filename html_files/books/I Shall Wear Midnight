        $('bookCover_55023060').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_55023060').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
