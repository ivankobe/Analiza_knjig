      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_64218').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_64218').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_64218').observe('prototip:hidden', function () {
