      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_50546014').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_50546014').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_50546014').observe('prototip:hidden', function () {
