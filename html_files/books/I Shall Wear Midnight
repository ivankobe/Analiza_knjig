  <div data-react-class="ReactComponents.ShareDialog" data-react-props="{&quot;signedIn&quot;:false,&quot;signInUrl&quot;:&quot;/user/sign_in&quot;,&quot;social&quot;:{&quot;inModal&quot;:{&quot;facebook&quot;:true,&quot;twitter&quot;:true,&quot;pinterest&quot;:true,&quot;copyLink&quot;:false},&quot;outsideModal&quot;:{&quot;facebook&quot;:true,&quot;twitter&quot;:true,&quot;pinterest&quot;:true,&quot;copyLink&quot;:false},&quot;fbOptions&quot;:{&quot;name&quot;:&quot;I Shall Wear Midnight by Terry Pratchett&quot;,&quot;caption&quot;:&quot;goodreads.com&quot;,&quot;description&quot;:&quot;&quot;,&quot;link&quot;:&quot;https://www.goodreads.com/book/show/7576115-i-shall-wear-midnight&quot;,&quot;picture&quot;:&quot;https://i.gr-assets.com/images/S/compressed.photo.goodreads.com/books/1328232764l/7576115._SY75_.jpg&quot;},&quot;twitterOptions&quot;:{&quot;text&quot;:&quot;I Shall Wear Midnight by Terry Pratchett&quot;,&quot;url&quot;:&quot;https://www.goodreads.com/book/show/7576115-i-shall-wear-midnight&quot;,&quot;via&quot;:&quot;goodreads&quot;},&quot;pinterestOptions&quot;:{&quot;description&quot;:&quot;I Shall Wear Midnight by Terry Pratchett&quot;,&quot;url&quot;:&quot;https://www.goodreads.com/book/show/7576115-i-shall-wear-midnight&quot;,&quot;media&quot;:&quot;https://i.gr-assets.com/images/S/compressed.photo.goodreads.com/books/1328232764l/7576115.jpg&quot;}},&quot;sharingData&quot;:{&quot;resourceTypeFriendlyText&quot;:&quot;book&quot;,&quot;resourceId&quot;:7576115},&quot;includePreview&quot;:true,&quot;previewData&quot;:{&quot;author&quot;:{&quot;name&quot;:&quot;Terry Pratchett&quot;,&quot;isGoodreadsAuthor&quot;:false},&quot;rating&quot;:&quot;4.28&quot;,&quot;title&quot;:&quot;I Shall Wear Midnight (Discworld, #38; Tiffany Aching, #4)&quot;,&quot;imageUrl&quot;:&quot;https://i.gr-assets.com/images/S/compressed.photo.goodreads.com/books/1328232764i/7576115._UX75_.jpg&quot;,&quot;ratingsCount&quot;:53772,&quot;textReviewsCount&quot;:2339}}"><div data-rea