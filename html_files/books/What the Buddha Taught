<div id="review_3900195688" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/3900195688" />
    <a title="Hai Quan" class="left imgcol" href="/user/show/20274872-hai-quan"><img alt="Hai Quan" src="https://images.gr-assets.com/users/1518929777p2/20274872.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/3900195688?book_show_action=true">Mar 20, 2021</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Hai Quan" class="user" itemprop="url" name="Hai Quan" href="/user/show/20274872-hai-quan">Hai Quan</a>
