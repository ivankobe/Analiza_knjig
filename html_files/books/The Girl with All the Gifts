        $('bookCover_6690798').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_6690798').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
