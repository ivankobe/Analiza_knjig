<div id="review_1573798041" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1573798041" />
    <a title="Alaina" class="left imgcol" href="/user/show/50620706-alaina"><img alt="Alaina" src="https://images.gr-assets.com/users/1451325817p2/50620706.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1573798041?book_show_action=true">Jun 09, 2017</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Alaina" class="user" itemprop="url" name="Alaina" href="/user/show/50620706-alaina">Alaina</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="really liked it"><span size="15x15" class="staticStar p10">really liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/50620706-alaina?shelf=fantasy">fantasy</a>, 
              <a class="actionLinkLite" href="/review/list/50620706-alaina?shelf=2020-ng">2020-ng</a>, 
              <a class="actionLinkLite" href="/review/list/50620706-alaina?shelf=free-ebook">free-ebook</a>, 
              <a class="actionLinkLite" href="/review/list/50620706-alaina?shelf=romance">romance</a>, 
              <a class="actionLinkLite" href="/review/list/50620706-alaina?shelf=sci-fi">sci-fi</a>, 
              <a class="actionLinkLite" href="/review/list/50620706-alaina?shelf=2017">2017</a>, 
              <a class="actionLinkLite" href="/review/list/50620706-alaina?shelf=favorites">favorites</a>, 
              <a class="actionLinkLite" href="/review/list/50620706-alaina?shelf=paranormal">paranormal</a>, 
              <a class="actionLinkLite" href="/review/list/50620706-alaina?shelf=young-adult">young-adult</a>, 
              <a class="actionLinkLite" href="/review/list/50620706-alaina?shelf=fiction">fiction</a>
          </div>
