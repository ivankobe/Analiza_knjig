        $('bookCover_15917050').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_15917050').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
