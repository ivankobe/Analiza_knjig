      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Elizabeth  " class="user" itemprop="url" name="Elizabeth  " href="/user/show/546969-elizabeth">Elizabeth  </a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was ok"><span size="15x15" class="staticStar p10">it was ok</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/546969-elizabeth?shelf=fiction-childrens">fiction-childrens</a>, 
              <a class="actionLinkLite" href="/review/list/546969-elizabeth?shelf=fiction-fairytales-folklore">fiction-fairytales-folklore</a>, 
              <a class="actionLinkLite" href="/review/list/546969-elizabeth?shelf=fiction-fantasy">fiction-fantasy</a>, 
