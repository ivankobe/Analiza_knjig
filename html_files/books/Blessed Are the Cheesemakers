        $('bookCover_50818113').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_50818113').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
