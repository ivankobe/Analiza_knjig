      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_6611038').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_6611038').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_6611038').observe('prototip:hidden', function () {
