      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_710'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/4447622-some-girls-bite?rating=\">all <span class=\"greyText\">(124713)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/4447622-some-girls-bite?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;N7NKyokyeYaNZxrrNA6b08AEmlDTj4gTCceI1nUK2zErYH69Iv6TnyjBXQaPviG2zRI32cJchwvhJEcgbEHz0w==&#39;)}); return false;\">5 stars <span class=\"greyText\">(23114)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/4447622-some-girls-bite?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;oH7nQzhBUwMsq15a3kYkHCAoPgPS8lhOIrg9IvJzIZq8rdM0k425GokNGbdl9p55LT6TisMhV1bKW/LU6zgJeA==&#39;)}); return false;\">4 stars <span class=\"greyText\">(22193)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n            