        $('bookCover_19552').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_19552').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_19552').observe('prototip:hidden', function () {
