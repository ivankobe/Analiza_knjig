      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_753'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/48119.Coyote_Waits?rating=\">all <span class=\"greyText\">(14693)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/48119.Coyote_Waits?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;ntTlxASQ7TI4PHr1TLrCkVb1ePekHipH3oQ/u/xEMCDPAMDN9Kja/ksl2Dd17EP9f33SuYU18tBOOS40m5fmlg==&#39;)}); return false;\">5 stars <span class=\"greyText\">(3472)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/48119.Coyote_Waits?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;rN4ibY4IQ8xSE9ZJr12eVJSOTAvdiSe/mw/L9f+YlIb9CgdkfjB0ACEKdIuWCx84vQbmRfyi/ygLstp6mEtCMA==&#39;)}); return false;\">4 stars <span class=\"greyText\">(4672)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=