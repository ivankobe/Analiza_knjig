        }
      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_164672').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_164672').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_164672').observe('prototip:hidden', function () {
