        $('bookCover_145386').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_145386').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
