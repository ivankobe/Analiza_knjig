        $('bookCover_29561415').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_29561415').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_29561415').observe('prototip:hidden', function () {
