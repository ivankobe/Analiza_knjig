<div id="review_37092600" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/37092600" />
    <a title="Jamie" class="left imgcol" href="/user/show/404400-jamie"><img alt="Jamie" src="https://images.gr-assets.com/users/1520026973p2/404400.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/37092600?book_show_action=true">Nov 06, 2008</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Jamie" class="user" itemprop="url" name="Jamie" href="/user/show/404400-jamie">Jamie</a>
