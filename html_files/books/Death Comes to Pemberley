      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_878'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/12875355-death-comes-to-pemberley?rating=\">all <span class=\"greyText\">(185868)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/12875355-death-comes-to-pemberley?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;+hNaV2VBJQp1HvOKt/BgRsXOmKa1Qmnj9SlIE63rJqmko1PoQG38EVbs7a/anz/w6buoFaNmyWiPrbrgFhUomg==&#39;)}); return false;\">5 stars <span class=\"greyText\">(16544)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/12875355-death-comes-to-pemberley?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;tbsteXeDqXH7ut3aX4IeW7xx+dRsUyZuXtLt7dcWU6jrCyTGUq9wathIw/8y7UHtkATJZ3p3huUkVh8ebOhdmw==&#39;)}); return false;\">4 stars <span class=\"greyText\">(26084)<\/span><\/a>\n                  <span class=\"loadingAnim