      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_71252').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_71252').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_71252').observe('prototip:hidden', function () {
