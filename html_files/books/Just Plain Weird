        $('bookCover_19240947').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_19240947').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
