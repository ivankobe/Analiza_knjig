        $('bookCover_452306').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_452306').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
