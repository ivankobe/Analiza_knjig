<div id="review_2765882483" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2765882483" />
    <a title="Mehrsa" class="left imgcol" href="/user/show/1045774-mehrsa"><img alt="Mehrsa" src="https://images.gr-assets.com/users/1218672696p2/1045774.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2765882483?book_show_action=true">Mar 27, 2019</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Mehrsa" class="user" itemprop="url" name="Mehrsa" href="/user/show/1045774-mehrsa">Mehrsa</a>
