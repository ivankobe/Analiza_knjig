      var newTip = new Tip($('rating_details_tip'), "\n        <div id=\"moreBookData\" class=\"bookMeta\" >\n  \n\n  \n\n<table border=\"0\" id=\"rating_distribution\" style=\"width: auto; max-width: 500px;\" cellspacing=\"5\" cellpadding=\"0\">\n      <tr>\n        <th width=\"25\" class=\"right\">5&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 100%;\" title=\"37 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/25778002-the-six-o-clock-scramble-meal-planner?rating=5\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'SaHdsY4jPqbM3x6oNrAnxXE3cUUDRsFDWjwu5c6KoUFNBpRqRmIxFFk1cOweQ6sg37dQaMeDFlD7IRyj5+hSgg==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          68% (37)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">4&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 24%;\" title=\"9 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/25778002-the-six-o-clock-scramble-meal-planner?rating=4\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'Fx224B1zrDBf8HkE+8d6iwUjIFLPx8duFQ9pidLiwZ8Tuv871TKjgsoaF0DTNPZuq6MBfwsCEH20ElvP+4AyXA==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          16% (9)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">3&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 10%;\" title=\"4 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/257