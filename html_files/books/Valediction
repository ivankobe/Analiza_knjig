        $('bookCover_425174').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_425174').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
