        $('bookCover_8438022').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_8438022').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
