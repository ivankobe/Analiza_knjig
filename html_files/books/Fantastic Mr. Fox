          $$('div.prototip').each(function (e) {
            if ($('bookCover_1051513').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_1051513').observe('prototip:hidden', function () {
