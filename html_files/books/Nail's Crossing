        $('bookCover_44448907').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_44448907').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
