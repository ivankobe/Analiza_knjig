        $('bookCover_51704877').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_51704877').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
