        $('bookCover_50092294').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_50092294').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
