        $('bookCover_7001285').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_7001285').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
