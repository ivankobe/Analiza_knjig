      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_769697').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_769697').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_769697').observe('prototip:hidden', function () {
