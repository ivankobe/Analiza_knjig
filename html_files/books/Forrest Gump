        $('bookCover_1105093').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1105093').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
