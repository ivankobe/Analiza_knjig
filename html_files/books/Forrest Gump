        $('bookCover_67238').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_67238').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
