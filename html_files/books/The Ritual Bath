        $('bookCover_185921').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_185921').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_185921').observe('prototip:hidden', function () {
