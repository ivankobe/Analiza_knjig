        $('bookCover_35724373').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_35724373').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
