        $('bookCover_325451').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_325451').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
