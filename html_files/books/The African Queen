      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_107327').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_107327').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_107327').observe('prototip:hidden', function () {
