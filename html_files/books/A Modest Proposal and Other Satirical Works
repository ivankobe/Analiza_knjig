      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_207313').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_207313').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_207313').observe('prototip:hidden', function () {
