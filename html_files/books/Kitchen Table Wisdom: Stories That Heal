        $('bookCover_160059').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_160059').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
