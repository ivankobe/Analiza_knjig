<div id="review_1786548997" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1786548997" />
    <a title="BAC" class="left imgcol" href="/user/show/54576060-bac"><img alt="BAC" src="https://s.gr-assets.com/assets/nophoto/user/f_50x66-6a03a5c12233c941481992b82eea8d23.png" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1786548997?book_show_action=true">Oct 16, 2016</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="BAC" class="user" itemprop="url" name="BAC" href="/user/show/54576060-bac">BAC</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was amazing"><span size="15x15" class="staticStar p10">it was amazing</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/54576060-bac?shelf=owned">owned</a>, 
              <a class="actionLinkLite" href="/review/list/54576060-bac?shelf=reference">reference</a>, 
              <a class="actionLinkLite" href="/review/list/54576060-bac?shelf=non-fiction">non-fiction</a>, 
              <a class="actionLinkLite" href="/review/list/54576060-bac?shelf=food-and-cooking">food-and-cooking</a>, 
