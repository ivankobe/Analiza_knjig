        $('bookCover_45726106').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_45726106').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
