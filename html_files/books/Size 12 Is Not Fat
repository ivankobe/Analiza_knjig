        $('bookCover_2120652').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_2120652').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
