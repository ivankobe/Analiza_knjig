<div id="review_977412683" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/977412683" />
    <a title="Em" class="left imgcol" href="/user/show/10017183-em"><img alt="Em" src="https://images.gr-assets.com/users/1546799282p2/10017183.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/977412683?book_show_action=true">Jun 25, 2014</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Em" class="user" itemprop="url" name="Em" href="/user/show/10017183-em">Em</a>
      </span>

