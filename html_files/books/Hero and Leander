  <div class="friendReviews elementListBrown" >
    <div class="section">
      

<div id="review_2590857359" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2590857359" />
    <a title="Deanna ✰ ☾" class="left imgcol" href="/user/show/15290216-deanna"><img alt="Deanna ✰ ☾" src="https://images.gr-assets.com/users/1444525340p2/15290216.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2590857359?book_show_action=true">Nov 12, 2018</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Deanna ✰ ☾" class="user" itemprop="url" name="Deanna ✰ ☾" href="/user/show/15290216-deanna">Deanna ✰ ☾</a>
