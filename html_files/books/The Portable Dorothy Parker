  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/662138265?book_show_action=true">Jul 08, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Rincey" class="user" itemprop="url" name="Rincey" href="/user/show/278339-rincey">Rincey</a>
