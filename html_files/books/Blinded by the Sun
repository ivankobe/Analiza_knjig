<div id="review_391701255" class="review notext" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/391701255" />
    <a title="Toni Kenyon" class="left imgcol" href="/user/show/7125275-toni-kenyon"><img alt="Toni Kenyon" src="https://images.gr-assets.com/users/1442638716p2/7125275.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Toni Kenyon" class="user" itemprop="url" name="Toni Kenyon" href="/user/show/7125275-toni-kenyon">Toni Kenyon</a>
      </span>

