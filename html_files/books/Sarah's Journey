      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('edition_17903252').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('edition_17903252').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('edition_17903252').observe('prototip:hidden', function () {
