<div id="review_571723936" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/571723936" />
    <a title="Buzz H." class="left imgcol" href="/user/show/13730081-buzz-h"><img alt="Buzz H." src="https://images.gr-assets.com/users/1359086282p2/13730081.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/571723936?book_show_action=true">Mar 26, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Buzz H." class="user" itemprop="url" name="Buzz H." href="/user/show/13730081-buzz-h">Buzz H.</a>
