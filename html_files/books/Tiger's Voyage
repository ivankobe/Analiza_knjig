//<![CDATA[
      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_160'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/7849341-tiger-s-voyage?rating=\">all <span class=\"greyText\">(58630)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/7849341-tiger-s-voyage?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;tDPPxPDD4cAuhTt5y/0+v0zRQ+d9R49VN70a7qEYPSy+vU8JOj92CNhEdVTfRme2j5ePnzYQ7mNq8dPKsl/A4g==&#39;)}); return false;\">5 stars <span class=\"greyText\">(19364)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/7849341-tiger-s-voyage?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;OYN08LBMPWde4J7tTBVVt3Ewqq/Y3Vd+AJT8Vk2yTBIzDfQ9erCqr6gh0MBYrgy+snZm15OKNkhd2DVyXvWx3A==&#39;)}); return false;\">4 stars <span class=\"greyText\">(9177)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                 