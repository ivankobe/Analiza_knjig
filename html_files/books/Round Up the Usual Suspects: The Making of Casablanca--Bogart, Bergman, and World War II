        $('bookCover_307443').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_307443').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
