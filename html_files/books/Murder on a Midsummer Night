        $('bookCover_382839').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_382839').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_382839').observe('prototip:hidden', function () {
        $$('span.elementTwo').each(function (e) {
          if (e.getStyle('display') !== 'none') {
