        $('bookCover_677161').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_677161').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
