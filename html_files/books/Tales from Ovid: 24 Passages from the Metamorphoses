      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_48635408').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_48635408').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_48635408').observe('prototip:hidden', function () {
