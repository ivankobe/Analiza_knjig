            if ($('edition_1366693').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('edition_1366693').observe('prototip:hidden', function () {
        $$('span.elementTwo').each(function (e) {
