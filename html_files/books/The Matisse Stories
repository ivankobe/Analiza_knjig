        $('bookCover_6547752').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_6547752').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_6547752').observe('prototip:hidden', function () {
