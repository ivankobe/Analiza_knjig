//<![CDATA[
      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_713'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/23398625-kitchens-of-the-great-midwest?rating=\">all <span class=\"greyText\">(93629)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/23398625-kitchens-of-the-great-midwest?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;H8qoxGuPRk6PngdcdmZuxDNjyuWe4w4GDuTRDPHO+P1viFP73co/VcBJ0Y213HYSwzmB+yWKqWhTivBw2/1d6Q==&#39;)}); return false;\">5 stars <span class=\"greyText\">(9158)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/23398625-kitchens-of-the-great-midwest?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;UvdnRiPmb6kYBMPYknXF0D9OXKqaITCe+nSPsibLzqEitZx5laMWslfTFQlRz90GzxQXtCFIl/CnGq7ODPhrtQ==&#39;)}); return false;\">4 stars <span class=\"greyText\">(16962)<\/span><\/a>\n                  <span class=