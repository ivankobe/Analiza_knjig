        $('edition_18616').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('edition_18616').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
