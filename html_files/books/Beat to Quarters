        $('bookCover_90953').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_90953').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
