        $('bookCover_2324124').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_2324124').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
