        $('bookCover_15858248').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_15858248').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_15858248').observe('prototip:hidden', function () {
        $$('span.elementTwo').each(function (e) {
          if (e.getStyle('display') !== 'none') {
