      <a class="uitext" rel="nofollow" id="span_class_gr-hyperlink_more_filters_span_tip_468" style="cursor: pointer; " href="#" onclick="return false"><span class="gr-hyperlink">More filters</span></a><script>
//<![CDATA[
      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_468'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/6194.Waiting_for_the_Barbarians?rating=\">all <span class=\"greyText\">(54533)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/6194.Waiting_for_the_Barbarians?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;DbB7uWze+3kQIucgkQSdovfhz5IeldvV7Pbea1p4aSuRgnFMiofFrPAp6yrYQN/pt5B5XHq8ezlWhuHaSCDJ1Q==&#39;)}); return false;\">5 stars <span class=\"greyText\">(8381)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/6194.Waiting_for_the_Barbarians?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;csutabHHHJ7pL2xe5IFTO8voNWagn6c9iWdS7BYis1fu+aecV54iSwkkYFStxRFwi5mDqMS2B9EzF21dBHoTqQ==&#39;)}); return false;\">4 stars <span class=\"greyText\">(10734)<\/span><\/a>\n                  <span class=\"loadingAnimation\">