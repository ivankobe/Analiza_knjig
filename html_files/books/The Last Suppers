        $('bookCover_614404').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_614404').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
