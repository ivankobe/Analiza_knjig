        $('bookCover_179780').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_179780').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
