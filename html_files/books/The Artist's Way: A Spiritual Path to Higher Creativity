      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Jim" class="user" itemprop="url" name="Jim" href="/user/show/6020795-jim">Jim</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was amazing"><span size="15x15" class="staticStar p10">it was amazing</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/6020795-jim?shelf=20th-century">20th-century</a>, 
              <a class="actionLinkLite" href="/review/list/6020795-jim?shelf=art-books">art-books</a>, 
              <a class="actionLinkLite" href="/review/list/6020795-jim?shelf=non-fiction">non-fiction</a>, 
              <a class="actionLinkLite" href="/review/list/6020795-jim?shelf=writing-creativity">writing-creativity</a>, 
