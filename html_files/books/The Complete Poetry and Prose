        $('bookCover_119239').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_119239').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
