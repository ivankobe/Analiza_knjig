        $('bookCover_30145150').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_30145150').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
