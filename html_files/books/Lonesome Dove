        $('bookCover_10920').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_10920').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
