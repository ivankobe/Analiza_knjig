          $$('div.prototip').each(function (e) {
            if ($('bookCover_14061955').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_14061955').observe('prototip:hidden', function () {
