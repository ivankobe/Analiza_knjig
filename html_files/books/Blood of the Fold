      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_43884').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_43884').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_43884').observe('prototip:hidden', function () {
