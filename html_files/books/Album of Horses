        $('bookCover_110737').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_110737').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
