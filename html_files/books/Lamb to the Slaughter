        $('bookCover_706809').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_706809').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
