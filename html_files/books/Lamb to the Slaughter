        $('bookCover_1279692').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1279692').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
