        $('bookCover_34715224').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_34715224').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
