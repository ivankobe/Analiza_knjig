        $('bookCover_17689918').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_17689918').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
