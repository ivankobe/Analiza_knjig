        $('bookCover_18663492').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_18663492').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
