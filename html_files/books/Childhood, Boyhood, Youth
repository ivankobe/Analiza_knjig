        $('bookCover_43263485').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_43263485').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
