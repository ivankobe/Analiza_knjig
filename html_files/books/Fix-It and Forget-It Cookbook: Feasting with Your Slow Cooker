        $('bookCover_184644').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_184644').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
