        $('bookCover_8578673').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_8578673').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
