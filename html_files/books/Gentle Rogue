        $('bookCover_107774').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_107774').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
