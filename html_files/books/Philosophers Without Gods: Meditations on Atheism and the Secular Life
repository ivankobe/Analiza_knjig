        $('bookCover_29875935').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_29875935').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
