<div id="review_3029055167" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/3029055167" />
    <a title="Phrynne" class="left imgcol" href="/user/show/8655084-phrynne"><img alt="Phrynne" src="https://images.gr-assets.com/users/1609359173p2/8655084.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/3029055167?book_show_action=true">Oct 30, 2019</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Phrynne" class="user" itemprop="url" name="Phrynne" href="/user/show/8655084-phrynne">Phrynne</a>
