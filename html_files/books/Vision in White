        $('bookCover_6400090').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_6400090').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
