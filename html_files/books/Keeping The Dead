        $('bookCover_7641662').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_7641662').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
