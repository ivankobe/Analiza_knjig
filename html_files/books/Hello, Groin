        $('bookCover_44900349').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_44900349').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
