        $('bookCover_43398196').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_43398196').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
