        $('bookCover_1456450').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1456450').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
