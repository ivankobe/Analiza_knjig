<div id="review_723434085" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/723434085" />
    <a title="Carol" class="left imgcol" href="/user/show/7668522-carol"><img alt="Carol" src="https://images.gr-assets.com/users/1546364878p2/7668522.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/723434085?book_show_action=true">Sep 19, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Carol" class="user" itemprop="url" name="Carol" href="/user/show/7668522-carol">Carol</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="really liked it"><span size="15x15" class="staticStar p10">really liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/7668522-carol?shelf=ebook">ebook</a>, 
              <a class="actionLinkLite" href="/review/list/7668522-carol?shelf=adventure">adventure</a>, 
              <a class="actionLinkLite" href="/review/list/7668522-carol?shelf=read-2013">read-2013</a>, 
              <a class="actionLinkLite" href="/review/list/7668522-carol?shelf=classics">classics</a>, 
              <a class="actionLinkLite" href="/review/list/7668522-carol?shelf=saw-the-movie">saw-the-movie</a>
