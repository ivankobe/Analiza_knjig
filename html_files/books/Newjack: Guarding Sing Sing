<div id="review_17363737" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/17363737" />
    <a title="Mike" class="left imgcol" href="/user/show/234559-mike"><img alt="Mike" src="https://s.gr-assets.com/assets/nophoto/user/m_50x66-82093808bca726cb3249a493fbd3bd0f.png" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/17363737?book_show_action=true">Mar 09, 2008</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Mike" class="user" itemprop="url" name="Mike" href="/user/show/234559-mike">Mike</a>
