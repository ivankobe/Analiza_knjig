        $('bookCover_35355104').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_35355104').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
