        $('bookCover_3828').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_3828').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
