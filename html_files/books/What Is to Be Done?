        $('bookCover_158358').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_158358').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
