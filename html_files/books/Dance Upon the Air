<div id="review_255749261" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/255749261" />
    <a title="CatBookMom" class="left imgcol" href="/user/show/7318623-catbookmom"><img alt="CatBookMom" src="https://images.gr-assets.com/users/1351197485p2/7318623.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/255749261?book_show_action=true">Jan 03, 2012</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="CatBookMom" class="user" itemprop="url" name="CatBookMom" href="/user/show/7318623-catbookmom">CatBookMom</a>
