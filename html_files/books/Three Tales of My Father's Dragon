          $$('div.prototip').each(function (e) {
            if ($('bookCover_34898').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_34898').observe('prototip:hidden', function () {
