        $('bookCover_34459').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_34459').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
