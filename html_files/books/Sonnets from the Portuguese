        $('bookCover_1134254').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1134254').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_1134254').observe('prototip:hidden', function () {
