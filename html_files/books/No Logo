        $('bookCover_38338').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_38338').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
