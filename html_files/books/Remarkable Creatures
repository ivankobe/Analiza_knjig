        $('bookCover_22524719').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_22524719').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
