      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_22206705').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_22206705').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_22206705').observe('prototip:hidden', function () {
