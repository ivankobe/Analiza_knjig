        $('bookCover_28953491').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_28953491').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
