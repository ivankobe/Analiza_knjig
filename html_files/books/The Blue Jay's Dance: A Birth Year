        $('bookCover_52873').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_52873').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
