          $$('div.prototip').each(function (e) {
            if ($('bookCover_44034500').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_44034500').observe('prototip:hidden', function () {
