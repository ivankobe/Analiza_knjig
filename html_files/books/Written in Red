          $$('div.prototip').each(function (e) {
            if ($('bookCover_6329547').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_6329547').observe('prototip:hidden', function () {
