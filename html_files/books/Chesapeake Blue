        $('bookCover_628139').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_628139').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
