        $('bookCover_50138038').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_50138038').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
