  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/3157912848?book_show_action=true">Jan 24, 2020</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Dillah" class="user" itemprop="url" name="Dillah" href="/user/show/109191639-dillah">Dillah</a>
