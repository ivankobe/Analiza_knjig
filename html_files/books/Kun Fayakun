        $('bookCover_9882894').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_9882894').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
