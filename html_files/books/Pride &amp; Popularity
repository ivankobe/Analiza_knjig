      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('edition_12486664').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('edition_12486664').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('edition_12486664').observe('prototip:hidden', function () {
