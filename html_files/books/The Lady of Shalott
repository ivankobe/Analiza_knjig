        $('bookCover_19428241').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_19428241').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
