        $('bookCover_40042024').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_40042024').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
