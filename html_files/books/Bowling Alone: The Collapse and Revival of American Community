        $('bookCover_55339408').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_55339408').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
