        $('bookCover_44575063').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_44575063').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
