        $('edition_38667').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('edition_38667').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
