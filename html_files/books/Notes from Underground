        $('bookCover_36052495').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_36052495').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
