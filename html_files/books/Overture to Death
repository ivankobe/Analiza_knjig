        $('bookCover_1610324').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1610324').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
