        $('bookCover_6472451').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_6472451').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
