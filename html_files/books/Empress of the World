        $('bookCover_316445').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_316445').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_316445').observe('prototip:hidden', function () {
