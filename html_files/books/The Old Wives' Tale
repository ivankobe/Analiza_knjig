        $('bookCover_30919').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_30919').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
