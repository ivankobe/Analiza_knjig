        $('bookCover_8579004').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_8579004').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
