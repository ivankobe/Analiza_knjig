        $('bookCover_210094').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_210094').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_210094').observe('prototip:hidden', function () {
