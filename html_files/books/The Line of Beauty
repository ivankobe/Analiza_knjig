        $('bookCover_3656').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_3656').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
