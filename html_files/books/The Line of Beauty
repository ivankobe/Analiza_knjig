        $('bookCover_1025100').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1025100').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
