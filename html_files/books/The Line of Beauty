        $('bookCover_8664368').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_8664368').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
