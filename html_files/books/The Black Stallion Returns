          $$('div.prototip').each(function (e) {
            if ($('bookCover_99738').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_99738').observe('prototip:hidden', function () {
