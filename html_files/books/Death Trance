      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_1983070').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1983070').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_1983070').observe('prototip:hidden', function () {
