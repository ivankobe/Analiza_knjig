        $('bookCover_29340182').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_29340182').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
