        $('bookCover_12774845').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_12774845').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
