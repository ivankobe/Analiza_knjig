        $('bookCover_29856533').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_29856533').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
