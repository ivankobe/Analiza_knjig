        $('bookCover_10856212').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_10856212').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
