        $('bookCover_43583366').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_43583366').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
