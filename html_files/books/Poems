        $('bookCover_54283').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_54283').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
