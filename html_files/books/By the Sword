      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_763'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/28735.By_the_Sword?rating=\">all <span class=\"greyText\">(28052)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/28735.By_the_Sword?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;eOveK8AALB8cQbm6rbAQdqXGAZs0ANk7zIX2y3KV9g72KTejnd5gf9qtKmWW76/irlEb7zc+k9P2EVtWFraxqw==&#39;)}); return false;\">5 stars <span class=\"greyText\">(9518)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/28735.By_the_Sword?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;KsAS68Io1ih3EtLJmL7uycaOgXrYBAAYdhLTYFcGgSmkAvtjn/aaSLH+QRaj4VFdzRmbDts6SvBMhn79MyXGjA==&#39;)}); return false;\">4 stars <span class=\"greyText\">(6676)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=