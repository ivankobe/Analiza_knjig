        $('bookCover_55016385').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_55016385').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
