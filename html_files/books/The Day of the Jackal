<div id="review_1705288427" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1705288427" />
    <a title="Sarah" class="left imgcol" href="/user/show/23841736-sarah"><img alt="Sarah" src="https://images.gr-assets.com/users/1532221628p2/23841736.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1705288427?book_show_action=true">Feb 16, 2017</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Sarah" class="user" itemprop="url" name="Sarah" href="/user/show/23841736-sarah">Sarah</a>
