        $('bookCover_968').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_968').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
