        $('bookCover_43904017').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_43904017').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
