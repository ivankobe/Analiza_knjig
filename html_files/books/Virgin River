<div id="review_2200646355" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2200646355" />
    <a title="Alla" class="left imgcol" href="/user/show/16232539-alla"><img alt="Alla" src="https://images.gr-assets.com/users/1574247437p2/16232539.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2200646355?book_show_action=true">May 08, 2019</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Alla" class="user" itemprop="url" name="Alla" href="/user/show/16232539-alla">Alla</a>
