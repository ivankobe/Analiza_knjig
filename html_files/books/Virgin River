        $('bookCover_4943352').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_4943352').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_4943352').observe('prototip:hidden', function () {
