      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_11506144').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_11506144').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_11506144').observe('prototip:hidden', function () {
