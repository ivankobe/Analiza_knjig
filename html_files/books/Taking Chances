        $('bookCover_7735333').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_7735333').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_7735333').observe('prototip:hidden', function () {
