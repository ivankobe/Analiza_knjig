<div id="review_1103259075" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1103259075" />
    <a title="Heidi" class="left imgcol" href="/user/show/5633520-heidi"><img alt="Heidi" src="https://images.gr-assets.com/users/1391454724p2/5633520.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1103259075?book_show_action=true">Nov 10, 2014</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Heidi" class="user" itemprop="url" name="Heidi" href="/user/show/5633520-heidi">Heidi</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="really liked it"><span size="15x15" class="staticStar p10">really liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/5633520-heidi?shelf=clean-romance">clean-romance</a>, 
              <a class="actionLinkLite" href="/review/list/5633520-heidi?shelf=lds-fiction">lds-fiction</a>, 
              <a class="actionLinkLite" href="/review/list/5633520-heidi?shelf=humor">humor</a>, 
              <a class="actionLinkLite" href="/review/list/5633520-heidi?shelf=swoon-worthy-hero">swoon-worthy-hero</a>, 
              <a class="actionLinkLite" href="/review/list/5633520-heidi?shelf=reading-again">reading-again</a>, 
              <a class="actionLinkLite" href="/review/list/5633520-heidi?shelf=adult-read">adult-read</a>, 
              <a class="actionLinkLite" href="/review/list/5633520-heidi?shelf=contemporary">contemporary</a>, 
              <a class="actionLinkLite" href="/review/list/5633520-heidi?shelf=tough-topics">tough-topics</a>
          </div>
