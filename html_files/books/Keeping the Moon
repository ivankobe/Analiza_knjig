        $('bookCover_16034187').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_16034187').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
