        $('bookCover_29289').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_29289').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_29289').observe('prototip:hidden', function () {
        $$('span.elementTwo').each(function (e) {
