        $('bookCover_48807382').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_48807382').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
