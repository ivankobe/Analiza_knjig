        $('bookCover_55200670').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_55200670').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
