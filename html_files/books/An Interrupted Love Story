        $('bookCover_55994373').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_55994373').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
