  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1939693375?book_show_action=true">Mar 17, 2017</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Maureen" class="user" itemprop="url" name="Maureen" href="/user/show/10762033-maureen">Maureen</a>
