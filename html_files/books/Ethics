      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('edition_13422597').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('edition_13422597').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('edition_13422597').observe('prototip:hidden', function () {
