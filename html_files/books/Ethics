        $('bookCover_80324').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_80324').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
