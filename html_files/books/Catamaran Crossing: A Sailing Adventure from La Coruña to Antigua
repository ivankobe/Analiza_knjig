      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('edition_53881901').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('edition_53881901').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('edition_53881901').observe('prototip:hidden', function () {
