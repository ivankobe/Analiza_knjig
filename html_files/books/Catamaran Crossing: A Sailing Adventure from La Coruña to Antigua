        $('bookCover_23677008').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_23677008').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
