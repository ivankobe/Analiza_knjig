        $('bookCover_243854').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_243854').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
