        $('bookCover_559773').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_559773').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
