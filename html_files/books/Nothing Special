        $('bookCover_405509').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_405509').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
