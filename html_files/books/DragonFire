        $('bookCover_7971985').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_7971985').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
