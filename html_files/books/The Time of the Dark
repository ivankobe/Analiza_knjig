        $('bookCover_13017352').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_13017352').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
