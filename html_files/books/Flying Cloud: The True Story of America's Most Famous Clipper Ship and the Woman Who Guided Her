        $('bookCover_72707').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_72707').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_72707').observe('prototip:hidden', function () {
