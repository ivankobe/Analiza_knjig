        $('bookCover_56277583').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_56277583').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_56277583').observe('prototip:hidden', function () {
