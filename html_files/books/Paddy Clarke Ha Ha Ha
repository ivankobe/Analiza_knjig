      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('edition_843687').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('edition_843687').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('edition_843687').observe('prototip:hidden', function () {
