        $('bookCover_53984774').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_53984774').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
