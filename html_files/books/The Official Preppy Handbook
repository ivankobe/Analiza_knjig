        $j("#reviewControls__ratingDetailsMiniGraph").prependTo("#rating_details_tip");
      });
    //]]>
    </script>
    </span>
    <a id="rating_details_tip" class="gr-hyperlink" style="cursor: pointer; " href="#" onclick="return false"> Rating details</a><script>
//<![CDATA[
      var newTip = new Tip($('rating_details_tip'), "\n        <div id=\"moreBookData\" class=\"bookMeta\" >\n  \n\n  \n\n<table border=\"0\" id=\"rating_distribution\" style=\"width: auto; max-width: 500px;\" cellspacing=\"5\" cellpadding=\"0\">\n      <tr>\n        <th width=\"25\" class=\"right\">5&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 100%;\" title=\"734 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/120254.The_Official_Preppy_Handbook?rating=5\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'UfG42/jQR6PoznMI41KKI1Cx1phjwA/tldzTd5Is97neVIYya+GB0cxWWvp3wjrGLwjv84sTJmh9I4LzNeuHmg==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          45% (734)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">4&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 69%;\" title=\"507 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/120254.The_Official_Preppy_Handbook?rating=4\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'+E5Zn9xSrb+BY73xTaCn4eV0S//QR4vmWQq95My78U9362d2T2NrzaX7lAPZMBcEms1ylDiUomOx9exga3yBbA==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          31% (507)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">3&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 39%;\" title=\"287 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/120254.The_Offici