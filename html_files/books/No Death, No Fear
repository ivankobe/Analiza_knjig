      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_141209').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_141209').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_141209').observe('prototip:hidden', function () {
