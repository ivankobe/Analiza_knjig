      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_784'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/5129.Brave_New_World?rating=\">all <span class=\"greyText\">(2535167)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/5129.Brave_New_World?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;TbvJbefEQ7LJUFTsUSwxEHozFCJUz6zWeAyn29TMYSUdb2ZqGhD3sHELBaT3s1mkUSkiPKbib4zsxG7rDCjvdg==&#39;)}); return false;\">5 stars <span class=\"greyText\">(567250)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/5129.Brave_New_World?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;8fGGaxBCTlZoTYM9AfHgxsurTaPD3rjiYhXQ6mzNQX2hJSls7Zb6VNAW0nWnbohy4LF7vTHze7j23RnatCnPLg==&#39;)}); return false;\">4 stars <span class=\"greyText\">(554119)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  