        $('bookCover_10778').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_10778').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
