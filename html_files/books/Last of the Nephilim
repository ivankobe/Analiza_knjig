        $('bookCover_53355542').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_53355542').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
