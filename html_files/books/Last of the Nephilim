      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_36099148').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_36099148').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_36099148').observe('prototip:hidden', function () {
