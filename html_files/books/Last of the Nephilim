        $('bookCover_57337459').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_57337459').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
