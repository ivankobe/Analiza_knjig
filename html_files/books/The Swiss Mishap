        $('bookCover_56068001').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_56068001').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
