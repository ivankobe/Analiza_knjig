<div id="review_570816856" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/570816856" />
    <a title="Catherine Weller" class="left imgcol" href="/user/show/2396795-catherine-weller"><img alt="Catherine Weller" src="https://images.gr-assets.com/users/1358207536p2/2396795.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/570816856?book_show_action=true">Mar 25, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Catherine Weller" class="user" itemprop="url" name="Catherine Weller" href="/user/show/2396795-catherine-weller">Catherine Weller</a>
