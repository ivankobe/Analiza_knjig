      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_54369251').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_54369251').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_54369251').observe('prototip:hidden', function () {
