      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_13153').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_13153').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_13153').observe('prototip:hidden', function () {
