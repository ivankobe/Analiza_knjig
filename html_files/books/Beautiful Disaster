        $('bookCover_15717943').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_15717943').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
