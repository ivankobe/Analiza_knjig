        $('bookCover_14622962').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_14622962').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
