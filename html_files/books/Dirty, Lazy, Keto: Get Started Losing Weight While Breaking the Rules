        $('bookCover_10766509').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_10766509').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
