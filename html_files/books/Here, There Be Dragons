        $('bookCover_6339053').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_6339053').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
