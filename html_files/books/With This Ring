        $('bookCover_29617').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_29617').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_29617').observe('prototip:hidden', function () {
