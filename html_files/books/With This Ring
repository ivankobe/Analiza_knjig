        $('bookCover_331005').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_331005').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
