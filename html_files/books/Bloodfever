        $('bookCover_12444298').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_12444298').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
