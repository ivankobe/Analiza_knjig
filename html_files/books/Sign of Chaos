        $('bookCover_61994').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_61994').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
