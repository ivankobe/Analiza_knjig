<div id="review_777741903" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/777741903" />
    <a title="Jaidee " class="left imgcol" href="/user/show/17295672-jaidee"><img alt="Jaidee " src="https://images.gr-assets.com/users/1558439086p2/17295672.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/777741903?book_show_action=true">Nov 30, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Jaidee " class="user" itemprop="url" name="Jaidee " href="/user/show/17295672-jaidee">Jaidee </a>
