<div id="review_28488359" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/28488359" />
    <a title="Suvi" class="left imgcol" href="/user/show/59357-suvi"><img alt="Suvi" src="https://images.gr-assets.com/users/1619389869p2/59357.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/28488359?book_show_action=true">Jul 28, 2008</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Suvi" class="user" itemprop="url" name="Suvi" href="/user/show/59357-suvi">Suvi</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was amazing"><span size="15x15" class="staticStar p10">it was amazing</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/59357-suvi?shelf=_europe-western">_europe-western</a>, 
              <a class="actionLinkLite" href="/review/list/59357-suvi?shelf=1800s">1800s</a>, 
              <a class="actionLinkLite" href="/review/list/59357-suvi?shelf=children">children</a>, 
              <a class="actionLinkLite" href="/review/list/59357-suvi?shelf=_favorites">_favorites</a>
          </div>
