<div id="review_98565349" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/98565349" />
    <a title="Stephen" class="left imgcol" href="/user/show/1413439-stephen"><img alt="Stephen" src="https://images.gr-assets.com/users/1311616811p2/1413439.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/98565349?book_show_action=true">Apr 14, 2010</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Stephen" class="user" itemprop="url" name="Stephen" href="/user/show/1413439-stephen">Stephen</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="liked it"><span size="15x15" class="staticStar p10">liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/1413439-stephen?shelf=ebooks">ebooks</a>, 
              <a class="actionLinkLite" href="/review/list/1413439-stephen?shelf=historical-horror">historical-horror</a>, 
              <a class="actionLinkLite" href="/review/list/1413439-stephen?shelf=supernatural-spies-and-slayers">supernatural-spies-and-slayers</a>, 
              <a class="actionLinkLite" href="/review/list/1413439-stephen?shelf=biographies">biographies</a>, 
              <a class="actionLinkLite" href="/review/list/1413439-stephen?shelf=audiobook">audiobook</a>, 
