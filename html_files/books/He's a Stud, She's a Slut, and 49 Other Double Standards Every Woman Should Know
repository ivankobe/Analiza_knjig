        $('bookCover_49151018').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_49151018').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
