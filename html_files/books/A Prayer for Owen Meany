        $('bookCover_310612').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_310612').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
