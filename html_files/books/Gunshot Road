<div id="review_2218844025" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2218844025" />
    <a title="K." class="left imgcol" href="/user/show/5663034-k"><img alt="K." src="https://images.gr-assets.com/users/1525318510p2/5663034.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2218844025?book_show_action=true">Dec 19, 2017</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="K." class="user" itemprop="url" name="K." href="/user/show/5663034-k">K.</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was ok"><span size="15x15" class="staticStar p10">it was ok</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/5663034-k?shelf=2017">2017</a>, 
              <a class="actionLinkLite" href="/review/list/5663034-k?shelf=australian">australian</a>, 
              <a class="actionLinkLite" href="/review/list/5663034-k?shelf=crime">crime</a>, 
              <a class="actionLinkLite" href="/review/list/5663034-k?shelf=bipoc-narrator">bipoc-narrator</a>, 
              <a class="actionLinkLite" href="/review/list/5663034-k?shelf=first-nations">first-nations</a>, 
              <a class="actionLinkLite" href="/review/list/5663034-k?shelf=omg-that-cover">omg-that-cover</a>, 
              <a class="actionLinkLite" href="/review/list/5663034-k?shelf=difficult-topics">difficult-topics</a>, 
              <a class="actionLinkLite" href="/review/list/5663034-k?shelf=library">library</a>, 
              <a class="actionLinkLite" href="/review/list/5663034-k?shelf=setting-australia">setting-australia</a>, 
