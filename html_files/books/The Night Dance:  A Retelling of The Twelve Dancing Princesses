        $('bookCover_303197').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_303197').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
