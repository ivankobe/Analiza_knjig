        $('bookCover_36478323').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_36478323').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
