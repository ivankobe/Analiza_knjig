      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_382'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/52385.Almanac_of_the_Dead?rating=\">all <span class=\"greyText\">(12538)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/52385.Almanac_of_the_Dead?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;Hz6ooxjOR4UYDRgEfWLqmH1rR4Du+4UjaOhGnvqfE+ZXdbgIfCQTX7MP/BXFEp2KnTZAEAQZy+iaQx1vT0lLUg==&#39;)}); return false;\">5 stars <span class=\"greyText\">(1038)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/52385.Almanac_of_the_Dead?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;j9wUJdhK8HYU9D8XniYJZQH6g7Fw7hEdlAc44ySKP5HHlwSOvKCkrL/22wYmVn534aeEIZoMX9ZmrGMSkVxnJQ==&#39;)}); return false;\">4 stars <span class=\"greyText\">(849)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/52385.Almanac_of_the_Dead?rating=3&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;4H0MJ0WKDmp1bUL2DMn6bB4PdD+3nGkWcywwuo+Q5qaoNhyMIWBasN5vpue0uY1+/lJzr11+J92Bh2tLOka+Eg==&#39;)}); return false;\">3 stars <span class=\"greyText\">(520)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/52385.Almanac_of_the_Dead?rating=2&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;lo4hXNS54iAa6kTNc7HKMiPtBGAH5HIQnFSs2oxAuN3exTH3sFO2+rHooNzLwb0gw7AD8O0GPNtu//crOZbgaQ==&#39;)}); return false;\">2 stars <span class=\"greyText\">(163)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/52385.Almanac_of_the_Dead?rating=1&#39;, {asynchronous:true, evalScripts:true,