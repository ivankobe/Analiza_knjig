        $('bookCover_8437379').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_8437379').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
