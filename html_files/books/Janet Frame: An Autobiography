      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_110402').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_110402').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_110402').observe('prototip:hidden', function () {
