      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_133496').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_133496').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_133496').observe('prototip:hidden', function () {
