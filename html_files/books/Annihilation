        $('bookCover_41160292').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_41160292').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
