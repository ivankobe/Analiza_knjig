        $('bookCover_48496518').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_48496518').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
