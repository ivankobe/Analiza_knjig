      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_2458974').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_2458974').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_2458974').observe('prototip:hidden', function () {
