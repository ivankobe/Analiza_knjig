        $('bookCover_38342486').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_38342486').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
