        $('bookCover_890').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_890').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
