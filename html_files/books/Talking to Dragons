      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_78'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/169871.Talking_to_Dragons?rating=\">all <span class=\"greyText\">(46410)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/169871.Talking_to_Dragons?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;L2GbGSqFw08yJM1/lW9VG3WtmTvmyTBaRJBqhS4keB7BrhMEyieh1eVPoFyWsclPHTfklcFgQqtahw/sBldBiA==&#39;)}); return false;\">5 stars <span class=\"greyText\">(15703)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/169871.Talking_to_Dragons?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;sTIAhEiU+zSYdEOvZqLYE1SY5TPlRBcrxolZjVElXm1f/YiZqDaZrk8fLoxlfERHPAKYncLtZdrYnjzkeVZn+w==&#39;)}); return false;\">4 stars <span class=\"greyText\">(11123)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n        