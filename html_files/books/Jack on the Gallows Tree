      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_18743333').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_18743333').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_18743333').observe('prototip:hidden', function () {
