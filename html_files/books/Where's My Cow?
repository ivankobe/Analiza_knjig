        $('bookCover_483760').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_483760').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
