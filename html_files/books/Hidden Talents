        $('bookCover_367686').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_367686').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
