      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_449'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/17977.The_Ancestor_s_Tale?rating=\">all <span class=\"greyText\">(72309)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/17977.The_Ancestor_s_Tale?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;wj6YS9rn08MjmsG0GanOHyBhwE0VneI0RgR8E6Lvh3B/N345L286xMRhh7EzsfEBLMduS9e73hEjq7CtIY84rQ==&#39;)}); return false;\">5 stars <span class=\"greyText\">(11372)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/17977.The_Ancestor_s_Tale?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;KiE0OOCTUCnRA00JnOlORarB2db84V6d8qQymVkiDsaXKNJKFRu5Ljb4Cwy28XFbpmd30D7HYriXC/4n2kKxGw==&#39;)}); return false;\">4 stars <span class=\"greyText\">(7942)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n        