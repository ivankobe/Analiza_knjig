        $('bookCover_43717362').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_43717362').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
