        $('bookCover_56590638').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_56590638').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
