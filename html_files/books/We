        $('bookCover_650777').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_650777').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
