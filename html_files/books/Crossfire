        $('bookCover_49201252').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_49201252').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
