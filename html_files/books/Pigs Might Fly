<div id="review_2282302073" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2282302073" />
    <a title="Oona" class="left imgcol" href="/user/show/315806-oona"><img alt="Oona" src="https://images.gr-assets.com/users/1384041876p2/315806.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2282302073?book_show_action=true">Feb 02, 2018</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Oona" class="user" itemprop="url" name="Oona" href="/user/show/315806-oona">Oona</a>
