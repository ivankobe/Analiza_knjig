<div id="review_2065342785" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2065342785" />
    <a title="Darby" class="left imgcol" href="/user/show/29458697-darby"><img alt="Darby" src="https://images.gr-assets.com/users/1496014228p2/29458697.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2065342785?book_show_action=true">Jul 20, 2017</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Darby" class="user" itemprop="url" name="Darby" href="/user/show/29458697-darby">Darby</a>
      </span>

