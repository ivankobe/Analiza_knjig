        $('bookCover_659600').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_659600').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
