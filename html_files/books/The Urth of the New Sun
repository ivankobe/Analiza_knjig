        $('bookCover_1673451').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1673451').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
