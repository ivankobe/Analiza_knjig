        $('bookCover_1137680').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1137680').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
