        $('bookCover_18090153').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_18090153').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
