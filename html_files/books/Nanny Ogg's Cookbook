        $('bookCover_55427204').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_55427204').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
