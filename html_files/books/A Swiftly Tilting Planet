<div id="review_1800558729" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1800558729" />
    <a title="Alaina" class="left imgcol" href="/user/show/50620706-alaina"><img alt="Alaina" src="https://images.gr-assets.com/users/1451325817p2/50620706.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1800558729?book_show_action=true">Mar 16, 2018</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Alaina" class="user" itemprop="url" name="Alaina" href="/user/show/50620706-alaina">Alaina</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="liked it"><span size="15x15" class="staticStar p10">liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/50620706-alaina?shelf=fantasy">fantasy</a>, 
              <a class="actionLinkLite" href="/review/list/50620706-alaina?shelf=march-challenge-2018">march-challenge-2018</a>, 
              <a class="actionLinkLite" href="/review/list/50620706-alaina?shelf=sci-fi">sci-fi</a>, 
              <a class="actionLinkLite" href="/review/list/50620706-alaina?shelf=young-adult">young-adult</a>, 
