          $$('div.prototip').each(function (e) {
            if ($('bookCover_33574273').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_33574273').observe('prototip:hidden', function () {
