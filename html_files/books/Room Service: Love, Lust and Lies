        $('bookCover_39626243').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_39626243').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
