        $('bookCover_130149').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_130149').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_130149').observe('prototip:hidden', function () {
