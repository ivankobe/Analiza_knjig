<script type="text/javascript" charset="utf-8">
//<![CDATA[  

  function submitShelfLink(unique_id, book_id, shelf_id, shelf_name, submit_form, exclusive) {
    var checkbox_id = 'shelf_name_' + unique_id + '_' + shelf_id;
    var element = document.getElementById(checkbox_id)

    var checked = element.checked
    if (checked && exclusive) {
      // can't uncheck a radio by clicking it!
      return
    }
    if(document.getElementById("savingMessage")){
      Element.show('savingMessage')
    }
    var element_id = 'shelfInDropdownName_' + unique_id + '_' + shelf_id;
    Element.update(element_id, "saving...");
    if (submit_form) {
      Element.hide('shelfDropdown_' + unique_id)
      var form = document.getElementById('addBookForm' + book_id)
      if (form) {
        form.shelf.value = shelf_name
        form.onsubmit()
      }
    }
    else {
      var action = checked ? 'remove' : ''
      element.checked = !element.checked
      new Ajax.Request('/shelf/add_to_shelf', {asynchronous:true, evalScripts:true, onSuccess:function(request){shelfSubmitted(request, book_id, checkbox_id, element_id, unique_id, shelf_name)}, parameters:'book_id=' + book_id + '&name=' + shelf_name + '&a=' + action + '&authenticity_token=' + encodeURIComponent('h7tjdIS95y40kHefTfbG/1SZKAG28BJh8igmrQ2iCTDo/mF7gUuSUuiyTbVxy9GK/9scM73pKQSjuey7+AJjhg==')})
    }
  }

  function shelfSubmitted(request, book_id, checkbox_id, element_id, unique_id, shelf_name) {
    Element.update('shelfListfalse_' + book_id, request.responseText)
    afterShelfSave(checkbox_id, element_id, unique_id, shelf_name.escapeHTML())
  }

  function refreshGroupBox(group_id, book_id) {
    new Ajax.Updater('addGroupBooks' + book_id + '', '/group/add_book_box', {asynchronous:true, evalScripts:true, onSuccess:function(request){refreshGroupBoxComplete(request, book_id);}, parameters:'id=' + group_id + '&book_id=' + book_id + '&refresh=true' + '&authenticity_token=' + encodeURIComponent('HyMBZy3uufJL+AaqF7oqsrOTP/Q88tSwSkR2UuLGGRZwZgNoKBjMjpfaPIArhz3HGNELxjfr79Ub1bxEF2ZzoA==')})
  }
//]]>
</script>


<script>
//<![CDATA[
      var newTip = new Tip($('bookCover_73954'), "\n\n  <h2><a class=\"readable bookTitle\" href=\"https://www.goodreads.com/book/show/73954.The_Eustace_Diamonds?from_choice=false&amp;from_home_module=false\">The Eustace Diamonds<\/a><\/h2>\n\n      <div>\n        by <a class=\"authorName\" href=\"/author/show/20524.Anthony_Trollope?a=9&amp;origin=related_works\">Anthony Trollope<\/a>\n      <\/div>\n\n          <div class=\"smallText uitext darkGreyText\">\n            <span class=\"minirating\"><span class=\"stars staticStars notranslate\"><span size=\"12x12\" class=\"staticStar p10\"><\/span><span size=\"12x12\" class=\"staticStar p10\"><\/span><span size=\"12x12\" class=\"staticStar p10\"><\/span><span size=\"12x12\" class=\"staticStar p6\"><\/span><span size=\"12x12\" class=\"staticStar p0\"><\/span><\/span> 3.96 avg rating &mdash; 4,159 ratings<\/span>            &mdash; published 1873\n          <\/div>\n\n    <div class=\"addBookTipDescription\">\n      \n<span id=\"freeTextContainer1750070226561881231\">Following the death of her husband Sir Florian, beautiful Lizzie Eustace mysteriously comes into possession of a hugely expensive diamond necklace. She maintains it was a gift from her husband, but the Eustace lawyers insist she give it up, and while<\/span>\n  <span id=\"freeText1750070226561881231\" style=\"display:none\">Following the death of her husband Sir Florian, beautiful Lizzie Eustace mysteriously comes into possession of a hugely expensive diamond necklace. She maintains it was a gift from her husband, but the Eustace lawyers insist she give it up, and while her cousin Frank takes her side, her new lover Lord Fawn states that he will only marry her if the necklace is surrendered. As gossip and scandal intensify, Lizzie\'s truthfulness is thrown into doubt, and, in her desire to keep the jewels, she is driven to increasingly desperate acts. The third in Trollope\'s Palliser series, The Eustace Diamonds bears all the hallmarks of his later works, blending dark cynicism with humour and a keen perception of human nature.<\/span>\n  <a data-text-id=\"1750070226561881231\" href=\"#\" onclick=\"swapContent(\$(this));; return false;\">...more<\/a>\n\n    <\/div>\n\n      <div class=\'wtrButtonContainer wtrSignedOut\' id=\'8_book_73954\'>\n<div class=\'wtrUp wtrLeft\'>\n<form action=\"/shelf/add_to_shelf\" accept-charset=\"UTF-8\" method=\"post\"><input name=\"utf8\" type=\"hidden\" value=\"&#x2713;\" /><input type=\"hidden\" name=\"authenticity_token\" value=\"ucal3+fo5NqU1ilI+gHSUIUteYIecsoqnL2a2xFSrDXWg6fQ4h6Rpkj0E2LGPMUlLm9NsBVr8U/NLFDN5PLGgw==\" />\n<input type=\"hidden\" name=\"book_id\" id=\"book_id\" value=\"73954\" />\n<input type=\"hidden\" name=\"name\" id=\"name\" value=\"to-read\" />\n<input type=\"hidden\" name=\"unique_id\" id=\"unique_id\" value=\"8_book_73954\" />\n<input type=\"hidden\" name=\"wtr_new\" id=\"wtr_new\" value=\"true\" />\n<input type=\"hidden\" name=\"from_choice\" id=\"from_choice\" value=\"false\" />\n<input type=\"hidden\" name=\"from_home_module\" id=\"from_home_module\" value=\"false\" />\n<input type=\"hidden\" name=\"ref\" id=\"ref\" value=\"\" class=\"wtrLeftUpRef\" />\n<input type=\"hidden\" name=\"existing_review\" id=\"existing_review\" value=\"false\" class=\"wtrExisting\" />\n<input type=\"hidden\" name=\"book_origin\" id=\"book_origin\" value=\"related_works\" />\n<input type=\"hidden\" name=\"page_url\" id=\"page_url\" value=\"/book/show/748318.The_Man_of_Property\" />\n<button class=\'wtrToRead\' type=\'submit\'>\n<span class=\'progressTrigger\'>Want to Read<\/span>\n<span class=\'progressIndicator\'>saving…<\/span>\n<\/button>\n<\/form>\n\n<\/div>\n\n<div class=\'wtrRight wtrUp\'>\n<form class=\"hiddenShelfForm\" action=\"/shelf/add_to_shelf\" accept-charset=\"UTF-8\" method=\"post\"><input name=\"utf8\" type=\"hidden\" value=\"&#x2713;\" /><input type=\"hidden\" name=\"authenticity_token\" value=\"4GXDeYaKbvDjNbPS/eYJ5H34lbABS+mblFxe/arOmZePIMF2g3wbjD8XifjB2x6R1rqhg