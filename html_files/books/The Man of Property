          $$('div.prototip').each(function (e) {
            if ($('bookCover_56970974').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_56970974').observe('prototip:hidden', function () {
