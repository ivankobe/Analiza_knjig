        $('bookCover_388997').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_388997').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
