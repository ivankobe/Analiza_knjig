        $('bookCover_53103895').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_53103895').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_53103895').observe('prototip:hidden', function () {
        $$('span.elementTwo').each(function (e) {
          if (e.getStyle('display') !== 'none') {
