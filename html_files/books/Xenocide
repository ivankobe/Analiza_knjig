        $('bookCover_56313556').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_56313556').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
