      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_25061').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_25061').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_25061').observe('prototip:hidden', function () {
