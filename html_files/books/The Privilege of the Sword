<div id="review_3728784571" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/3728784571" />
    <a title="Michele" class="left imgcol" href="/user/show/47573813-michele"><img alt="Michele" src="https://images.gr-assets.com/users/1609089019p2/47573813.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/3728784571?book_show_action=true">Dec 30, 2020</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Michele" class="user" itemprop="url" name="Michele" href="/user/show/47573813-michele">Michele</a>
