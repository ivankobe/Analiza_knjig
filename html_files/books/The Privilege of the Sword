        $('bookCover_613268').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_613268').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
