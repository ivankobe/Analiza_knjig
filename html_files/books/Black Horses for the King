        $('bookCover_164255').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_164255').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
