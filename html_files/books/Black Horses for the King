        $('bookCover_429293').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_429293').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_429293').observe('prototip:hidden', function () {
