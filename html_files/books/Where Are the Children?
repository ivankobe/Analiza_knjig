        $('bookCover_1540976').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1540976').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
