<div id="review_3072642706" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/3072642706" />
    <a title="Kevin" class="left imgcol" href="/user/show/1660832-kevin"><img alt="Kevin" src="https://images.gr-assets.com/users/1233009063p2/1660832.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/3072642706?book_show_action=true">Dec 05, 2019</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Kevin" class="user" itemprop="url" name="Kevin" href="/user/show/1660832-kevin">Kevin</a>
