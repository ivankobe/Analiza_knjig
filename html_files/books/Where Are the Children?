        $('bookCover_50492330').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_50492330').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_50492330').observe('prototip:hidden', function () {
