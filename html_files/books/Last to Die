        $('bookCover_28502605').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_28502605').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
