        $('bookCover_111126').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_111126').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
