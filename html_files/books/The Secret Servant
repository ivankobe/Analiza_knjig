<div id="review_1479699953" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1479699953" />
    <a title="Nancy" class="left imgcol" href="/user/show/20473691-nancy"><img alt="Nancy" src="https://images.gr-assets.com/users/1410275344p2/20473691.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1479699953?book_show_action=true">Nov 30, 2016</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Nancy" class="user" itemprop="url" name="Nancy" href="/user/show/20473691-nancy">Nancy</a>
