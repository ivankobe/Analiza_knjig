        $('bookCover_658625').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_658625').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
