        $('bookCover_543137').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_543137').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
