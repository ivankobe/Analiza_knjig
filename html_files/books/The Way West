        $('bookCover_13760').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_13760').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
