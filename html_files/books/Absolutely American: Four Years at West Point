        $('bookCover_45422550').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_45422550').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
