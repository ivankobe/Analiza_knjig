        $('bookCover_57763769').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_57763769').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
