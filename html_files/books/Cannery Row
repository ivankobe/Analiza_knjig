        $('bookCover_25575277').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_25575277').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
