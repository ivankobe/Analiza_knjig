        $('bookCover_3876').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_3876').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_3876').observe('prototip:hidden', function () {
