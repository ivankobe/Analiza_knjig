<div id="review_1070038250" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1070038250" />
    <a title="Nick" class="left imgcol" href="/user/show/301548-nick"><img alt="Nick" src="https://images.gr-assets.com/users/1411498560p2/301548.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1070038250?book_show_action=true">Oct 02, 2014</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Nick" class="user" itemprop="url" name="Nick" href="/user/show/301548-nick">Nick</a>
