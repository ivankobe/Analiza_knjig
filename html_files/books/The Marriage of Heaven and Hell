        $('bookCover_112110').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_112110').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
