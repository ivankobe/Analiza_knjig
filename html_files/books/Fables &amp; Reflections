        $('bookCover_109245').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_109245').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
