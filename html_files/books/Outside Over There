        $('bookCover_22208').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_22208').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
