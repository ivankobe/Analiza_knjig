        $('bookCover_13664840').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_13664840').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
