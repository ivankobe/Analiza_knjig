        $('bookCover_42514859').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_42514859').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
