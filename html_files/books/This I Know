<div id="review_2276230086" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2276230086" />
    <a title="Anneke" class="left imgcol" href="/user/show/6569017-anneke"><img alt="Anneke" src="https://images.gr-assets.com/users/1589938347p2/6569017.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2276230086?book_show_action=true">Jan 28, 2018</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Anneke" class="user" itemprop="url" name="Anneke" href="/user/show/6569017-anneke">Anneke</a>
