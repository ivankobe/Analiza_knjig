        $('bookCover_20986991').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_20986991').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
