        $('bookCover_24565702').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_24565702').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
