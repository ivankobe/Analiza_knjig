        $('bookCover_43933').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_43933').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
