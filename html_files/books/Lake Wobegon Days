        $('bookCover_399999').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_399999').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
