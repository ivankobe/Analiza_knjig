        $('bookCover_21798827').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_21798827').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
