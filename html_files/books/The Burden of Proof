        $('bookCover_772745').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_772745').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
