        $('bookCover_10290471').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_10290471').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
