      <a class="uitext" rel="nofollow" id="span_class_gr-hyperlink_more_filters_span_tip_884" style="cursor: pointer; " href="#" onclick="return false"><span class="gr-hyperlink">More filters</span></a><script>
//<![CDATA[
      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_884'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/83422.A_Dark_Adapted_Eye?rating=\">all <span class=\"greyText\">(16508)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/83422.A_Dark_Adapted_Eye?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;Z5srR8WT78KYSdIqnlG989R5HmUjl5hNT10BB824uYmAlNsx+jXKuPEHrnmnTMsv+JH0AYY1dwgbKy7gGhgbhw==&#39;)}); return false;\">5 stars <span class=\"greyText\">(3151)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/83422.A_Dark_Adapted_Eye?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;Hf+1e85J1k1EYEkUGSjGRGstBVz9z/6E9bxojm68JJn68EUN8e/zNy0uNUcgNbCYR8XvOFhtEcGhykdpuRyGlw==&#39;)}); return false;\">4 stars <span class=\"greyText\">(3551)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n            