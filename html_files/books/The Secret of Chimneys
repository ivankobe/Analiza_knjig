        $('bookCover_52582758').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_52582758').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
