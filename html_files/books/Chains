<div id="review_33815837" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/33815837" />
    <a title="Staci" class="left imgcol" href="/user/show/177641-staci"><img alt="Staci" src="https://images.gr-assets.com/users/1445133100p2/177641.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/33815837?book_show_action=true">Sep 25, 2008</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Staci" class="user" itemprop="url" name="Staci" href="/user/show/177641-staci">Staci</a>
