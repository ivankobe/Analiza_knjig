        $('bookCover_28696175').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_28696175').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
