        $('bookCover_247060').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_247060').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_247060').observe('prototip:hidden', function () {
