        $('bookCover_73764').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_73764').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
