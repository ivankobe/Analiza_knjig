        $('bookCover_767719').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_767719').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
