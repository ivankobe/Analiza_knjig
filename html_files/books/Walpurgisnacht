        $('bookCover_796007').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_796007').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
