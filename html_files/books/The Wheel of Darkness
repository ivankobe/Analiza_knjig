        $('bookCover_67035').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_67035').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
