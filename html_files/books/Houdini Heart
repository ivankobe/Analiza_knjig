      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_52626890').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_52626890').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_52626890').observe('prototip:hidden', function () {
