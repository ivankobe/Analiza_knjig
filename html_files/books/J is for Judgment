        $('bookCover_147074').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_147074').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
