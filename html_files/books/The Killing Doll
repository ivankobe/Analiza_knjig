        $('bookCover_1381618').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1381618').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
