        $('bookCover_308621').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_308621').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
