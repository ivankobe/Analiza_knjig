        $('bookCover_7624086').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_7624086').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
