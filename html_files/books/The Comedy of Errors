<div id="review_2355710372" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2355710372" />
    <a title="Carlo" class="left imgcol" href="/user/show/66397212-carlo"><img alt="Carlo" src="https://images.gr-assets.com/users/1497987441p2/66397212.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2355710372?book_show_action=true">Apr 10, 2018</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Carlo" class="user" itemprop="url" name="Carlo" href="/user/show/66397212-carlo">Carlo</a>
