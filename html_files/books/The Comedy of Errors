        $('bookCover_752994').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_752994').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
