        $('bookCover_13501883').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_13501883').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
