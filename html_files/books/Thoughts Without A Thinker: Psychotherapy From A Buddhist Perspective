<div id="review_1192463999" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1192463999" />
    <a title="Carol" class="left imgcol" href="/user/show/1390553-carol"><img alt="Carol" src="https://images.gr-assets.com/users/1414075226p2/1390553.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1192463999?book_show_action=true">Feb 06, 2015</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Carol" class="user" itemprop="url" name="Carol" href="/user/show/1390553-carol">Carol</a>
