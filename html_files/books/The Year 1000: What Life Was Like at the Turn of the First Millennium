        $('bookCover_24611571').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_24611571').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
