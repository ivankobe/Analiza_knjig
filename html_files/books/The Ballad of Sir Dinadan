      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_16158523').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_16158523').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_16158523').observe('prototip:hidden', function () {
