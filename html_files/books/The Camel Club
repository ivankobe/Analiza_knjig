          $$('div.prototip').each(function (e) {
            if ($('bookCover_24192').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_24192').observe('prototip:hidden', function () {
