<div id="review_530036263" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/530036263" />
    <a title="Corey" class="left imgcol" href="/user/show/12949855-corey"><img alt="Corey" src="https://images.gr-assets.com/users/1618178120p2/12949855.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/530036263?book_show_action=true">Feb 08, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Corey" class="user" itemprop="url" name="Corey" href="/user/show/12949855-corey">Corey</a>
