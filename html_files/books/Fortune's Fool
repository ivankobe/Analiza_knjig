          $$('div.prototip').each(function (e) {
            if ($('bookCover_30200').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_30200').observe('prototip:hidden', function () {
