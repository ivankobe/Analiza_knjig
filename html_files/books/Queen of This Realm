      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_757693').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_757693').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_757693').observe('prototip:hidden', function () {
