      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_241787').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_241787').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_241787').observe('prototip:hidden', function () {
