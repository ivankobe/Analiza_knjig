        $('bookCover_1588597').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1588597').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
