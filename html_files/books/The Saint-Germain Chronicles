        $('bookCover_13138901').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_13138901').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
