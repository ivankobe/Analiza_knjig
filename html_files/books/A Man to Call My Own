      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_28859783').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_28859783').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_28859783').observe('prototip:hidden', function () {
