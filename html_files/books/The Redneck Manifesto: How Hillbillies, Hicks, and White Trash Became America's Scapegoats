        $('bookCover_117896').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_117896').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
