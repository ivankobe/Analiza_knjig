        $('bookCover_57990506').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_57990506').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
