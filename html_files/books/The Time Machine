<div id="review_687330289" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/687330289" />
    <a title="Adrian" class="left imgcol" href="/user/show/22809267-adrian"><img alt="Adrian" src="https://images.gr-assets.com/users/1484242635p2/22809267.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/687330289?book_show_action=true">Aug 06, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Adrian" class="user" itemprop="url" name="Adrian" href="/user/show/22809267-adrian">Adrian</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="really liked it"><span size="15x15" class="staticStar p10">really liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/22809267-adrian?shelf=sf-classic">sf-classic</a>, 
              <a class="actionLinkLite" href="/review/list/22809267-adrian?shelf=sci-fi">sci-fi</a>, 
              <a class="actionLinkLite" href="/review/list/22809267-adrian?shelf=sf-time-travel">sf-time-travel</a>, 
              <a class="actionLinkLite" href="/review/list/22809267-adrian?shelf=classic-books">classic-books</a>, 
              <a class="actionLinkLite" href="/review/list/22809267-adrian?shelf=zz-owned-books">zz-owned-books</a>
          </div>
