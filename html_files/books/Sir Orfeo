<div id="review_4073101039" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/4073101039" />
    <a title="Jaslyn" class="left imgcol" href="/user/show/79688581-jaslyn"><img alt="Jaslyn" src="https://images.gr-assets.com/users/1553724906p2/79688581.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/4073101039?book_show_action=true">Jun 22, 2021</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Jaslyn" class="user" itemprop="url" name="Jaslyn" href="/user/show/79688581-jaslyn">Jaslyn</a>
