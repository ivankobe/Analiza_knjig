<div id="review_2037729571" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2037729571" />
    <a title="Sahar" class="left imgcol" href="/user/show/13688597-sahar"><img alt="Sahar" src="https://images.gr-assets.com/users/1446013776p2/13688597.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2037729571?book_show_action=true">Jun 23, 2017</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Sahar" class="user" itemprop="url" name="Sahar" href="/user/show/13688597-sahar">Sahar</a>
