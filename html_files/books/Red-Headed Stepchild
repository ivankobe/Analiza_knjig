      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_7109044').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_7109044').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_7109044').observe('prototip:hidden', function () {
