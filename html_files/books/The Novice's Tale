        $('bookCover_40718688').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_40718688').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
