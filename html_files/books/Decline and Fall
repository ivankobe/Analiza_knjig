        $('bookCover_1376768').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1376768').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_1376768').observe('prototip:hidden', function () {
