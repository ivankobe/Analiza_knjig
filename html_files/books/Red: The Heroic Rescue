            if ($('bookCover_199155').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_199155').observe('prototip:hidden', function () {
