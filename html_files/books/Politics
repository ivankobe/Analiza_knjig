  <div class="friendReviews elementListBrown" >
    <div class="section">
      

<div id="review_871449528" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/871449528" />
    <a title="Ana" class="left imgcol" href="/user/show/28781546-ana"><img alt="Ana" src="https://images.gr-assets.com/users/1450253963p2/28781546.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/871449528?book_show_action=true">May 21, 2017</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Ana" class="user" itemprop="url" name="Ana" href="/user/show/28781546-ana">Ana</a>
