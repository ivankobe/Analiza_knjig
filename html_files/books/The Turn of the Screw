<option value="th">ไทย &lrm;(11)</option>
<option value="ka">ქართული &lrm;(1)</option>
<option value="zh">中文 &lrm;(1)</option>
<option value="ja">日本語 &lrm;(1)</option></select>
</form>    </div>

    <div class="reviewControls--left">

      <a class="uitext" rel="nofollow" id="span_class_gr-hyperlink_more_filters_span_tip_714" style="cursor: pointer; " href="#" onclick="return false"><span class="gr-hyperlink">More filters</span></a><script>
//<![CDATA[
      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_714'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/12948.The_Turn_of_the_Screw?rating=\">all <span class=\"greyText\">(246828)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/12948.The_Turn_of_the_Screw?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;Z1mzrUnI3+dR3CThyaJWwA2y3P2/d3Jg7JRh14kAgh3KbVanLcVb9s/3bHsKcUwRonUGWDh4+nZKjQIBAzg8rg==&#39;)}); return false;\">5 stars <span class=\"greyText\">(21609)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/12948.The_Turn_of_the_Screw?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;8iUjhSr+0/wDkxPoiSYwfnLfGRmb/J91zPYAuQQVRbBfEcaPTvNX7Z24W3JK9Sqv3RjDvBzzF2Nq72Nvji37Aw==&#39;)}); return false;\">4 stars <span class=\"greyText\">(35939)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n