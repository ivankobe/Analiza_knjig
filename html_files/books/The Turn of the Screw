        $('bookCover_1940880').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1940880').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
