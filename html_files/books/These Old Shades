        $('bookCover_1943376').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1943376').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
