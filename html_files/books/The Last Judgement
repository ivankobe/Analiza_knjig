<div id="review_2364290690" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2364290690" />
    <a title="Silvio111" class="left imgcol" href="/user/show/6695836-silvio111"><img alt="Silvio111" src="https://images.gr-assets.com/users/1320498635p2/6695836.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2364290690?book_show_action=true">Apr 17, 2018</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Silvio111" class="user" itemprop="url" name="Silvio111" href="/user/show/6695836-silvio111">Silvio111</a>
