      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_13417497').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_13417497').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_13417497').observe('prototip:hidden', function () {
