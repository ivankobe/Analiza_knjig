        $('bookCover_44570430').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_44570430').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
