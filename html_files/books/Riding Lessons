        $('bookCover_91076').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_91076').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
