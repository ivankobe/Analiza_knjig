        $('bookCover_7150766').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_7150766').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
