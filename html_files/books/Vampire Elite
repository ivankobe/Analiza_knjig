      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_22'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/18131745-vampire-elite?rating=\">all <span class=\"greyText\">(1865)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/18131745-vampire-elite?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;/Ade2YKZCyE90IbeoOoMPLkodQ6sngf/2JRZQtTW7a9D9T7eXEmctKQTJgqC5vHVwwsFour4R01WfkIfYPNEOg==&#39;)}); return false;\">5 stars <span class=\"greyText\">(190)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/18131745-vampire-elite?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;9tNG69SV6VVXABjmY29Dva6sK29hq9o2GWfVG43UG45JISbsCkV+wM7DuDJBY75U1I9bwyfNmoSXjc5GOfGyGw==&#39;)}); return false;\">4 stars <span class=\"greyText\">(106)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <spa