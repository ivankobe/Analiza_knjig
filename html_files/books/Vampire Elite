        $('bookCover_24476010').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_24476010').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_24476010').observe('prototip:hidden', function () {
