        $('bookCover_8163722').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_8163722').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
