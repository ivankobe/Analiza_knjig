        $('bookCover_2993730').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_2993730').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_2993730').observe('prototip:hidden', function () {
