        $('bookCover_57101645').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_57101645').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
