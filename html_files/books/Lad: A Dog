        $('bookCover_325636').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_325636').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
