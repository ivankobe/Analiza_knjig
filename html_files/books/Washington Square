        $('bookCover_31175').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_31175').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
