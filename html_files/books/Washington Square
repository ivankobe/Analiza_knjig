        $('bookCover_298230').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_298230').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
