      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_80'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/172764.Full_Moon_Rising?rating=\">all <span class=\"greyText\">(58414)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/172764.Full_Moon_Rising?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;L18Et29HSBk54+W3+jRN2GrFhs+V9dKEFLr/jVway65UVjSvPxdDRIKRFodlDeF7by2ISh3794g7VCyA+kWasg==&#39;)}); return false;\">5 stars <span class=\"greyText\">(11947)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/172764.Full_Moon_Rising?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;oyCyQl/4gdpfyBH/5wkeWPYz6CuuOrB0B79mVeqDUZfYKYJaD6iKh+S64s94MLL789vmriY0lXgoUbVYTNwAiw==&#39;)}); return false;\">4 stars <span class=\"greyText\">(10835)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n              