        $('bookCover_22571643').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_22571643').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
