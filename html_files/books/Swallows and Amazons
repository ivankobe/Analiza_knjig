<div id="review_35951158" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/35951158" />
    <a title="Jenne" class="left imgcol" href="/user/show/1645580-jenne"><img alt="Jenne" src="https://images.gr-assets.com/users/1227652433p2/1645580.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/35951158?book_show_action=true">Oct 22, 2008</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Jenne" class="user" itemprop="url" name="Jenne" href="/user/show/1645580-jenne">Jenne</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was amazing"><span size="15x15" class="staticStar p10">it was amazing</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/1645580-jenne?shelf=childrens-books">childrens-books</a>, 
              <a class="actionLinkLite" href="/review/list/1645580-jenne?shelf=reviewed">reviewed</a>, 
              <a class="actionLinkLite" href="/review/list/1645580-jenne?shelf=ships">ships</a>, 
              <a class="actionLinkLite" href="/review/list/1645580-jenne?shelf=books-for-baby">books-for-baby</a>
          </div>
