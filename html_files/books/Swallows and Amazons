        $('bookCover_329735').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_329735').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
