        $('bookCover_152706').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_152706').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
