        $('bookCover_79930').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_79930').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
