        $('bookCover_45362354').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_45362354').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
