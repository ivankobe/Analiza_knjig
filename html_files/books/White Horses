<div id="review_2008571468" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2008571468" />
    <a title="Judy" class="left imgcol" href="/user/show/5778052-judy"><img alt="Judy" src="https://images.gr-assets.com/users/1493898986p2/5778052.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2008571468?book_show_action=true">May 26, 2017</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Judy" class="user" itemprop="url" name="Judy" href="/user/show/5778052-judy">Judy</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="liked it"><span size="15x15" class="staticStar p10">liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>



        

