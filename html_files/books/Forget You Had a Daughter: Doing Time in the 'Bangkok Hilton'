      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_43375612').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_43375612').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_43375612').observe('prototip:hidden', function () {
