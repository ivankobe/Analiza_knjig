        $('bookCover_23288383').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_23288383').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
