        $('bookCover_18324411').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_18324411').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
