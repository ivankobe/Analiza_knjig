      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_89724').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_89724').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_89724').observe('prototip:hidden', function () {
