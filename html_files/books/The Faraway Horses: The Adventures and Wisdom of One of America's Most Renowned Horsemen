        $('bookCover_5852305').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_5852305').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
