        $('bookCover_14758').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_14758').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
