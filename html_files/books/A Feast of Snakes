  <div class="friendReviews elementListBrown" >
    <div class="section">
      

<div id="review_1504266496" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1504266496" />
    <a title="David Keaton" class="left imgcol" href="/user/show/3647043-david-keaton"><img alt="David Keaton" src="https://images.gr-assets.com/users/1442508894p2/3647043.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1504266496?book_show_action=true">Jan 10, 2016</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="David Keaton" class="user" itemprop="url" name="David Keaton" href="/user/show/3647043-david-keaton">David Keaton</a>
