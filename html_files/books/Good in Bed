        $('bookCover_29547').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_29547').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
