        $('bookCover_403129').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_403129').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
