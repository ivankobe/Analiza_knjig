      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Erin " class="user" itemprop="url" name="Erin " href="/user/show/32799998-erin">Erin </a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="liked it"><span size="15x15" class="staticStar p10">liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>

          <span class="greyText">&nbsp;&middot;&nbsp;</span>
          <a class="lightGreyText" title="Mass Market Paperback " href="/book/show/10426635-gerald-s-game">review of another edition</a>


        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/32799998-erin?shelf=movie">movie</a>, 
              <a class="actionLinkLite" href="/review/list/32799998-erin?shelf=king-me">king-me</a>, 
              <a class="actionLinkLite" href="/review/list/32799998-erin?shelf=book-club">book-club</a>, 
