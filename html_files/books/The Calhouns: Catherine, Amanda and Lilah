        $('bookCover_31868085').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_31868085').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
