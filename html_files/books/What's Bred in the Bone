      <a class="uitext" rel="nofollow" id="span_class_gr-hyperlink_more_filters_span_tip_319" style="cursor: pointer; " href="#" onclick="return false"><span class="gr-hyperlink">More filters</span></a><script>
//<![CDATA[
      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_319'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/265767.What_s_Bred_in_the_Bone?rating=\">all <span class=\"greyText\">(9456)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/265767.What_s_Bred_in_the_Bone?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;PepV3W2UKXP9qYU3XW5GlsuSbuxX7fYSOIZfgYYthyGB23A+EvIviaNy6eyYoXR7mOJvaR5iA362XpLiT5jS7w==&#39;)}); return false;\">5 stars <span class=\"greyText\">(2669)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/265767.What_s_Bred_in_the_Bone?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;ucpqjyFt8/4EDP6t/jgYTcXiEfzr6pGCe/ArYr+R2oEF+09sXgv1BFrXknY79yqglpIQeaJlZO71KOYBdiSPTw==&#39;)}); return false;\">4 stars <span class=\"greyText\">(2226)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/sp