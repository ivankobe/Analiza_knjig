        $('bookCover_386187').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_386187').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
