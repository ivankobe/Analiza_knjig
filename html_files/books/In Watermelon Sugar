        $('bookCover_35297901').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_35297901').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
