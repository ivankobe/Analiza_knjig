  <div data-react-class="ReactComponents.ShareDialog" data-react-props="{&quot;signedIn&quot;:false,&quot;signInUrl&quot;:&quot;/user/sign_in&quot;,&quot;social&quot;:{&quot;inModal&quot;:{&quot;facebook&quot;:true,&quot;twitter&quot;:true,&quot;pinterest&quot;:true,&quot;copyLink&quot;:false},&quot;outsideModal&quot;:{&quot;facebook&quot;:true,&quot;twitter&quot;:true,&quot;pinterest&quot;:true,&quot;copyLink&quot;:false},&quot;fbOptions&quot;:{&quot;name&quot;:&quot;Three Cups Of Tea by Greg Mortenson&quot;,&quot;caption&quot;:&quot;goodreads.com&quot;,&quot;description&quot;:&quot;‘Here we drink three cups of tea to do business; the first you are a stranger, the second you become a friend, and the third, you join our family, and for our family we are prepared to do anything – even die.’Haji Ali, Korphe Village Chief, Karakoram mo...&quot;,&quot;link&quot;:&quot;https://www.goodreads.com/book/show/2496795.Three_Cups_Of_Tea&quot;,&quot;picture&quot;:&quot;https://i.gr-assets.com/images/S/compressed.photo.goodreads.com/books/1328032253l/2496795._SY75_.jpg&quot;},&quot;twitterOptions&quot;:{&quot;text&quot;:&quot;Three Cups Of Tea by Greg Mortenson&quot;,&quot;url&quot;:&quot;https://www.goodreads.com/book/show/2496795.Three_Cups_Of_Tea&quot;,&quot;via&quot;:&quot;goodreads&quot;},&quot;pinterestOptions&quot;:{&quot;description&quot;:&quot;Three Cups Of Tea by Greg Mortenson&quot;,&quot;url&quot;:&quot;https://www.goodreads.com/book/show/2496795.Three_Cups_Of_Tea&quot;,&quot;media&quot;:&quot;https://i.gr-assets.com/images/S/compressed.photo.goodreads.com/books/1328032253l/2496795.jpg&quot;}},&quot;sharingData&quot;:{&quot;resourceTypeFriendlyText&quot;:&quot;book&quot;,&quot;resourceId&quot;:2496795},&quot;includePreview&quot;:true,&quot;previewData&quot;:{&quot;author&quot;:{&quot;name&quot;:&quot;Greg Mortenson&quot;,&quot;isGoodreadsAuthor&quot;:true},&quot;rating&quot;:&quot;3.65&quot;,&quot;title&quot;:&quot;Three Cups Of Tea&quot;,&quot;imageUrl&quot;:&quot;ht