      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_665'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/56315500-the-walls-of-orion?rating=\">all <span class=\"greyText\">(559)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/56315500-the-walls-of-orion?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;/m8cwZI9/Y2JZLPFYoYBbPawK+ScY2Lyzf3XM8h5hunsW6Pyvieq1j0/WPflHkg9JeSjt5xFHLrUt5A3VwDPJA==&#39;)}); return false;\">5 stars <span class=\"greyText\">(51)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/56315500-the-walls-of-orion?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;InjvEmoJQTVsaePxPdEtjW9sH9QJoJknyDKLcgQtuU4wTFAhRhMWbtgyCMO6SWTcvDiXhwmG52/ReMx2m1Twgw==&#39;)}); return false;\">4 stars <span class=\"greyText\">(17)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n         