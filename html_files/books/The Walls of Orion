        $('bookCover_57390556').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_57390556').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_57390556').observe('prototip:hidden', function () {
