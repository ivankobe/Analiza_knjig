        $('bookCover_28434').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_28434').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
