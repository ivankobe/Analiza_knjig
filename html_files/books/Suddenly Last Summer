        $('bookCover_6105608').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_6105608').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
