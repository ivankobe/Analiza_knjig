        $('bookCover_31828642').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_31828642').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
