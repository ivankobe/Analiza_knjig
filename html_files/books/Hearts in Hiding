        $('bookCover_36110909').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_36110909').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
