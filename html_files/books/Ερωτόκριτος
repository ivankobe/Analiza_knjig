        $('bookCover_6272423').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_6272423').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
