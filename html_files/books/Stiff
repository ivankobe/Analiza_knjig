      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_272642').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_272642').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_272642').observe('prototip:hidden', function () {
