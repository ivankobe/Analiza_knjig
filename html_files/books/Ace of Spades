        $('bookCover_55624056').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_55624056').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
