        $('bookCover_50145510').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_50145510').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
