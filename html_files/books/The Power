        $('bookCover_68984').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_68984').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_68984').observe('prototip:hidden', function () {
