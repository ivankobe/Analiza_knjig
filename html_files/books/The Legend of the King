        $j("#reviewControls__ratingDetailsMiniGraph").prependTo("#rating_details_tip");
      });
    //]]>
    </script>
    </span>
    <a id="rating_details_tip" class="gr-hyperlink" style="cursor: pointer; " href="#" onclick="return false"> Rating details</a><script>
//<![CDATA[
      var newTip = new Tip($('rating_details_tip'), "\n        <div id=\"moreBookData\" class=\"bookMeta\" >\n  \n\n  \n\n<table border=\"0\" id=\"rating_distribution\" style=\"width: auto; max-width: 500px;\" cellspacing=\"5\" cellpadding=\"0\">\n      <tr>\n        <th width=\"25\" class=\"right\">5&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 100%;\" title=\"498 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/7885589-the-legend-of-the-king?rating=5\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'rbbHmNoLMoPKJyUwyJ7VTC2QBv5YC6t0Fv3Q9za/7pycR77kDWyH1tZnYS98zZw7eYxF9KbEDWFUZ9DeB89OLA==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          49% (498)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">4&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 64%;\" title=\"323 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/7885589-the-legend-of-the-king?rating=4\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'q/2EEaSTo2FCW6OOrV12EEAul6t74iKmO7jY2bF03xWaDP1tc/QWNF4b55EZDj9nFDLUoYUthLN5ItjwgAR/pQ==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          31% (323)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">3&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 30%;\" title=\"151 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/7885589-the-legend-of-the-k