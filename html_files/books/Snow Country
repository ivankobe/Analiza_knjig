        $('bookCover_62812').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_62812').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
