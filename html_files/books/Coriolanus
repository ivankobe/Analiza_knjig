        $('bookCover_752868').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_752868').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
