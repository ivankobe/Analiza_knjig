      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_1199084').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1199084').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_1199084').observe('prototip:hidden', function () {
