        $('bookCover_1433750').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1433750').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
