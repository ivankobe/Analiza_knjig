        $('bookCover_23613').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_23613').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
