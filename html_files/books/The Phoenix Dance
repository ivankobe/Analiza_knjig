        $('bookCover_43822062').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_43822062').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
