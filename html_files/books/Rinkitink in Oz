<div id="review_2291886823" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2291886823" />
    <a title="Janelle" class="left imgcol" href="/user/show/6097225-janelle"><img alt="Janelle" src="https://images.gr-assets.com/users/1493241562p2/6097225.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2291886823?book_show_action=true">Mar 11, 2018</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Janelle" class="user" itemprop="url" name="Janelle" href="/user/show/6097225-janelle">Janelle</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="really liked it"><span size="15x15" class="staticStar p10">really liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/6097225-janelle?shelf=kids-books">kids-books</a>, 
              <a class="actionLinkLite" href="/review/list/6097225-janelle?shelf=4-stars">4-stars</a>, 
              <a class="actionLinkLite" href="/review/list/6097225-janelle?shelf=audiobooks-for-sleep-read">audiobooks-for-sleep-read</a>, 
              <a class="actionLinkLite" href="/review/list/6097225-janelle?shelf=classics">classics</a>, 
              <a class="actionLinkLite" href="/review/list/6097225-janelle?shelf=fantasy">fantasy</a>, 
              <a class="actionLinkLite" href="/review/list/6097225-janelle?shelf=fiction">fiction</a>
          </div>
