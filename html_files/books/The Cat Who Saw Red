        $('bookCover_17257653').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_17257653').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
