<div id="review_3305031495" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/3305031495" />
    <a title="Andy" class="left imgcol" href="/user/show/5841469-andy"><img alt="Andy" src="https://images.gr-assets.com/users/1597282744p2/5841469.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/3305031495?book_show_action=true">Apr 29, 2020</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Andy" class="user" itemprop="url" name="Andy" href="/user/show/5841469-andy">Andy</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was ok"><span size="15x15" class="staticStar p10">it was ok</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/5841469-andy?shelf=2020-read">2020-read</a>, 
              <a class="actionLinkLite" href="/review/list/5841469-andy?shelf=rad-reads-2020">rad-reads-2020</a>, 
              <a class="actionLinkLite" href="/review/list/5841469-andy?shelf=bookclub-read">bookclub-read</a>, 
