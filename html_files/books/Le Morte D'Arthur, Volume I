        $('bookCover_7348').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_7348').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
