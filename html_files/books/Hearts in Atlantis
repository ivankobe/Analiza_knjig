        $('bookCover_35699538').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_35699538').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
