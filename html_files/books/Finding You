        $('bookCover_30145154').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_30145154').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
