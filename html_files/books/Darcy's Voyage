<div id="review_3022340284" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/3022340284" />
    <a title="Ashley" class="left imgcol" href="/user/show/103755095-ashley"><img alt="Ashley" src="https://images.gr-assets.com/users/1591313537p2/103755095.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/3022340284?book_show_action=true">Oct 24, 2019</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Ashley" class="user" itemprop="url" name="Ashley" href="/user/show/103755095-ashley">Ashley</a>
