        $('bookCover_32903079').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_32903079').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
