        $('bookCover_18160436').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_18160436').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
