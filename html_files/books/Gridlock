      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_42271840').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_42271840').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_42271840').observe('prototip:hidden', function () {
