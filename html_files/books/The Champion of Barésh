      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_54870676').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_54870676').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_54870676').observe('prototip:hidden', function () {
