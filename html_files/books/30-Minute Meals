      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_402017').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_402017').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_402017').observe('prototip:hidden', function () {
