        $('bookCover_42038554').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_42038554').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
