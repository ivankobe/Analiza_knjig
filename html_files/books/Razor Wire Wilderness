        $('bookCover_57352729').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_57352729').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
