        $('bookCover_1097229').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1097229').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
