          $$('div.prototip').each(function (e) {
            if ($('bookCover_7784').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_7784').observe('prototip:hidden', function () {
