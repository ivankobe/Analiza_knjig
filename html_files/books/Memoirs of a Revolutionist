  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/689997604?book_show_action=true">Aug 08, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="sage" class="user" itemprop="url" name="sage" href="/user/show/115687-sage">sage</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="liked it"><span size="15x15" class="staticStar p10">liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/115687-sage?shelf=history">history</a>, 
              <a class="actionLinkLite" href="/review/list/115687-sage?shelf=nonfiction">nonfiction</a>, 
              <a class="actionLinkLite" href="/review/list/115687-sage?shelf=russia">russia</a>, 
              <a class="actionLinkLite" href="/review/list/115687-sage?shelf=gender-politics">gender-politics</a>, 
              <a class="actionLinkLite" href="/review/list/115687-sage?shelf=did-i-have-comments">did-i-have-comments</a>, 
