        $('bookCover_153747').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_153747').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
