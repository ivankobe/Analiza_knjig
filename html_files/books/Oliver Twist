        $('bookCover_2956').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_2956').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_2956').observe('prototip:hidden', function () {
