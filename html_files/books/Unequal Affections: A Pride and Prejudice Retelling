        $('bookCover_30320913').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_30320913').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
