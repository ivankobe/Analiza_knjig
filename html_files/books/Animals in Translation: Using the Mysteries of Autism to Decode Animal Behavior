<div id="review_3131222162" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/3131222162" />
    <a title="Helen" class="left imgcol" href="/user/show/3970974-helen"><img alt="Helen" src="https://images.gr-assets.com/users/1567746369p2/3970974.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/3131222162?book_show_action=true">Jan 18, 2020</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Helen" class="user" itemprop="url" name="Helen" href="/user/show/3970974-helen">Helen</a>
