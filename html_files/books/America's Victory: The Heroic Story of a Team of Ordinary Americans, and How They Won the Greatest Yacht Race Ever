        $('bookCover_39964740').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_39964740').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
