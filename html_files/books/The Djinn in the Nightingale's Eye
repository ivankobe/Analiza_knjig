      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_743'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/86895.The_Djinn_in_the_Nightingale_s_Eye?rating=\">all <span class=\"greyText\">(7623)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/86895.The_Djinn_in_the_Nightingale_s_Eye?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;tPSpmUGfeGhKFdK4SLv2y6KMVcltmmVpSzdREjXLkRqTFHC/y0P4l7hKD6wHsxt5lcCS1+0gUATaYuiUHONKNA==&#39;)}); return false;\">5 stars <span class=\"greyText\">(865)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/86895.The_Djinn_in_the_Nightingale_s_Eye?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;8Zp+s/M/vhWAqJNmTlnR4F3N9uN93xa3W1LxzjBooqzWeqeVeeM+6nL3TnIBUTxSaoEx/f1lI9rKB0hIGUB5gg==&#39;)}); return false;\">4 stars <span class=\"greyText\">(1225)<\/span><\/a>\n                  <span cla