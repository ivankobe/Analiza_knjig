        $('bookCover_256683').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_256683').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
