      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_642790').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_642790').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_642790').observe('prototip:hidden', function () {
