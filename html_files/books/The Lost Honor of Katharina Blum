        $('bookCover_1210286').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1210286').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
