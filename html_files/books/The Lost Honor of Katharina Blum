        $('bookCover_55884186').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_55884186').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
