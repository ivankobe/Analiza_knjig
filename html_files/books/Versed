        $('bookCover_9811').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_9811').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
