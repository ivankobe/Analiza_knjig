        $('bookCover_55525261').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_55525261').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
