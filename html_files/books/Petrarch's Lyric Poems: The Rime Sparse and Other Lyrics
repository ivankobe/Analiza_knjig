          $$('div.prototip').each(function (e) {
            if ($('bookCover_2669650').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_2669650').observe('prototip:hidden', function () {
