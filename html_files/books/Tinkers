        $('bookCover_1736739').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1736739').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
