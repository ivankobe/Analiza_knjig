      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_223455').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_223455').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_223455').observe('prototip:hidden', function () {
