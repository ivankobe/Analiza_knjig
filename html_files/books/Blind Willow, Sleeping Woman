    <div class="js-dataTooltip" data-use-wtr-tooltip='true'>
        <div class="js-tooltipTrigger tooltipTrigger" data-resource-type='Book' data-resource-id='11297' data-placement='bottom-left'>
          <a href="/book/show/11297.Norwegian_Wood"><img id="more_book_11297" alt="Norwegian Wood" title="Norwegian Wood" width="50" src="https://i.gr-assets.com/images/S/compressed.photo.goodreads.com/books/1386924361l/11297._SX98_.jpg" /></a>
