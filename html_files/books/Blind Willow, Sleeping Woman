        $('bookCover_11274').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_11274').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
