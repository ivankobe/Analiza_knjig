        $('bookCover_16117823').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_16117823').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
