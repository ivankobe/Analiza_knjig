        $('bookCover_22557272').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_22557272').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
