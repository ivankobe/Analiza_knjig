<div id="review_1827012243" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1827012243" />
    <a title="Hana" class="left imgcol" href="/user/show/10571559-hana"><img alt="Hana" src="https://images.gr-assets.com/users/1458172215p2/10571559.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1827012243?book_show_action=true">Jan 23, 2017</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Hana" class="user" itemprop="url" name="Hana" href="/user/show/10571559-hana">Hana</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was amazing"><span size="15x15" class="staticStar p10">it was amazing</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span></span>

          <span class="greyText">&nbsp;&middot;&nbsp;</span>
