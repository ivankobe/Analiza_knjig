      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_187809').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_187809').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_187809').observe('prototip:hidden', function () {
