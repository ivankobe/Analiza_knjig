        $('bookCover_12372').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_12372').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
