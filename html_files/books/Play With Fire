        $('bookCover_56543989').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_56543989').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
