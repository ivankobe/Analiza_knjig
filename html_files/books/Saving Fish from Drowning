        $('bookCover_24472').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_24472').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
