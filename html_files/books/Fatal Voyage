        $('bookCover_435025').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_435025').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
