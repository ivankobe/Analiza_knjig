        $('bookCover_1359071').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1359071').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_1359071').observe('prototip:hidden', function () {
