  <div class="friendReviews elementListBrown" >
    <div class="section">
      

<div id="review_732515009" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/732515009" />
    <a title="Riley" class="left imgcol" href="/user/show/23179211-riley"><img alt="Riley" src="https://images.gr-assets.com/users/1609618236p2/23179211.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/732515009?book_show_action=true">Oct 02, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Riley" class="user" itemprop="url" name="Riley" href="/user/show/23179211-riley">Riley</a>
