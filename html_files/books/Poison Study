        $('bookCover_6811150').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_6811150').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
