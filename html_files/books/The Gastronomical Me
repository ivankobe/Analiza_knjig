        $('bookCover_48930306').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_48930306').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
