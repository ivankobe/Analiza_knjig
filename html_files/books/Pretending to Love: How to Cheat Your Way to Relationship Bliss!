        $('bookCover_184594').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_184594').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
