<div id="review_42212223" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/42212223" />
    <a title="Cara" class="left imgcol" href="/user/show/1876075-cara"><img alt="Cara" src="https://images.gr-assets.com/users/1350665534p2/1876075.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/42212223?book_show_action=true">Jan 07, 2009</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Cara" class="user" itemprop="url" name="Cara" href="/user/show/1876075-cara">Cara</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was amazing"><span size="15x15" class="staticStar p10">it was amazing</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/1876075-cara?shelf=fantasy">fantasy</a>, 
              <a class="actionLinkLite" href="/review/list/1876075-cara?shelf=series">series</a>, 
              <a class="actionLinkLite" href="/review/list/1876075-cara?shelf=favorites">favorites</a>, 
              <a class="actionLinkLite" href="/review/list/1876075-cara?shelf=awesome-covers">awesome-covers</a>
