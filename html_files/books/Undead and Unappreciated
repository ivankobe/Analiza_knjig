        $('bookCover_50747').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_50747').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
