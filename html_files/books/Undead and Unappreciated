        $('bookCover_30279').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_30279').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
