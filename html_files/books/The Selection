        $('bookCover_7896527').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_7896527').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
