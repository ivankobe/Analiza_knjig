        $('bookCover_287505').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_287505').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
