        $('bookCover_198177').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_198177').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
