        $('bookCover_49354511').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_49354511').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
