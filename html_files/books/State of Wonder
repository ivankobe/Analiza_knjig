        $('bookCover_52698452').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_52698452').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
