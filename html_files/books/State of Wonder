      <a class="uitext" rel="nofollow" id="span_class_gr-hyperlink_more_filters_span_tip_982" style="cursor: pointer; " href="#" onclick="return false"><span class="gr-hyperlink">More filters</span></a><script>
//<![CDATA[
      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_982'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/9118135-state-of-wonder?rating=\">all <span class=\"greyText\">(271224)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/9118135-state-of-wonder?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;YXty8+ElTREZJDOQd57KMjxluV1aOl0h9Oo1hDCgjlMGmkUQU/NQJm15rPvN7fqtXnLw+4LUHHShCdHW+ipMfQ==&#39;)}); return false;\">5 stars <span class=\"greyText\">(47383)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/9118135-state-of-wonder?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;3vGHgHbVoxXPxEz62O579LLHsXlB8opuzg+T1MENDXq5ELBjxAO+IruZ05FinUtr0ND435kcyzub7HeGC4fPVA==&#39;)}); return false;\">4 stars <span class=\"greyText\">(71201)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n            