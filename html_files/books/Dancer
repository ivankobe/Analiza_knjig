        $('bookCover_55994371').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_55994371').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
