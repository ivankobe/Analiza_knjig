        $('bookCover_42604661').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_42604661').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
