      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_52439531').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_52439531').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_52439531').observe('prototip:hidden', function () {
