<div id="review_124789377" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/124789377" />
    <a title="Ahmad Sharabiani" class="left imgcol" href="/user/show/614778-ahmad-sharabiani"><img alt="Ahmad Sharabiani" src="https://images.gr-assets.com/users/1194930432p2/614778.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/124789377?book_show_action=true">Oct 04, 2010</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Ahmad Sharabiani" class="user" itemprop="url" name="Ahmad Sharabiani" href="/user/show/614778-ahmad-sharabiani">Ahmad Sharabiani</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="liked it"><span size="15x15" class="staticStar p10">liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/614778-ahmad-sharabiani?shelf=historical">historical</a>, 
              <a class="actionLinkLite" href="/review/list/614778-ahmad-sharabiani?shelf=20th-century">20th-century</a>, 
              <a class="actionLinkLite" href="/review/list/614778-ahmad-sharabiani?shelf=1001-books">1001-books</a>, 
              <a class="actionLinkLite" href="/review/list/614778-ahmad-sharabiani?shelf=literature">literature</a>, 
              <a class="actionLinkLite" href="/review/list/614778-ahmad-sharabiani?shelf=canadian">canadian</a>, 
              <a class="actionLinkLite" href="/review/list/614778-ahmad-sharabiani?shelf=classics">classics</a>, 
              <a class="actionLinkLite" href="/review/list/614778-ahmad-sharabiani?shelf=fiction">fiction</a>, 
              <a class="actionLinkLite" href="/review/list/614778-ahmad-sharabiani?shelf=mystery">mystery</a>
