        $('bookCover_6409342').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_6409342').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
