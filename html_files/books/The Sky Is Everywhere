      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_790'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/6604794-the-sky-is-everywhere?rating=\">all <span class=\"greyText\">(237467)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/6604794-the-sky-is-everywhere?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;TXrwvcDxuZ+sT1twtg5YUcvJUi3LqCBdIwM/R2XsbvUvJijLyPuDCmJxpyfUPYjR6HFDEdaqDNeRSaF+f0tkQw==&#39;)}); return false;\">5 stars <span class=\"greyText\">(37523)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/6604794-the-sky-is-everywhere?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;xPPA/qvVXxZNDHic8N0H3jnzweSVhbm7Nqi8OI4GnVOmrxiIo99lg4MyhMuS7tdeGkvQ2IiHlTGE4iIBlKGX5Q==&#39;)}); return false;\">4 stars <span class=\"greyText\">(32747)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/s