    <a id="rating_details_tip" class="gr-hyperlink" style="cursor: pointer; " href="#" onclick="return false"> Rating details</a><script>
//<![CDATA[
      var newTip = new Tip($('rating_details_tip'), "\n        <div id=\"moreBookData\" class=\"bookMeta\" >\n  \n\n  \n\n<table border=\"0\" id=\"rating_distribution\" style=\"width: auto; max-width: 500px;\" cellspacing=\"5\" cellpadding=\"0\">\n      <tr>\n        <th width=\"25\" class=\"right\">5&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 29%;\" title=\"5 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/3332330-childmare?rating=5\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'OEvPwOrgP0LofJ5/fqt/c69FkZcolZpSd3jIeaf1XALMBXRPv9cn+su2nEj1I9RMPzM+wSmaI406W3GUCX/MLw==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          11% (5)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">4&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 88%;\" title=\"15 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/3332330-childmare?rating=4\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'H4bers70uU55I0aQwbfBBU7Zl06bP55eMUJUK+pSicvryGUhm8Oh9lrpRKdKP2o63q84GJowJ4F8Ye3GRNgZ5g==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          34% (15)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">3&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 100%;\" title=\"17 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/3332330-childmare?rating=3\', {asynchronous:true, evalScript