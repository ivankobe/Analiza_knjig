<div id="review_1617952635" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1617952635" />
    <a title="Esther" class="left imgcol" href="/user/show/39527750-esther"><img alt="Esther" src="https://images.gr-assets.com/users/1466732291p2/39527750.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1617952635?book_show_action=true">May 21, 2019</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Esther" class="user" itemprop="url" name="Esther" href="/user/show/39527750-esther">Esther</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was amazing"><span size="15x15" class="staticStar p10">it was amazing</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/39527750-esther?shelf=fiction">fiction</a>, 
              <a class="actionLinkLite" href="/review/list/39527750-esther?shelf=historical-fiction">historical-fiction</a>, 
              <a class="actionLinkLite" href="/review/list/39527750-esther?shelf=adventure">adventure</a>, 
              <a class="actionLinkLite" href="/review/list/39527750-esther?shelf=young-adult">young-adult</a>, 
              <a class="actionLinkLite" href="/review/list/39527750-esther?shelf=pirates">pirates</a>, 
