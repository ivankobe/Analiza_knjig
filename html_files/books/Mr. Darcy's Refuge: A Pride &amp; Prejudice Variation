<div id="review_630404023" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/630404023" />
    <a title="Shanti" class="left imgcol" href="/user/show/11742858-shanti"><img alt="Shanti" src="https://images.gr-assets.com/users/1354965590p2/11742858.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/630404023?book_show_action=true">Jun 01, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Shanti" class="user" itemprop="url" name="Shanti" href="/user/show/11742858-shanti">Shanti</a>
