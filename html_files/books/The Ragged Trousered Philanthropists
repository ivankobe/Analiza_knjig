<div id="review_2436193425" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2436193425" />
    <a title="Trevor" class="left imgcol" href="/user/show/175635-trevor"><img alt="Trevor" src="https://images.gr-assets.com/users/1565151545p2/175635.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2436193425?book_show_action=true">Jun 26, 2018</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Trevor" class="user" itemprop="url" name="Trevor" href="/user/show/175635-trevor">Trevor</a>
