        $('bookCover_21947381').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_21947381').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
