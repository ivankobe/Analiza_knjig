        $('bookCover_52535010').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_52535010').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
