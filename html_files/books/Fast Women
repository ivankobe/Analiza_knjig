        $('bookCover_55321656').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_55321656').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
