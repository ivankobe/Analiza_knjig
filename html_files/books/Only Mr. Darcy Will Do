      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_56902334').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_56902334').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_56902334').observe('prototip:hidden', function () {
