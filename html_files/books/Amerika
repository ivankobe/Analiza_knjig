        $('bookCover_752806').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_752806').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
