<div id="review_3410462290" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/3410462290" />
    <a title="Alaina" class="left imgcol" href="/user/show/50620706-alaina"><img alt="Alaina" src="https://images.gr-assets.com/users/1451325817p2/50620706.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/3410462290?book_show_action=true">Jul 27, 2020</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Alaina" class="user" itemprop="url" name="Alaina" href="/user/show/50620706-alaina">Alaina</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="really liked it"><span size="15x15" class="staticStar p10">really liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/50620706-alaina?shelf=fantasy">fantasy</a>, 
              <a class="actionLinkLite" href="/review/list/50620706-alaina?shelf=2020-ng">2020-ng</a>, 
              <a class="actionLinkLite" href="/review/list/50620706-alaina?shelf=historical-fiction">historical-fiction</a>, 
              <a class="actionLinkLite" href="/review/list/50620706-alaina?shelf=2020-arcs">2020-arcs</a>, 
