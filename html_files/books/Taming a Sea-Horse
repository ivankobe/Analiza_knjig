        $('bookCover_15824167').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_15824167').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
