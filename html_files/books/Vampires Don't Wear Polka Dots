          $$('div.prototip').each(function (e) {
            if ($('bookCover_2216751').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_2216751').observe('prototip:hidden', function () {
