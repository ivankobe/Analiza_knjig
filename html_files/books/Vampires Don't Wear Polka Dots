        $('bookCover_260387').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_260387').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
