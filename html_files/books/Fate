        $('bookCover_53206275').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_53206275').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
