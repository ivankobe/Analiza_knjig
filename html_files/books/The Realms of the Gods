<div id="review_681567838" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/681567838" />
    <a title="Wench" class="left imgcol" href="/user/show/10288067-wench"><img alt="Wench" src="https://images.gr-assets.com/users/1358300438p2/10288067.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/681567838?book_show_action=true">Jul 30, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Wench" class="user" itemprop="url" name="Wench" href="/user/show/10288067-wench">Wench</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was ok"><span size="15x15" class="staticStar p10">it was ok</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/10288067-wench?shelf=thrown-across-the-room">thrown-across-the-room</a>, 
              <a class="actionLinkLite" href="/review/list/10288067-wench?shelf=content-notes">content-notes</a>, 
              <a class="actionLinkLite" href="/review/list/10288067-wench?shelf=fantasy">fantasy</a>, 
              <a class="actionLinkLite" href="/review/list/10288067-wench?shelf=2013">2013</a>, 
              <a class="actionLinkLite" href="/review/list/10288067-wench?shelf=ya">ya</a>, 
