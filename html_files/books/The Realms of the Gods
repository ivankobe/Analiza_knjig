<div id="review_1562897438" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1562897438" />
    <a title="Beth" class="left imgcol" href="/user/show/4063913-beth"><img alt="Beth" src="https://images.gr-assets.com/users/1351396026p2/4063913.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1562897438?book_show_action=true">Feb 27, 2016</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Beth" class="user" itemprop="url" name="Beth" href="/user/show/4063913-beth">Beth</a>
