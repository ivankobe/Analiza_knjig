      var newTip = new Tip($('bookCover_6369831'), "\n\n  <h2><a class=\"readable bookTitle\" href=\"https://www.goodreads.com/book/show/6369831-the-death-of-winter?from_choice=false&amp;from_home_module=false\">The Death of Winter (Luthiel&#39;s Song, #3)<\/a><\/h2>\n\n      <div>\n        by <a class=\"authorName\" href=\"/author/show/291916.Robert_Fanney?origin=series_works\">Robert Fanney<\/a><span title=\"Goodreads Author!\">*<\/span>\n      <\/div>\n\n          <div class=\"smallText uitext darkGreyText\">\n            <span class=\"minirating\"><span class=\"stars staticStars notranslate\"><span size=\"12x12\" class=\"staticStar p10\"><\/span><span size=\"12x12\" class=\"staticStar p10\"><\/span><span size=\"12x12\" class=\"staticStar p10\"><\/span><span size=\"12x12\" class=\"staticStar p10\"><\/span><span size=\"12x12\" class=\"staticStar p3\"><\/span><\/span> 4.47 avg rating &mdash; 43 ratings<\/span>            &mdash; published \n          <\/div>\n\n    <div class=\"addBookTipDescription\">\n      \n<span id=\"freeTextContainer1302460250078149223\">One by one, the stars went out and they blamed it all on Luthiel...<\/span>\n\n    <\/div>\n\n      <div class=\'wtrButtonContainer wtrSignedOut\' id=\'6_book_6369831\'>\n<div class=\'wtrUp wtrLeft\'>\n<form action=\"/shelf/add_to_shelf\" accept-charset=\"UTF-8\" method=\"post\"><input name=\"utf8\" type=\"hidden\" value=\"&#x2713;\" /><input type=\"hidden\" name=\"authenticity_token\" value=\"m58zVVd5exPQhTPsrJfAQMlQVx/HZlNlgQnlXFpbpJcQQdALpw7I6xzG2wNAUUINQPspYvGBPqcMPOBAu3O7+w==\" />\n<input type=\"hidden\" name=\"book_id\" id=\"book_id\" value=\"6369831\" />\n<input type=\"hidden\" name=\"name\" id=\"name\" value=\"to-read\" />\n<input type=\"hidden\" name=\"unique_id\" id=\"unique_id\" value=\"6_book_6369831\" />\n<input type=\"hidden\" name=\"wtr_new\" id=\"wtr_new\" value=\"true\" />\n<input type=\"hidden\" name=\"from_choice\" id=\"from_choice\" value=\"false\" />\n<input type=\"hidden\" name=\"from_ho