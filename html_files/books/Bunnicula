        $('bookCover_10065').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_10065').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
