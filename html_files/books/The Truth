        $('bookCover_16186590').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_16186590').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
