        $('bookCover_27257657').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_27257657').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
