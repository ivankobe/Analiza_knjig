        $('bookCover_55010338').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_55010338').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
