        $('bookCover_7202831').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_7202831').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
