<div id="review_1420562271" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1420562271" />
    <a title="Paige  Bookdragon" class="left imgcol" href="/user/show/15550986-paige-bookdragon"><img alt="Paige  Bookdragon" src="https://images.gr-assets.com/users/1570121828p2/15550986.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1420562271?book_show_action=true">Oct 19, 2015</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Paige  Bookdragon" class="user" itemprop="url" name="Paige  Bookdragon" href="/user/show/15550986-paige-bookdragon">Paige  Bookdragon</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="liked it"><span size="15x15" class="staticStar p10">liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/15550986-paige-bookdragon?shelf=adult">adult</a>, 
              <a class="actionLinkLite" href="/review/list/15550986-paige-bookdragon?shelf=realistic-fiction">realistic-fiction</a>, 
              <a class="actionLinkLite" href="/review/list/15550986-paige-bookdragon?shelf=mystery">mystery</a>, 
              <a class="actionLinkLite" href="/review/list/15550986-paige-bookdragon?shelf=read-2015">read-2015</a>, 
              <a class="actionLinkLite" href="/review/list/15550986-paige-bookdragon?shelf=suspense">suspense</a>, 
              <a class="actionLinkLite" href="/review/list/15550986-paige-bookdragon?shelf=crime">crime</a>
          </div>
