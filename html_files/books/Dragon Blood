        $('bookCover_3598830').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_3598830').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
