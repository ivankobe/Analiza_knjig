        $('bookCover_104378').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_104378').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
