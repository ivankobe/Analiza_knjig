        <div data-react-class="ReactComponents.ReviewCta" data-react-props="{&quot;bookId&quot;:&quot;232888&quot;,&quot;user&quot;:{&quot;viewableName&quot;:&quot;&quot;,&quot;profileImage&quot;:&quot;https://s.gr-assets.com/assets/nophoto/user/u_50x66-632230dc9882b4352d753eedf9396530.png&quot;,&quot;profileUrl&quot;:null},&quot;actionType&quot;:&quot;community_reviews_cta&quot;,&quot;destinationPath&quot;:&quot;/review/edit/232888&quot;,&quot;bookTitle&quot;:&quot;The New Basics Cookbook&quot;,&quot;ratingClickstreamRef&quot;:&quot;cr_rtg_str_bsh&quot;}"><div class="reviewCta" data-reactid=".29dwzfmb25a" data-react-checksum="-226476705"><div class="reviewCta__left" data-reactid=".29dwzfmb25a.0"><img class="rectangularIcon rectangularIcon--border" src="https://s.gr-assets.com/assets/nophoto/user/u_50x66-632230dc9882b4352d753eedf9396530.png" alt="" data-reactid=".29dwzfmb25a.0.0"/></div><div class="reviewCta__right" data-reactid=".29dwzfmb25a.1"><div class="u-marginLeftXSmall reviewCta__label" data-reactid=".29dwzfmb25a.1.0"><noscript data-reactid=".29dwzfmb25a.1.0.0"></noscript><span data-reactid=".29dwzfmb25a.1.0.1">Start your review of </span><span data-reactid=".29dwzfmb25a.1.0.2">The New Basics Cookbook</span></div><div class="reviewCta__controls" data-reactid=".29dwzfmb25a.1.1"><div class="userRating" data-reactid=".29dwzfmb25a.1.1.0"><div class="userRating__starsWrapper" tabindex="0" role="group" data-reactid=".29dwzfmb25a.1.1.0.1"><button class="userRating__star userRating__star--mediumStars" title="Rate 1 star" data-reactid=".29dwzfmb25a.1.1.0.1.$1"></button><button class="userRating__star userRating__star--mediumStars" title="Rate 2 stars" data-reactid=".29dwzfmb25a.1.1.0.1.$2"></button><button class="userRating__star userRating__star--mediumStars" title="Rate 3 stars" data-reactid=".29dwzfmb25a.1.1.0.1.$3"></button><button class="userRating__star userRating__star--mediumStars" title="Rate 4 stars" data-reactid=".29dwzfmb25a.1.1.0.1.$4"></button><button cl