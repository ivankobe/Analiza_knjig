        $('bookCover_335459').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_335459').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
