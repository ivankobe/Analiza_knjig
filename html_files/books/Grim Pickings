        $('bookCover_18714334').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_18714334').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
