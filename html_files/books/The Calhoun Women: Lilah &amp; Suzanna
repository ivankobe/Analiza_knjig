<script type="text/javascript" charset="utf-8">
//<![CDATA[  

  function submitShelfLink(unique_id, book_id, shelf_id, shelf_name, submit_form, exclusive) {
    var checkbox_id = 'shelf_name_' + unique_id + '_' + shelf_id;
    var element = document.getElementById(checkbox_id)

    var checked = element.checked
    if (checked && exclusive) {
      // can't uncheck a radio by clicking it!
      return
    }
    if(document.getElementById("savingMessage")){
      Element.show('savingMessage')
    }
    var element_id = 'shelfInDropdownName_' + unique_id + '_' + shelf_id;
    Element.update(element_id, "saving...");
    if (submit_form) {
      Element.hide('shelfDropdown_' + unique_id)
      var form = document.getElementById('addBookForm' + book_id)
      if (form) {
        form.shelf.value = shelf_name
        form.onsubmit()
      }
    }
    else {
      var action = checked ? 'remove' : ''
      element.checked = !element.checked
      new Ajax.Request('/shelf/add_to_shelf', {asynchronous:true, evalScripts:true, onSuccess:function(request){shelfSubmitted(request, book_id, checkbox_id, element_id, unique_id, shelf_name)}, parameters:'book_id=' + book_id + '&name=' + shelf_name + '&a=' + action + '&authenticity_token=' + encodeURIComponent('Mrm81HBUJ7PpnREJcWFtrAK+A08dReyRUg4XlMiAcTqt+zmTuCqzqSMFyBz5mcZFfaoR5MYNYWUwgY8CdnzxJw==')})
    }
  }

  function shelfSubmitted(request, book_id, checkbox_id, element_id, unique_id, shelf_name) {
    Element.update('shelfListfalse_' + book_id, request.responseText)
    afterShelfSave(checkbox_id, element_id, unique_id, shelf_name.escapeHTML())
  }

  function refreshGroupBox(group_id, book_id) {
    new Ajax.Updater('addGroupBooks' + book_id + '', '/group/add_book_box', {asynchronous:true, evalScripts:true, onSuccess:function(request){refreshGroupBoxComplete(request, book_id);}, parameters:'id=' + group_id + '&book_id=' + book_id + '&refresh=true' + '&authenticity_token=' + encodeURIComponent('MpQT09Cw5aKJX+GbC94g+E+XAiMy0v3EVI+ZxEmhWB6t1paUGM5xuEPHOI6DJosRMIMQiOmacDA2AAFS913YAw==')})
  }
//]]>
</script>


<script>
//<![CDATA[
      var newTip = new Tip($('bookCover_2013705'), "\n\n  <h2><a class=\"readable bookTitle\" href=\"https://www.goodreads.com/book/show/2013705.The_Phalanx_Dragon?from_choice=false&amp;from_home_module=false\">The Phalanx Dragon<\/a><\/h2>\n\n      <div>\n        by <a class=\"authorName\" href=\"/author/show/347221.Timothy_Rizzi?a=9&amp;origin=related_works\">Timothy Rizzi<\/a>\n      <\/div>\n\n          <div class=\"smallText uitext darkGreyText\">\n            <span class=\"minirating\"><span class=\"stars staticStars notranslate\"><span size=\"12x12\" class=\"staticStar p10\"><\/span><span size=\"12x12\" class=\"staticStar p10\"><\/span><span size=\"12x12\" class=\"staticStar p10\"><\/span><span size=\"12x12\" class=\"staticStar p3\"><\/span><span size=\"12x12\" class=\"staticStar p0\"><\/span><\/span> 3.19 avg rating &mdash; 26 ratings<\/span>            &mdash; published 1994\n          <\/div>\n\n    <div class=\"addBookTipDescription\">\n      \n<span id=\"freeTextContainer15785675494082077645\">The Phalanx Dragon is Rizzi&apos;s top-gun best. When Iranian soldiers salvage an intact U.S. cruise missile that has veered off-course during the Gulf War, Iran&apos;s intelligence bureau puts a team together to decipher the missile&apos;s advanced computer-chip t<\/span>\n  <span id=\"freeText15785675494082077645\" style=\"display:none\">The Phalanx Dragon is Rizzi\'s top-gun best. When Iranian soldiers salvage an intact U.S. cruise missile that has veered off-course during the Gulf War, Iran\'s intelligence bureau puts a team together to decipher the missile\'s advanced computer-chip technology. Once they\'ve broken into the missile\'s systems, fundamentalist religious leaders in Tehran use the computer chips to upgrade already existing weapons and take up where Iraq\'s Saddam Hussein left off, cutting the flow of oil from the Persian Gulf and driving up gasoline prices worldwide until Iran\'s economy - and its armed forces - are resuscitated. To meet this threat, the U.S. sends in a carrier group, and Duke James, hero of Nightstalker and Strike of the Cobra, is launched from the U.S.S. Enterprise to destroy the secret Iranian factory. He is up against the best antiaircraft technology in the world, developed, ironically, in the United States.<\/span>\n  <a data-text-id=\"15785675494082077645\" href=\"#\" onclick=\"swapContent(\$(this));; return false;\">...more<\/a>\n\n    <\/div>\n\n      <div class=\'wtrButtonContainer wtrSignedOut\' id=\'8_book_2013705\'>\n<div class=\'wtrUp wtrLeft\'>\n<form action=\"/shelf/add_to_shelf\" accept-charset=\"UTF-8\" method=\"post\"><input name=\"utf8\" type=\"hidden\" value=\"&#x2713;\" /><input type=\"hidden\" name=\"authenticity_token\" value=\"QXpOmU1BFBFli+KuGBochps58aX3CMM5VBWzbLR+wrfeOMvehT+AC68TO7uQ4rdv5C3jDixATs02miv6CoJCqg==\" />\n<input type=\"hidden\" name=\"book_id\" id=\"book_id\" value=\"2013705\" />\n<input type=\"hidden\" name=\"name\" id=\"name\" value=\"to-read\" />\n<input type=\"hidden\" name=\"unique_id\" id=\"unique_id\" value=\"8_book_2013705\" />\n<input type=\"hidden\" name=\"wtr_new\" id=\"wtr_new\" value=\"true\" />\n<input type=\"hidden\" name=\"from_choice\" id=\"from_choice\" value=\"false\" />\n<input type=\"hidden\" name=\"from_home_module\" id=\"from_home_module\" value=\"false\" />\n<input type=\"hidden\" name=\"ref\" id=\"ref\" value=\"\" class=\"wtrLeftUpRef\" />\n<input type=\"hidden\" name=\"existing_review\" id=\"existing_review\" value=\"false\" class=\"wtrExisting\" />\n<input type=\"hidden\" name=\"book_origin\" id=\"book_origin\" value=\"related_works\" />\n<input type=\"hidden\" name=\"page_url\" id=\"page_url\" value=\"/book/show/114195.The_Calhoun_Women\" />\n<button class=\'wtrToRead\' type=\'submit\'>\n<span class=\'progressTrigger\'>Want to Read<\/span>\n<span class=\'progressIndicator\'>saving…<\/span>\n<\/button>\n<\/form>\n\n<\/div>\n\n<div class=\'wtrRight wtrUp\'>\n<form class=\"hiddenShelfForm\" action=\"/shelf/add_to_shelf\" accept-c