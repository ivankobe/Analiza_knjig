        $('bookCover_991550').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_991550').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
