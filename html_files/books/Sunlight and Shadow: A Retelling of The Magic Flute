        $('bookCover_248290').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_248290').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
