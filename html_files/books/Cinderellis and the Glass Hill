        $('bookCover_674749').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_674749').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
