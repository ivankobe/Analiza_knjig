      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_11'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/334643.Abhorsen?rating=\">all <span class=\"greyText\">(162125)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/334643.Abhorsen?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;u7SoH3RjE9Xnx1+6vJRFrLKA/0r+2hNGsmbJeqhqQmtKrSdzmTY6gBFcY687KpeYmmv6Lq21u0kMO17lD35Cew==&#39;)}); return false;\">5 stars <span class=\"greyText\">(59732)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/334643.Abhorsen?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;rK9mShYQAaVVtdKrgdMh8LowU8EsRNDSsbXw5167ah5dtukm+0Uo8KMu7r4GbfPEkttWpX8reN0P6Gd4+a9qDg==&#39;)}); return false;\">4 stars <span class=\"greyText\">(38995)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadi