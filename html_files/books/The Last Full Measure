        $('bookCover_44234').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_44234').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
