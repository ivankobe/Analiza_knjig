        $('bookCover_52731223').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_52731223').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
