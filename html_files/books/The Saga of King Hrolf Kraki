<div id="review_629195996" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/629195996" />
    <a title="Eadweard" class="left imgcol" href="/user/show/16652983-eadweard"><img alt="Eadweard" src="https://images.gr-assets.com/users/1522813003p2/16652983.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/629195996?book_show_action=true">May 31, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Eadweard" class="user" itemprop="url" name="Eadweard" href="/user/show/16652983-eadweard">Eadweard</a>
