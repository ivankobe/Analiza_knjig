<div id="review_3191237416" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/3191237416" />
    <a title="Bregje " class="left imgcol" href="/user/show/7545577-bregje"><img alt="Bregje " src="https://images.gr-assets.com/users/1540066889p2/7545577.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/3191237416?book_show_action=true">Feb 16, 2020</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Bregje " class="user" itemprop="url" name="Bregje " href="/user/show/7545577-bregje">Bregje </a>
