<div id="review_3249436309" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/3249436309" />
    <a title="Zoë&amp;#x1f52e;" class="left imgcol" href="/user/show/19583413-zo-x1f52e"><img alt="Zoë&amp;#x1f52e;" src="https://images.gr-assets.com/users/1525964717p2/19583413.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/3249436309?book_show_action=true">Mar 27, 2020</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Zoë&amp;#x1f52e;" class="user" itemprop="url" name="Zoë&amp;#x1f52e;" href="/user/show/19583413-zo-x1f52e">Zoë&amp;#x1f52e;</a>
