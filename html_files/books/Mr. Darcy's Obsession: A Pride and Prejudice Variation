        $('bookCover_48754079').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_48754079').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
