        $('bookCover_1856297').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1856297').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
