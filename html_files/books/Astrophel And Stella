        $('bookCover_15997').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_15997').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
