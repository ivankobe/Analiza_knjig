      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_14383').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_14383').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_14383').observe('prototip:hidden', function () {
