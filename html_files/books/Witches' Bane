        $('bookCover_23014654').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_23014654').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
