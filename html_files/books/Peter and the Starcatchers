<div id="review_1812835507" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1812835507" />
    <a title="Peter" class="left imgcol" href="/user/show/59376096-peter"><img alt="Peter" src="https://s.gr-assets.com/assets/nophoto/user/m_50x66-82093808bca726cb3249a493fbd3bd0f.png" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1812835507?book_show_action=true">Nov 15, 2016</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Peter" class="user" itemprop="url" name="Peter" href="/user/show/59376096-peter">Peter</a>
