        $('bookCover_155884').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_155884').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
