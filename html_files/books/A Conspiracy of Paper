        $('bookCover_29957204').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_29957204').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
