        $('bookCover_2248573').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_2248573').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
