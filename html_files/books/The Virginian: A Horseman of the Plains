        $('bookCover_121193').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_121193').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
