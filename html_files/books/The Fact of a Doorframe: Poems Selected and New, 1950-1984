        $('bookCover_618944').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_618944').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
