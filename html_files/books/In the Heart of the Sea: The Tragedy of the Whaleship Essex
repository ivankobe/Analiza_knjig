      <a class="uitext" rel="nofollow" id="span_class_gr-hyperlink_more_filters_span_tip_540" style="cursor: pointer; " href="#" onclick="return false"><span class="gr-hyperlink">More filters</span></a><script>
//<![CDATA[
      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_540'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/17780.In_the_Heart_of_the_Sea?rating=\">all <span class=\"greyText\">(212034)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/17780.In_the_Heart_of_the_Sea?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;WeWKHyaOemYO08EHXMeHVyIvNi9iDMD614MiigEWTSdgs3Ga2ZzkNzgzc+g22NNFZQZ5jY+rW7irCk+XZAFKMg==&#39;)}); return false;\">5 stars <span class=\"greyText\">(39030)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/17780.In_the_Heart_of_the_Sea?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;ckE3kHy4ltVoAuspngzMm1boII0IJPuEk0IgTnU00RVLF8wVg6oIhF7iWcb0E5iJEcFvL+WDYMbvy01TECPWAA==&#39;)}); return false;\">4 stars <span class=\"greyText\">(37149)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/s