        $('bookCover_374224').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_374224').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
