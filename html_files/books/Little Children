//<![CDATA[
      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_545'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/37426.Little_Children?rating=\">all <span class=\"greyText\">(47669)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/37426.Little_Children?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;J630x7BWSxPvpzOdMewbfn533cZeYA4y5/VyXCY2qV27Bxb8dX/6QlWgvCHihWPzTOyZ5okVJnvcR2nV0kWKLQ==&#39;)}); return false;\">5 stars <span class=\"greyText\">(5328)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/37426.Little_Children?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;IJxD4jWRwb4DzQw67+aEbrFPl+zFC5VPcVUNjF3B6ES8NqHZ8Lhw77nKg4Y8j/zjg9TTzBJ+vQZK5xYFqbLLNA==&#39;)}); return false;\">4 stars <span class=\"greyText\">(12304)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <s