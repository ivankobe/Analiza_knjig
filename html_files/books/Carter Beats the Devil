<div id="review_3039271053" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/3039271053" />
    <a title="Cheryl" class="left imgcol" href="/user/show/22026875-cheryl"><img alt="Cheryl" src="https://images.gr-assets.com/users/1373374354p2/22026875.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/3039271053?book_show_action=true">Nov 11, 2019</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Cheryl" class="user" itemprop="url" name="Cheryl" href="/user/show/22026875-cheryl">Cheryl</a>
      </span>

