        $('bookCover_45702325').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_45702325').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
