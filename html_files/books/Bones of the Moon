        $('bookCover_401105').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_401105').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_401105').observe('prototip:hidden', function () {
