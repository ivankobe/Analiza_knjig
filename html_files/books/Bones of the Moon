<div id="review_135485722" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/135485722" />
    <a title="Fox" class="left imgcol" href="/user/show/1702050-fox"><img alt="Fox" src="https://images.gr-assets.com/users/1256254888p2/1702050.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/135485722?book_show_action=true">Dec 17, 2010</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Fox" class="user" itemprop="url" name="Fox" href="/user/show/1702050-fox">Fox</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was amazing"><span size="15x15" class="staticStar p10">it was amazing</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/1702050-fox?shelf=fiction">fiction</a>, 
              <a class="actionLinkLite" href="/review/list/1702050-fox?shelf=fantasy">fantasy</a>, 
              <a class="actionLinkLite" href="/review/list/1702050-fox?shelf=sci-fi">sci-fi</a>, 
              <a class="actionLinkLite" href="/review/list/1702050-fox?shelf=2011">2011</a>, 
              <a class="actionLinkLite" href="/review/list/1702050-fox?shelf=own">own</a>, 
              <a class="actionLinkLite" href="/review/list/1702050-fox?shelf=horror">horror</a>, 
              <a class="actionLinkLite" href="/review/list/1702050-fox?shelf=mars-book">mars-book</a>
