        $('bookCover_53916157').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_53916157').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_53916157').observe('prototip:hidden', function () {
