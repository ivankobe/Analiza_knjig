        $('bookCover_301928').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_301928').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
