        $('bookCover_27').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_27').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
