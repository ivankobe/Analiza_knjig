<div id="review_2731718651" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2731718651" />
    <a title="emma" class="left imgcol" href="/user/show/5967293-emma"><img alt="emma" src="https://images.gr-assets.com/users/1492024441p2/5967293.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2731718651?book_show_action=true">Mar 10, 2019</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="emma" class="user" itemprop="url" name="emma" href="/user/show/5967293-emma">emma</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was amazing"><span size="15x15" class="staticStar p10">it was amazing</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span></span>



        

