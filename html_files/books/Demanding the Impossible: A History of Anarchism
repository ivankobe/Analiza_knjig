  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/512229205?book_show_action=true">Jan 20, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Marie" class="user" itemprop="url" name="Marie" href="/user/show/5453584-marie">Marie</a>
