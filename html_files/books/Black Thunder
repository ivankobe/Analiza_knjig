<div id="review_1729152101" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1729152101" />
    <a title="Charlie" class="left imgcol" href="/user/show/30273042-charlie"><img alt="Charlie" src="https://s.gr-assets.com/assets/nophoto/user/u_50x66-632230dc9882b4352d753eedf9396530.png" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1729152101?book_show_action=true">Aug 15, 2016</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Charlie" class="user" itemprop="url" name="Charlie" href="/user/show/30273042-charlie">Charlie</a>
