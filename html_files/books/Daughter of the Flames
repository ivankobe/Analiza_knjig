      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_44293197').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_44293197').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_44293197').observe('prototip:hidden', function () {
