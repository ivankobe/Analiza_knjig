        $('bookCover_11101772').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_11101772').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
