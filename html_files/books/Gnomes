<div id="review_2580633807" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2580633807" />
    <a title="Joanka" class="left imgcol" href="/user/show/20233424-joanka"><img alt="Joanka" src="https://images.gr-assets.com/users/1476184344p2/20233424.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2580633807?book_show_action=true">Nov 01, 2018</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Joanka" class="user" itemprop="url" name="Joanka" href="/user/show/20233424-joanka">Joanka</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="liked it"><span size="15x15" class="staticStar p10">liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/20233424-joanka?shelf=men">men</a>, 
              <a class="actionLinkLite" href="/review/list/20233424-joanka?shelf=fantasy-scifi-and-co">fantasy-scifi-and-co</a>, 
              <a class="actionLinkLite" href="/review/list/20233424-joanka?shelf=fiction">fiction</a>, 
              <a class="actionLinkLite" href="/review/list/20233424-joanka?shelf=europe">europe</a>, 
              <a class="actionLinkLite" href="/review/list/20233424-joanka?shelf=kids">kids</a>
          </div>
