        $('bookCover_18524037').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_18524037').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
