        $('bookCover_863348').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_863348').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
