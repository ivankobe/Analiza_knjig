        $('bookCover_17882177').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_17882177').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
