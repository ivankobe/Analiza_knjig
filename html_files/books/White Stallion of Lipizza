        $('bookCover_2185558').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_2185558').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
