        $('bookCover_235117').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_235117').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
