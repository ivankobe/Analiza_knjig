        $('bookCover_72543').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_72543').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
