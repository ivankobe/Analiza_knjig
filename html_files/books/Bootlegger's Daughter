<div id="review_26565821" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/26565821" />
    <a title="LJ" class="left imgcol" href="/user/show/250195-lj"><img alt="LJ" src="https://images.gr-assets.com/users/1436467895p2/250195.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/26565821?book_show_action=true">Jul 07, 2008</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="LJ" class="user" itemprop="url" name="LJ" href="/user/show/250195-lj">LJ</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was amazing"><span size="15x15" class="staticStar p10">it was amazing</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/250195-lj?shelf=legal_thriller">legal_thriller</a>, 
              <a class="actionLinkLite" href="/review/list/250195-lj?shelf=female_author">female_author</a>, 
              <a class="actionLinkLite" href="/review/list/250195-lj?shelf=mystery">mystery</a>, 
              <a class="actionLinkLite" href="/review/list/250195-lj?shelf=traditional_mystery">traditional_mystery</a>, 
              <a class="actionLinkLite" href="/review/list/250195-lj?shelf=contemporary_post_1945">contemporary_post_1945</a>, 
              <a class="actionLinkLite" href="/review/list/250195-lj?shelf=north_carolina">north_carolina</a>
          </div>
