        $('bookCover_49247281').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_49247281').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
