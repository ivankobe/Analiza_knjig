      var newTip = new Tip($('rating_details_tip'), "\n        <div id=\"moreBookData\" class=\"bookMeta\" >\n  \n\n  \n\n<table border=\"0\" id=\"rating_distribution\" style=\"width: auto; max-width: 500px;\" cellspacing=\"5\" cellpadding=\"0\">\n      <tr>\n        <th width=\"25\" class=\"right\">5&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 71%;\" title=\"32 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/431797.The_Voltairine_de_Cleyre_Reader?rating=5\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'795HRRA1w7kxlhKrJJQp8whNkdB/r5+SMjtVfqNwZj14AwCLwN2F4qGUdj9BkNDc89A5+srHSLW7qrj+AHFUAQ==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          31% (32)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">4&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 100%;\" title=\"45 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/431797.The_Voltairine_de_Cleyre_Reader?rating=4\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'J7K508ew81c5RA0fn7XfJIMlfosC3f2C6hlJx9tEUr2wb/4dF1i1DKlGaYv6sSYLeLjWobe1KqVjiKRHeEVggQ==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          43% (45)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">3&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 42%;\" title=\"19 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/431797.The_Volta