  <div class="friendReviews elementListBrown" >
    <div class="section">
      

<div id="review_2205876306" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2205876306" />
    <a title="T.M. Lakomy" class="left imgcol" href="/user/show/64772070-t-m-lakomy"><img alt="T.M. Lakomy" src="https://images.gr-assets.com/users/1499002222p2/64772070.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
