<div id="review_2546977372" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2546977372" />
    <a title="Koen" class="left imgcol" href="/user/show/33311084-koen"><img alt="Koen" src="https://images.gr-assets.com/users/1613227540p2/33311084.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2546977372?book_show_action=true">Oct 06, 2018</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Koen" class="user" itemprop="url" name="Koen" href="/user/show/33311084-koen">Koen</a>
