        $('bookCover_53877298').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_53877298').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
