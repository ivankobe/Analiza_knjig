          $$('div.prototip').each(function (e) {
            if ($('bookCover_84369').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_84369').observe('prototip:hidden', function () {
