        $('bookCover_156171').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_156171').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
