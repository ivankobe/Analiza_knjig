        $('bookCover_11879594').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_11879594').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
