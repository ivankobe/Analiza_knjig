      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_17285992').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_17285992').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_17285992').observe('prototip:hidden', function () {
