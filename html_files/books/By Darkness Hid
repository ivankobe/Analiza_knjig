        $('bookCover_24466484').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_24466484').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
