        $('bookCover_1339023').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1339023').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
