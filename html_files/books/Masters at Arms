<div id="review_441562316" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/441562316" />
    <a title="Steph" class="left imgcol" href="/user/show/2142019-steph"><img alt="Steph" src="https://images.gr-assets.com/users/1610750689p2/2142019.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/441562316?book_show_action=true">Oct 25, 2012</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Steph" class="user" itemprop="url" name="Steph" href="/user/show/2142019-steph">Steph</a>
