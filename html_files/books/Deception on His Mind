        $('bookCover_394228').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_394228').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
