      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_29918720').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_29918720').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_29918720').observe('prototip:hidden', function () {
