        $('bookCover_6751118').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_6751118').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
