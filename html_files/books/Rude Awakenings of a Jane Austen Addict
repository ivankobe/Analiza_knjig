        $('bookCover_15018709').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_15018709').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
