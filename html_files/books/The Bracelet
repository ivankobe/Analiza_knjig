        $('bookCover_51270862').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_51270862').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_51270862').observe('prototip:hidden', function () {
