      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_52246066').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_52246066').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_52246066').observe('prototip:hidden', function () {
