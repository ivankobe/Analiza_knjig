        $('bookCover_1930938').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1930938').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
