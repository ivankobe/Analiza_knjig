        $('bookCover_7124053').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_7124053').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
