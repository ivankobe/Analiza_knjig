        $('bookCover_30687916').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_30687916').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
