        $('bookCover_386162').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_386162').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
