        $('bookCover_73051').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_73051').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
