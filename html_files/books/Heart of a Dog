        $('bookCover_770345').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_770345').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
