      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_5577077').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_5577077').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_5577077').observe('prototip:hidden', function () {
