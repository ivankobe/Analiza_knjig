          $$('div.prototip').each(function (e) {
            if ($('bookCover_1422250').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_1422250').observe('prototip:hidden', function () {
