<div id="review_2536166380" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2536166380" />
    <a title="Montzalee Wittmann" class="left imgcol" href="/user/show/32532051-montzalee-wittmann"><img alt="Montzalee Wittmann" src="https://images.gr-assets.com/users/1624230240p2/32532051.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2536166380?book_show_action=true">Sep 23, 2018</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Montzalee Wittmann" class="user" itemprop="url" name="Montzalee Wittmann" href="/user/show/32532051-montzalee-wittmann">Montzalee Wittmann</a>
