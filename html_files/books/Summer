<div id="review_98919482" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/98919482" />
    <a title="Kalliope" class="left imgcol" href="/user/show/3593962-kalliope"><img alt="Kalliope" src="https://images.gr-assets.com/users/1428482885p2/3593962.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/98919482?book_show_action=true">Apr 17, 2010</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Kalliope" class="user" itemprop="url" name="Kalliope" href="/user/show/3593962-kalliope">Kalliope</a>
