  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2209842549?book_show_action=true">Jan 15, 2018</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="İntellecta" class="user" itemprop="url" name="İntellecta" href="/user/show/67897611-i-ntellecta">İntellecta</a>
