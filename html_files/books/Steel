        $('bookCover_7059341').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_7059341').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
