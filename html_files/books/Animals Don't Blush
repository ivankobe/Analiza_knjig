      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_41015277').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_41015277').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_41015277').observe('prototip:hidden', function () {
