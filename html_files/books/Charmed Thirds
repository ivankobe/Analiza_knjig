        $('bookCover_199782').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_199782').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
