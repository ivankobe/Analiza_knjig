<div id="review_2873224431" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2873224431" />
    <a title="Erin" class="left imgcol" href="/user/show/21172673-erin"><img alt="Erin" src="https://images.gr-assets.com/users/1546836153p2/21172673.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2873224431?book_show_action=true">Jun 29, 2019</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Erin" class="user" itemprop="url" name="Erin" href="/user/show/21172673-erin">Erin</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="liked it"><span size="15x15" class="staticStar p10">liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>

