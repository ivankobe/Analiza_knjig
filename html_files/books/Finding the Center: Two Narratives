      var newTip = new Tip($('rating_details_tip'), "\n        <div id=\"moreBookData\" class=\"bookMeta\" >\n  \n\n  \n\n<table border=\"0\" id=\"rating_distribution\" style=\"width: auto; max-width: 500px;\" cellspacing=\"5\" cellpadding=\"0\">\n      <tr>\n        <th width=\"25\" class=\"right\">5&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 32%;\" title=\"21 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/6138387-finding-the-center?rating=5\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'UzQ/c+b2Xn0DXzmrPUduoTjmpmE0yGSH9gc3Z0mYIl6XxiuDtmTY5V+ok/othYe6Ym7IF6H5pNEIQmhaDwlWFg==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          15% (21)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">4&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 100%;\" title=\"65 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/6138387-finding-the-center?rating=4\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'bSAJRtG1wgxy51JMLhZRw83PC4AydlrvQ1tsiI+tV8+p0h22gSdElC4Q+B0+1LjYl0dl9qdHmrm9HjO1yTwjhw==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          47% (65)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">3&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 55%;\" title=\"36 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/6138387-finding-the-center?rating=3\', {