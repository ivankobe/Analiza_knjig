        $('bookCover_40125217').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_40125217').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
