    <a id="rating_details_tip" class="gr-hyperlink" style="cursor: pointer; " href="#" onclick="return false"> Rating details</a><script>
//<![CDATA[
      var newTip = new Tip($('rating_details_tip'), "\n        <div id=\"moreBookData\" class=\"bookMeta\" >\n  \n\n  \n\n<table border=\"0\" id=\"rating_distribution\" style=\"width: auto; max-width: 500px;\" cellspacing=\"5\" cellpadding=\"0\">\n      <tr>\n        <th width=\"25\" class=\"right\">5&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 100%;\" title=\"31 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/8163063-cover-me?rating=5\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'VUNUIxLHoPF3wgUYhV1E02ZH/GQNqa1EGshNSQXSIp/40I3k4HiG6YExKsUddElSKupUBShaF+7vYzNMAYCjog==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          37% (31)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">4&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 48%;\" title=\"15 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/8163063-cover-me?rating=4\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'bCdsfV0kzB3EgxIBPed0QyuLpZDUQx4iZZ8RuYpcwEPBtLW6r5vqBTJwPdylznnCZyYN8fGwpIiQNG+8jg5Bfg==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          18% (15)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">3&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 77%;\" title=\"24 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/8163063-cover-me?rating=3\', {asynchronous:true, evalScripts