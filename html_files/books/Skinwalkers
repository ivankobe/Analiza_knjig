        $('bookCover_425100').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_425100').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
