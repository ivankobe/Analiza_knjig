        $('bookCover_163767').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_163767').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_163767').observe('prototip:hidden', function () {
