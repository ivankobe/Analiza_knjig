        $('bookCover_36516808').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_36516808').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_36516808').observe('prototip:hidden', function () {
        $$('span.elementTwo').each(function (e) {
          if (e.getStyle('display') !== 'none') {
            var lessLink = e.next();
