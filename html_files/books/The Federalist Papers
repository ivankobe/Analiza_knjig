<div id="review_102439561" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/102439561" />
    <a title="Amy" class="left imgcol" href="/user/show/2329228-amy"><img alt="Amy" src="https://images.gr-assets.com/users/1433629372p2/2329228.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/102439561?book_show_action=true">Jun 05, 2020</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Amy" class="user" itemprop="url" name="Amy" href="/user/show/2329228-amy">Amy</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was amazing"><span size="15x15" class="staticStar p10">it was amazing</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/2329228-amy?shelf=classics">classics</a>, 
              <a class="actionLinkLite" href="/review/list/2329228-amy?shelf=mr-baer-reccomended">mr-baer-reccomended</a>, 
              <a class="actionLinkLite" href="/review/list/2329228-amy?shelf=jurisprudence">jurisprudence</a>, 
              <a class="actionLinkLite" href="/review/list/2329228-amy?shelf=audio">audio</a>
          </div>
