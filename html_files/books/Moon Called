        $('bookCover_20705702').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_20705702').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
