<div id="review_540202" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/540202" />
    <a title="Cora" class="left imgcol" href="/user/show/46828-cora"><img alt="Cora" src="https://images.gr-assets.com/users/1175536519p2/46828.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/540202?book_show_action=true">Apr 02, 2007</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Cora" class="user" itemprop="url" name="Cora" href="/user/show/46828-cora">Cora</a>
      </span>

