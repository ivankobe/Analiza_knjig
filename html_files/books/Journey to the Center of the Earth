        $('bookCover_1970383').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1970383').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
