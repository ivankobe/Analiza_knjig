  <div data-react-class="ReactComponents.ShareDialog" data-react-props="{&quot;signedIn&quot;:false,&quot;signInUrl&quot;:&quot;/user/sign_in&quot;,&quot;social&quot;:{&quot;inModal&quot;:{&quot;facebook&quot;:true,&quot;twitter&quot;:true,&quot;pinterest&quot;:true,&quot;copyLink&quot;:false},&quot;outsideModal&quot;:{&quot;facebook&quot;:true,&quot;twitter&quot;:true,&quot;pinterest&quot;:true,&quot;copyLink&quot;:false},&quot;fbOptions&quot;:{&quot;name&quot;:&quot;The Gilded Ones by Namina Forna&quot;,&quot;caption&quot;:&quot;goodreads.com&quot;,&quot;description&quot;:&quot;&quot;,&quot;link&quot;:&quot;https://www.goodreads.com/book/show/40024121-the-gilded-ones&quot;,&quot;picture&quot;:&quot;https://i.gr-assets.com/images/S/compressed.photo.goodreads.com/books/1585578175l/40024121._SY75_.jpg&quot;},&quot;twitterOptions&quot;:{&quot;text&quot;:&quot;The Gilded Ones by Namina Forna&quot;,&quot;url&quot;:&quot;https://www.goodreads.com/book/show/40024121-the-gilded-ones&quot;,&quot;via&quot;:&quot;goodreads&quot;},&quot;pinterestOptions&quot;:{&quot;description&quot;:&quot;The Gilded Ones by Namina Forna&quot;,&quot;url&quot;:&quot;https://www.goodreads.com/book/show/40024121-the-gilded-ones&quot;,&quot;media&quot;:&quot;https://i.gr-assets.com/images/S/compressed.photo.goodreads.com/books/1585578175l/40024121._SY475_.jpg&quot;}},&quot;sharingData&quot;:{&quot;resourceTypeFriendlyText&quot;:&quot;book&quot;,&quot;resourceId&quot;:40024121},&quot;includePreview&quot;:true,&quot;previewData&quot;:{&quot;author&quot;:{&quot;name&quot;:&quot;Namina Forna&quot;,&quot;isGoodreadsAuthor&quot;:true},&quot;rating&quot;:&quot;4.07&quot;,&quot;title&quot;:&quot;The Gilded Ones (The Gilded Ones, #1)&quot;,&quot;imageUrl&quot;:&quot;https://i.gr-assets.com/images/S/compressed.photo.goodreads.com/books/1585578175i/40024121._UX75_.jpg&quot;,&quot;ratingsCount&quot;:15673,&quot;textReviewsCount&quot;:3641}}"><div data-reactid=".2eo3wmoh5ma" data-react-checksum="997641466"><di