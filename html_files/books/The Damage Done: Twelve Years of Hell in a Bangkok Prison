        $('bookCover_230543').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_230543').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
