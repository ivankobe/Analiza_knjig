        $('bookCover_393636').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_393636').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
