        $('bookCover_56868924').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_56868924').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
