        $('bookCover_17365069').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_17365069').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
