        $('bookCover_13536860').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_13536860').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
