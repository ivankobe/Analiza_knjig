        $('bookCover_69435').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_69435').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
