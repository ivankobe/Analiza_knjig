      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_524'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/1875.The_History_of_Sexuality_Volume_1?rating=\">all <span class=\"greyText\">(58863)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/1875.The_History_of_Sexuality_Volume_1?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;AGBTM73+wt/6/2nSZn3GowD1/nASPSp6IV7i9vrhtp1XjKY7Wu0F8JWaIZtBzorEzUhU4XcqKARnUNpKEYtrGw==&#39;)}); return false;\">5 stars <span class=\"greyText\">(7089)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/1875.The_History_of_Sexuality_Volume_1?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;I6MZBlMVcY1djUaaEMvIOpEnXGlOBB575y1bFGeYD9J0T+wOtAa2ojLoDtM3eIRdXJr2+CsTHAWhI2OojPLSVA==&#39;)}); return false;\">4 stars <span class=\"greyText\">(7393)<\/span><\/a>\n                  <span class=\