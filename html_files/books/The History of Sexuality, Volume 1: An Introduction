<div id="review_15465359" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/15465359" />
    <a title="a.novel.femme" class="left imgcol" href="/user/show/908491-a-novel-femme"><img alt="a.novel.femme" src="https://images.gr-assets.com/users/1565815584p2/908491.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/15465359?book_show_action=true">Feb 14, 2008</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="a.novel.femme" class="user" itemprop="url" name="a.novel.femme" href="/user/show/908491-a-novel-femme">a.novel.femme</a>
