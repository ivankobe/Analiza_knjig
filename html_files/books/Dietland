          $$('div.prototip').each(function (e) {
            if ($('bookCover_4937206').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_4937206').observe('prototip:hidden', function () {
