        $('bookCover_38619').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_38619').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
