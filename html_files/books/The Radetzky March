        $('bookCover_55881201').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_55881201').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
