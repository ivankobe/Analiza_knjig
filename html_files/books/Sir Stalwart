        $('bookCover_114955').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_114955').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
