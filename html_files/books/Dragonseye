        $('bookCover_45167409').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_45167409').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
