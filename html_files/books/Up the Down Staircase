        $('bookCover_55249702').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_55249702').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
