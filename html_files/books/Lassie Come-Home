<div id="review_202995614" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/202995614" />
    <a title="booklady" class="left imgcol" href="/user/show/350218-booklady"><img alt="booklady" src="https://images.gr-assets.com/users/1609115994p2/350218.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/202995614?book_show_action=true">Aug 28, 2011</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="booklady" class="user" itemprop="url" name="booklady" href="/user/show/350218-booklady">booklady</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was amazing"><span size="15x15" class="staticStar p10">it was amazing</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/350218-booklady?shelf=all-ages">all-ages</a>, 
              <a class="actionLinkLite" href="/review/list/350218-booklady?shelf=children">children</a>, 
              <a class="actionLinkLite" href="/review/list/350218-booklady?shelf=worth-reading-over-and-over">worth-reading-over-and-over</a>, 
              <a class="actionLinkLite" href="/review/list/350218-booklady?shelf=adventure">adventure</a>, 
              <a class="actionLinkLite" href="/review/list/350218-booklady?shelf=classic">classic</a>, 
              <a class="actionLinkLite" href="/review/list/350218-booklady?shelf=family">family</a>, 
              <a class="actionLinkLite" href="/review/list/350218-booklady?shelf=fiction">fiction</a>, 
              <a class="actionLinkLite" href="/review/list/350218-booklady?shelf=historical-fiction">historical-fiction</a>, 
              <a class="actionLinkLite" href="/review/list/350218-booklady?shelf=2011">2011</a>, 
