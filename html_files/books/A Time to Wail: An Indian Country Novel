      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_50992614').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_50992614').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_50992614').observe('prototip:hidden', function () {
