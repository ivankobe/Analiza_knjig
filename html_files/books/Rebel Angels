        $('bookCover_6392455').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_6392455').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
