        $('bookCover_12553775').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_12553775').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
