        $('bookCover_48713832').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_48713832').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
