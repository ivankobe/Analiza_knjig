      var newTip = new Tip($('rating_details_tip'), "\n        <div id=\"moreBookData\" class=\"bookMeta\" >\n  \n\n  \n\n<table border=\"0\" id=\"rating_distribution\" style=\"width: auto; max-width: 500px;\" cellspacing=\"5\" cellpadding=\"0\">\n      <tr>\n        <th width=\"25\" class=\"right\">5&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 54%;\" title=\"288 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/554330.The_Strange_Files_of_Fremont_Jones?rating=5\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'BMRdAMArl76/DA+TKShm8jQTJGUzB3r5k/g6kK3Dt74OUEgTaXN9IPtRB0rGLAp6NC3Jc9B54hVqDcLiOEE97A==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          22% (288)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">4&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 100%;\" title=\"531 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/554330.The_Strange_Files_of_Fremont_Jones?rating=4\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'gXNSVejMng37mJbiQYgAlnlaKxUDRCR3ZXGo5HI4kOWL50dGQZR0k7/FnjuujGweeWTGA+A6vJuchFCW57oatw==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          41% (531)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">3&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 68%;\" title=\"362 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/55433