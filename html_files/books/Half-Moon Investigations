        $('bookCover_334038').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_334038').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
