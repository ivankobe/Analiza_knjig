        $('bookCover_149138').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_149138').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
