      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_739'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/11595276-the-miseducation-of-cameron-post?rating=\">all <span class=\"greyText\">(129327)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/11595276-the-miseducation-of-cameron-post?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;t5heAAcCagaNaMiRko580dj/ERsIJ0nvA2AGuXkbb0B3P/urtE8hN6+9Q6Jtbe1U4WQs7kg5KnfRJ6GCa5P1FQ==&#39;)}); return false;\">5 stars <span class=\"greyText\">(16697)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/11595276-the-miseducation-of-cameron-post?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;f7TmOlmk/MngJe7tVrgd00m+P8R39P/BljlKw1ATsry/E0OR6um3+MLwZd6pW4xWcCUCMTfqnFlEfu34Qpso6Q==&#39;)}); return false;\">4 stars <span class=\"greyText\">(16101)<\/span><\/a>\n                  <