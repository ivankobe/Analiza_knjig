        $('bookCover_57374492').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_57374492').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
