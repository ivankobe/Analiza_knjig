        $('bookCover_1238339').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1238339').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
