        $('bookCover_12291438').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_12291438').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
