        $('bookCover_2638736').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_2638736').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_2638736').observe('prototip:hidden', function () {
        $$('span.elementTwo').each(function (e) {
          if (e.getStyle('display') !== 'none') {
