<div id="review_834494605" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/834494605" />
    <a title="Jen" class="left imgcol" href="/user/show/27709782-jen"><img alt="Jen" src="https://images.gr-assets.com/users/1592393550p2/27709782.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/834494605?book_show_action=true">Jan 25, 2014</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Jen" class="user" itemprop="url" name="Jen" href="/user/show/27709782-jen">Jen</a>
