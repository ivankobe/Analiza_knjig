        $('bookCover_18104367').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_18104367').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
