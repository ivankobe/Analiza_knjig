      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_5933646').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_5933646').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_5933646').observe('prototip:hidden', function () {
