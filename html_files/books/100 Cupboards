      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_22091081').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_22091081').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_22091081').observe('prototip:hidden', function () {
