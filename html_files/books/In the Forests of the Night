        $('bookCover_291896').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_291896').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
