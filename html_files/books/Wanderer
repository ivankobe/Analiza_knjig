          $$('div.prototip').each(function (e) {
            if ($('bookCover_55940723').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_55940723').observe('prototip:hidden', function () {
