        $('bookCover_4992047').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_4992047').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_4992047').observe('prototip:hidden', function () {
