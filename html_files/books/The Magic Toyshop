        $('bookCover_28501482').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_28501482').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
