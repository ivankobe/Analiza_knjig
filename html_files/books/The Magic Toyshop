      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_409464').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_409464').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_409464').observe('prototip:hidden', function () {
