        $('bookCover_14201').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_14201').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
