        $('bookCover_8534237').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_8534237').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
