<div id="review_1146020695" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1146020695" />
    <a title="Michelle" class="left imgcol" href="/user/show/1283597-michelle"><img alt="Michelle" src="https://images.gr-assets.com/users/1302269745p2/1283597.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1146020695?book_show_action=true">Dec 29, 2014</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Michelle" class="user" itemprop="url" name="Michelle" href="/user/show/1283597-michelle">Michelle</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="liked it"><span size="15x15" class="staticStar p10">liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>



        

