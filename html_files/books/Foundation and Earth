        $('bookCover_44767458').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_44767458').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
