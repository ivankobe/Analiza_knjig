<div id="review_2165072120" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2165072120" />
    <a title="Idarah" class="left imgcol" href="/user/show/1283951-idarah"><img alt="Idarah" src="https://images.gr-assets.com/users/1621951086p2/1283951.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2165072120?book_show_action=true">Nov 04, 2017</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
