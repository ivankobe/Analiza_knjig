        $('bookCover_53568139').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_53568139').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
