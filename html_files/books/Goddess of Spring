        $('bookCover_53497628').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_53497628').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
