          $$('div.prototip').each(function (e) {
            if ($('bookCover_3079197').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_3079197').observe('prototip:hidden', function () {
