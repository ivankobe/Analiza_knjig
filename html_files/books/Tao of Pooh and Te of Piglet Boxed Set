        $('bookCover_2256').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_2256').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
