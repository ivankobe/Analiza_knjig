        $('bookCover_85306').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_85306').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_85306').observe('prototip:hidden', function () {
