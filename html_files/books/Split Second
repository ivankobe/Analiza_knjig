<div id="review_2034864435" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2034864435" />
    <a title="Josen" class="left imgcol" href="/user/show/3660423-josen"><img alt="Josen" src="https://images.gr-assets.com/users/1577999503p2/3660423.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2034864435?book_show_action=true">Jul 05, 2017</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Josen" class="user" itemprop="url" name="Josen" href="/user/show/3660423-josen">Josen</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="really liked it"><span size="15x15" class="staticStar p10">really liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span></span>

          <span class="greyText">&nbsp;&middot;&nbsp;</span>
          <a class="lightGreyText" title="Audio CD 9781600241345" href="/book/show/3210180-split-second">review of another edition</a>


        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/3660423-josen?shelf=series">series</a>, 
              <a class="actionLinkLite" href="/review/list/3660423-josen?shelf=audio">audio</a>, 
              <a class="actionLinkLite" href="/review/list/3660423-josen?shelf=2017-library">2017-library</a>
          </div>
