        $('bookCover_41746324').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_41746324').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
