        $('bookCover_8127').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_8127').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
