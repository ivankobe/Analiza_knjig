          $$('div.prototip').each(function (e) {
            if ($('bookCover_56854580').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_56854580').observe('prototip:hidden', function () {
