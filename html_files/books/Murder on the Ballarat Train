        $('bookCover_41022009').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_41022009').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_41022009').observe('prototip:hidden', function () {
