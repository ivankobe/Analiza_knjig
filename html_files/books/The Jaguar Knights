      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_51815833').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_51815833').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_51815833').observe('prototip:hidden', function () {
