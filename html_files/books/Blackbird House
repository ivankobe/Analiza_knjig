        $('bookCover_56240582').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_56240582').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
