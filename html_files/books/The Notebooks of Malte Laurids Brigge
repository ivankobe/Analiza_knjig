        $('bookCover_648535').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_648535').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
