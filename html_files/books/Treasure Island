<div id="review_3117791975" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/3117791975" />
    <a title="Peter" class="left imgcol" href="/user/show/10477405-peter"><img alt="Peter" src="https://images.gr-assets.com/users/1340652114p2/10477405.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/3117791975?book_show_action=true">Jan 02, 2020</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Peter" class="user" itemprop="url" name="Peter" href="/user/show/10477405-peter">Peter</a>
