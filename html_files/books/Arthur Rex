        $('bookCover_13584314').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_13584314').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
