        $('bookCover_815715').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_815715').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
