        $('bookCover_442347').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_442347').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
