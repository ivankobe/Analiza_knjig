        $('bookCover_52382796').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_52382796').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
