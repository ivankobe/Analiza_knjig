        $('bookCover_40611463').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_40611463').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
