      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_363168').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_363168').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_363168').observe('prototip:hidden', function () {
