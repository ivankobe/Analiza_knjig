        $('bookCover_525818').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_525818').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
