      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_610'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/22076.From_a_Buick_8?rating=\">all <span class=\"greyText\">(102881)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/22076.From_a_Buick_8?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;PJMfb7g1tK6Qxc0FqbDOoZvVafw/2ksNA7LKtCHHkWU+Kbgg90IS2hiehREFtgiolEfibN+sw+jWzcc6uPftnA==&#39;)}); return false;\">5 stars <span class=\"greyText\">(11656)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/22076.From_a_Buick_8?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;M8PuEiNMkP/4deOBzNFUPfMVNmO4sRyNGmYBBtye0XUxeUldbDs2i3Auq5Vg15I0/Ie981jHlGjPGQyIRa6tjA==&#39;)}); return false;\">4 stars <span class=\"greyText\">(17830)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <sp