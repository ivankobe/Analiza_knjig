<div id="review_940244412" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/940244412" />
    <a title="Ian" class="left imgcol" href="/user/show/2463942-ian"><img alt="Ian" src="https://s.gr-assets.com/assets/nophoto/user/m_50x66-82093808bca726cb3249a493fbd3bd0f.png" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/940244412?book_show_action=true">May 17, 2014</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Ian" class="user" itemprop="url" name="Ian" href="/user/show/2463942-ian">Ian</a>
