      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_6916963').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_6916963').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_6916963').observe('prototip:hidden', function () {
