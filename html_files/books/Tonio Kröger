      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_686'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/437191.Tonio_Kr_ger?rating=\">all <span class=\"greyText\">(10947)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/437191.Tonio_Kr_ger?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;sGd+FwAjWF0gH4dav9IGMCcsD4O4ZG+6hYKMID1+2+FvtltMgo1DQPMvl7UZbXglqT67Br1K/LIm3xsIh2yeAg==&#39;)}); return false;\">5 stars <span class=\"greyText\">(1617)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/437191.Tonio_Kr_ger?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;Uvl493/UkpQ1LEuC2oTUWdzBowkqZeN9QWCzBMJd2L2NKF2s/XqJieYcW218O6pMUtMXjC9LcHXiPSQseE+dXg==&#39;)}); return false;\">4 stars <span class=\"greyText\">(2178)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span cla