        $('bookCover_68531').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_68531').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
