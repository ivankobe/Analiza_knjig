<div id="review_3862225303" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/3862225303" />
    <a title="Bhavya {on temporary hiatus}" class="left imgcol" href="/user/show/128191325-bhavya-on-temporary-hiatus"><img alt="Bhavya {on temporary hiatus}" src="https://images.gr-assets.com/users/1611319656p2/128191325.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/3862225303?book_show_action=true">Feb 26, 2021</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Bhavya {on temporary hiatus}" class="user" itemprop="url" name="Bhavya {on temporary hiatus}" href="/user/show/128191325-bhavya-on-temporary-hiatus">Bhavya {on temporary hiatus}</a>
