        $('bookCover_13144471').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_13144471').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
