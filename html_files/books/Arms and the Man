        $('bookCover_281326').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_281326').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
