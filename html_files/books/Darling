        $('bookCover_55601709').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_55601709').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
