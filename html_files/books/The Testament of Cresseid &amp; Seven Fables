        $('bookCover_820905').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_820905').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
