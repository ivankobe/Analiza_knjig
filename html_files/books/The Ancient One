        $('bookCover_9751723').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_9751723').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
