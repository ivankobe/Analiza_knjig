      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_847'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/77699.Dinner_at_the_Homesick_Restaurant?rating=\">all <span class=\"greyText\">(51829)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/77699.Dinner_at_the_Homesick_Restaurant?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;2JBj3cAV5IsAHsC6Y3whI+DliOi4t21sS06tkq3fxuJEvOnoIUR8soAngZQAtY6FJAEZlUMm0lqKfYMH2WKyWw==&#39;)}); return false;\">5 stars <span class=\"greyText\">(6621)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/77699.Dinner_at_the_Homesick_Restaurant?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;jNh3O+21wChBe4OlDi3x5r9mPV0/UE/bzNgpohtx+kYQ9P0ODORYEcFCwott5F5Ae4KsIMTB8O0N6wc3b8yO/w==&#39;)}); return false;\">4 stars <span class=\"greyText\">(10824)<\/span><\/a>\n                  <span cla