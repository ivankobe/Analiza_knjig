<div id="review_930427983" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/930427983" />
    <a title="Katerina" class="left imgcol" href="/user/show/16054419-katerina"><img alt="Katerina" src="https://images.gr-assets.com/users/1361738213p2/16054419.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/930427983?book_show_action=true">May 07, 2014</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Katerina" class="user" itemprop="url" name="Katerina" href="/user/show/16054419-katerina">Katerina</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was amazing"><span size="15x15" class="staticStar p10">it was amazing</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/16054419-katerina?shelf=memory-drift">memory-drift</a>, 
              <a class="actionLinkLite" href="/review/list/16054419-katerina?shelf=overwhelming">overwhelming</a>, 
              <a class="actionLinkLite" href="/review/list/16054419-katerina?shelf=2014_read">2014_read</a>, 
              <a class="actionLinkLite" href="/review/list/16054419-katerina?shelf=pageturner">pageturner</a>, 
              <a class="actionLinkLite" href="/review/list/16054419-katerina?shelf=american_lit">american_lit</a>, 
              <a class="actionLinkLite" href="/review/list/16054419-katerina?shelf=best">best</a>, 
              <a class="actionLinkLite" href="/review/list/16054419-katerina?shelf=comfort-zone">comfort-zone</a>, 
