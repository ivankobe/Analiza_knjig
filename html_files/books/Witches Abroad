      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_48558800').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_48558800').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_48558800').observe('prototip:hidden', function () {
