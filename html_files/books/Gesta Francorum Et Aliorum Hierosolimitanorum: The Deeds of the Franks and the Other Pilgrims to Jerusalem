        $('bookCover_60142').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_60142').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
