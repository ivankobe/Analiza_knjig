        $('bookCover_259054').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_259054').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
