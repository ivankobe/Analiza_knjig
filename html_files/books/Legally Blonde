<div id="review_1765502189" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1765502189" />
    <a title="Alaina" class="left imgcol" href="/user/show/50620706-alaina"><img alt="Alaina" src="https://images.gr-assets.com/users/1451325817p2/50620706.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1765502189?book_show_action=true">Jul 08, 2021</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Alaina" class="user" itemprop="url" name="Alaina" href="/user/show/50620706-alaina">Alaina</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="did not like it"><span size="15x15" class="staticStar p10">did not like it</span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/50620706-alaina?shelf=contemporary">contemporary</a>, 
              <a class="actionLinkLite" href="/review/list/50620706-alaina?shelf=fiction">fiction</a>, 
              <a class="actionLinkLite" href="/review/list/50620706-alaina?shelf=young-adult">young-adult</a>, 
              <a class="actionLinkLite" href="/review/list/50620706-alaina?shelf=2021">2021</a>, 
