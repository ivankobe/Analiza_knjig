        $('bookCover_26032887').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_26032887').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_26032887').observe('prototip:hidden', function () {
