<div id="review_36801678" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/36801678" />
    <a title="Kim" class="left imgcol" href="/user/show/1611556-kim"><img alt="Kim" src="https://images.gr-assets.com/users/1223751788p2/1611556.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/36801678?book_show_action=true">Nov 02, 2008</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Kim" class="user" itemprop="url" name="Kim" href="/user/show/1611556-kim">Kim</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="liked it"><span size="15x15" class="staticStar p10">liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>


