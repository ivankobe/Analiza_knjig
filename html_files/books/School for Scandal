        $('bookCover_240250').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_240250').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
