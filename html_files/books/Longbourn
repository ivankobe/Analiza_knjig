        $('bookCover_43890641').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_43890641').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
