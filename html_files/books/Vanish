<script type="text/javascript" charset="utf-8">
//<![CDATA[  

  function submitShelfLink(unique_id, book_id, shelf_id, shelf_name, submit_form, exclusive) {
    var checkbox_id = 'shelf_name_' + unique_id + '_' + shelf_id;
    var element = document.getElementById(checkbox_id)

    var checked = element.checked
    if (checked && exclusive) {
      // can't uncheck a radio by clicking it!
      return
    }
    if(document.getElementById("savingMessage")){
      Element.show('savingMessage')
    }
    var element_id = 'shelfInDropdownName_' + unique_id + '_' + shelf_id;
    Element.update(element_id, "saving...");
    if (submit_form) {
      Element.hide('shelfDropdown_' + unique_id)
      var form = document.getElementById('addBookForm' + book_id)
      if (form) {
        form.shelf.value = shelf_name
        form.onsubmit()
      }
    }
    else {
      var action = checked ? 'remove' : ''
      element.checked = !element.checked
      new Ajax.Request('/shelf/add_to_shelf', {asynchronous:true, evalScripts:true, onSuccess:function(request){shelfSubmitted(request, book_id, checkbox_id, element_id, unique_id, shelf_name)}, parameters:'book_id=' + book_id + '&name=' + shelf_name + '&a=' + action + '&authenticity_token=' + encodeURIComponent('0MEpeHtMic+DgswcoRTOkSJTFTMMwjKkOqbOXVYMHqfAZ2KHjdaT72a16v4VjyK81P8HhpWi/eFppenWhuQSAg==')})
    }
  }

  function shelfSubmitted(request, book_id, checkbox_id, element_id, unique_id, shelf_name) {
    Element.update('shelfListfalse_' + book_id, request.responseText)
    afterShelfSave(checkbox_id, element_id, unique_id, shelf_name.escapeHTML())
  }

  function refreshGroupBox(group_id, book_id) {
    new Ajax.Updater('addGroupBooks' + book_id + '', '/group/add_book_box', {asynchronous:true, evalScripts:true, onSuccess:function(request){refreshGroupBoxComplete(request, book_id);}, parameters:'id=' + group_id + '&book_id=' + book_id + '&refresh=true' + '&authenticity_token=' + encodeURIComponent('t1+NZFEABdCm5K2MF9m5wu9ZQIPZ8rv+pVqQKM7R8oGn+cabp5of8EPTi26jQlXvGfVSNkCSdLv2WbejHjn+JA==')})
  }
//]]>
</script>


<script>
//<![CDATA[
      var newTip = new Tip($('bookCover_55118813'), "\n\n  <h2><a class=\"readable bookTitle\" href=\"https://www.goodreads.com/book/show/55118813?from_choice=false&amp;from_home_module=false\">Куќата со жолта фасада<\/a><\/h2>\n\n      <div>\n        by <a class=\"authorName\" href=\"/author/show/20629739._?a=9&amp;origin=related_works\">Билјана Мартиновска<\/a>\n      <\/div>\n\n          <div class=\"smallText uitext darkGreyText\">\n            <span class=\"minirating\"><span class=\"stars staticStars notranslate\"><span size=\"12x12\" class=\"staticStar p10\"><\/span><span size=\"12x12\" class=\"staticStar p10\"><\/span><span size=\"12x12\" class=\"staticStar p10\"><\/span><span size=\"12x12\" class=\"staticStar p10\"><\/span><span size=\"12x12\" class=\"staticStar p3\"><\/span><\/span> 4.41 avg rating &mdash; 34 ratings<\/span>            &mdash; published \n          <\/div>\n\n    <div class=\"addBookTipDescription\">\n      \n<span id=\"freeTextContainer3817106716252355356\">Едно необично и неочекувано запознавање, целосно го менува животот на младата Ева и засекогаш ѝ го заробува срцето. Љубовта која се разви од тој магичен момент, е толку волшебна што ни самата не може да поверува. Среќниот и исполнет живот со Виктор, <\/span>\n  <span id=\"freeText3817106716252355356\" style=\"display:none\">Едно необично и неочекувано запознавање, целосно го менува животот на младата Ева и засекогаш ѝ го заробува срцето. Љубовта која се разви од тој магичен момент, е толку волшебна што ни самата не може да поверува. Среќниот и исполнет живот со Виктор, е многу повеќе од она што таа верува и очекува од еден маж. Нивната приказна е завиена во мистериозноста на неговото појавување во нејзиниот живот и настаните кои само ја зацврстуваат љубовта меѓу нив. Ева повеќе не може да го замисли животот без Виктор, сè до оној момент кога ќе мора да научи да живее без него.\n\nПриказна за силата на љубовта и силата на жената која останува да стои исправена пред предизвиците кои ѝ ги носи судбината и може да го поднесе секој товар, поради желбата за продолжување на животот.\n\nБилјана Мартиновска е родена на 8 август 1977 година во Струга. Книгата „Куќата со жолта фасада“ е нејзино второ авторско дело. Живее и твори во родниот град.<\/span>\n  <a data-text-id=\"3817106716252355356\" href=\"#\" onclick=\"swapContent(\$(this));; return false;\">...more<\/a>\n\n    <\/div>\n\n      <div class=\'wtrButtonContainer wtrSignedOut\' id=\'8_book_55118813\'>\n<div class=\'wtrUp wtrLeft\'>\n<form action=\"/shelf/add_to_shelf\" accept-charset=\"UTF-8\" method=\"post\"><input name=\"utf8\" type=\"hidden\" value=\"&#x2713;\" /><input type=\"hidden\" name=\"authenticity_token\" value=\"VNIaMSZdGlCUhViOSLtMc0JXUFAyAtGtzly//OFS4AFEdFHO0McAcHGyfmz8IKBetPtC5atiHuidX5h3MbrspA==\" />\n<input type=\"hidden\" name=\"book_id\" id=\"book_id\" value=\"55118813\" />\n<input type=\"hidden\" name=\"name\" id=\"name\" value=\"to-read\" />\n<input type=\"hidden\" name=\"unique_id\" id=\"unique_id\" value=\"8_book_55118813\" />\n<input type=\"hidden\" name=\"wtr_new\" id=\"wtr_new\" value=\"true\" />\n<input type=\"hidden\" name=\"from_choice\" id=\"from_choice\" value=\"false\" />\n<input type=\"hidden\" name=\"from_home_module\" id=\"from_home_module\" value=\"false\" />\n<input type=\"hidden\" name=\"ref\" id=\"ref\" value=\"\" class=\"wtrLeftUpRef\" />\n<input type=\"hidden\" name=\"existing_review\" id=\"existing_review\" value=\"false\" class=\"wtrExisting\" />\n<input type=\"hidden\" name=\"book_origin\" id=\"book_origin\" value=\"related_works\" />\n<input type=\"hidden\" name=\"page_url\" id=\"page_url\" value=\"/book/show/32255.Vanish\" />\n<button class=\'wtrToRead\' type=\'submit\'>\n<span class=\'progressTrigger\'>Want to Read<\/span>\n<span class=\'progressIndicator\'>saving…<\/span>\n<\/button>\n<\/form>\n\n<\/div>\n\n<div class=\'wtrRight wtrUp\'>\n<form class=\"hiddenShelfForm\" action=\"/shelf/add_to_shelf\" accept-charset=\"UTF-8\" method=\"post\"><input na