        $('bookCover_200883').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_200883').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
