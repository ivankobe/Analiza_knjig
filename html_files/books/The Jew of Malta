  <div class="friendReviews elementListBrown" >
    <div class="section">
      

<div id="review_752357880" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/752357880" />
    <a title="Tiffany" class="left imgcol" href="/user/show/2100454-tiffany"><img alt="Tiffany" src="https://images.gr-assets.com/users/1593581438p2/2100454.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/752357880?book_show_action=true">Oct 29, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
