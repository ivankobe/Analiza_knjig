        $('bookCover_158271').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_158271').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
