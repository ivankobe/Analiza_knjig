        $('bookCover_763047').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_763047').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
