        $('bookCover_31101').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_31101').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
