        $('bookCover_57622934').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_57622934').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
