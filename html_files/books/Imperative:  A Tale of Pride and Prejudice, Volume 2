  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/551226663?book_show_action=true">Mar 03, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Leslie" class="user" itemprop="url" name="Leslie" href="/user/show/846253-leslie">Leslie</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was ok"><span size="15x15" class="staticStar p10">it was ok</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/846253-leslie?shelf=austenesque">austenesque</a>, 
              <a class="actionLinkLite" href="/review/list/846253-leslie?shelf=purchased">purchased</a>, 
              <a class="actionLinkLite" href="/review/list/846253-leslie?shelf=ppown">ppown</a>
