        $('bookCover_25980653').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_25980653').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
