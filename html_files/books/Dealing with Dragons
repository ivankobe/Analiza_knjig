        $('bookCover_169879').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_169879').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
