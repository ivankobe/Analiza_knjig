        $('bookCover_6325305').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_6325305').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
