        $('bookCover_25883848').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_25883848').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
