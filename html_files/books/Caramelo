        $('bookCover_183105').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_183105').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
