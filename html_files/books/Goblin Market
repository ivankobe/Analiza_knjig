        $('bookCover_724371').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_724371').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
