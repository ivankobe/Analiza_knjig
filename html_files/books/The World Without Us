<div id="review_380134829" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/380134829" />
    <a title="Michael" class="left imgcol" href="/user/show/4086118-michael"><img alt="Michael" src="https://images.gr-assets.com/users/1567611029p2/4086118.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/380134829?book_show_action=true">Jul 30, 2012</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Michael" class="user" itemprop="url" name="Michael" href="/user/show/4086118-michael">Michael</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="really liked it"><span size="15x15" class="staticStar p10">really liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/4086118-michael?shelf=social-commentary">social-commentary</a>, 
              <a class="actionLinkLite" href="/review/list/4086118-michael?shelf=nature">nature</a>, 
              <a class="actionLinkLite" href="/review/list/4086118-michael?shelf=environment">environment</a>, 
              <a class="actionLinkLite" href="/review/list/4086118-michael?shelf=evolution">evolution</a>, 
              <a class="actionLinkLite" href="/review/list/4086118-michael?shelf=ecology">ecology</a>, 
              <a class="actionLinkLite" href="/review/list/4086118-michael?shelf=non-fiction">non-fiction</a>, 
              <a class="actionLinkLite" href="/review/list/4086118-michael?shelf=history">history</a>, 
              <a class="actionLinkLite" href="/review/list/4086118-michael?shelf=anthropology">anthropology</a>
          </div>
