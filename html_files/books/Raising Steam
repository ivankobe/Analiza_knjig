<div id="review_1650188277" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1650188277" />
    <a title="Ahmad Sharabiani" class="left imgcol" href="/user/show/614778-ahmad-sharabiani"><img alt="Ahmad Sharabiani" src="https://images.gr-assets.com/users/1194930432p2/614778.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1650188277?book_show_action=true">May 27, 2016</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Ahmad Sharabiani" class="user" itemprop="url" name="Ahmad Sharabiani" href="/user/show/614778-ahmad-sharabiani">Ahmad Sharabiani</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="really liked it"><span size="15x15" class="staticStar p10">really liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/614778-ahmad-sharabiani?shelf=21th-century">21th-century</a>, 
              <a class="actionLinkLite" href="/review/list/614778-ahmad-sharabiani?shelf=fiction">fiction</a>, 
              <a class="actionLinkLite" href="/review/list/614778-ahmad-sharabiani?shelf=humor">humor</a>, 
              <a class="actionLinkLite" href="/review/list/614778-ahmad-sharabiani?shelf=adventure">adventure</a>, 
              <a class="actionLinkLite" href="/review/list/614778-ahmad-sharabiani?shelf=fantasy">fantasy</a>, 
              <a class="actionLinkLite" href="/review/list/614778-ahmad-sharabiani?shelf=comedy">comedy</a>, 
              <a class="actionLinkLite" href="/review/list/614778-ahmad-sharabiani?shelf=science">science</a>, 
              <a class="actionLinkLite" href="/review/list/614778-ahmad-sharabiani?shelf=adult">adult</a>
