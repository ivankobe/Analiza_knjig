        $('bookCover_313110').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_313110').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
