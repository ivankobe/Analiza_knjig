<div id="review_2708670737" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2708670737" />
    <a title="Jess" class="left imgcol" href="/user/show/6513370-jess"><img alt="Jess" src="https://images.gr-assets.com/users/1559996198p2/6513370.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2708670737?book_show_action=true">Feb 10, 2019</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Jess" class="user" itemprop="url" name="Jess" href="/user/show/6513370-jess">Jess</a>
