        $('bookCover_620750').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_620750').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
