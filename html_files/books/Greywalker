          $$('div.prototip').each(function (e) {
            if ($('bookCover_3475145').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_3475145').observe('prototip:hidden', function () {
