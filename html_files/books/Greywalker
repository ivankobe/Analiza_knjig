      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_23590373').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_23590373').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_23590373').observe('prototip:hidden', function () {
