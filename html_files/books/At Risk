        $('bookCover_2356363').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_2356363').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
