  <div class="friendReviews elementListBrown" >
    <div class="section">
      

<div id="review_3383683686" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/3383683686" />
    <a title="Rachel" class="left imgcol" href="/user/show/3965659-rachel"><img alt="Rachel" src="https://images.gr-assets.com/users/1595877858p2/3965659.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/3383683686?book_show_action=true">Jun 12, 2020</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Rachel" class="user" itemprop="url" name="Rachel" href="/user/show/3965659-rachel">Rachel</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was ok"><span size="15x15" class="staticStar p10">it was ok</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>



        

