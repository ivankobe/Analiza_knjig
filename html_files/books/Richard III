        $('bookCover_3049').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_3049').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
