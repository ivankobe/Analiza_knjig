        $('bookCover_27250890').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_27250890').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
