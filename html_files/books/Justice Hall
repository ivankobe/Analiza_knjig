        $('bookCover_36510437').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_36510437').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
