        $('bookCover_9777').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_9777').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
