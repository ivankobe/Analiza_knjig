  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2323577444?book_show_action=true">Mar 10, 2018</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="PattyMacDotComma" class="user" itemprop="url" name="PattyMacDotComma" href="/user/show/1128612-pattymacdotcomma">PattyMacDotComma</a>
