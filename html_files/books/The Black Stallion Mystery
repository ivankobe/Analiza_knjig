        $('bookCover_1329726').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1329726').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
