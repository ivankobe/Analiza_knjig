        $('bookCover_51372783').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_51372783').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
