        $('bookCover_150144').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_150144').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
