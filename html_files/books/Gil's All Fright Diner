        $('bookCover_11689404').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_11689404').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
