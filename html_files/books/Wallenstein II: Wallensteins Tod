        $('bookCover_680596').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_680596').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
