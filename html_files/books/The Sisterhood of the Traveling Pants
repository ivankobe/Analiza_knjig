      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_228333').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_228333').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_228333').observe('prototip:hidden', function () {
