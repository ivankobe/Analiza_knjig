      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('edition_214438').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('edition_214438').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('edition_214438').observe('prototip:hidden', function () {
