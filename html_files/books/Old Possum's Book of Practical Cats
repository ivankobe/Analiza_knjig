        $('bookCover_56221360').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_56221360').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
