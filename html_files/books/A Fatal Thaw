        $('bookCover_398477').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_398477').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
