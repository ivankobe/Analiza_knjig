<div id="review_868284338" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/868284338" />
    <a title="Stephen" class="left imgcol" href="/user/show/990463-stephen"><img alt="Stephen" src="https://images.gr-assets.com/users/1292973527p2/990463.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/868284338?book_show_action=true">Mar 01, 2014</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Stephen" class="user" itemprop="url" name="Stephen" href="/user/show/990463-stephen">Stephen</a>
