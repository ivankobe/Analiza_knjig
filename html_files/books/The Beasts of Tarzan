        $('bookCover_685404').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_685404').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
