      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_338593').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_338593').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_338593').observe('prototip:hidden', function () {
