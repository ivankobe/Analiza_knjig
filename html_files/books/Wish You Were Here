        $('bookCover_165369').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_165369').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
