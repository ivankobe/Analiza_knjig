      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('edition_8508467').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('edition_8508467').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('edition_8508467').observe('prototip:hidden', function () {
