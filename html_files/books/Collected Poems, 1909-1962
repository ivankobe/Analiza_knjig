<div id="review_3151507992" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/3151507992" />
    <a title="Jack Lawrence" class="left imgcol" href="/user/show/56889923-jack-lawrence"><img alt="Jack Lawrence" src="https://s.gr-assets.com/assets/nophoto/user/m_50x66-82093808bca726cb3249a493fbd3bd0f.png" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/3151507992?book_show_action=true">Mar 09, 2020</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Jack Lawrence" class="user" itemprop="url" name="Jack Lawrence" href="/user/show/56889923-jack-lawrence">Jack Lawrence</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was amazing"><span size="15x15" class="staticStar p10">it was amazing</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span></span>



        

