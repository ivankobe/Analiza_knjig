      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_54615879').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_54615879').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_54615879').observe('prototip:hidden', function () {
