        $('bookCover_45835867').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_45835867').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
