        $('bookCover_402013').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_402013').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
