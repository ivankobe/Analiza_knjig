        $('bookCover_243401').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_243401').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
