      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_52329908').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_52329908').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_52329908').observe('prototip:hidden', function () {
