        $('bookCover_54236071').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_54236071').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
