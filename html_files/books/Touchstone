      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_23'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/1272835.Touchstone?rating=\">all <span class=\"greyText\">(7454)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/1272835.Touchstone?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;a4/vg/ByGi+rFo8/J5GPx971lrUdY5IzObi9FKVXLXuRQlxPfcHhcSXi3lyI9I48Bu1cStT5+MXkMfHb38Yk4w==&#39;)}); return false;\">5 stars <span class=\"greyText\">(864)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/1272835.Touchstone?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;yku5noozKy9IVT70W7x0HnjgmZATF7y1+SKTnOv5T1YwhgpSB4DQccahb5f02XXloPhTb9qN1kMkq99TkWhGzg==&#39;)}); return false;\">4 stars <span class=\"greyText\">(1546)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"l