        $('bookCover_43980').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_43980').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
