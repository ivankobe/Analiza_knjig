          $$('div.prototip').each(function (e) {
            if ($('bookCover_55809807').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_55809807').observe('prototip:hidden', function () {
