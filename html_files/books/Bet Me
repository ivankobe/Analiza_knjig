      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_686'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/854757.Bet_Me?rating=\">all <span class=\"greyText\">(155171)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/854757.Bet_Me?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;0ETOzxmYaVq2vnAnU3+DsDBH0K/Dg4n73tjUp3E0Om+v6kdJkTcik1S10jGZEQkR23/Tm6k9MB/Sa6/Lps6dPQ==&#39;)}); return false;\">5 stars <span class=\"greyText\">(31769)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/854757.Bet_Me?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;OJuxLg3ZXBPRBvH6/wrQPA4tudHANa9gnsTGRK4nUPFHNTiohXYX2jMNU+w1ZFqd5RW65aqLFoSSd70oed33ow==&#39;)}); return false;\">4 stars <span class=\"greyText\">(29393)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingErr