      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_42191124').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_42191124').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_42191124').observe('prototip:hidden', function () {
