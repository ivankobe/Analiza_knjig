  <div class="friendReviews elementListBrown" >
    <div class="section">
      

<div id="review_2039003235" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2039003235" />
    <a title="Malachy" class="left imgcol" href="/user/show/32957811-malachy"><img alt="Malachy" src="https://s.gr-assets.com/assets/nophoto/user/u_50x66-632230dc9882b4352d753eedf9396530.png" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2039003235?book_show_action=true">Jun 24, 2017</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Malachy" class="user" itemprop="url" name="Malachy" href="/user/show/32957811-malachy">Malachy</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was amazing"><span size="15x15" class="staticStar p10">it was amazing</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span></span>



        

