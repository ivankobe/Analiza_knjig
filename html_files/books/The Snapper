      <a class="uitext" rel="nofollow" id="span_class_gr-hyperlink_more_filters_span_tip_709" style="cursor: pointer; " href="#" onclick="return false"><span class="gr-hyperlink">More filters</span></a><script>
//<![CDATA[
      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_709'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/296793.The_Snapper?rating=\">all <span class=\"greyText\">(10856)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/296793.The_Snapper?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;L1gMSF/fHM5BthdgbOux2PqH85GGDvbQGxOoXEss3QSqiVHmZ8VmthTHMEWnuBFxxXUbheX16Zmh+AovKtjE1Q==&#39;)}); return false;\">5 stars <span class=\"greyText\">(2024)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/296793.The_Snapper?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;IPV4Hb3Zh/lTuO0Z3BhmBTmKH1IwTDc2MMDgBOw/JMWlJCWzhcP9gQbJyjwXS8asBnj3RlO3KH+KK0J3jcs9FA==&#39;)}); return false;\">4 stars <span class=\"greyText\">(3346)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=