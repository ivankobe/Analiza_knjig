        $('bookCover_1046404').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1046404').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
