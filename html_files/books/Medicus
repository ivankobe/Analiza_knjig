        $('bookCover_55619129').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_55619129').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
