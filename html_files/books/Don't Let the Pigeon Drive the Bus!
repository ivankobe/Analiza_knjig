      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_759611').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_759611').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_759611').observe('prototip:hidden', function () {
