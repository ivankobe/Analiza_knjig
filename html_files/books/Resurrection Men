<div id="review_2218909388" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2218909388" />
    <a title="Ellen" class="left imgcol" href="/user/show/5779782-ellen"><img alt="Ellen" src="https://images.gr-assets.com/users/1340229710p2/5779782.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2218909388?book_show_action=true">Dec 19, 2017</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Ellen" class="user" itemprop="url" name="Ellen" href="/user/show/5779782-ellen">Ellen</a>
