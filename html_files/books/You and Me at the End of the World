        $('bookCover_54238295').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_54238295').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
