        $('bookCover_21565595').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_21565595').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
