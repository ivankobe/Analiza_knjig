      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_23611595').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_23611595').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_23611595').observe('prototip:hidden', function () {
