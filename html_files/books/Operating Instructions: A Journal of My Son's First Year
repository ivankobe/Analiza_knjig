        $('bookCover_40121328').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_40121328').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
