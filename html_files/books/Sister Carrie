        $('bookCover_17728').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_17728').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
