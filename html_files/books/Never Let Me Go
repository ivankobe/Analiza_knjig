<div id="review_392384724" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/392384724" />
    <a title="Scarlet" class="left imgcol" href="/user/show/3839903-scarlet"><img alt="Scarlet" src="https://images.gr-assets.com/users/1556098025p2/3839903.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/392384724?book_show_action=true">Aug 15, 2012</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Scarlet" class="user" itemprop="url" name="Scarlet" href="/user/show/3839903-scarlet">Scarlet</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was amazing"><span size="15x15" class="staticStar p10">it was amazing</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/3839903-scarlet?shelf=booker-longlist-shortlist">booker-longlist-shortlist</a>, 
              <a class="actionLinkLite" href="/review/list/3839903-scarlet?shelf=classics">classics</a>, 
              <a class="actionLinkLite" href="/review/list/3839903-scarlet?shelf=lush-writing">lush-writing</a>, 
              <a class="actionLinkLite" href="/review/list/3839903-scarlet?shelf=5-starred-perfection">5-starred-perfection</a>, 
              <a class="actionLinkLite" href="/review/list/3839903-scarlet?shelf=dystopia">dystopia</a>, 
              <a class="actionLinkLite" href="/review/list/3839903-scarlet?shelf=books-i-own">books-i-own</a>, 
              <a class="actionLinkLite" href="/review/list/3839903-scarlet?shelf=uk">uk</a>, 
              <a class="actionLinkLite" href="/review/list/3839903-scarlet?shelf=made-me-cry">made-me-cry</a>, 
              <a class="actionLinkLite" href="/review/list/3839903-scarlet?shelf=most-loved">most-loved</a>, 
              <a class="actionLinkLite" href="/review/list/3839903-scarlet?shelf=favorite-author">favorite-author</a>
          </div>
