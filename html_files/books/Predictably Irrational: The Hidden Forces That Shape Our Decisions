<div id="review_3113173064" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/3113173064" />
    <a title="Janet Newport" class="left imgcol" href="/user/show/7765576-janet-newport"><img alt="Janet Newport" src="https://images.gr-assets.com/users/1335017758p2/7765576.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/3113173064?book_show_action=true">Jan 01, 2020</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Janet Newport" class="user" itemprop="url" name="Janet Newport" href="/user/show/7765576-janet-newport">Janet Newport</a>
