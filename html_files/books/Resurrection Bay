<div id="review_2063507421" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2063507421" />
    <a title="Andrea" class="left imgcol" href="/user/show/9582364-andrea"><img alt="Andrea" src="https://images.gr-assets.com/users/1608965527p2/9582364.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2063507421?book_show_action=true">Jan 15, 2020</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Andrea" class="user" itemprop="url" name="Andrea" href="/user/show/9582364-andrea">Andrea</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="really liked it"><span size="15x15" class="staticStar p10">really liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/9582364-andrea?shelf=challenge">challenge</a>, 
              <a class="actionLinkLite" href="/review/list/9582364-andrea?shelf=series">series</a>, 
              <a class="actionLinkLite" href="/review/list/9582364-andrea?shelf=australian-author">australian-author</a>, 
              <a class="actionLinkLite" href="/review/list/9582364-andrea?shelf=personal-challenge-2020">personal-challenge-2020</a>
          </div>
