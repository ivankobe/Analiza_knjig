        $('bookCover_278797').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_278797').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
