        $('bookCover_11908').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_11908').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
