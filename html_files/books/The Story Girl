        $('bookCover_3208020').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_3208020').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_3208020').observe('prototip:hidden', function () {
