        $('bookCover_49021976').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_49021976').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
