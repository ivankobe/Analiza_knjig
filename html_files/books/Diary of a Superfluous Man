        $('bookCover_279614').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_279614').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_279614').observe('prototip:hidden', function () {
