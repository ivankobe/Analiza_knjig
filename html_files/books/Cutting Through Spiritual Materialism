<div id="review_30121719" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/30121719" />
    <a title="Melmcbride" class="left imgcol" href="/user/show/1394724-melmcbride"><img alt="Melmcbride" src="https://images.gr-assets.com/users/1217865124p2/1394724.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/30121719?book_show_action=true">Aug 14, 2008</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Melmcbride" class="user" itemprop="url" name="Melmcbride" href="/user/show/1394724-melmcbride">Melmcbride</a>
