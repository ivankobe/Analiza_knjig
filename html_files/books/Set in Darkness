      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_184'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/69807.Set_in_Darkness?rating=\">all <span class=\"greyText\">(16812)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/69807.Set_in_Darkness?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;pqKV2k96/XH6OEhxAbugxZTwpxLI52luGRIi/f3351Rd7WDt2vYrrxxwvdGOaLxKB6y5Vl2Hpg1n1ezIU6jQOg==&#39;)}); return false;\">5 stars <span class=\"greyText\">(3796)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/69807.Set_in_Darkness?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;0TWXGqnGS7D6iSl6vckmunudBxDgTpwQKTywQuGUZc8qemItPEqdbhzB3NoyGjo16MEZVHUuU3NX+353T8tSoQ==&#39;)}); return false;\">4 stars <span class=\"greyText\">(5238)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <sp