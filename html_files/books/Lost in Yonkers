        $('bookCover_2437710').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_2437710').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
