      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_30013').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_30013').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_30013').observe('prototip:hidden', function () {
