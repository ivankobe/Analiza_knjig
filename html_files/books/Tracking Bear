        $('bookCover_25718904').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_25718904').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
