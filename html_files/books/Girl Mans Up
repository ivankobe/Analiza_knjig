        $('bookCover_13069935').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_13069935').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
