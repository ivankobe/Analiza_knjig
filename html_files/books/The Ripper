      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_434'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/11401915-the-ripper?rating=\">all <span class=\"greyText\">(51055)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/11401915-the-ripper?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;Vjl2C8dm+2WmEYR3u6Cl4OrXicQgf6y2Hl4PeeYYa1eVfcM9NQjoZrr3I1Vq70oax+Usvbqk5ObD3C6X12SEEA==&#39;)}); return false;\">5 stars <span class=\"greyText\">(18969)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/11401915-the-ripper?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;gD+WlvQj967R4XT8bdmAuEomeL8h+yFUBGFokC8OAptDeyOgBk3krc0H0968lm9CZxTdxrsgaQTZ40l+HnLt3A==&#39;)}); return false;\">4 stars <span class=\"greyText\">(6281)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span cl