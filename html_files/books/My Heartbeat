        $('bookCover_790289').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_790289').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
