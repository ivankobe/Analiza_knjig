        $('bookCover_542883').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_542883').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
