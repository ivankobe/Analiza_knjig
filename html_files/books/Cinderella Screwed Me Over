        $('bookCover_49631521').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_49631521').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
