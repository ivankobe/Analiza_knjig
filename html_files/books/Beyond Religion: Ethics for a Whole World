        $('bookCover_84259').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_84259').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
