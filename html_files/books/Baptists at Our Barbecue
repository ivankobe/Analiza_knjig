        $('bookCover_9917879').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_9917879').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
