        $('bookCover_50622390').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_50622390').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
