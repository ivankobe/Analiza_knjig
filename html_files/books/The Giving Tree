        $('bookCover_197084').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_197084').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
