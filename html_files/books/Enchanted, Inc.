        $('bookCover_3713038').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_3713038').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
