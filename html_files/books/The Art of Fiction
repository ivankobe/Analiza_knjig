        $('bookCover_263341').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_263341').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
