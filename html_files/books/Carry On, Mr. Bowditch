        $('bookCover_79667').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_79667').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
