        $('bookCover_45978').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_45978').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_45978').observe('prototip:hidden', function () {
