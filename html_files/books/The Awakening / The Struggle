        $('bookCover_9303577').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_9303577').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
