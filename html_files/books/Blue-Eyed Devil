        $('bookCover_373606').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_373606').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
