<div id="review_2504234079" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2504234079" />
    <a title="Rachel Drenning" class="left imgcol" href="/user/show/16291061-rachel-drenning"><img alt="Rachel Drenning" src="https://images.gr-assets.com/users/1605639004p2/16291061.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2504234079?book_show_action=true">Aug 27, 2018</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Rachel Drenning" class="user" itemprop="url" name="Rachel Drenning" href="/user/show/16291061-rachel-drenning">Rachel Drenning</a>
