        $('bookCover_609454').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_609454').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_609454').observe('prototip:hidden', function () {
