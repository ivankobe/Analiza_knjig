<div id="review_730191000" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/730191000" />
    <a title="Helen" class="left imgcol" href="/user/show/8104024-helen"><img alt="Helen" src="https://images.gr-assets.com/users/1463460801p2/8104024.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/730191000?book_show_action=true">Sep 28, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Helen" class="user" itemprop="url" name="Helen" href="/user/show/8104024-helen">Helen</a>
