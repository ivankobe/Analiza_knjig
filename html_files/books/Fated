        $('bookCover_40011468').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_40011468').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
