        $('bookCover_57369118').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_57369118').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
