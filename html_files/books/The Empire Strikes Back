        $('bookCover_77236').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_77236').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
