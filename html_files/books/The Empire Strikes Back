        $('bookCover_52306589').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_52306589').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
