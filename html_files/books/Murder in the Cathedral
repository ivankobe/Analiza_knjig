        $('bookCover_55120306').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_55120306').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
