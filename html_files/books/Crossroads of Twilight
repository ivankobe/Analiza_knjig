        $('bookCover_55399').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_55399').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_55399').observe('prototip:hidden', function () {
