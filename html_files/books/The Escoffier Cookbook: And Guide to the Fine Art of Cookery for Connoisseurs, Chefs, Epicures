        $('bookCover_54125257').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_54125257').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_54125257').observe('prototip:hidden', function () {
