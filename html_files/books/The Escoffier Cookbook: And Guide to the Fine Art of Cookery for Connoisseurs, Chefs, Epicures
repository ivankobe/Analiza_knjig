        $('bookCover_815612').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_815612').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
