        $('bookCover_2191181').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_2191181').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_2191181').observe('prototip:hidden', function () {
