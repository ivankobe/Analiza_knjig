        $('bookCover_175599').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_175599').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
