<div id="review_3341405815" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/3341405815" />
    <a title="Hazel" class="left imgcol" href="/user/show/82007298-hazel"><img alt="Hazel" src="https://images.gr-assets.com/users/1625581046p2/82007298.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/3341405815?book_show_action=true">May 27, 2020</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Hazel" class="user" itemprop="url" name="Hazel" href="/user/show/82007298-hazel">Hazel</a>
