        $('bookCover_45007887').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_45007887').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
