            Shelves:
              <a class="actionLinkLite" href="/review/list/198122-kati?shelf=2008">2008</a>, 
              <a class="actionLinkLite" href="/review/list/198122-kati?shelf=work-translations">work-translations</a>, 
              <a class="actionLinkLite" href="/review/list/198122-kati?shelf=in-english">in-english</a>, 
              <a class="actionLinkLite" href="/review/list/198122-kati?shelf=2010">2010</a>, 
