        $('bookCover_57959667').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_57959667').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
