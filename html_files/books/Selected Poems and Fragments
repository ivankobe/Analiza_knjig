        $('bookCover_297226').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_297226').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
