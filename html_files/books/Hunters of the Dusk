        $('bookCover_8962').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_8962').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
