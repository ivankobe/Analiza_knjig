        $('bookCover_56948801').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_56948801').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
