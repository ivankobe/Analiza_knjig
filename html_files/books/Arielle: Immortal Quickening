        $('bookCover_22529375').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_22529375').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
