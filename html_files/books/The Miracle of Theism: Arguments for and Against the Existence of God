  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2254676098?book_show_action=true">Jan 12, 2018</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Icanclick" class="user" itemprop="url" name="Icanclick" href="/user/show/76365991-icanclick">Icanclick</a>
