    <div class="elementListBrownSeparator"></div>



  <div class="friendReviews elementListBrown" >
    <div class="section">
      

<div id="review_2158841218" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2158841218" />
    <a title="Kate" class="left imgcol" href="/user/show/29383287-kate"><img alt="Kate" src="https://images.gr-assets.com/users/1579560971p2/29383287.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
