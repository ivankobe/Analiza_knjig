        $('bookCover_55817471').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_55817471').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_55817471').observe('prototip:hidden', function () {
