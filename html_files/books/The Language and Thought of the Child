        $('bookCover_100098').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_100098').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
