        $('bookCover_8300711').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_8300711').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
