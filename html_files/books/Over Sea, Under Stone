      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_24782').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_24782').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_24782').observe('prototip:hidden', function () {
