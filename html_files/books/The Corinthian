        $('bookCover_55940551').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_55940551').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_55940551').observe('prototip:hidden', function () {
