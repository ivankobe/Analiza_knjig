        $('bookCover_50525921').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_50525921').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
