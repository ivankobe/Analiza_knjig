        $('bookCover_50728266').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_50728266').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
