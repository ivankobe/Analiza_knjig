        $('bookCover_88459').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_88459').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
