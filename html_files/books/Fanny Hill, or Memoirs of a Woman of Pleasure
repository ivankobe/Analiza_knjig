      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_784'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/195144.Fanny_Hill_or_Memoirs_of_a_Woman_of_Pleasure?rating=\">all <span class=\"greyText\">(28541)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/195144.Fanny_Hill_or_Memoirs_of_a_Woman_of_Pleasure?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;RjavRPm64audlWu17/uahlfUcAy3ZqzmyrqtvSk17oXovM54aFdMAddnBatIxFYzu2+hi32QzMgB5Iiy7Vc/kA==&#39;)}); return false;\">5 stars <span class=\"greyText\">(1660)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/195144.Fanny_Hill_or_Memoirs_of_a_Woman_of_Pleasure?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;zSzthRXIAg3O691Y+Po/Fq3XjRJuHMOOXEuUKHcZf91jpoy5hCWvp4QZs0ZfxfOjQWxclaTqo6CXFbEns3uuyA==&#39;)}); return false;\">4 stars <span class=\"greyText\">(3016)<\/span