        $('bookCover_13538900').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_13538900').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
