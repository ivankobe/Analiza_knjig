<div id="review_2433264199" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2433264199" />
    <a title="Ailsa" class="left imgcol" href="/user/show/6117156-ailsa"><img alt="Ailsa" src="https://images.gr-assets.com/users/1442286271p2/6117156.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2433264199?book_show_action=true">Jun 26, 2018</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Ailsa" class="user" itemprop="url" name="Ailsa" href="/user/show/6117156-ailsa">Ailsa</a>
