      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_251'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/74406.Fifth_Business?rating=\">all <span class=\"greyText\">(35634)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/74406.Fifth_Business?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;owCRKIYkL7t+F9YC87Cmfabx1OmV3ITu1B4oo/czeGNgf3PtzFzhl410rOgmJaZUagx7ROgt1Hbf+JqaMtZ43Q==&#39;)}); return false;\">5 stars <span class=\"greyText\">(6730)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/74406.Fifth_Business?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;u+Il2ZnvFsUBYQex68DwnF5I7+Z2A4iXpi1PqT6+Ltp4nccc05fY6fICfVs+VfC1krVASwvy2A+ty/2Q+1suZA==&#39;)}); return false;\">4 stars <span class=\"greyText\">(6471)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span 