      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_679'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/425100.Dance_Hall_of_the_Dead?rating=\">all <span class=\"greyText\">(18369)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/425100.Dance_Hall_of_the_Dead?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;JiuNh5GrjCT3RGS1Fg4yQqhP9X/bmoBxlC7dSuyHbT20TBhgON8igivdwylB0w9lV4OvbFwoukyN7PBwbs/uSg==&#39;)}); return false;\">5 stars <span class=\"greyText\">(3639)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/425100.Dance_Hall_of_the_Dead?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;1hu9K7lD/R49bkZnR50O09dAGb6eVSrJMKEr07u8hjdEfCjMEDdTuOH34fsQQDP0KIxDrRnnEPQpYwbpOfQFQA==&#39;)}); return false;\">4 stars <span class=\"greyText\">(5391)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span