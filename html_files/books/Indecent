<div id="review_2111858210" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2111858210" />
    <a title="Theresa" class="left imgcol" href="/user/show/38029020-theresa"><img alt="Theresa" src="https://images.gr-assets.com/users/1514840229p2/38029020.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2111858210?book_show_action=true">Oct 13, 2017</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Theresa" class="user" itemprop="url" name="Theresa" href="/user/show/38029020-theresa">Theresa</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="really liked it"><span size="15x15" class="staticStar p10">really liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/38029020-theresa?shelf=giveaways">giveaways</a>, 
              <a class="actionLinkLite" href="/review/list/38029020-theresa?shelf=my-favorite-books">my-favorite-books</a>, 
              <a class="actionLinkLite" href="/review/list/38029020-theresa?shelf=my-favorite-books-of-2017">my-favorite-books-of-2017</a>, 
              <a class="actionLinkLite" href="/review/list/38029020-theresa?shelf=mental-illness">mental-illness</a>
          </div>
