        $('bookCover_52736046').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_52736046').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
