        $('bookCover_857528').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_857528').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
