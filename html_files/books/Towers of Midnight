        $('bookCover_1268479').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1268479').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
