      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_34002132').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_34002132').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_34002132').observe('prototip:hidden', function () {
