      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_967'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/35167685-surely-you-re-joking-mr-feynman?rating=\">all <span class=\"greyText\">(384425)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/35167685-surely-you-re-joking-mr-feynman?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;CP2fhIxGdkjCqaDfP966OO9irCQuZg1gZES9Od2zG/rQPIE2QeyRdIz66g/iFDRunilwFj0GzinqJUOZQW4LXg==&#39;)}); return false;\">5 stars <span class=\"greyText\">(90822)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/35167685-surely-you-re-joking-mr-feynman?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;J0XGJc8cPBFNV6lT+THkkvNj6wY43hDfxTp6WPe2Tn//hNiXArbbLQME44Mk+2rEgig3NCu+05ZLW4T4a2te2w==&#39;)}); return false;\">4 stars <span class=\"greyText\">(52187)<\/span><\/a>\n                  <spa