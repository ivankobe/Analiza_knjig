        $('bookCover_120231').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_120231').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
