      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_999159').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_999159').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_999159').observe('prototip:hidden', function () {
