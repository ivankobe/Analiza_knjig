        $('bookCover_176874').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_176874').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
