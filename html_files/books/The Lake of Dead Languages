        $('bookCover_11377310').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_11377310').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
