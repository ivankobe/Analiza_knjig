        $('bookCover_13069261').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_13069261').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
