<div id="review_1306550140" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1306550140" />
    <a title="Claire" class="left imgcol" href="/user/show/7191275-claire"><img alt="Claire" src="https://images.gr-assets.com/users/1361416826p2/7191275.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1306550140?book_show_action=true">Jan 07, 2020</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Claire" class="user" itemprop="url" name="Claire" href="/user/show/7191275-claire">Claire</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="really liked it"><span size="15x15" class="staticStar p10">really liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/7191275-claire?shelf=scifi-fantasy">scifi-fantasy</a>, 
              <a class="actionLinkLite" href="/review/list/7191275-claire?shelf=unicorns">unicorns</a>, 
              <a class="actionLinkLite" href="/review/list/7191275-claire?shelf=beautiful-book-covers">beautiful-book-covers</a>, 
              <a class="actionLinkLite" href="/review/list/7191275-claire?shelf=awesome-illustrations">awesome-illustrations</a>, 
              <a class="actionLinkLite" href="/review/list/7191275-claire?shelf=compelling-female-characters">compelling-female-characters</a>, 
              <a class="actionLinkLite" href="/review/list/7191275-claire?shelf=young-adult">young-adult</a>, 
              <a class="actionLinkLite" href="/review/list/7191275-claire?shelf=heroic-fantasy">heroic-fantasy</a>, 
              <a class="actionLinkLite" href="/review/list/7191275-claire?shelf=fantastic-world-building">fantastic-world-building</a>, 
              <a class="actionLinkLite" href="/review/list/7191275-claire?shelf=2020-reading-challenge">2020-reading-challenge</a>, 
