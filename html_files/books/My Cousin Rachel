    <div class="js-dataTooltip" data-use-wtr-tooltip='true'>
        <div class="js-tooltipTrigger tooltipTrigger" data-resource-type='Book' data-resource-id='17899948' data-placement='bottom-left'>
          <a href="/book/show/17899948-rebecca"><img id="more_book_17899948" alt="Rebecca" title="Rebecca" width="50" src="https://i.gr-assets.com/images/S/compressed.photo.goodreads.com/books/1386605169l/17899948._SX98_.jpg" /></a>
        </div>
        <div class="js-tooltipTrigger tooltipTrigger" data-resource-type='Book' data-resource-id='18869967' data-placement='bottom-left'>
          <a href="/book/show/18869967-jamaica-inn"><img id="more_book_18869967" alt="Jamaica Inn" title="Jamaica Inn" width="50" src="https://i.gr-assets.com/images/S/compressed.photo.goodreads.com/books/1386605082l/18869967._SX98_.jpg" /></a>
