      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_319810').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_319810').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_319810').observe('prototip:hidden', function () {
