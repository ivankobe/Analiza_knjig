          $$('div.prototip').each(function (e) {
            if ($('bookCover_1913655').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_1913655').observe('prototip:hidden', function () {
