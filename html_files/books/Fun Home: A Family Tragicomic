        $('bookCover_991197').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_991197').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
