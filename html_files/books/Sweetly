<div id="review_522453581" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/522453581" />
    <a title="jv poore" class="left imgcol" href="/user/show/5157918-jv-poore"><img alt="jv poore" src="https://images.gr-assets.com/users/1570108420p2/5157918.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/522453581?book_show_action=true">Jan 31, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="jv poore" class="user" itemprop="url" name="jv poore" href="/user/show/5157918-jv-poore">jv poore</a>
