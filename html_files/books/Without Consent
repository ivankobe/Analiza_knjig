<div id="review_1863250737" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1863250737" />
    <a title="Chava" class="left imgcol" href="/user/show/20295484-chava"><img alt="Chava" src="https://images.gr-assets.com/users/1368404883p2/20295484.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1863250737?book_show_action=true">Jan 29, 2017</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Chava" class="user" itemprop="url" name="Chava" href="/user/show/20295484-chava">Chava</a>
