<div id="review_972467247" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/972467247" />
    <a title="Jülie ☼♄ " class="left imgcol" href="/user/show/20814018-j-lie"><img alt="Jülie ☼♄ " src="https://images.gr-assets.com/users/1483253921p2/20814018.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/972467247?book_show_action=true">Jun 19, 2014</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Jülie ☼♄ " class="user" itemprop="url" name="Jülie ☼♄ " href="/user/show/20814018-j-lie">Jülie ☼♄ </a>
