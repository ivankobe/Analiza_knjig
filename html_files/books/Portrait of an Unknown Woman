        $('bookCover_55505679').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_55505679').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
