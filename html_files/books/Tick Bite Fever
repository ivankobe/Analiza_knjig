        $('bookCover_8099187').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_8099187').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
