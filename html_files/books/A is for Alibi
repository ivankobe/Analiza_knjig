        $('bookCover_7061').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_7061').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
