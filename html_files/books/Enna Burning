        $('bookCover_31428970').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_31428970').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
