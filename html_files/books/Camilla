<div id="review_3362997156" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/3362997156" />
    <a title="George" class="left imgcol" href="/user/show/10168276-george"><img alt="George" src="https://images.gr-assets.com/users/1511867378p2/10168276.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/3362997156?book_show_action=true">Jun 01, 2020</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="George" class="user" itemprop="url" name="George" href="/user/show/10168276-george">George</a>
