        $('bookCover_549488').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_549488').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
