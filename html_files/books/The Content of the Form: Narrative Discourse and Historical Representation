        $('bookCover_52076947').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_52076947').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
