        $('bookCover_53406779').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_53406779').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
