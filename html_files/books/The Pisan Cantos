        $('bookCover_587924').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_587924').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
