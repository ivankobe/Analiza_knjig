        $('bookCover_108615').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_108615').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
