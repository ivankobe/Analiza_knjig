          $$('div.prototip').each(function (e) {
            if ($('bookCover_22823333').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_22823333').observe('prototip:hidden', function () {
