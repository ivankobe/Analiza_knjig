        $('bookCover_28381').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_28381').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
