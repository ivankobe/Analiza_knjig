        $('bookCover_45857086').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_45857086').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
