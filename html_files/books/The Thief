      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Lara" class="user" itemprop="url" name="Lara" href="/user/show/309689-lara">Lara</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="did not like it"><span size="15x15" class="staticStar p10">did not like it</span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>

          <span class="greyText">&nbsp;&middot;&nbsp;</span>
          <a class="lightGreyText" title="Paperback 9780140388343" href="/book/show/1082753.The_Thief">review of another edition</a>


        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/309689-lara?shelf=abandoned">abandoned</a>, 
              <a class="actionLinkLite" href="/review/list/309689-lara?shelf=action-adventure">action-adventure</a>, 
              <a class="actionLinkLite" href="/review/list/309689-lara?shelf=fantasy">fantasy</a>, 
