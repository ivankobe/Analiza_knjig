        $('bookCover_101411').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_101411').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
