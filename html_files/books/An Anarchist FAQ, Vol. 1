      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_90416').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_90416').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_90416').observe('prototip:hidden', function () {
