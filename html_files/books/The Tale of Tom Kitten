        $('bookCover_19321').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_19321').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
