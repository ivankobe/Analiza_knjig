        $('bookCover_56049509').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_56049509').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
