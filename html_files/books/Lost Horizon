        $('bookCover_116239').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_116239').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
