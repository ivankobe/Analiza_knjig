        $('bookCover_341396').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_341396').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
