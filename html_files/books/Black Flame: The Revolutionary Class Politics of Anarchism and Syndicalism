        $('bookCover_22238144').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_22238144').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
