        $('bookCover_7816264').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_7816264').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
