        $('bookCover_57079493').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_57079493').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
