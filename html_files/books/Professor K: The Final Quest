      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_55381157').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_55381157').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_55381157').observe('prototip:hidden', function () {
