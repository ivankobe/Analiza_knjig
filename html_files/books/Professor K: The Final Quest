      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('edition_56755342').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('edition_56755342').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('edition_56755342').observe('prototip:hidden', function () {
