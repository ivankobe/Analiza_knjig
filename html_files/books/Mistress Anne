<div id="review_527939820" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/527939820" />
    <a title="Helene Harrison" class="left imgcol" href="/user/show/8138275-helene-harrison"><img alt="Helene Harrison" src="https://images.gr-assets.com/users/1431630162p2/8138275.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/527939820?book_show_action=true">Feb 06, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Helene Harrison" class="user" itemprop="url" name="Helene Harrison" href="/user/show/8138275-helene-harrison">Helene Harrison</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="liked it"><span size="15x15" class="staticStar p10">liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/8138275-helene-harrison?shelf=tudors">tudors</a>, 
              <a class="actionLinkLite" href="/review/list/8138275-helene-harrison?shelf=format-paperback">format-paperback</a>, 
              <a class="actionLinkLite" href="/review/list/8138275-helene-harrison?shelf=2013-challenge">2013-challenge</a>, 
              <a class="actionLinkLite" href="/review/list/8138275-helene-harrison?shelf=non-fiction">non-fiction</a>, 
              <a class="actionLinkLite" href="/review/list/8138275-helene-harrison?shelf=stars-3">stars-3</a>, 
