<div id="review_3057445641" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/3057445641" />
    <a title="Jess☺️" class="left imgcol" href="/user/show/80941120-jess"><img alt="Jess☺️" src="https://images.gr-assets.com/users/1609580893p2/80941120.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/3057445641?book_show_action=true">Dec 30, 2019</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Jess☺️" class="user" itemprop="url" name="Jess☺️" href="/user/show/80941120-jess">Jess☺️</a>
