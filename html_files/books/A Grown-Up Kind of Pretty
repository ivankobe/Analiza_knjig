      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_53138081').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_53138081').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_53138081').observe('prototip:hidden', function () {
