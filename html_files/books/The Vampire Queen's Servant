      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('edition_49623795').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('edition_49623795').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('edition_49623795').observe('prototip:hidden', function () {
