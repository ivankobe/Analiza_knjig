    <div class="js-dataTooltip" data-use-wtr-tooltip='true'>
        <div class="js-tooltipTrigger tooltipTrigger" data-resource-type='Book' data-resource-id='23772' data-placement='bottom-left'>
          <a href="/book/show/23772.Green_Eggs_and_Ham"><img id="more_book_23772" alt="Green Eggs and Ham" title="Green Eggs and Ham" width="50" src="https://i.gr-assets.com/images/S/compressed.photo.goodreads.com/books/1468680100l/23772._SX98_.jpg" /></a>
