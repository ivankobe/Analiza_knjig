<div id="review_106433325" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/106433325" />
    <a title="Michael" class="left imgcol" href="/user/show/3403278-michael"><img alt="Michael" src="https://images.gr-assets.com/users/1604899399p2/3403278.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/106433325?book_show_action=true">Mar 07, 2017</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Michael" class="user" itemprop="url" name="Michael" href="/user/show/3403278-michael">Michael</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was amazing"><span size="15x15" class="staticStar p10">it was amazing</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span></span>

          <span class="greyText">&nbsp;&middot;&nbsp;</span>
          <a class="lightGreyText" title="Paperback 9780300136371" href="/book/show/4858960-the-republic">review of another edition</a>


        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/3403278-michael?shelf=aa-greeklit">aa-greeklit</a>, 
              <a class="actionLinkLite" href="/review/list/3403278-michael?shelf=aa-utopia">aa-utopia</a>, 
              <a class="actionLinkLite" href="/review/list/3403278-michael?shelf=fantasy">fantasy</a>, 
              <a class="actionLinkLite" href="/review/list/3403278-michael?shelf=translation">translation</a>, 
              <a class="actionLinkLite" href="/review/list/3403278-michael?shelf=nonfiction">nonfiction</a>, 
