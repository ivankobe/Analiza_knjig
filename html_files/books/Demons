<div id="review_960707157" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/960707157" />
    <a title="Londi" class="left imgcol" href="/user/show/32076715-londi"><img alt="Londi" src="https://images.gr-assets.com/users/1621516303p2/32076715.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/960707157?book_show_action=true">Aug 19, 2017</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Londi" class="user" itemprop="url" name="Londi" href="/user/show/32076715-londi">Londi</a>
