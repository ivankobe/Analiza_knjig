      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Luffy" class="user" itemprop="url" name="Luffy" href="/user/show/7407364-luffy">Luffy</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was amazing"><span size="15x15" class="staticStar p10">it was amazing</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/7407364-luffy?shelf=5-star">5-star</a>, 
              <a class="actionLinkLite" href="/review/list/7407364-luffy?shelf=children">children</a>, 
              <a class="actionLinkLite" href="/review/list/7407364-luffy?shelf=page-turner">page-turner</a>, 
              <a class="actionLinkLite" href="/review/list/7407364-luffy?shelf=popular">popular</a>, 
