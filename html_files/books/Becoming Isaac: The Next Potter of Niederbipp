        $('bookCover_51330171').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_51330171').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
