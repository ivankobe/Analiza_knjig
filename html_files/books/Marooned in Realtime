        $('bookCover_53205854').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_53205854').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
