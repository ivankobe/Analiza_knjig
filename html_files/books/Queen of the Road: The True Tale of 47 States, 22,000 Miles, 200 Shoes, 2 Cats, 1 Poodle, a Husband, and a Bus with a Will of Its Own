        $('bookCover_37833491').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_37833491').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
