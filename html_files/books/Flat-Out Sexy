        $('bookCover_22607787').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_22607787').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
