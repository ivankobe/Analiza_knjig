        $('bookCover_57233112').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_57233112').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
