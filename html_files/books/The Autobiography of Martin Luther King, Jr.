      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Abbie" class="user" itemprop="url" name="Abbie" href="/user/show/8633580-abbie">Abbie</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was amazing"><span size="15x15" class="staticStar p10">it was amazing</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/8633580-abbie?shelf=nonfiction">nonfiction</a>, 
              <a class="actionLinkLite" href="/review/list/8633580-abbie?shelf=favorites">favorites</a>, 
              <a class="actionLinkLite" href="/review/list/8633580-abbie?shelf=spiritual">spiritual</a>, 
              <a class="actionLinkLite" href="/review/list/8633580-abbie?shelf=5-star">5-star</a>
          </div>
