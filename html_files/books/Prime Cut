          $$('div.prototip').each(function (e) {
            if ($('bookCover_526827').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_526827').observe('prototip:hidden', function () {
