        $('bookCover_778310').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_778310').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
