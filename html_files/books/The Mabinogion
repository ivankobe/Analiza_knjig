        $('bookCover_700460').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_700460').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
