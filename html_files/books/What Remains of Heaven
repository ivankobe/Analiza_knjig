        $('bookCover_50255728').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_50255728').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
