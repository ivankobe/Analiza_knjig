        $('bookCover_82676').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_82676').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
