      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('edition_1844078').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('edition_1844078').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('edition_1844078').observe('prototip:hidden', function () {
