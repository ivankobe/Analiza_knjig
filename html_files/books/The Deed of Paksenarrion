        $('bookCover_202087').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_202087').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
