      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_9466032').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_9466032').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_9466032').observe('prototip:hidden', function () {
