          $$('div.prototip').each(function (e) {
            if ($('bookCover_7114761').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_7114761').observe('prototip:hidden', function () {
