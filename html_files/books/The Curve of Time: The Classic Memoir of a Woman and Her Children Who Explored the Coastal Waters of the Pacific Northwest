  <div class="friendReviews elementListBrown" >
    <div class="section">
      

<div id="review_200150407" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/200150407" />
    <a title="Eve" class="left imgcol" href="/user/show/1238236-eve"><img alt="Eve" src="https://s.gr-assets.com/assets/nophoto/user/f_50x66-6a03a5c12233c941481992b82eea8d23.png" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
