          $$('div.prototip').each(function (e) {
            if ($('bookCover_52198').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_52198').observe('prototip:hidden', function () {
