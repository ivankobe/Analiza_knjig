  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/3100314270?book_show_action=true">Jan 19, 2020</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Always Pouting" class="user" itemprop="url" name="Always Pouting" href="/user/show/48650982-always-pouting">Always Pouting</a>
