  <div class="friendReviews elementListBrown" >
    <div class="section">
      

<div id="review_101271919" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/101271919" />
    <a title="Chelsea Szendi" class="left imgcol" href="/user/show/3675222-chelsea-szendi"><img alt="Chelsea Szendi" src="https://images.gr-assets.com/users/1333460966p2/3675222.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/101271919?book_show_action=true">May 04, 2010</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Chelsea Szendi" class="user" itemprop="url" name="Chelsea Szendi" href="/user/show/3675222-chelsea-szendi">Chelsea Szendi</a>
