      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_9067850').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_9067850').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_9067850').observe('prototip:hidden', function () {
