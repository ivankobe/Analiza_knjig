        $('bookCover_37586').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_37586').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
