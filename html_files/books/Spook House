        $('bookCover_57726367').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_57726367').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
