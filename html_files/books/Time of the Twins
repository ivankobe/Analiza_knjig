        $('bookCover_534499').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_534499').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
