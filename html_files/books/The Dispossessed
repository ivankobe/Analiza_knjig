      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_76171').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_76171').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_76171').observe('prototip:hidden', function () {
