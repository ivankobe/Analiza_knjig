      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_1147020').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1147020').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_1147020').observe('prototip:hidden', function () {
