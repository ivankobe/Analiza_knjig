        $('bookCover_1430122').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1430122').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
