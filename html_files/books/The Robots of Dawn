      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_35816').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_35816').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_35816').observe('prototip:hidden', function () {
