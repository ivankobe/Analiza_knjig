<div id="review_2271502968" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2271502968" />
    <a title="Ce" class="left imgcol" href="/user/show/22284360-ce"><img alt="Ce" src="https://images.gr-assets.com/users/1420259184p2/22284360.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2271502968?book_show_action=true">Mar 11, 2018</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Ce" class="user" itemprop="url" name="Ce" href="/user/show/22284360-ce">Ce</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was ok"><span size="15x15" class="staticStar p10">it was ok</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/22284360-ce?shelf=science-fiction">science-fiction</a>, 
              <a class="actionLinkLite" href="/review/list/22284360-ce?shelf=read-in-english">read-in-english</a>, 
              <a class="actionLinkLite" href="/review/list/22284360-ce?shelf=space-opera">space-opera</a>, 
              <a class="actionLinkLite" href="/review/list/22284360-ce?shelf=series-foundation">series-foundation</a>, 
