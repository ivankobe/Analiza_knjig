<div id="review_520744265" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/520744265" />
    <a title="Luis Capelo" class="left imgcol" href="/user/show/5942630-luis-capelo"><img alt="Luis Capelo" src="https://images.gr-assets.com/users/1414762303p2/5942630.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/520744265?book_show_action=true">Jan 29, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Luis Capelo" class="user" itemprop="url" name="Luis Capelo" href="/user/show/5942630-luis-capelo">Luis Capelo</a>
