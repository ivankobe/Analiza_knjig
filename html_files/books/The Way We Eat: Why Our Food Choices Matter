        $('bookCover_315425').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_315425').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
