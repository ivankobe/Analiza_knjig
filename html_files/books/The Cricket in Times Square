        $('bookCover_766917').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_766917').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
