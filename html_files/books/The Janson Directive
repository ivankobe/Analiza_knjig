        $('bookCover_25632938').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_25632938').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
