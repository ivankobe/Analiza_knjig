        $('bookCover_53515772').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_53515772').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
