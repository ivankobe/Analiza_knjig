        $('bookCover_3242424').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_3242424').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
