        $('bookCover_39088477').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_39088477').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
