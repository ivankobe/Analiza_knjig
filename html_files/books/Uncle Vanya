        $('bookCover_142262').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_142262').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
