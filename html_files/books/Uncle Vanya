        $('bookCover_292275').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_292275').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_292275').observe('prototip:hidden', function () {
