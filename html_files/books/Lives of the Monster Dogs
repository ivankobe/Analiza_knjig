        $('bookCover_156925').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_156925').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
