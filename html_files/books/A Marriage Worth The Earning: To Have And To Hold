        $('bookCover_58258024').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_58258024').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
