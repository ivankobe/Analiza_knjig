        $('bookCover_45454139').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_45454139').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
