<div id="review_2012908927" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2012908927" />
    <a title="may ➹" class="left imgcol" href="/user/show/68157476-may"><img alt="may ➹" src="https://images.gr-assets.com/users/1538949856p2/68157476.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2012908927?book_show_action=true">Oct 20, 2017</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="may ➹" class="user" itemprop="url" name="may ➹" href="/user/show/68157476-may">may ➹</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="did not like it"><span size="15x15" class="staticStar p10">did not like it</span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/68157476-may?shelf=young-adult">young-adult</a>, 
              <a class="actionLinkLite" href="/review/list/68157476-may?shelf=romance">romance</a>, 
              <a class="actionLinkLite" href="/review/list/68157476-may?shelf=buddy-reads">buddy-reads</a>, 
              <a class="actionLinkLite" href="/review/list/68157476-may?shelf=library">library</a>, 
              <a class="actionLinkLite" href="/review/list/68157476-may?shelf=pov-female">pov-female</a>, 
              <a class="actionLinkLite" href="/review/list/68157476-may?shelf=boring">boring</a>, 
              <a class="actionLinkLite" href="/review/list/68157476-may?shelf=unpopular-opinion">unpopular-opinion</a>, 
              <a class="actionLinkLite" href="/review/list/68157476-may?shelf=read-2017">read-2017</a>, 
