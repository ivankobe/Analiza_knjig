      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_102382').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_102382').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_102382').observe('prototip:hidden', function () {
