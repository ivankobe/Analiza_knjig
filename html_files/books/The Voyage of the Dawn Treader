        $('bookCover_28186').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_28186').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_28186').observe('prototip:hidden', function () {
