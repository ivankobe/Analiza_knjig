        $('bookCover_52358025').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_52358025').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
