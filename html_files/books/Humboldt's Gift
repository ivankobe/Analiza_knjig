        $('bookCover_51199459').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_51199459').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
