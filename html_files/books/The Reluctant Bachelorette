        $('bookCover_50198730').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_50198730').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
