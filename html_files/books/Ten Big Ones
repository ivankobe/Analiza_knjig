      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_20338359').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_20338359').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_20338359').observe('prototip:hidden', function () {
