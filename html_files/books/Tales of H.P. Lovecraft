        $('bookCover_25773165').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_25773165').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
