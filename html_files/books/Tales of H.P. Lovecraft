      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_67930').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_67930').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_67930').observe('prototip:hidden', function () {
