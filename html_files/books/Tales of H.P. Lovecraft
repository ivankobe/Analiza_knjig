      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_40934733').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_40934733').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_40934733').observe('prototip:hidden', function () {
