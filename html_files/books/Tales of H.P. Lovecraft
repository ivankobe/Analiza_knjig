<div id="review_2968765828" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2968765828" />
    <a title="Amanda Hupe" class="left imgcol" href="/user/show/11658047-amanda-hupe"><img alt="Amanda Hupe" src="https://images.gr-assets.com/users/1535036536p2/11658047.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2968765828?book_show_action=true">Sep 07, 2019</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Amanda Hupe" class="user" itemprop="url" name="Amanda Hupe" href="/user/show/11658047-amanda-hupe">Amanda Hupe</a>
