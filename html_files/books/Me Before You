        $('bookCover_33648131').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_33648131').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
