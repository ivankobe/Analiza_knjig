        $('bookCover_264068').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_264068').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
