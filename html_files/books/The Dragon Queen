        $('bookCover_30046401').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_30046401').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
