<div id="review_395912108" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/395912108" />
    <a title="Jokoloyo" class="left imgcol" href="/user/show/1902630-jokoloyo"><img alt="Jokoloyo" src="https://images.gr-assets.com/users/1423473158p2/1902630.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/395912108?book_show_action=true">Aug 19, 2012</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Jokoloyo" class="user" itemprop="url" name="Jokoloyo" href="/user/show/1902630-jokoloyo">Jokoloyo</a>
