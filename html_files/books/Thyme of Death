        $('bookCover_107047').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_107047').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
