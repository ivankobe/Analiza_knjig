        $('bookCover_49119506').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_49119506').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
