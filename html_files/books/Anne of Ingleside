        }
      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_57546414').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_57546414').hasClassName('ignored')) {
