        $('bookCover_6598280').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_6598280').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
