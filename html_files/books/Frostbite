    <div class="elementListBrownSeparator"></div>



  <div class="friendReviews elementListBrown" >
    <div class="section">
      

<div id="review_1134562411" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1134562411" />
    <a title="Pearl Angeli" class="left imgcol" href="/user/show/31514572-pearl-angeli"><img alt="Pearl Angeli" src="https://images.gr-assets.com/users/1588420465p2/31514572.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1134562411?book_show_action=true">Dec 17, 2014</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Pearl Angeli" class="user" itemprop="url" name="Pearl Angeli" href="/user/show/31514572-pearl-angeli">Pearl Angeli</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was amazing"><span size="15x15" class="staticStar p10">it was amazing</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/31514572-pearl-angeli?shelf=2015-reads">2015-reads</a>, 
              <a class="actionLinkLite" href="/review/list/31514572-pearl-angeli?shelf=vampires">vampires</a>, 
              <a class="actionLinkLite" href="/review/list/31514572-pearl-angeli?shelf=owned-physical-copy">owned-physical-copy</a>, 
              <a class="actionLinkLite" href="/review/list/31514572-pearl-angeli?shelf=fantasy">fantasy</a>
