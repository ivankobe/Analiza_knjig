        $('bookCover_22926485').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_22926485').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
