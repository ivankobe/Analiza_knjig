      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_197'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/3863861-just-the-sexiest-man-alive?rating=\">all <span class=\"greyText\">(93501)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/3863861-just-the-sexiest-man-alive?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;zLTMiPK7pdJb9345Dq1ebpX+Z4IO+KLlBKEHkAbVpggRfVznOIEdWIjHFmviLqbSUvwgtkbFqsxx2QDvaPmWrw==&#39;)}); return false;\">5 stars <span class=\"greyText\">(14504)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/3863861-just-the-sexiest-man-alive?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;MF4bL/RoURA6PmtVp1754waTgs515tw5UYIUIVbz7Eztl4tAPlLpmukOAwdL3QFfwZHF+j3b1BAk+hNeON/c6w==&#39;)}); return false;\">4 stars <span class=\"greyText\">(15931)<\/span><\/a>\n                  <span class=\"loadingAn