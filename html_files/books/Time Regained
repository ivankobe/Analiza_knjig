        $('bookCover_2183').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_2183').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
