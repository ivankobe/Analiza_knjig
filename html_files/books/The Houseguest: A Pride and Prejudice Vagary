        $('bookCover_55692222').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_55692222').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
