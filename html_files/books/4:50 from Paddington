        $('bookCover_36671154').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_36671154').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
