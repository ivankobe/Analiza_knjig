        $('bookCover_27212409').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_27212409').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
