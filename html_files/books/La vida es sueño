        $('bookCover_14762208').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_14762208').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
