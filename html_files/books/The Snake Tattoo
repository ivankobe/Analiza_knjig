        $('bookCover_529266').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_529266').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
