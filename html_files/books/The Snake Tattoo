        $('bookCover_48576726').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_48576726').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
