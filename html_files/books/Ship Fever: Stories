        $('bookCover_7690728').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_7690728').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
