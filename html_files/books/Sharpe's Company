        $('bookCover_328907').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_328907').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
