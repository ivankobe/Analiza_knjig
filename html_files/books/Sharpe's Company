        $('bookCover_57669527').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_57669527').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_57669527').observe('prototip:hidden', function () {
        $$('span.elementTwo').each(function (e) {
