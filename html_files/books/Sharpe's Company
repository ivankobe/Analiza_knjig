      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_62901').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_62901').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_62901').observe('prototip:hidden', function () {
