      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Susan" class="user" itemprop="url" name="Susan" href="/user/show/3095594-susan">Susan</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="liked it"><span size="15x15" class="staticStar p10">liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/3095594-susan?shelf=fiction">fiction</a>, 
              <a class="actionLinkLite" href="/review/list/3095594-susan?shelf=own">own</a>, 
              <a class="actionLinkLite" href="/review/list/3095594-susan?shelf=science-fiction">science-fiction</a>, 
              <a class="actionLinkLite" href="/review/list/3095594-susan?shelf=digital">digital</a>, 
              <a class="actionLinkLite" href="/review/list/3095594-susan?shelf=short-stories">short-stories</a>
          </div>
