        $('bookCover_366427').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_366427').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
