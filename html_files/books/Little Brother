          $$('div.prototip').each(function (e) {
            if ($('bookCover_21526603').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_21526603').observe('prototip:hidden', function () {
