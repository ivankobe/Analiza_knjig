      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_317'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/904538.The_Book_of_Tea?rating=\">all <span class=\"greyText\">(31179)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/904538.The_Book_of_Tea?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;mL9rg0By0qxBGLsAf8GD6BvdHAfmHb4CIAZ6ksZc6cT3C3KsQTavtromco0pA8z5Dq8XHDlMCZdNHkMvL9CegQ==&#39;)}); return false;\">5 stars <span class=\"greyText\">(3301)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/904538.The_Book_of_Tea?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;zsqHFWdXx9ElNte+C114A77yfPDBPHZJPT9u8Zpdoouhfp46ZhO6y94IHjNdnzcSq4B36x5twdxQJ1dMc9HVzg==&#39;)}); return false;\">4 stars <span class=\"greyText\">(4054)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  