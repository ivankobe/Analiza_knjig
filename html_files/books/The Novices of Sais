        $('bookCover_246056').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_246056').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
