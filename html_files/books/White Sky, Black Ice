<div id="review_2610235436" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2610235436" />
    <a title="Cris" class="left imgcol" href="/user/show/1449734-cris"><img alt="Cris" src="https://images.gr-assets.com/users/1219448192p2/1449734.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2610235436?book_show_action=true">Dec 01, 2018</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Cris" class="user" itemprop="url" name="Cris" href="/user/show/1449734-cris">Cris</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="liked it"><span size="15x15" class="staticStar p10">liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/1449734-cris?shelf=mysteries">mysteries</a>, 
              <a class="actionLinkLite" href="/review/list/1449734-cris?shelf=cultural">cultural</a>, 
              <a class="actionLinkLite" href="/review/list/1449734-cris?shelf=character-driven">character-driven</a>, 
