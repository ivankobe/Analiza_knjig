      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_401072').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_401072').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_401072').observe('prototip:hidden', function () {
