        $('bookCover_43593420').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_43593420').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
