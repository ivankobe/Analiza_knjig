      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_466705').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_466705').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_466705').observe('prototip:hidden', function () {
