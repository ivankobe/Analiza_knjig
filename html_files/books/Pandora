<div id="review_2914226088" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2914226088" />
    <a title="Cătălin" class="left imgcol" href="/user/show/35592478-c-t-lin"><img alt="Cătălin" src="https://images.gr-assets.com/users/1535223975p2/35592478.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2914226088?book_show_action=true">Jul 31, 2019</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Cătălin" class="user" itemprop="url" name="Cătălin" href="/user/show/35592478-c-t-lin">Cătălin</a>
