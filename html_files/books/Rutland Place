        $('bookCover_35525675').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_35525675').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
