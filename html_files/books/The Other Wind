      <a class="uitext" rel="nofollow" id="span_class_gr-hyperlink_more_filters_span_tip_699" style="cursor: pointer; " href="#" onclick="return false"><span class="gr-hyperlink">More filters</span></a><script>
//<![CDATA[
      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_699'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/13658.The_Other_Wind?rating=\">all <span class=\"greyText\">(44122)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/13658.The_Other_Wind?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;ciWs0b8RgP/oFwVAL6gU619Ix2/Tjg2MA4S/b8PRg0MAoxksPDpYjqWYBRuyiRCzX8a/TuJcAJIfiCQt4Rkn7Q==&#39;)}); return false;\">5 stars <span class=\"greyText\">(8450)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/13658.The_Other_Wind?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;fiYuKm0mnub/1uqVZ6wAg0J9bqt6WOfSd7FKukqmDCUMoJvX7g1Gl7JZ6s76jQTbQvMWikuK6sxrvdH4aG6oiw==&#39;)}); return false;\">4 stars <span class=\"greyText\">(8131)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span 