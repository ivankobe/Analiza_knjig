        $('bookCover_107779').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_107779').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
