        $('bookCover_7062').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_7062').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
