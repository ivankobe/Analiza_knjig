<div id="review_520312776" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/520312776" />
    <a title="~Bellegirl91~" class="left imgcol" href="/user/show/3893565-bellegirl91"><img alt="~Bellegirl91~" src="https://images.gr-assets.com/users/1507600606p2/3893565.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/520312776?book_show_action=true">Jan 28, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="~Bellegirl91~" class="user" itemprop="url" name="~Bellegirl91~" href="/user/show/3893565-bellegirl91">~Bellegirl91~</a>
