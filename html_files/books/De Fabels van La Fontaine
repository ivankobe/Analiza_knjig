        $('bookCover_627374').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_627374').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
