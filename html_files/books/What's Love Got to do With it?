        $('bookCover_3382238').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_3382238').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
