<div id="review_1445909129" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1445909129" />
    <a title="grllopez" class="left imgcol" href="/user/show/8402385-grllopez"><img alt="grllopez" src="https://images.gr-assets.com/users/1614809227p2/8402385.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1445909129?book_show_action=true">Apr 09, 2019</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="grllopez" class="user" itemprop="url" name="grllopez" href="/user/show/8402385-grllopez">grllopez</a>
