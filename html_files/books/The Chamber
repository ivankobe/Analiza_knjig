        $('bookCover_24250610').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_24250610').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
