        $('bookCover_7340135').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_7340135').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_7340135').observe('prototip:hidden', function () {
