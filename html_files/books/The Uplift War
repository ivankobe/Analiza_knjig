        $('bookCover_826525').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_826525').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
