        $('bookCover_940323').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_940323').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_940323').observe('prototip:hidden', function () {
