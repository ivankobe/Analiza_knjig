      <a class="uitext" rel="nofollow" id="span_class_gr-hyperlink_more_filters_span_tip_946" style="cursor: pointer; " href="#" onclick="return false"><span class="gr-hyperlink">More filters</span></a><script>
//<![CDATA[
      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_946'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/11229.The_Sea_The_Sea?rating=\">all <span class=\"greyText\">(49918)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/11229.The_Sea_The_Sea?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;m9nFb9yDatqozc6qdg4hPW1iDru16eKzcjnn8JvcaU0kiBTz1vJuF5uDfl2gZfqxxWAcCzIWPTTI4o86T3KO+w==&#39;)}); return false;\">5 stars <span class=\"greyText\">(5835)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/11229.The_Sea_The_Sea?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;PJ/Vo9KnYHd7JvosWGuv8HDRtaRms7MihK21lgESDLiDzgQ/2NZkukhoStuOAHR82NOnFOFMbKU+dt1c1bzrDg==&#39;)}); return false;\">4 stars <span class=\"greyText\">(6435)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <sp