      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_60063').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_60063').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_60063').observe('prototip:hidden', function () {
