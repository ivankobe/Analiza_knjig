        $('bookCover_257213').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_257213').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_257213').observe('prototip:hidden', function () {
        $$('span.elementTwo').each(function (e) {
          if (e.getStyle('display') !== 'none') {
