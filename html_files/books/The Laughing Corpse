        $('bookCover_58413839').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_58413839').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
