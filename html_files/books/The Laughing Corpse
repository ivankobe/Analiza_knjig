      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_190361').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_190361').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_190361').observe('prototip:hidden', function () {
