        $('bookCover_165162').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_165162').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
