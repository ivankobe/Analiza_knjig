      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="S" class="user" itemprop="url" name="S" href="/user/show/314334-s">S</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="really liked it"><span size="15x15" class="staticStar p10">really liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/314334-s?shelf=historical">historical</a>, 
              <a class="actionLinkLite" href="/review/list/314334-s?shelf=sci-fi-fantasy">sci-fi-fantasy</a>, 
              <a class="actionLinkLite" href="/review/list/314334-s?shelf=celtic">celtic</a>, 
              <a class="actionLinkLite" href="/review/list/314334-s?shelf=mystery">mystery</a>, 
              <a class="actionLinkLite" href="/review/list/314334-s?shelf=arthurian">arthurian</a>, 
