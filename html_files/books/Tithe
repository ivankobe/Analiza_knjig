        $('bookCover_42952728').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_42952728').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
