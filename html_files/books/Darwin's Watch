<div id="review_2474857087" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2474857087" />
    <a title="Molokov" class="left imgcol" href="/user/show/6286612-molokov"><img alt="Molokov" src="https://images.gr-assets.com/users/1315903853p2/6286612.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2474857087?book_show_action=true">Aug 06, 2018</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Molokov" class="user" itemprop="url" name="Molokov" href="/user/show/6286612-molokov">Molokov</a>
