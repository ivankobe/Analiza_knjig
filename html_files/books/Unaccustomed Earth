        $('bookCover_4950').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_4950').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
