        $('bookCover_11337').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_11337').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_11337').observe('prototip:hidden', function () {
