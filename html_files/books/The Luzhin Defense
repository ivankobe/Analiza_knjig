      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_268'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/8153.The_Luzhin_Defense?rating=\">all <span class=\"greyText\">(21703)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/8153.The_Luzhin_Defense?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;wzcr8+nbgIINCZ6MNJ+VfFMwANqwi2URSzPadUSZsE5X+TMIUT+9kBpA/iBE+adCSnoCO/AGZBOiVtMm5LQFgA==&#39;)}); return false;\">5 stars <span class=\"greyText\">(3360)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/8153.The_Luzhin_Defense?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;UBEAfIaGEmV4cbQVs95DghZhdpJ4AFueediQDkyabJ7E3xiHPmIvd2841LnDuHG8Dyt0cziNWpyQvZld7LfZUA==&#39;)}); return false;\">4 stars <span class=\"greyText\">(4262)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n               