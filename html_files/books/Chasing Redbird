        $('bookCover_185723').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_185723').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
