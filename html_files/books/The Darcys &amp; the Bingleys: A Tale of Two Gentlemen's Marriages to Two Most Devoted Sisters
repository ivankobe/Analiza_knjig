        $('bookCover_12305385').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_12305385').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
