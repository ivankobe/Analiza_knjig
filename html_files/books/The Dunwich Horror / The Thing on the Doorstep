        $('bookCover_35704243').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_35704243').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
