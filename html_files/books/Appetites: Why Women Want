        $('bookCover_46032521').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_46032521').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
