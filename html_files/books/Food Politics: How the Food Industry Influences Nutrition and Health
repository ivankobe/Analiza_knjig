      <a class="uitext" rel="nofollow" id="span_class_gr-hyperlink_more_filters_span_tip_729" style="cursor: pointer; " href="#" onclick="return false"><span class="gr-hyperlink">More filters</span></a><script>
//<![CDATA[
      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_729'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/19132.Food_Politics?rating=\">all <span class=\"greyText\">(16847)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/19132.Food_Politics?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;Sr8Z7woDHZLGOhJAXaGq5Ai8ik8sR2KhPdY7P5uffGLkka8q7/5oONCu5dGaotzOfCqlhS5mY00bbHERihluWQ==&#39;)}); return false;\">5 stars <span class=\"greyText\">(1345)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/19132.Food_Politics?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;S7tUW4JPPU2/bygsmgfxSHIyrFfFPu68ZIR74+L6mtvlleKeZ7JI56n7371dBIdiBqSDnccf71BCPjHN83yI4A==&#39;)}); return false;\">4 stars <span class=\"greyText\">(1621)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span cla