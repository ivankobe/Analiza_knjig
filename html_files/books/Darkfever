        $('bookCover_6050298').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_6050298').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
