          $$('div.prototip').each(function (e) {
            if ($('bookCover_53750266').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_53750266').observe('prototip:hidden', function () {
