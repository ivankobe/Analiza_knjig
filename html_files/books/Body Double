<div id="review_2270530097" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2270530097" />
    <a title="CD {Boulder Blvd}" class="left imgcol" href="/user/show/31857175-cd-boulder-blvd"><img alt="CD {Boulder Blvd}" src="https://images.gr-assets.com/users/1413765420p2/31857175.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2270530097?book_show_action=true">Jan 25, 2018</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="CD {Boulder Blvd}" class="user" itemprop="url" name="CD {Boulder Blvd}" href="/user/show/31857175-cd-boulder-blvd">CD {Boulder Blvd}</a>
