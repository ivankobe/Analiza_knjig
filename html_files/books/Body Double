<div id="review_3147176647" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/3147176647" />
    <a title=" Marla" class="left imgcol" href="/user/show/593597-marla"><img alt=" Marla" src="https://images.gr-assets.com/users/1290639235p2/593597.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/3147176647?book_show_action=true">Sep 20, 2020</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title=" Marla" class="user" itemprop="url" name=" Marla" href="/user/show/593597-marla"> Marla</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="really liked it"><span size="15x15" class="staticStar p10">really liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/593597-marla?shelf=adult">adult</a>, 
              <a class="actionLinkLite" href="/review/list/593597-marla?shelf=girl-power">girl-power</a>, 
              <a class="actionLinkLite" href="/review/list/593597-marla?shelf=with-reservations">with-reservations</a>, 
              <a class="actionLinkLite" href="/review/list/593597-marla?shelf=2020">2020</a>, 
              <a class="actionLinkLite" href="/review/list/593597-marla?shelf=series">series</a>, 
