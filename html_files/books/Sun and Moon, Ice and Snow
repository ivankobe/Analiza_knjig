          $$('div.prototip').each(function (e) {
            if ($('bookCover_157055').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_157055').observe('prototip:hidden', function () {
