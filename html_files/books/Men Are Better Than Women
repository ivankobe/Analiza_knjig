        $('bookCover_55277906').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_55277906').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
