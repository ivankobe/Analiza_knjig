        $('edition_6193937').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('edition_6193937').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
