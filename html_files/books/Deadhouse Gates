      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_133'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/55401.Deadhouse_Gates?rating=\">all <span class=\"greyText\">(94042)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/55401.Deadhouse_Gates?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;xxiOZegE9FgjQ/Xlmk49iTrvYUkCFyvfXHCbvOWgCywPUX6HZloNMYUAzkq7rLCPWSFi7gUFCXt8xugfFEzzIw==&#39;)}); return false;\">5 stars <span class=\"greyText\">(30687)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/55401.Deadhouse_Gates?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;pGcugvIxuUuVqTtYxd/P1a/lsQ1OcKjCcPsGI5DgFxFsLt5gfG9AIjPqAPfkPULTzCuyqkliimZQTXWAYQzvHg==&#39;)}); return false;\">4 stars <span class=\"greyText\">(21544)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <