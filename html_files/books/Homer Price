        $('bookCover_859358').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_859358').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
