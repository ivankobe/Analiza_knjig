      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('edition_18871538').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('edition_18871538').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('edition_18871538').observe('prototip:hidden', function () {
