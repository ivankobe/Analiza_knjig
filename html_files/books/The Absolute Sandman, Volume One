        $('bookCover_41839478').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_41839478').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
