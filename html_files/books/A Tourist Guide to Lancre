        $('bookCover_46015758').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_46015758').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
