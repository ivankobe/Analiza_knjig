      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_25419').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_25419').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_25419').observe('prototip:hidden', function () {
