      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('edition_974035').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('edition_974035').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('edition_974035').observe('prototip:hidden', function () {
