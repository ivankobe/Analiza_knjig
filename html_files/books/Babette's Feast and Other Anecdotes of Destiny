<div id="review_2679390003" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2679390003" />
    <a title="Sara" class="left imgcol" href="/user/show/2957633-sara"><img alt="Sara" src="https://images.gr-assets.com/users/1268790519p2/2957633.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2679390003?book_show_action=true">Jan 19, 2019</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Sara" class="user" itemprop="url" name="Sara" href="/user/show/2957633-sara">Sara</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="liked it"><span size="15x15" class="staticStar p10">liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>



        

