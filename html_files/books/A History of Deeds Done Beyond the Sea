        $('bookCover_53970').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_53970').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
