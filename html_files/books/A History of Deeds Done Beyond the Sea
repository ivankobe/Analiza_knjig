<div id="review_3203876102" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/3203876102" />
    <a title="Lars" class="left imgcol" href="/user/show/110529787-lars"><img alt="Lars" src="https://images.gr-assets.com/users/1608807515p2/110529787.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/3203876102?book_show_action=true">Apr 18, 2020</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Lars" class="user" itemprop="url" name="Lars" href="/user/show/110529787-lars">Lars</a>
