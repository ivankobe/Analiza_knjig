<div id="review_80689053" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/80689053" />
    <a title="Tony" class="left imgcol" href="/user/show/33771-tony"><img alt="Tony" src="https://images.gr-assets.com/users/1292869016p2/33771.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/80689053?book_show_action=true">Dec 11, 2009</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Tony" class="user" itemprop="url" name="Tony" href="/user/show/33771-tony">Tony</a>
