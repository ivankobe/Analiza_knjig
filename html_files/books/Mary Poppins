        $('bookCover_24568747').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_24568747').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
