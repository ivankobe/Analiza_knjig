        $('bookCover_247936').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_247936').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
