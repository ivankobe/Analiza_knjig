        $('bookCover_56303766').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_56303766').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
