      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_35271473').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_35271473').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_35271473').observe('prototip:hidden', function () {
