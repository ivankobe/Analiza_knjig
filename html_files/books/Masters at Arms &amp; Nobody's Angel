        $('bookCover_17405850').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_17405850').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_17405850').observe('prototip:hidden', function () {
