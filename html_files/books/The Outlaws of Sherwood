<div id="review_116918727" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/116918727" />
    <a title="Avrelia" class="left imgcol" href="/user/show/2187951-avrelia"><img alt="Avrelia" src="https://images.gr-assets.com/users/1287080218p2/2187951.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/116918727?book_show_action=true">Aug 15, 2010</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Avrelia" class="user" itemprop="url" name="Avrelia" href="/user/show/2187951-avrelia">Avrelia</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="liked it"><span size="15x15" class="staticStar p10">liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/2187951-avrelia?shelf=from-the-library">from-the-library</a>, 
              <a class="actionLinkLite" href="/review/list/2187951-avrelia?shelf=sff">sff</a>, 
              <a class="actionLinkLite" href="/review/list/2187951-avrelia?shelf=fancy-fantasy">fancy-fantasy</a>, 
              <a class="actionLinkLite" href="/review/list/2187951-avrelia?shelf=atmospheric">atmospheric</a>, 
              <a class="actionLinkLite" href="/review/list/2187951-avrelia?shelf=i-should-ve-read-it-15-years-ago">i-should-ve-read-it-15-years-ago</a>, 
              <a class="actionLinkLite" href="/review/list/2187951-avrelia?shelf=fairy-tales-retelling">fairy-tales-retelling</a>, 
              <a class="actionLinkLite" href="/review/list/2187951-avrelia?shelf=fantasy">fantasy</a>, 
              <a class="actionLinkLite" href="/review/list/2187951-avrelia?shelf=historical-fantasy">historical-fantasy</a>
          </div>
