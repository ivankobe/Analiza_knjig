        $('bookCover_467140').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_467140').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
