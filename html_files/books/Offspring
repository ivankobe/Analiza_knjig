        $('edition_555715').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('edition_555715').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('edition_555715').observe('prototip:hidden', function () {
        $$('span.elementTwo').each(function (e) {
