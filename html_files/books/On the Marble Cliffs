        $('bookCover_589257').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_589257').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
