        $('bookCover_678241').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_678241').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
