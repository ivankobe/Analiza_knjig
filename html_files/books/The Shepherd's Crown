        $('bookCover_55855546').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_55855546').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
