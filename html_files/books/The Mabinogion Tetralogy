        $('bookCover_758153').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_758153').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
