        $('bookCover_824964').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_824964').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_824964').observe('prototip:hidden', function () {
