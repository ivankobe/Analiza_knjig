        $('bookCover_55604632').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_55604632').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_55604632').observe('prototip:hidden', function () {
