        $('bookCover_56624555').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_56624555').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
