      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_41733208').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_41733208').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_41733208').observe('prototip:hidden', function () {
