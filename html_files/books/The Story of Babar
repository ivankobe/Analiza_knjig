        $('bookCover_817720').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_817720').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
