        $('bookCover_33024254').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_33024254').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
