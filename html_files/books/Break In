        $('bookCover_19323847').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_19323847').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
