        $('bookCover_13060818').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_13060818').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
