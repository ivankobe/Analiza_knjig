        $('bookCover_77103').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_77103').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
