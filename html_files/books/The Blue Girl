        $('bookCover_55777862').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_55777862').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
