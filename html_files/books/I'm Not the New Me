      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Lori" class="user" itemprop="url" name="Lori" href="/user/show/1332418-lori">Lori</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was ok"><span size="15x15" class="staticStar p10">it was ok</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/1332418-lori?shelf=humor">humor</a>, 
              <a class="actionLinkLite" href="/review/list/1332418-lori?shelf=basement-laundry-room-series">basement-laundry-room-series</a>, 
              <a class="actionLinkLite" href="/review/list/1332418-lori?shelf=nonfiction">nonfiction</a>, 
              <a class="actionLinkLite" href="/review/list/1332418-lori?shelf=memoir">memoir</a>, 
              <a class="actionLinkLite" href="/review/list/1332418-lori?shelf=women">women</a>
          </div>
