        $('bookCover_74694').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_74694').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
