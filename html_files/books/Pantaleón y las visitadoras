        $('bookCover_60756').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_60756').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
