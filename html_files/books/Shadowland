<div id="review_2094521843" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2094521843" />
    <a title="De&#39;Siree Fairley" class="left imgcol" href="/user/show/59949803-de-siree-fairley"><img alt="De&#39;Siree Fairley" src="https://images.gr-assets.com/users/1545175007p2/59949803.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2094521843?book_show_action=true">Aug 20, 2017</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="De&#39;Siree Fairley" class="user" itemprop="url" name="De&#39;Siree Fairley" href="/user/show/59949803-de-siree-fairley">De&#39;Siree Fairley</a>
