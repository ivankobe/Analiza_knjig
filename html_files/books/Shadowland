        $('bookCover_156926').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_156926').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
