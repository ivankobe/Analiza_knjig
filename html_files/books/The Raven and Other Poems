<div id="review_2169608520" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2169608520" />
    <a title="Mischenko" class="left imgcol" href="/user/show/1194517-mischenko"><img alt="Mischenko" src="https://images.gr-assets.com/users/1618608214p2/1194517.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2169608520?book_show_action=true">Oct 31, 2017</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Mischenko" class="user" itemprop="url" name="Mischenko" href="/user/show/1194517-mischenko">Mischenko</a>
