        $('bookCover_24874331').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_24874331').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
