      <a class="uitext" rel="nofollow" id="span_class_gr-hyperlink_more_filters_span_tip_952" style="cursor: pointer; " href="#" onclick="return false"><span class="gr-hyperlink">More filters</span></a><script>
//<![CDATA[
      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_952'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/604578.The_Captain_s_Daughter?rating=\">all <span class=\"greyText\">(33251)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/604578.The_Captain_s_Daughter?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;Q0sGm6ylzPR62t3yEkmMfI211q8JkrVwqPBTHdqu/5Ms5JzsKdvKCDIGdB09JW9mk//27PYfNBF2jIQHP1MlRA==&#39;)}); return false;\">5 stars <span class=\"greyText\">(4703)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/604578.The_Captain_s_Daughter?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;U1bBCuwSCh/qv0+Ujeiq0R7WIEYXvn4Jki6oL7Qs+208+Vt9aWwM46Jj5nuihEnLAJwABegz/2hMUn81UdEhug==&#39;)}); return false;\">4 stars <span class=\"greyText\">(6532)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span