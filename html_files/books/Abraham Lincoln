        $('bookCover_630761').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_630761').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
