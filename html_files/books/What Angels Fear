        $('bookCover_33019790').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_33019790').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
