        $('bookCover_920576').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_920576').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
