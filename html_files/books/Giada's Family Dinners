        $('bookCover_101249').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_101249').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
