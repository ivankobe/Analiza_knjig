        $('bookCover_33793443').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_33793443').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
