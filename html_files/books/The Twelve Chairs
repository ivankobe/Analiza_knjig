<div id="review_771983230" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/771983230" />
    <a title="Betül" class="left imgcol" href="/user/show/18248241-bet-l"><img alt="Betül" src="https://images.gr-assets.com/users/1540845592p2/18248241.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/771983230?book_show_action=true">Jan 12, 2017</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Betül" class="user" itemprop="url" name="Betül" href="/user/show/18248241-bet-l">Betül</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was amazing"><span size="15x15" class="staticStar p10">it was amazing</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span></span>



        

