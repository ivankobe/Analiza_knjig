        $('bookCover_51591633').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_51591633').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
