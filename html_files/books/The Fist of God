  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/656936650?book_show_action=true">Jul 02, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Pamela" class="user" itemprop="url" name="Pamela" href="/user/show/17527831-pamela">Pamela</a>
