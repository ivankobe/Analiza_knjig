      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_20727362').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_20727362').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_20727362').observe('prototip:hidden', function () {
