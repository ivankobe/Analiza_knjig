<div id="review_3116051688" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/3116051688" />
    <a title="Wiebke (1book1review)" class="left imgcol" href="/user/show/6315232-wiebke-1book1review"><img alt="Wiebke (1book1review)" src="https://images.gr-assets.com/users/1341913075p2/6315232.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/3116051688?book_show_action=true">Jan 18, 2020</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Wiebke (1book1review)" class="user" itemprop="url" name="Wiebke (1book1review)" href="/user/show/6315232-wiebke-1book1review">Wiebke (1book1review)</a>
