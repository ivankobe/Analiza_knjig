        $('bookCover_6578909').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_6578909').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
