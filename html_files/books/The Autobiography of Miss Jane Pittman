<div id="review_1710299031" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1710299031" />
    <a title="Chrissie" class="left imgcol" href="/user/show/632247-chrissie"><img alt="Chrissie" src="https://images.gr-assets.com/users/1321799585p2/632247.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1710299031?book_show_action=true">Jul 26, 2016</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Chrissie" class="user" itemprop="url" name="Chrissie" href="/user/show/632247-chrissie">Chrissie</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="did not like it"><span size="15x15" class="staticStar p10">did not like it</span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/632247-chrissie?shelf=audible">audible</a>, 
              <a class="actionLinkLite" href="/review/list/632247-chrissie?shelf=usa">usa</a>, 
              <a class="actionLinkLite" href="/review/list/632247-chrissie?shelf=2016-read">2016-read</a>, 
              <a class="actionLinkLite" href="/review/list/632247-chrissie?shelf=southern">southern</a>, 
              <a class="actionLinkLite" href="/review/list/632247-chrissie?shelf=classics">classics</a>, 
              <a class="actionLinkLite" href="/review/list/632247-chrissie?shelf=returned">returned</a>, 
              <a class="actionLinkLite" href="/review/list/632247-chrissie?shelf=hf">hf</a>, 
              <a class="actionLinkLite" href="/review/list/632247-chrissie?shelf=race">race</a>, 
              <a class="actionLinkLite" href="/review/list/632247-chrissie?shelf=disliked">disliked</a>
          </div>
