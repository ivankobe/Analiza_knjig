        $('bookCover_69647').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_69647').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
