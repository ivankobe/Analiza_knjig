        $('bookCover_20391541').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_20391541').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
