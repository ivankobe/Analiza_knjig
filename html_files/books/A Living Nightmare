      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_22914308').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_22914308').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_22914308').observe('prototip:hidden', function () {
