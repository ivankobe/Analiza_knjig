<div id="review_2862034477" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2862034477" />
    <a title="KAS" class="left imgcol" href="/user/show/20691061-kas"><img alt="KAS" src="https://images.gr-assets.com/users/1472348331p2/20691061.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2862034477?book_show_action=true">Jun 17, 2019</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="KAS" class="user" itemprop="url" name="KAS" href="/user/show/20691061-kas">KAS</a>
