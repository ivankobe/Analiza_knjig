        $('bookCover_15815348').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_15815348').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
