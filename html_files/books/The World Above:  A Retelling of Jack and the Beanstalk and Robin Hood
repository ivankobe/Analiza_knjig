    <a id="rating_details_tip" class="gr-hyperlink" style="cursor: pointer; " href="#" onclick="return false"> Rating details</a><script>
//<![CDATA[
      var newTip = new Tip($('rating_details_tip'), "\n        <div id=\"moreBookData\" class=\"bookMeta\" >\n  \n\n  \n\n<table border=\"0\" id=\"rating_distribution\" style=\"width: auto; max-width: 500px;\" cellspacing=\"5\" cellpadding=\"0\">\n      <tr>\n        <th width=\"25\" class=\"right\">5&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 100%;\" title=\"1486 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/7107790-the-world-above?rating=5\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'oeeT1aFgHrt6rg5IC2FGgnC4SbhFj1yPkbYJO/zuG2XtrZF+7LtvcSREcLoSA4JmM1viuD8EyyeiTRvqKK4NgQ==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          33% (1486)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">4&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 94%;\" title=\"1401 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/7107790-the-world-above?rating=4\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'K7qfVbsn9sQzgiKz+GuWB4P14meXPWruXHjMKUVZArBn8J3+9vyHDm1oXEHhCVLjwBZJZ+22/UZvg974kRkUVA==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          32% (1401)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">3&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 76%;\" title=\"1142 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/7107790-the-world-above?rating=3\', 