        $('bookCover_57684198').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_57684198').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
