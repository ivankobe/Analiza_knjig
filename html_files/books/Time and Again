<div id="review_1956055048" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1956055048" />
    <a title="Carla Remy" class="left imgcol" href="/user/show/3318055-carla-remy"><img alt="Carla Remy" src="https://images.gr-assets.com/users/1623000448p2/3318055.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1956055048?book_show_action=true">Mar 29, 2017</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Carla Remy" class="user" itemprop="url" name="Carla Remy" href="/user/show/3318055-carla-remy">Carla Remy</a>
