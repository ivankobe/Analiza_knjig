        $('bookCover_55655108').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_55655108').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_55655108').observe('prototip:hidden', function () {
