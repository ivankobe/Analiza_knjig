        $('bookCover_107974').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_107974').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
