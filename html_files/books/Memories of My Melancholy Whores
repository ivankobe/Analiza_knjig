        $('bookCover_63040').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_63040').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
