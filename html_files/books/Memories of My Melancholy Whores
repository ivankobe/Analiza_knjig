<div id="review_2115002669" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2115002669" />
    <a title="Berfin Kanat" class="left imgcol" href="/user/show/5669204-berfin-kanat"><img alt="Berfin Kanat" src="https://images.gr-assets.com/users/1548781156p2/5669204.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2115002669?book_show_action=true">Sep 06, 2017</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Berfin Kanat" class="user" itemprop="url" name="Berfin Kanat" href="/user/show/5669204-berfin-kanat">Berfin Kanat</a>
