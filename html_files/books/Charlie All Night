        $('bookCover_30856').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_30856').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
