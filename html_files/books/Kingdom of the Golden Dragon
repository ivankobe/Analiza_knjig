        $('bookCover_829827').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_829827').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
