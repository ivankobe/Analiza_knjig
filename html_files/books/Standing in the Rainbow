        $('bookCover_56133680').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_56133680').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_56133680').observe('prototip:hidden', function () {
        $$('span.elementTwo').each(function (e) {
