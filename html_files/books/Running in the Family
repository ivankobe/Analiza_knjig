        $('bookCover_1884').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1884').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
