        $('bookCover_17306681').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_17306681').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
