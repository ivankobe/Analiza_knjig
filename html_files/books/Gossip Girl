<div id="review_18192524" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/18192524" />
    <a title="Madeline" class="left imgcol" href="/user/show/111921-madeline"><img alt="Madeline" src="https://images.gr-assets.com/users/1220803522p2/111921.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/18192524?book_show_action=true">Mar 20, 2008</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Madeline" class="user" itemprop="url" name="Madeline" href="/user/show/111921-madeline">Madeline</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="did not like it"><span size="15x15" class="staticStar p10">did not like it</span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/111921-madeline?shelf=kids-and-young-adult">kids-and-young-adult</a>, 
              <a class="actionLinkLite" href="/review/list/111921-madeline?shelf=ugh">ugh</a>, 
              <a class="actionLinkLite" href="/review/list/111921-madeline?shelf=crap-tastic">crap-tastic</a>, 
              <a class="actionLinkLite" href="/review/list/111921-madeline?shelf=no-judgements">no-judgements</a>
          </div>
