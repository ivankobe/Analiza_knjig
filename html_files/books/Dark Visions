        $('bookCover_8724084').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_8724084').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
