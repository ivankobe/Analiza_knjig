      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_1159218').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1159218').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_1159218').observe('prototip:hidden', function () {
