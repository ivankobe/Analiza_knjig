  <div class="friendReviews elementListBrown" >
    <div class="section">
      

<div id="review_1360182792" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1360182792" />
    <a title="Charlene" class="left imgcol" href="/user/show/6087199-charlene"><img alt="Charlene" src="https://images.gr-assets.com/users/1372406238p2/6087199.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1360182792?book_show_action=true">Aug 10, 2015</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Charlene" class="user" itemprop="url" name="Charlene" href="/user/show/6087199-charlene">Charlene</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="really liked it"><span size="15x15" class="staticStar p10">really liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/6087199-charlene?shelf=academia">academia</a>, 
              <a class="actionLinkLite" href="/review/list/6087199-charlene?shelf=mystery">mystery</a>, 
              <a class="actionLinkLite" href="/review/list/6087199-charlene?shelf=crime">crime</a>, 
