        $('bookCover_37793').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_37793').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
