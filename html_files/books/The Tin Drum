        $('bookCover_69880').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_69880').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
