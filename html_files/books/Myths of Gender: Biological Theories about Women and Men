        $('bookCover_42182169').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_42182169').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
