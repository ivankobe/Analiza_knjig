        $('bookCover_138456').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_138456').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
