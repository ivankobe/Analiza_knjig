    <div class="js-dataTooltip" data-use-wtr-tooltip='true'>
        <div class="js-tooltipTrigger tooltipTrigger" data-resource-type='Book' data-resource-id='22271' data-placement='bottom-left'>
          <a href="/book/show/22271.Veronica"><img id="more_book_22271" alt="Veronica" title="Veronica" width="50" src="https://i.gr-assets.com/images/S/compressed.photo.goodreads.com/books/1437249466l/22271._SX98_.jpg" /></a>
        </div>
        <div class="js-tooltipTrigger tooltipTrigger" data-resource-type='Book' data-resource-id='22270' data-placement='bottom-left'>
