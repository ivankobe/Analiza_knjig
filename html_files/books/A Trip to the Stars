      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_12635798').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_12635798').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_12635798').observe('prototip:hidden', function () {
