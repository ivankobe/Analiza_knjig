<div id="review_20524948" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/20524948" />
    <a title="booklady" class="left imgcol" href="/user/show/350218-booklady"><img alt="booklady" src="https://images.gr-assets.com/users/1609115994p2/350218.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/20524948?book_show_action=true">Apr 19, 2008</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="booklady" class="user" itemprop="url" name="booklady" href="/user/show/350218-booklady">booklady</a>
