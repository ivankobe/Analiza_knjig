<div id="review_1313004516" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1313004516" />
    <a title="Ygraine" class="left imgcol" href="/user/show/38411791-ygraine"><img alt="Ygraine" src="https://images.gr-assets.com/users/1610603579p2/38411791.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1313004516?book_show_action=true">Sep 26, 2016</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Ygraine" class="user" itemprop="url" name="Ygraine" href="/user/show/38411791-ygraine">Ygraine</a>
