        $('bookCover_54815513').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_54815513').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
