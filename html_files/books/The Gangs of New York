      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_987'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/24569.The_Gangs_of_New_York?rating=\">all <span class=\"greyText\">(9920)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/24569.The_Gangs_of_New_York?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;sUXRLwBm5iX4Og88H8UcxcYvW0/iSDijCw9nJo8jW9veJs9gbOBK1ptPDGzuNv6DdEQe4uuT71mV8xbQakG3gQ==&#39;)}); return false;\">5 stars <span class=\"greyText\">(694)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/24569.The_Gangs_of_New_York?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;+IXJDUjj9x9NMuF7jrjsDC2oT0rOo1ZGfE4YNOZVYKyX5tdCJGVb7C5H4it/Sw5Kn8MK58d4gbzismnCAzeM9g==&#39;)}); return false;\">4 stars <span class=\"greyText\">(1207)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n     