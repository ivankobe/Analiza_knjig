<div id="review_3640390074" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/3640390074" />
    <a title="Rachel" class="left imgcol" href="/user/show/13329837-rachel"><img alt="Rachel" src="https://images.gr-assets.com/users/1609557793p2/13329837.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/3640390074?book_show_action=true">Nov 11, 2020</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Rachel" class="user" itemprop="url" name="Rachel" href="/user/show/13329837-rachel">Rachel</a>
