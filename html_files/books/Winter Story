          $$('div.prototip').each(function (e) {
            if ($('bookCover_421613').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_421613').observe('prototip:hidden', function () {
