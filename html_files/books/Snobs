        $('bookCover_53294602').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_53294602').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
