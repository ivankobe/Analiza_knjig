          $$('div.prototip').each(function (e) {
            if ($('edition_1338983').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('edition_1338983').observe('prototip:hidden', function () {
