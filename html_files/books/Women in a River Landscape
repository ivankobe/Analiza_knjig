        $('bookCover_86147').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_86147').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
