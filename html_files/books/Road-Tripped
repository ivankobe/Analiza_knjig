        $('bookCover_52109335').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_52109335').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
