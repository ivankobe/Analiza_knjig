        $('bookCover_46460').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_46460').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_46460').observe('prototip:hidden', function () {
