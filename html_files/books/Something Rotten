        $('bookCover_38223804').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_38223804').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
