        $('bookCover_142776').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_142776').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
