<div id="review_3262405181" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/3262405181" />
    <a title="majo " class="left imgcol" href="/user/show/39317215-majo"><img alt="majo " src="https://images.gr-assets.com/users/1577812855p2/39317215.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/3262405181?book_show_action=true">Apr 18, 2020</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="majo " class="user" itemprop="url" name="majo " href="/user/show/39317215-majo">majo </a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was ok"><span size="15x15" class="staticStar p10">it was ok</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/39317215-majo?shelf=series-im-not-going-to-finish">series-im-not-going-to-finish</a>, 
              <a class="actionLinkLite" href="/review/list/39317215-majo?shelf=urban-fantasy">urban-fantasy</a>, 
              <a class="actionLinkLite" href="/review/list/39317215-majo?shelf=disappointing">disappointing</a>, 
              <a class="actionLinkLite" href="/review/list/39317215-majo?shelf=year-2020">year-2020</a>, 
