        $('bookCover_33931902').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_33931902').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
