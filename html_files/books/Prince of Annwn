        $('bookCover_19593').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_19593').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
