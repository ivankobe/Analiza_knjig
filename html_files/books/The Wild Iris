        $('bookCover_56527733').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_56527733').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
