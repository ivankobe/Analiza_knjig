        $('bookCover_6393631').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_6393631').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
