          $$('div.prototip').each(function (e) {
            if ($('bookCover_420571').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_420571').observe('prototip:hidden', function () {
