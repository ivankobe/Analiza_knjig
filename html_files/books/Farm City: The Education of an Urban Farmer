        $('bookCover_43822076').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_43822076').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
