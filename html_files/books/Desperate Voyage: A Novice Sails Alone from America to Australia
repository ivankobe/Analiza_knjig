        $('bookCover_13398093').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_13398093').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
