        $('bookCover_23229220').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_23229220').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
