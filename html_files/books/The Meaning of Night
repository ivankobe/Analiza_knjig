        $('bookCover_20613603').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_20613603').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
