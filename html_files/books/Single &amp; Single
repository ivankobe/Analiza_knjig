  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/606902791?book_show_action=true">May 04, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Pawanraj" class="user" itemprop="url" name="Pawanraj" href="/user/show/6162847-pawanraj">Pawanraj</a>
