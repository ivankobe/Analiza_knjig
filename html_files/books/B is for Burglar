      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_12483864').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_12483864').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_12483864').observe('prototip:hidden', function () {
