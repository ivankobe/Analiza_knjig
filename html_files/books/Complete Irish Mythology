        $('bookCover_17608111').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_17608111').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_17608111').observe('prototip:hidden', function () {
