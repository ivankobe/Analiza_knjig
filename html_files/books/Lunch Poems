        $('bookCover_99713').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_99713').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
