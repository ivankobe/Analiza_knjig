        $('bookCover_1384387').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1384387').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
