        $('bookCover_31342175').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_31342175').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
