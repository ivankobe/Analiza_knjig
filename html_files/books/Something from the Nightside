<div id="review_558735700" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/558735700" />
    <a title="Jen" class="left imgcol" href="/user/show/12797140-jen"><img alt="Jen" src="https://images.gr-assets.com/users/1500484453p2/12797140.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/558735700?book_show_action=true">Mar 12, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Jen" class="user" itemprop="url" name="Jen" href="/user/show/12797140-jen">Jen</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was amazing"><span size="15x15" class="staticStar p10">it was amazing</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/12797140-jen?shelf=x2014-gr-adult-or-ya">x2014-gr-adult-or-ya</a>, 
              <a class="actionLinkLite" href="/review/list/12797140-jen?shelf=features-ghosts-or-spirits">features-ghosts-or-spirits</a>, 
              <a class="actionLinkLite" href="/review/list/12797140-jen?shelf=books-with-lol-moments">books-with-lol-moments</a>, 
              <a class="actionLinkLite" href="/review/list/12797140-jen?shelf=features-aliens">features-aliens</a>, 
              <a class="actionLinkLite" href="/review/list/12797140-jen?shelf=features-psychics">features-psychics</a>, 
              <a class="actionLinkLite" href="/review/list/12797140-jen?shelf=owned-paper">owned-paper</a>, 
