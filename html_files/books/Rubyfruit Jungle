        $('bookCover_8913370').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_8913370').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
