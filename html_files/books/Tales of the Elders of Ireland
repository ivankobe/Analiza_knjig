      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_127028').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_127028').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_127028').observe('prototip:hidden', function () {
