<div id="review_2310512145" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2310512145" />
    <a title="Hayley" class="left imgcol" href="/user/show/51236564-hayley"><img alt="Hayley" src="https://images.gr-assets.com/users/1453671840p2/51236564.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2310512145?book_show_action=true">Mar 02, 2018</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Hayley" class="user" itemprop="url" name="Hayley" href="/user/show/51236564-hayley">Hayley</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was amazing"><span size="15x15" class="staticStar p10">it was amazing</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span></span>



        

