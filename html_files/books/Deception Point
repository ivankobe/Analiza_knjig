<div id="review_3135280924" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/3135280924" />
    <a title="Vintage" class="left imgcol" href="/user/show/27204643-vintage"><img alt="Vintage" src="https://images.gr-assets.com/users/1475253836p2/27204643.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/3135280924?book_show_action=true">Jan 10, 2020</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Vintage" class="user" itemprop="url" name="Vintage" href="/user/show/27204643-vintage">Vintage</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="liked it"><span size="15x15" class="staticStar p10">liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/27204643-vintage?shelf=bad-parents">bad-parents</a>, 
              <a class="actionLinkLite" href="/review/list/27204643-vintage?shelf=ridiculousness">ridiculousness</a>, 
              <a class="actionLinkLite" href="/review/list/27204643-vintage?shelf=deception">deception</a>, 
              <a class="actionLinkLite" href="/review/list/27204643-vintage?shelf=suspense">suspense</a>, 
              <a class="actionLinkLite" href="/review/list/27204643-vintage?shelf=worst-parent-ever">worst-parent-ever</a>, 
              <a class="actionLinkLite" href="/review/list/27204643-vintage?shelf=jumped-the-shark">jumped-the-shark</a>, 
