          $$('div.prototip').each(function (e) {
            if ($('bookCover_1153501').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_1153501').observe('prototip:hidden', function () {
