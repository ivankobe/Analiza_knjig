        $('bookCover_34384161').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_34384161').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
