<div id="review_798628617" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/798628617" />
    <a title="Silvana" class="left imgcol" href="/user/show/666003-silvana"><img alt="Silvana" src="https://images.gr-assets.com/users/1606622144p2/666003.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/798628617?book_show_action=true">Dec 24, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Silvana" class="user" itemprop="url" name="Silvana" href="/user/show/666003-silvana">Silvana</a>
