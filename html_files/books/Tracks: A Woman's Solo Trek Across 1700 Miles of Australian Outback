<div id="review_1136652157" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1136652157" />
    <a title="Rochelle" class="left imgcol" href="/user/show/743198-rochelle"><img alt="Rochelle" src="https://images.gr-assets.com/users/1576736496p2/743198.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1136652157?book_show_action=true">Dec 20, 2014</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Rochelle" class="user" itemprop="url" name="Rochelle" href="/user/show/743198-rochelle">Rochelle</a>
