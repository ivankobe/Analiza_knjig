      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_7719245').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_7719245').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_7719245').observe('prototip:hidden', function () {
