<div id="review_81231670" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/81231670" />
    <a title="Mary" class="left imgcol" href="/user/show/1059025-mary"><img alt="Mary" src="https://images.gr-assets.com/users/1207451762p2/1059025.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/81231670?book_show_action=true">Dec 16, 2009</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Mary" class="user" itemprop="url" name="Mary" href="/user/show/1059025-mary">Mary</a>
