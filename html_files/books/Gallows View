        $('bookCover_44148076').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_44148076').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
