<script type="text/javascript" charset="utf-8">
//<![CDATA[  

  function submitShelfLink(unique_id, book_id, shelf_id, shelf_name, submit_form, exclusive) {
    var checkbox_id = 'shelf_name_' + unique_id + '_' + shelf_id;
    var element = document.getElementById(checkbox_id)

    var checked = element.checked
    if (checked && exclusive) {
      // can't uncheck a radio by clicking it!
      return
    }
    if(document.getElementById("savingMessage")){
      Element.show('savingMessage')
    }
    var element_id = 'shelfInDropdownName_' + unique_id + '_' + shelf_id;
    Element.update(element_id, "saving...");
    if (submit_form) {
      Element.hide('shelfDropdown_' + unique_id)
      var form = document.getElementById('addBookForm' + book_id)
      if (form) {
        form.shelf.value = shelf_name
        form.onsubmit()
      }
    }
    else {
      var action = checked ? 'remove' : ''
      element.checked = !element.checked
      new Ajax.Request('/shelf/add_to_shelf', {asynchronous:true, evalScripts:true, onSuccess:function(request){shelfSubmitted(request, book_id, checkbox_id, element_id, unique_id, shelf_name)}, parameters:'book_id=' + book_id + '&name=' + shelf_name + '&a=' + action + '&authenticity_token=' + encodeURIComponent('GtBUwzVmz8mY1DvbpnfXEm/t6bc+7ZhqvFB/LzQEFlGmoAoCYGULYpbsLGffZ7E8vR/0jOZcXCq12mCblsCIog==')})
    }
  }

  function shelfSubmitted(request, book_id, checkbox_id, element_id, unique_id, shelf_name) {
    Element.update('shelfListfalse_' + book_id, request.responseText)
    afterShelfSave(checkbox_id, element_id, unique_id, shelf_name.escapeHTML())
  }

  function refreshGroupBox(group_id, book_id) {
    new Ajax.Updater('addGroupBooks' + book_id + '', '/group/add_book_box', {asynchronous:true, evalScripts:true, onSuccess:function(request){refreshGroupBoxComplete(request, book_id);}, parameters:'id=' + group_id + '&book_id=' + book_id + '&refresh=true' + '&authenticity_token=' + encodeURIComponent('MNqFQP91OagXt+2wd74f/z0ngWuSyBoZB9RAUld9oq2MqtuBqnb9AxmP+gwOrnnR79WcUEp53lkOXl/m9bk8Xg==')})
  }
//]]>
</script>


<script>
//<![CDATA[
      var newTip = new Tip($('bookCover_34430911'), "\n\n  <h2><a class=\"readable bookTitle\" href=\"https://www.goodreads.com/book/show/34430911-peter-robinson-reading-order-and-checklist?from_choice=false&amp;from_home_module=false\">Peter Robinson Reading Order and Checklist: The complete guide to the Inspector Banks series, standalone novels and short stories<\/a><\/h2>\n\n      <div>\n        by <a class=\"authorName\" href=\"/author/show/16001628.CrimeLineUp?a=9&amp;origin=related_works\">CrimeLineUp<\/a>\n      <\/div>\n\n          <div class=\"smallText uitext darkGreyText\">\n            <span class=\"minirating\"><span class=\"stars staticStars notranslate\"><span size=\"12x12\" class=\"staticStar p10\"><\/span><span size=\"12x12\" class=\"staticStar p10\"><\/span><span size=\"12x12\" class=\"staticStar p10\"><\/span><span size=\"12x12\" class=\"staticStar p6\"><\/span><span size=\"12x12\" class=\"staticStar p0\"><\/span><\/span> 3.85 avg rating &mdash; 66 ratings<\/span>            &mdash; published \n          <\/div>\n\n    <div class=\"addBookTipDescription\">\n      \n<span id=\"freeTextContainer3484393360898301859\">Discover the great Inspector Banks series of crime novels by Peter Robinson with this handy guide. Peter Robinson has written nearly 30 books in a 30-year writing career, with the majority being focused on the cases and personal life of Inspector Ban<\/span>\n  <span id=\"freeText3484393360898301859\" style=\"display:none\">Discover the great Inspector Banks series of crime novels by Peter Robinson with this handy guide. Peter Robinson has written nearly 30 books in a 30-year writing career, with the majority being focused on the cases and personal life of Inspector Banks but there are also collections of short stories and a couple of stand-alone novels. Robinson has built up a cast of characters that shine among the stars of crime fiction. His books track the personal events as well as the police careers of Alan Banks and his co-workers and they are gripping stories with a great sense of place. \n  This book includes:  An Introduction to Peter Robinson. A list of novels by Peter Robinson in order of publication. Further reading about Peter Robinson, including links to interviews and resources. Reading checklist of novels by Peter Robinson in alphabetical order.  \n \n  You will receive free updates to this book when new Peter Robinson books are published. Please ensure you have the Update function enabled on your Kindle device or app.\n<\/span>\n  <a data-text-id=\"3484393360898301859\" href=\"#\" onclick=\"swapContent(\$(this));; return false;\">...more<\/a>\n\n    <\/div>\n\n      <div class=\'wtrButtonContainer wtrSignedOut\' id=\'8_book_34430911\'>\n<div class=\'wtrUp wtrLeft\'>\n<form action=\"/shelf/add_to_shelf\" accept-charset=\"UTF-8\" method=\"post\"><input name=\"utf8\" type=\"hidden\" value=\"&#x2713;\" /><input type=\"hidden\" name=\"authenticity_token\" value=\"BVa5Xdtmbs3xUY1fnNaVXANLDKY41dbXoJC9HT1GOg25JuecjmWqZv9pmuPlxvNy0bkRneBkEpepGqKpn4Kk/g==\" />\n<input type=\"hidden\" name=\"book_id\" id=\"book_id\" value=\"34430911\" />\n<input type=\"hidden\" name=\"name\" id=\"name\" value=\"to-read\" />\n<input type=\"hidden\" name=\"unique_id\" id=\"unique_id\" value=\"8_book_34430911\" />\n<input type=\"hidden\" name=\"wtr_new\" id=\"wtr_new\" value=\"true\" />\n<input type=\"hidden\" name=\"from_choice\" id=\"from_choice\" value=\"false\" />\n<input type=\"hidden\" name=\"from_home_module\" id=\"from_home_module\" value=\"false\" />\n<input type=\"hidden\" name=\"ref\" id=\"ref\" value=\"\" class=\"wtrLeftUpRef\" />\n<input type=\"hidden\" name=\"existing_review\" id=\"existing_review\" value=\"false\" class=\"wtrExisting\" />\n<input type=\"hidden\" name=\"book_origin\" id=\"book_origin\" value=\"related_works\" />\n<input type=\"hidden\" name=\"page_url\" id=\"page_url\" value=\"/book/show/102139.Gallows_View\" />\n<button class=\'wtrToRead\' type=\'submit\'>\n<span class=\'progress