      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_28208').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_28208').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_28208').observe('prototip:hidden', function () {
