        $('bookCover_799691').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_799691').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
