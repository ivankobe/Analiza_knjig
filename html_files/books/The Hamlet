        $('bookCover_434271').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_434271').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
