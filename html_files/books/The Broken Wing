        $('bookCover_570842').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_570842').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
