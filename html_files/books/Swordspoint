        $('bookCover_20002').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_20002').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
