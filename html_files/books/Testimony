        $('bookCover_229759').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_229759').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
