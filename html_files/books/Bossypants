      <a class="uitext" rel="nofollow" id="span_class_gr-hyperlink_more_filters_span_tip_751" style="cursor: pointer; " href="#" onclick="return false"><span class="gr-hyperlink">More filters</span></a><script>
//<![CDATA[
      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_751'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/9418327-bossypants?rating=\">all <span class=\"greyText\">(1523558)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/9418327-bossypants?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;vqheJXGafhOv49CEsqFp21BusJjiBAOHzUpAQR9cnGwjemr9tkUNkePFOW7/uq8l8P3PLrs4CR/lr4j4V47jZQ==&#39;)}); return false;\">5 stars <span class=\"greyText\">(317436)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/9418327-bossypants?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;SfHMKjy3FiDuQ0ZrgdTvNtNZMpEqWAxBEowYPbfgdU/UI/jy+2hloqJlr4HMzynIc8pNJ3NkBtk6adCE/zIKRg==&#39;)}); return false;\">4 stars <span class=\"greyText\">(326111)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span 