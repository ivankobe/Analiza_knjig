        $('bookCover_111113').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_111113').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
