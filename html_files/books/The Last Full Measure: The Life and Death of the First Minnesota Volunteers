        $('bookCover_29925').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_29925').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
