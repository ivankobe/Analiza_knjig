        $('bookCover_12873027').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_12873027').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
