omMiddle' }, hideOn: false, width: 400, hideAfter: 0.05, delay: 0.35 });
      $('bookCover_394734').observe('prototip:shown', function() {
        if (this.up('#box')) {
          $$('div.prototip').each(function(i){i.setStyle({zIndex: $('box').getStyle('z-index')})});
        } else {
          $$('div.prototip').each(function(i){i.setStyle({zIndex: 6000})});
