<div id="review_3855736990" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/3855736990" />
    <a title="Larry" class="left imgcol" href="/user/show/2166987-larry"><img alt="Larry" src="https://images.gr-assets.com/users/1578407338p2/2166987.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/3855736990?book_show_action=true">Mar 28, 2021</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Larry" class="user" itemprop="url" name="Larry" href="/user/show/2166987-larry">Larry</a>
