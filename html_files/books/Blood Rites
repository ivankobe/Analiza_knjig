      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_518'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/99383.Blood_Rites?rating=\">all <span class=\"greyText\">(165127)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/99383.Blood_Rites?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;R4Fpj3ZAJmb5mklb6FY1fRiE9MIQRAIR0C61by3Jp0Kct7ipJlctdtcaDewuaPttxQTiK2AoZ7W5QRNFSZnXpg==&#39;)}); return false;\">5 stars <span class=\"greyText\">(55274)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/99383.Blood_Rites?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;ROopVZLU0/6D5s0f0XE4ka+VqB1OoaUb6c99XF0bxRKf3PhzwsPY7q1miagXT/aBchW+9D7NwL+AoNt2OUu19g==&#39;)}); return false;\">4 stars <span class=\"greyText\">(47781)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=