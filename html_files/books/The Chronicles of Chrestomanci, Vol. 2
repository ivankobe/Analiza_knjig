        $('bookCover_86238').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_86238').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
