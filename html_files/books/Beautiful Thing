      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_55665503').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_55665503').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_55665503').observe('prototip:hidden', function () {
