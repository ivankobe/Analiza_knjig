<div id="review_553033553" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/553033553" />
    <a title="Dawn Wells" class="left imgcol" href="/user/show/11180984-dawn-wells"><img alt="Dawn Wells" src="https://images.gr-assets.com/users/1474316892p2/11180984.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/553033553?book_show_action=true">Mar 05, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Dawn Wells" class="user" itemprop="url" name="Dawn Wells" href="/user/show/11180984-dawn-wells">Dawn Wells</a>
