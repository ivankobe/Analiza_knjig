    <div class="js-dataTooltip" data-use-wtr-tooltip='true'>
        <div class="js-tooltipTrigger tooltipTrigger" data-resource-type='Book' data-resource-id='6867' data-placement='bottom-left'>
          <a href="/book/show/6867.Atonement"><img id="more_book_6867" alt="Atonement" title="Atonement" width="50" src="https://i.gr-assets.com/images/S/compressed.photo.goodreads.com/books/1320449708l/6867._SX98_.jpg" /></a>
