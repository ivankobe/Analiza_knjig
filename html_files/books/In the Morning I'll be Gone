        $('bookCover_8996717').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_8996717').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
