        $('bookCover_15810448').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_15810448').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
