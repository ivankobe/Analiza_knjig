      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_676'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/513275.Jakob_von_Gunten?rating=\">all <span class=\"greyText\">(14092)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/513275.Jakob_von_Gunten?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;pRzP4WOq3W2O0co8WYzwygujBSjU+XNrClYvmlF/h6Nq978UVEh+7urk/GssAEhzFol8cmJtiz5HRpNP4+KTkw==&#39;)}); return false;\">5 stars <span class=\"greyText\">(1458)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/513275.Jakob_von_Gunten?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;aqg/YUKoZBrHzzKGUGH/6SWG8tNlqykzB4qy+Ypqda2lQ0+UdUrHmaP6BNEl7UdQOKyLidM/0WZKmg4sOPdhnQ==&#39;)}); return false;\">4 stars <span class=\"greyText\">(1552)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n               