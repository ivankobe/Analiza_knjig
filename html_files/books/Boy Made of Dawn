        $('bookCover_38672738').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_38672738').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
