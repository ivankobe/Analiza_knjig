          $$('div.prototip').each(function (e) {
            if ($('bookCover_2414750').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_2414750').observe('prototip:hidden', function () {
