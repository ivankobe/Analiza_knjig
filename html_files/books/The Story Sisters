        $('bookCover_1167720').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1167720').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
