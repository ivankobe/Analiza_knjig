        $('bookCover_3321658').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_3321658').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
