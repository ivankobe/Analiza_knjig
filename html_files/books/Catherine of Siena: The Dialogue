        $('bookCover_754771').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_754771').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
