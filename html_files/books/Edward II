        $('bookCover_830073').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_830073').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
