        $('bookCover_36590405').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_36590405').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
