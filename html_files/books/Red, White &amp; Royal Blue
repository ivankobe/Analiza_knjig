        $('bookCover_32768522').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_32768522').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
