        $('bookCover_836746').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_836746').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
