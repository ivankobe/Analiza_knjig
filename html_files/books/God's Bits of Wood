      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_69'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/123748.God_s_Bits_of_Wood?rating=\">all <span class=\"greyText\">(8713)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/123748.God_s_Bits_of_Wood?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;r3my8Nv1xl2I17+JaOnp60MJeltn5U1zmUaUxDVtAH+DuY6Rx9v57pB1sP+Ys6E63UwJjWB2emjcT3ZfUvLZdQ==&#39;)}); return false;\">5 stars <span class=\"greyText\">(809)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/123748.God_s_Bits_of_Wood?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;rCnyXcNwStf5lA2rJXkjPO1bHbDt95zku9gqHdx/O9KA6c483151ZOE2At3VI2vtcx5uZupkq//+0ciGu+Di2A==&#39;)}); return false;\">4 stars <span class=\"greyText\">(968)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/123748.God_s_Bits_of_Wood?rating=3&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;sDONxRPH2vkeMMghvbAbZbSvACoeoLfTjwwT4JqGUIec87GkD+nlSgaSx1dN6lO0Kupz/BkzgMjKBfF7/RmJjQ==&#39;)}); return false;\">3 stars <span class=\"greyText\">(534)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/123748.God_s_Bits_of_Wood?rating=2&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;yU3ec92ZoiCjq/ETM+r4Ggw8wYf9XQBezw+B+mM6FJrljeISwbedk7sJ/mXDsLDLknmyUfrON0WKBmNhBKXNkA==&#39;)}); return false;\">2 stars <span class=\"greyText\">(119)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/123748.God_s_Bits_of_Wood?rating=1&#39;, {asynchronous:true, evalScripts:true, me