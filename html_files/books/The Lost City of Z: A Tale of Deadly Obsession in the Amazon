  <div data-react-class="ReactComponents.ShareDialog" data-react-props="{&quot;signedIn&quot;:false,&quot;signInUrl&quot;:&quot;/user/sign_in&quot;,&quot;social&quot;:{&quot;inModal&quot;:{&quot;facebook&quot;:true,&quot;twitter&quot;:true,&quot;pinterest&quot;:true,&quot;copyLink&quot;:false},&quot;outsideModal&quot;:{&quot;facebook&quot;:true,&quot;twitter&quot;:true,&quot;pinterest&quot;:true,&quot;copyLink&quot;:false},&quot;fbOptions&quot;:{&quot;name&quot;:&quot;The Lost City of Z: A Tale of Deadly Obsession in the Amazon by David Grann&quot;,&quot;caption&quot;:&quot;goodreads.com&quot;,&quot;description&quot;:&quot;A grand mystery reaching back centuries. A sensational disappearance that made headlines around the world. A quest for truth that leads to death, madness or disappearance for those who seek to solve it. The Lost City of Z is a blockbuster adventure narr...&quot;,&quot;link&quot;:&quot;https://www.goodreads.com/book/show/3398625-the-lost-city-of-z&quot;,&quot;picture&quot;:&quot;https://i.gr-assets.com/images/S/compressed.photo.goodreads.com/books/1320487318l/3398625._SY75_.jpg&quot;},&quot;twitterOptions&quot;:{&quot;text&quot;:&quot;The Lost City of Z: A Tale of Deadly Obsession in the Amazon by David Grann&quot;,&quot;url&quot;:&quot;https://www.goodreads.com/book/show/3398625-the-lost-city-of-z&quot;,&quot;via&quot;:&quot;goodreads&quot;},&quot;pinterestOptions&quot;:{&quot;description&quot;:&quot;The Lost City of Z: A Tale of Deadly Obsession in the Amazon by David Grann&quot;,&quot;url&quot;:&quot;https://www.goodreads.com/book/show/3398625-the-lost-city-of-z&quot;,&quot;media&quot;:&quot;https://i.gr-assets.com/images/S/compressed.photo.goodreads.com/books/1320487318l/3398625.jpg&quot;}},&quot;sharingData&quot;:{&quot;resourceTypeFriendlyText&quot;:&quot;book&quot;,&quot;resourceId&quot;:3398625},&quot;includePreview&quot;:true,&quot;previewData&quot;:{&quot;author&quot;:{&quot;name&quot;:&quot;David Grann&quot;,&quot;isGoodreadsAuthor&quot;