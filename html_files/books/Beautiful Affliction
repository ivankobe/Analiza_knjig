        $('bookCover_9146963').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_9146963').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
