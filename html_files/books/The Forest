        $('bookCover_25229231').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_25229231').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
