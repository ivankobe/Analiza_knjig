        $('bookCover_18944207').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_18944207').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
