        $('bookCover_15547').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_15547').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
