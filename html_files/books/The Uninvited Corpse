        $('bookCover_49296234').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_49296234').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
