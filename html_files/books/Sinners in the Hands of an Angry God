<div id="review_1040011242" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1040011242" />
    <a title="Sarah Marie" class="left imgcol" href="/user/show/6515737-sarah-marie"><img alt="Sarah Marie" src="https://images.gr-assets.com/users/1589089368p2/6515737.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1040011242?book_show_action=true">Aug 29, 2014</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Sarah Marie" class="user" itemprop="url" name="Sarah Marie" href="/user/show/6515737-sarah-marie">Sarah Marie</a>
