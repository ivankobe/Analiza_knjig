        $('bookCover_35729').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_35729').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
