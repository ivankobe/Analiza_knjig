        $('bookCover_122211').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_122211').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
