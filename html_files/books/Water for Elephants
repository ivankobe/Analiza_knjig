      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_19501').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_19501').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_19501').observe('prototip:hidden', function () {
