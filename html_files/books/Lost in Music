        $('bookCover_23529').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_23529').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
