<div id="review_1815311692" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1815311692" />
    <a title="Cyndi" class="left imgcol" href="/user/show/7132123-cyndi"><img alt="Cyndi" src="https://images.gr-assets.com/users/1510671513p2/7132123.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1815311692?book_show_action=true">Nov 18, 2016</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Cyndi" class="user" itemprop="url" name="Cyndi" href="/user/show/7132123-cyndi">Cyndi</a>
