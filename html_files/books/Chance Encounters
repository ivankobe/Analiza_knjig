        $('bookCover_55666161').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_55666161').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
