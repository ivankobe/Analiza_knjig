        $('bookCover_1192801').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1192801').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
