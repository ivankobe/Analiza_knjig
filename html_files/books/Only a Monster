        $('bookCover_52239058').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_52239058').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
