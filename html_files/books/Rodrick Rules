        $('bookCover_9807271').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_9807271').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
