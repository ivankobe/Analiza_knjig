        $('bookCover_438323').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_438323').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
