<div id="review_135789886" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/135789886" />
    <a title="OceMei Belikova♥" class="left imgcol" href="/user/show/2986527-ocemei-belikova"><img alt="OceMei Belikova♥" src="https://images.gr-assets.com/users/1470242190p2/2986527.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/135789886?book_show_action=true">Dec 19, 2010</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="OceMei Belikova♥" class="user" itemprop="url" name="OceMei Belikova♥" href="/user/show/2986527-ocemei-belikova">OceMei Belikova♥</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was amazing"><span size="15x15" class="staticStar p10">it was amazing</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/2986527-ocemei-belikova?shelf=ebooks">ebooks</a>, 
              <a class="actionLinkLite" href="/review/list/2986527-ocemei-belikova?shelf=read-in-2011">read-in-2011</a>, 
              <a class="actionLinkLite" href="/review/list/2986527-ocemei-belikova?shelf=ipod-touch">ipod-touch</a>, 
              <a class="actionLinkLite" href="/review/list/2986527-ocemei-belikova?shelf=young-adult">young-adult</a>
          </div>
