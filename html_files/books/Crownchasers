<div id="review_3299013507" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/3299013507" />
    <a title="Rebecca Coffindaffer" class="left imgcol" href="/user/show/6026840-rebecca-coffindaffer"><img alt="Rebecca Coffindaffer" src="https://images.gr-assets.com/users/1554838645p2/6026840.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/3299013507?book_show_action=true">Apr 30, 2020</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Rebecca Coffindaffer" class="user" itemprop="url" name="Rebecca Coffindaffer" href="/user/show/6026840-rebecca-coffindaffer">Rebecca Coffindaffer</a>
