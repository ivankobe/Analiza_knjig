        $('bookCover_547334').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_547334').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
