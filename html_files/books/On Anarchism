        $('bookCover_8524528').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_8524528').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
