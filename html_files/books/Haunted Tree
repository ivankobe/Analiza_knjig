        $('bookCover_28287908').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_28287908').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_28287908').observe('prototip:hidden', function () {
