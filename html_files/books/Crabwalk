      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_952'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/24077.Crabwalk?rating=\">all <span class=\"greyText\">(6664)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/24077.Crabwalk?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;B/6kaqxuJHOgp8e3euFpo4RzbUkIuoHyaxKhRLKyLhZ4250M1EiAO07jdl8/5vMuMJq6u4IKdTUuvc6+3PXOiw==&#39;)}); return false;\">5 stars <span class=\"greyText\">(633)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/24077.Crabwalk?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;Dg9s/NNQzWWyGU5rbODkOIci29Q/K/dAdyUwc0XZHMpxKlWaq3ZpLVxd/4Mp5361M8sMJrWbA4cyil+JK578Vw==&#39;)}); return false;\">4 stars <span class=\"greyText\">(1070)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError