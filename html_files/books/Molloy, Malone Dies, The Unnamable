        $('bookCover_76527').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_76527').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
