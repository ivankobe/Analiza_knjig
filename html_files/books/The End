        $('bookCover_57674439').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_57674439').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
