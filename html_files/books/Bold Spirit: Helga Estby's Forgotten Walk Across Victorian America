<div id="review_1037300364" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1037300364" />
    <a title="Ellis" class="left imgcol" href="/user/show/612579-ellis"><img alt="Ellis" src="https://images.gr-assets.com/users/1194823468p2/612579.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1037300364?book_show_action=true">Aug 26, 2014</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Ellis" class="user" itemprop="url" name="Ellis" href="/user/show/612579-ellis">Ellis</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="liked it"><span size="15x15" class="staticStar p10">liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/612579-ellis?shelf=culture">culture</a>, 
              <a class="actionLinkLite" href="/review/list/612579-ellis?shelf=nonfiction">nonfiction</a>, 
              <a class="actionLinkLite" href="/review/list/612579-ellis?shelf=biography">biography</a>, 
              <a class="actionLinkLite" href="/review/list/612579-ellis?shelf=history">history</a>, 
