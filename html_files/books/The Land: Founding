        $('bookCover_36348525').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_36348525').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
