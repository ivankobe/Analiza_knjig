        $('bookCover_34327194').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_34327194').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
