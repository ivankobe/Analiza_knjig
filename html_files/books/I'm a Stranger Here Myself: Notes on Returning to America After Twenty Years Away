<div id="review_1217124556" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1217124556" />
    <a title="Melki" class="left imgcol" href="/user/show/5293536-melki"><img alt="Melki" src="https://images.gr-assets.com/users/1308071306p2/5293536.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1217124556?book_show_action=true">Mar 03, 2015</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Melki" class="user" itemprop="url" name="Melki" href="/user/show/5293536-melki">Melki</a>
