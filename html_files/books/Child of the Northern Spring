        $('bookCover_1853145').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1853145').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
