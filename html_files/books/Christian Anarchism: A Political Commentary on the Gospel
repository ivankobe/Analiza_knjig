        $('bookCover_550165').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_550165').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
