<div id="review_2908476560" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2908476560" />
    <a title="Kaethe Douglas" class="left imgcol" href="/user/show/1072582-kaethe-douglas"><img alt="Kaethe Douglas" src="https://images.gr-assets.com/users/1573927565p2/1072582.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2908476560?book_show_action=true">Jul 23, 2019</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Kaethe Douglas" class="user" itemprop="url" name="Kaethe Douglas" href="/user/show/1072582-kaethe-douglas">Kaethe Douglas</a>
