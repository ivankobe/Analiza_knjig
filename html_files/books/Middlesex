        $('bookCover_187020').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_187020').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
