        $('bookCover_817965').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_817965').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
