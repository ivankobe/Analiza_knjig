        $('bookCover_380853').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_380853').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
