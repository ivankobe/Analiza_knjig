        $('bookCover_52022909').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_52022909').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
