        $('bookCover_7315573').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_7315573').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
