        $('bookCover_15801672').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_15801672').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
