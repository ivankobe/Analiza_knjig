  <div class="friendReviews elementListBrown" >
    <div class="section">
      

<div id="review_433375693" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/433375693" />
    <a title="Elevetha " class="left imgcol" href="/user/show/11828657-elevetha"><img alt="Elevetha " src="https://images.gr-assets.com/users/1461899805p2/11828657.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/433375693?book_show_action=true">Oct 13, 2012</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Elevetha " class="user" itemprop="url" name="Elevetha " href="/user/show/11828657-elevetha">Elevetha </a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was ok"><span size="15x15" class="staticStar p10">it was ok</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>

          <span class="greyText">&nbsp;&middot;&nbsp;</span>
          <a class="lightGreyText" title="Library Binding 9780062009500" href="/book/show/13623844-dodger">review of another edition</a>


        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/11828657-elevetha?shelf=nothing-special-here">nothing-special-here</a>, 
              <a class="actionLinkLite" href="/review/list/11828657-elevetha?shelf=images-involved-in-review">images-involved-in-review</a>, 
              <a class="actionLinkLite" href="/review/list/11828657-elevetha?shelf=2013">2013</a>, 
