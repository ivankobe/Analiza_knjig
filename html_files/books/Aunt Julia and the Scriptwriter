      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_822'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/77298.Aunt_Julia_and_the_Scriptwriter?rating=\">all <span class=\"greyText\">(31411)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/77298.Aunt_Julia_and_the_Scriptwriter?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;zhASBSCPRCqJViCsHn7/ZfA229kV+PAjJFBCU2vLT+qag5qPJ9o8UmlcnyCNeETXq8fFCryrFFDrCmTwPf8ZzQ==&#39;)}); return false;\">5 stars <span class=\"greyText\">(5029)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/77298.Aunt_Julia_and_the_Scriptwriter?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;IoYZsp6zK1ik7phvvKX0CFD1Q9QhczHhMJuJW0OlLSJ2FZE4meZTIETkJ+Mvo0+6CwRdB4gg1ZL/wa/4FZF7BQ==&#39;)}); return false;\">4 stars <span class=\"greyText\">(7024)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/77298.Aunt_Julia_and_the_Scriptwriter?rating=3&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;WMDGduks1PYZfH0xGwQwc7SJiIc+GI0c7Y7pG5hbhuAMU0787nmsjvl2wr2IAovB73iWVJdLaW8i1M+4zm/Qxw==&#39;)}); return false;\">3 stars <span class=\"greyText\">(3950)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/77298.Aunt_Julia_and_the_Scriptwriter?rating=2&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;+2hinU7Qel8UaQu7QJmTUX1XsncefYKVXjJM5fus0uqv++oXSYUCJ/RjtDfTnyjjJqaspLcuZuaRaGpGrZiEzQ==&#39;)}); return false;\">2 stars <span class=\"greyText\">(851)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/77298.Aunt_Julia