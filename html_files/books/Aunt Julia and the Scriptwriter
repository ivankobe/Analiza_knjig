        $('bookCover_17307267').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_17307267').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
