              <a class="actionLinkLite" href="/review/list/1022982-brad?shelf=out-loud">out-loud</a>, 
              <a class="actionLinkLite" href="/review/list/1022982-brad?shelf=read-in-2017">read-in-2017</a>, 
              <a class="actionLinkLite" href="/review/list/1022982-brad?shelf=read-to-milos">read-to-milos</a>, 
              <a class="actionLinkLite" href="/review/list/1022982-brad?shelf=read-to-scoutie">read-to-scoutie</a>, 
              <a class="actionLinkLite" href="/review/list/1022982-brad?shelf=children">children</a>, 
              <a class="actionLinkLite" href="/review/list/1022982-brad?shelf=fantasy">fantasy</a>, 
              <a class="actionLinkLite" href="/review/list/1022982-brad?shelf=read-to-bronte">read-to-bronte</a>, 
