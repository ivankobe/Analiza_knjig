        $('bookCover_10729216').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_10729216').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_10729216').observe('prototip:hidden', function () {
