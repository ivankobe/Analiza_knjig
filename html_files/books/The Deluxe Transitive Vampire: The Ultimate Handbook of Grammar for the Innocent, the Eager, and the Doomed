        $('bookCover_35520564').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_35520564').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
