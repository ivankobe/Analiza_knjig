        $('bookCover_32263').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_32263').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
