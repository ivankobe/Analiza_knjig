      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_126675').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_126675').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_126675').observe('prototip:hidden', function () {
