        $('bookCover_54592511').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_54592511').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
