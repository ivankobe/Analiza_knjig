<div id="review_3230350742" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/3230350742" />
    <a title="Jack Heath" class="left imgcol" href="/user/show/70983025-jack-heath"><img alt="Jack Heath" src="https://s.gr-assets.com/assets/nophoto/user/m_50x66-82093808bca726cb3249a493fbd3bd0f.png" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/3230350742?book_show_action=true">Mar 14, 2020</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Jack Heath" class="user" itemprop="url" name="Jack Heath" href="/user/show/70983025-jack-heath">Jack Heath</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was amazing"><span size="15x15" class="staticStar p10">it was amazing</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/70983025-jack-heath?shelf=category-f-mystery">category-f-mystery</a>, 
              <a class="actionLinkLite" href="/review/list/70983025-jack-heath?shelf=xx">xx</a>, 
              <a class="actionLinkLite" href="/review/list/70983025-jack-heath?shelf=other-j-characters">other-j-characters</a>, 
              <a class="actionLinkLite" href="/review/list/70983025-jack-heath?shelf=other-k-favourite">other-k-favourite</a>, 
              <a class="actionLinkLite" href="/review/list/70983025-jack-heath?shelf=other-m-following">other-m-following</a>, 
              <a class="actionLinkLite" href="/review/list/70983025-jack-heath?shelf=other-l-fifty">other-l-fifty</a>
          </div>
