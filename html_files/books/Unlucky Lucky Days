<div id="review_42384971" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/42384971" />
    <a title="Rhonda" class="left imgcol" href="/user/show/232229-rhonda"><img alt="Rhonda" src="https://images.gr-assets.com/users/1213119256p2/232229.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/42384971?book_show_action=true">Jan 08, 2009</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Rhonda" class="user" itemprop="url" name="Rhonda" href="/user/show/232229-rhonda">Rhonda</a>
