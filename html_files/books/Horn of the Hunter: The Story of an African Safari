        $('bookCover_762732').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_762732').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
