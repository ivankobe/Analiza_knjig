        $('bookCover_50458609').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_50458609').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
