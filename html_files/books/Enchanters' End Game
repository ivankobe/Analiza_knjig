      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_123690').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_123690').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_123690').observe('prototip:hidden', function () {
