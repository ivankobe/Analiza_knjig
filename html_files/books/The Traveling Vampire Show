<div id="review_243407528" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/243407528" />
    <a title="Char " class="left imgcol" href="/user/show/6581119-char"><img alt="Char " src="https://images.gr-assets.com/users/1415394568p2/6581119.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/243407528?book_show_action=true">Dec 03, 2018</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Char " class="user" itemprop="url" name="Char " href="/user/show/6581119-char">Char </a>
