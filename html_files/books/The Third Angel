<div id="review_2929999322" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2929999322" />
    <a title="Magdelanye" class="left imgcol" href="/user/show/5208681-magdelanye"><img alt="Magdelanye" src="https://images.gr-assets.com/users/1315558024p2/5208681.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2929999322?book_show_action=true">Aug 23, 2019</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Magdelanye" class="user" itemprop="url" name="Magdelanye" href="/user/show/5208681-magdelanye">Magdelanye</a>
