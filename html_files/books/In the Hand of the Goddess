            if ($('bookCover_3431323').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_3431323').observe('prototip:hidden', function () {
