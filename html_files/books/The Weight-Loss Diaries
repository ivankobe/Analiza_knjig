        $('bookCover_32470949').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_32470949').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
