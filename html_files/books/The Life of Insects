        $('bookCover_1567725').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1567725').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
