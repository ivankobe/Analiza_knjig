        $('bookCover_47763').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_47763').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
