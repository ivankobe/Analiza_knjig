        $('bookCover_166585').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_166585').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
