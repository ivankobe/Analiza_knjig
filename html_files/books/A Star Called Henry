      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_7507554').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_7507554').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_7507554').observe('prototip:hidden', function () {
