        $('bookCover_49085287').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_49085287').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
