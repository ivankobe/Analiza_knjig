        $('bookCover_29363942').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_29363942').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
