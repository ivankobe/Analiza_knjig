      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_188'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/6261189-unholy-ghosts?rating=\">all <span class=\"greyText\">(43342)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/6261189-unholy-ghosts?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;2iYsU/R+cHxPOohua+LXMNrr+1MAbnbYfr0nOiF1sQFRjrcA2EyNyfDwJFJ8vrtALYeIChcohwC78eKyJLLyXg==&#39;)}); return false;\">5 stars <span class=\"greyText\">(4809)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/6261189-unholy-ghosts?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;bYc0YJHns572hcHn5HUYY1nlXFBtCd8F1q7cNC0W/a7mL68zvdVOK0lPbdvzKXQTrokvCXpPLt0T4hm8KNG+8Q==&#39;)}); return false;\">4 stars <span class=\"greyText\">(5736)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/6261189-unholy-ghosts?rating=3&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;0cdTEx0RYBDcS7blTXYZxdHROOqHqwfQBo1tjzKruStab8hAMSOdpWOBGtlaKnW1Jr1Ls5Dt9gjDwagHN2z6dA==&#39;)}); return false;\">3 stars <span class=\"greyText\">(3884)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/6261189-unholy-ghosts?rating=2&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;t/aV6hqB0DIktktHTfR/aK2ljo03Q2ukwiTkLQDKRMc8Xg65NrMth5t853taqBMYWsn91CAFmnwHaCGlBQ0HmA==&#39;)}); return false;\">2 stars <span class=\"greyText\">(1658)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/6261189-unholy-ghosts?rating=1&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;