        $('bookCover_7491418').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_7491418').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
