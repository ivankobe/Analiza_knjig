      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Rex" class="user" itemprop="url" name="Rex" href="/user/show/32635507-rex">Rex</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was amazing"><span size="15x15" class="staticStar p10">it was amazing</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/32635507-rex?shelf=favorites">favorites</a>, 
              <a class="actionLinkLite" href="/review/list/32635507-rex?shelf=poetry">poetry</a>, 
              <a class="actionLinkLite" href="/review/list/32635507-rex?shelf=literature">literature</a>, 
              <a class="actionLinkLite" href="/review/list/32635507-rex?shelf=philosophy">philosophy</a>, 
              <a class="actionLinkLite" href="/review/list/32635507-rex?shelf=education">education</a>, 
