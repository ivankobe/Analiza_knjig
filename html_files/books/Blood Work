        $('bookCover_44422464').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_44422464').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
