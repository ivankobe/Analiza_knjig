        $('bookCover_42036538').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_42036538').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
