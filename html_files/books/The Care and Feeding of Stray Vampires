        $('bookCover_29432622').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_29432622').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
