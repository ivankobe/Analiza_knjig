      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_48759').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_48759').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_48759').observe('prototip:hidden', function () {
