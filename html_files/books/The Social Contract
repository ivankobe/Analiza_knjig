        $('bookCover_73945').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_73945').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
