        $('bookCover_16383').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_16383').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
