        $('bookCover_6694937').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_6694937').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
