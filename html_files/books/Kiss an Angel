      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_290'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/73086.Kiss_an_Angel?rating=\">all <span class=\"greyText\">(82651)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/73086.Kiss_an_Angel?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;lpAVU5UXPxmay4TDnyArFGSREmIhBzZq57oTzpy/iZSXmyx5HOtcJY5zM2UIW4gGumy7ANeYZeIbpY7lgIC6RA==&#39;)}); return false;\">5 stars <span class=\"greyText\">(25084)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/73086.Kiss_an_Angel?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;TOuaxCwf0BnSn2bHitR4loAny3wSibDe9wgXtpxzRqJN4KPupeOzJcYn0WEdr9uEXtpiHuQW41YLF4qdgEx1cg==&#39;)}); return false;\">4 stars <span class=\"greyText\">(15718)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span c