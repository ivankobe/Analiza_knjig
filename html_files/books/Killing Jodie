        $('bookCover_487445').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_487445').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
