        $('bookCover_613475').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_613475').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
