<div id="review_123200553" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/123200553" />
    <a title="AH" class="left imgcol" href="/user/show/3255548-ah"><img alt="AH" src="https://images.gr-assets.com/users/1275413869p2/3255548.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/123200553?book_show_action=true">Sep 24, 2010</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="AH" class="user" itemprop="url" name="AH" href="/user/show/3255548-ah">AH</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="really liked it"><span size="15x15" class="staticStar p10">really liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/3255548-ah?shelf=first-in-series">first-in-series</a>, 
              <a class="actionLinkLite" href="/review/list/3255548-ah?shelf=urban-fantasy">urban-fantasy</a>, 
              <a class="actionLinkLite" href="/review/list/3255548-ah?shelf=babr-reviews">babr-reviews</a>, 
              <a class="actionLinkLite" href="/review/list/3255548-ah?shelf=demons">demons</a>, 
              <a class="actionLinkLite" href="/review/list/3255548-ah?shelf=paranormal">paranormal</a>, 
              <a class="actionLinkLite" href="/review/list/3255548-ah?shelf=djinn">djinn</a>, 
              <a class="actionLinkLite" href="/review/list/3255548-ah?shelf=own">own</a>, 
