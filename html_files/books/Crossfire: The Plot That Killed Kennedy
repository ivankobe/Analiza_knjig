<div id="review_110062335" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/110062335" />
    <a title="Benjamin" class="left imgcol" href="/user/show/3032162-benjamin"><img alt="Benjamin" src="https://s.gr-assets.com/assets/nophoto/user/m_50x66-82093808bca726cb3249a493fbd3bd0f.png" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/110062335?book_show_action=true">Jul 04, 2010</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Benjamin" class="user" itemprop="url" name="Benjamin" rel="nofollow" href="/user/show/3032162-benjamin">Benjamin</a>
