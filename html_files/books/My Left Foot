        $('bookCover_9363889').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_9363889').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
