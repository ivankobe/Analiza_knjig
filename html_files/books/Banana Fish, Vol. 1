        $('bookCover_45372679').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_45372679').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
