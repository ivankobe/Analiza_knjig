      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_18518321').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_18518321').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_18518321').observe('prototip:hidden', function () {
