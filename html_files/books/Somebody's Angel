      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_21691436').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_21691436').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_21691436').observe('prototip:hidden', function () {
