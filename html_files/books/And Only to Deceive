      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_267869').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_267869').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_267869').observe('prototip:hidden', function () {
