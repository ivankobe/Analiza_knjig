        $('bookCover_142540').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_142540').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_142540').observe('prototip:hidden', function () {
