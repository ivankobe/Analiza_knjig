        $('bookCover_11466').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_11466').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
