        $('bookCover_158944').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_158944').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
