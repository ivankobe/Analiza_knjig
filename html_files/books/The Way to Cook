        $('bookCover_1191').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1191').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
