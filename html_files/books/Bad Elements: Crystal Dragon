        $('bookCover_22554393').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_22554393').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
