        $('bookCover_357209').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_357209').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
