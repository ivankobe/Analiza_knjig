        $('bookCover_1309267').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1309267').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
