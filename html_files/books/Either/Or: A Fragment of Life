        $('bookCover_118317').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_118317').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
