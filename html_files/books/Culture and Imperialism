        $('bookCover_40630').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_40630').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
