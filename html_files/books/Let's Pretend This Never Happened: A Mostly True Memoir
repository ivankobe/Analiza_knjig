        $('bookCover_10176').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_10176').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
