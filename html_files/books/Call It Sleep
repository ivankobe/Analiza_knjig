        $('bookCover_28937').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_28937').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
