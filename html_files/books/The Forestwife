        $('bookCover_56476180').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_56476180').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
