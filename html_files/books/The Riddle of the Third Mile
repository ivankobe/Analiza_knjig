        $('bookCover_37538').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_37538').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
