        $('bookCover_846033').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_846033').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
