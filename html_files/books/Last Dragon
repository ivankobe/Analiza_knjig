<div id="review_2527953605" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2527953605" />
    <a title="Carlsagansghost" class="left imgcol" href="/user/show/86676210-carlsagansghost"><img alt="Carlsagansghost" src="https://images.gr-assets.com/users/1536771036p2/86676210.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2527953605?book_show_action=true">Sep 12, 2018</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Carlsagansghost" class="user" itemprop="url" name="Carlsagansghost" href="/user/show/86676210-carlsagansghost">Carlsagansghost</a>
