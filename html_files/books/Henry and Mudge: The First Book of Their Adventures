        $('bookCover_202041').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_202041').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
