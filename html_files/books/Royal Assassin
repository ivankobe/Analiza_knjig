      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_2890090').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_2890090').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_2890090').observe('prototip:hidden', function () {
