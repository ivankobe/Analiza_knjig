      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_810881').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_810881').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_810881').observe('prototip:hidden', function () {
