        $('bookCover_40949958').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_40949958').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
