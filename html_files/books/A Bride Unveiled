        $('bookCover_54817569').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_54817569').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
