      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_11978910').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_11978910').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_11978910').observe('prototip:hidden', function () {
