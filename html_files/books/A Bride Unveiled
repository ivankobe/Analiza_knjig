      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_71655').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_71655').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_71655').observe('prototip:hidden', function () {
