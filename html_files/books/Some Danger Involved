      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_55522008').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_55522008').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_55522008').observe('prototip:hidden', function () {
