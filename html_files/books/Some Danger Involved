        $('bookCover_23014611').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_23014611').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
