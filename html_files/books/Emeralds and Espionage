      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_635'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/1359418.Emeralds_and_Espionage?rating=\">all <span class=\"greyText\">(3288)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/1359418.Emeralds_and_Espionage?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;5QfT1M0v0daWc0Y4ZJmE2F7j19di0vRLGp+qR97I8hBXqQaIWcSaP4vvP/SrM5IxhOiBEnH2a8jbmYwWEcAXNw==&#39;)}); return false;\">5 stars <span class=\"greyText\">(914)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/1359418.Emeralds_and_Espionage?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;lpQtz/sptc0IW7JurvrO4hnZ/cHLF/ke7/4Yvn5ynHskOviTb8L+JBXHy6JhUNgLw9KrBNgzZp0u+D7vsXp5XA==&#39;)}); return false;\">4 stars <span class=\"greyText\">(749)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span