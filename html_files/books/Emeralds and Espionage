        $('bookCover_55527581').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_55527581').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
