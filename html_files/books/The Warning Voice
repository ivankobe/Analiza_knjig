        $('bookCover_139874').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_139874').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
