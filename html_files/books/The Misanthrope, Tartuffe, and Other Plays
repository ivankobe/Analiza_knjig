    <div class="js-dataTooltip" data-use-wtr-tooltip='true'>
        <div class="js-tooltipTrigger tooltipTrigger" data-resource-type='Book' data-resource-id='52823' data-placement='bottom-left'>
          <a href="/book/show/52823.Tartuffe"><img id="more_book_52823" alt="Tartuffe" title="Tartuffe" width="50" src="https://i.gr-assets.com/images/S/compressed.photo.goodreads.com/books/1319501006l/52823._SX98_.jpg" /></a>
