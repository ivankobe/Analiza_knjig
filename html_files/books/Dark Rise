        $('bookCover_41951626').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_41951626').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
