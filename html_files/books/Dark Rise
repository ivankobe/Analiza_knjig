      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_31389365').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_31389365').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_31389365').observe('prototip:hidden', function () {
