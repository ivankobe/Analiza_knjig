        $('bookCover_2794').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_2794').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
