        $('bookCover_57907191').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_57907191').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_57907191').observe('prototip:hidden', function () {
        $$('span.elementTwo').each(function (e) {
          if (e.getStyle('display') !== 'none') {
