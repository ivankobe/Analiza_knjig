        $('bookCover_151620').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_151620').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
