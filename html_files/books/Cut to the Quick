        $('bookCover_311162').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_311162').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
