        $('bookCover_119240').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_119240').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
