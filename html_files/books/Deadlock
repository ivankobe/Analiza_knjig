<div id="review_2500499615" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2500499615" />
    <a title="Helen" class="left imgcol" href="/user/show/15475791-helen"><img alt="Helen" src="https://images.gr-assets.com/users/1571495476p2/15475791.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2500499615?book_show_action=true">Aug 20, 2018</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Helen" class="user" itemprop="url" name="Helen" href="/user/show/15475791-helen">Helen</a>
