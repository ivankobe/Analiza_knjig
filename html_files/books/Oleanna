        $('bookCover_435095').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_435095').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
