        $('bookCover_213667').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_213667').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
