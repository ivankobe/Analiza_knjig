      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_6186357').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_6186357').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_6186357').observe('prototip:hidden', function () {
