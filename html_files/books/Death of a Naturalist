        $('bookCover_395090').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_395090').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
