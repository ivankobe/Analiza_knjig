        $('bookCover_1391333').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1391333').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
