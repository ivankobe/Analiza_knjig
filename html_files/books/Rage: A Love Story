        $('bookCover_40801157').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_40801157').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_40801157').observe('prototip:hidden', function () {
