        $('bookCover_32827').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_32827').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
