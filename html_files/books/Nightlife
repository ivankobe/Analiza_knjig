  <div class="friendReviews elementListBrown" >
    <div class="section">
      

<div id="review_10890822" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/10890822" />
    <a title="Dara" class="left imgcol" href="/user/show/162098-dara"><img alt="Dara" src="https://images.gr-assets.com/users/1184399471p2/162098.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/10890822?book_show_action=true">Dec 22, 2007</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Dara" class="user" itemprop="url" name="Dara" href="/user/show/162098-dara">Dara</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was ok"><span size="15x15" class="staticStar p10">it was ok</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>



        

