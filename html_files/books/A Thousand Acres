        $('bookCover_77554').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_77554').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
