        $('bookCover_25508114').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_25508114').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
