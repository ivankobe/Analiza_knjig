        $('bookCover_53152636').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_53152636').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
