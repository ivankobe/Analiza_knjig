        $('bookCover_52838845').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_52838845').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
