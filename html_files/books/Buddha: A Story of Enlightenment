<div id="review_1707447687" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1707447687" />
    <a title="Kayleigh" class="left imgcol" href="/user/show/33184365-kayleigh"><img alt="Kayleigh" src="https://images.gr-assets.com/users/1619190511p2/33184365.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1707447687?book_show_action=true">May 24, 2019</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Kayleigh" class="user" itemprop="url" name="Kayleigh" href="/user/show/33184365-kayleigh">Kayleigh</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="liked it"><span size="15x15" class="staticStar p10">liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>



        

