  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/566516090?book_show_action=true">Mar 20, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Jenelle" class="user" itemprop="url" name="Jenelle" href="/user/show/105360-jenelle">Jenelle</a>
