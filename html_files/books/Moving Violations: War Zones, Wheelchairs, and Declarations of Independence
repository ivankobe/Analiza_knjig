        $('bookCover_3686').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_3686').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
