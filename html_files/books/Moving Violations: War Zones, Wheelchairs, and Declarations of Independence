        $('bookCover_19661').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_19661').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
