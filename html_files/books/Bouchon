<div id="review_516709011" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/516709011" />
    <a title="Matt" class="left imgcol" href="/user/show/7059657-matt"><img alt="Matt" src="https://s.gr-assets.com/assets/nophoto/user/m_50x66-82093808bca726cb3249a493fbd3bd0f.png" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/516709011?book_show_action=true">Jan 25, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Matt" class="user" itemprop="url" name="Matt" href="/user/show/7059657-matt">Matt</a>
      </span>

