        $('bookCover_52578294').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_52578294').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
