      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_126'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/561909.The_Hiding_Place?rating=\">all <span class=\"greyText\">(455752)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/561909.The_Hiding_Place?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;D6DQ+7eK6OLIDjCksBHf9EFmOuaEg5Sfq1FZ/1gNwX6qOmGTOt+5y1UZ5QljCZucscPXX07QWcL0D26oKntMvw==&#39;)}); return false;\">5 stars <span class=\"greyText\">(174996)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/561909.The_Hiding_Place?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;lSl3OfSmW4fxLxiwjQD8c/xpByXmeKneQS6KjpmVvF0ws8ZRefMKrmw4zR1eGLgbDMzqnCwrZIMecL3Z6+MxnA==&#39;)}); return false;\">4 stars <span class=\"greyText\">(65508)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n           