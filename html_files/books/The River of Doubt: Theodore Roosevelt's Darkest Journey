      $('bookCover_56389340').observe('prototip:shown', function() {
        if (this.up('#box')) {
          $$('div.prototip').each(function(i){i.setStyle({zIndex: $('box').getStyle('z-index')})});
        } else {
          $$('div.prototip').each(function(i){i.setStyle({zIndex: 6000})});
        }
      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_56389340').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_56389340').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_56389340').observe('prototip:hidden', function () {
        $$('span.elementTwo').each(function (e) {
          if (e.getStyle('display') !== 'none') {
            var lessLink = e.next();
            swapContent(lessLink);
          }
        });
      });

//]]>
</script>

</li>
<li class='cover' id='bookCover_9938498'>
<a href="https://www.goodreads.com/book/show/9938498-in-the-garden-of-beasts"><img alt="In the Garden of Beasts: Love, Terror, and an American Family in Hitler&#39;s Berlin" src="https://i.gr-assets.com/images/S/compressed.photo.goodreads.com/books/1327864303l/9938498._SX98_.jpg" />
