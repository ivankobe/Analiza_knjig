      <a class="uitext" rel="nofollow" id="span_class_gr-hyperlink_more_filters_span_tip_757" style="cursor: pointer; " href="#" onclick="return false"><span class="gr-hyperlink">More filters</span></a><script>
//<![CDATA[
      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_757'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/6101718-the-magicians?rating=\">all <span class=\"greyText\">(493580)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/6101718-the-magicians?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;QR8hNaGsnLfgAxKJdqAmn2hHfxYM/vvdQnI8TkFmu/O4UjUTILe8EgdyVU7RDOAuDw5hQKm2RSefh11JcZcd/Q==&#39;)}); return false;\">5 stars <span class=\"greyText\">(56881)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/6101718-the-magicians?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;PICpIoq8g+foQ+ZFcO/RS6KES67uZH7MN7W1EfqZL5jFzb0EC6ejQg8yoYLXQxf6xc1V+EsswDbqQNQWymiJlg==&#39;)}); return false;\">4 stars <span class=\"greyText\">(78256)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  