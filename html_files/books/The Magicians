        $('bookCover_48677404').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_48677404').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
