        $('bookCover_6482837').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_6482837').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
