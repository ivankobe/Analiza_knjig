        $('bookCover_16280678').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_16280678').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
