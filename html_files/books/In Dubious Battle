        $('bookCover_820461').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_820461').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
