        $('bookCover_349129').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_349129').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
