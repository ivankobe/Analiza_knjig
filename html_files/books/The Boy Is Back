        $('bookCover_54303829').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_54303829').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
