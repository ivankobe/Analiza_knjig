        $('bookCover_485995').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_485995').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
