        $('edition_13032093').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('edition_13032093').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
