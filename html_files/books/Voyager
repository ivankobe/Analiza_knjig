<strong id="timer_325131" class="countdownText">a</strong>

<script type="text/javascript" charset="utf-8">
//<![CDATA[
  var timer_325131_end_at = 1514857 + new Date().getTime()/1000;

  function timer_325131_updateTimer() {
    var time_left = "";
    var secs_left = timer_325131_end_at - new Date().getTime()/1000;

    if(secs_left <= 0) {
      document.getElementById("timer_325131").innerHTML = "closed";
      clearInterval(timer_325131_updater);
      return;
    }

    var minutes_left = secs_left / 60;
    var hours_left = minutes_left / 60;
    var days_left = Math.floor(hours_left / 24);

    if(days_left > 0) {
      if(false){
        time_left += days_left + ":";
      }else{
        time_left += days_left + " days and ";
      }
    }
    if(false){
      time_left += Math.floor(hours_left%24/10)
      time_left += Math.floor(hours_left%24)%10 + ":";
    }else{
      time_left += Math.floor(hours_left%24) + ":";
    }
    time_left += Math.floor(minutes_left%60/10);
    time_left += Math.floor(minutes_left%10) + ":";
    time_left += Math.floor(secs_left%60/10);
    time_left += Math.floor(secs_left%10);

    document.getElementById("timer_325131").innerHTML = time_left;
  }

  timer_325131_updateTimer();
  var timer_325131_updater = setInterval(timer_325131_updateTimer, 100);
//]]>
</script>
