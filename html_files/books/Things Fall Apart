      <a class="uitext" rel="nofollow" id="span_class_gr-hyperlink_more_filters_span_tip_612" style="cursor: pointer; " href="#" onclick="return false"><span class="gr-hyperlink">More filters</span></a><script>
//<![CDATA[
      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_612'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/37781.Things_Fall_Apart?rating=\">all <span class=\"greyText\">(567703)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/37781.Things_Fall_Apart?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;JDstO1DluI1Py08F+Dc89FAz4z6kS9gnZJn3YMjwOMZTNa912xApcUkGg9/Wu2BCWT+sflGvNVclQrqq8Legxg==&#39;)}); return false;\">5 stars <span class=\"greyText\">(78573)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/37781.Things_Fall_Apart?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;k2DE+7D1T9iEaLsN0m7/nJNaq5liGNgqCAG7CSuC5wLkbka1OwDeJIKld9f84qMqmlbk2Zf8NVpJ2vbDE8V/Ag==&#39;)}); return false;\">4 stars <span class=\"greyText\">(111865)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n           