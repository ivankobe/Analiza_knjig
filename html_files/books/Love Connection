        $('bookCover_56091294').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_56091294').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
