        $('bookCover_6892870').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_6892870').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
