      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_57667188').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_57667188').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_57667188').observe('prototip:hidden', function () {
