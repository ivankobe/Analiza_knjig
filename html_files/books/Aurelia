        $('bookCover_3751593').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_3751593').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
