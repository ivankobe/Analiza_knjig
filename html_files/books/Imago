        $('bookCover_45154547').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_45154547').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
