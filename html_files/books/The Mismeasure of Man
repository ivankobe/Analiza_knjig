      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_135'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/54218.The_Mismeasure_of_Man?rating=\">all <span class=\"greyText\">(25913)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/54218.The_Mismeasure_of_Man?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;OAPNTX5ilUC0BZ9XAMrKR1iBUJ8LfIe3NRSBIYN7lp83rbgKZmRMBt+wkiB0q5viMSDaDc+Aj8i7Bh1X8j7Wlg==&#39;)}); return false;\">5 stars <span class=\"greyText\">(3184)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/54218.The_Mismeasure_of_Man?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;xE5H2qRJKrdIult5QQuaXL/HvgUMaf+UtIKCqTsp8x/L4DKdvE/z8SMPVg41asv51mY0l8iV9+s6kB7fSmyzFg==&#39;)}); return false;\">4 stars <span class=\"greyText\">(3021)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n   