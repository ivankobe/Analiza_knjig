<div id="review_2543102162" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2543102162" />
    <a title="Mehrsa" class="left imgcol" href="/user/show/1045774-mehrsa"><img alt="Mehrsa" src="https://images.gr-assets.com/users/1218672696p2/1045774.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2543102162?book_show_action=true">Sep 26, 2018</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Mehrsa" class="user" itemprop="url" name="Mehrsa" href="/user/show/1045774-mehrsa">Mehrsa</a>
