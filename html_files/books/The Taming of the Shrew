        $('bookCover_754158').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_754158').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
