      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_562'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/492920.Sharpe_s_Sword?rating=\">all <span class=\"greyText\">(13178)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/492920.Sharpe_s_Sword?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;3deOt1F2yctomO9RD2oIlXxbZuv/F19fns7q/9KJOhJ1W34D/1BmFOESuEsiYl6MufJ+mlk0hjxrsTKBJG2Ubw==&#39;)}); return false;\">5 stars <span class=\"greyText\">(3670)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/492920.Sharpe_s_Sword?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;DGt8NusngKIkhNZWarfbdwKqAOCJ+euQDdyX/Y9sMuek54yCRQEvfa0OgUxHv41uxwMYkS/aMvP4o0+DeYicmg==&#39;)}); return false;\">4 stars <span class=\"greyText\">(3723)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <sp