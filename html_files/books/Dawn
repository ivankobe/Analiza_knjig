<div id="review_2111204963" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2111204963" />
    <a title="Beverly" class="left imgcol" href="/user/show/68708752-beverly"><img alt="Beverly" src="https://images.gr-assets.com/users/1505852430p2/68708752.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2111204963?book_show_action=true">Dec 31, 2017</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Beverly" class="user" itemprop="url" name="Beverly" href="/user/show/68708752-beverly">Beverly</a>
