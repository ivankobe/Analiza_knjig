        $('bookCover_56676848').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_56676848').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
