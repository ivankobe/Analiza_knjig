<div id="review_1341667038" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1341667038" />
    <a title="Mary" class="left imgcol" href="/user/show/25340667-mary"><img alt="Mary" src="https://images.gr-assets.com/users/1437502501p2/25340667.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1341667038?book_show_action=true">Jul 21, 2015</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Mary" class="user" itemprop="url" name="Mary" href="/user/show/25340667-mary">Mary</a>
