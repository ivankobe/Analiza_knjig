        $('bookCover_57484665').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_57484665').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
