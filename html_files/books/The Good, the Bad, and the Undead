        $('bookCover_30263').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_30263').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
