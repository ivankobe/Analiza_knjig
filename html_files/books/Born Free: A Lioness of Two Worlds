<div id="review_2236647101" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2236647101" />
    <a title="LibraryCin" class="left imgcol" href="/user/show/12860715-librarycin"><img alt="LibraryCin" src="https://images.gr-assets.com/users/1610600349p2/12860715.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2236647101?book_show_action=true">Jan 01, 2018</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="LibraryCin" class="user" itemprop="url" name="LibraryCin" href="/user/show/12860715-librarycin">LibraryCin</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="really liked it"><span size="15x15" class="staticStar p10">really liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/12860715-librarycin?shelf=africa">africa</a>, 
              <a class="actionLinkLite" href="/review/list/12860715-librarycin?shelf=biography">biography</a>, 
              <a class="actionLinkLite" href="/review/list/12860715-librarycin?shelf=animals">animals</a>, 
              <a class="actionLinkLite" href="/review/list/12860715-librarycin?shelf=lions">lions</a>
          </div>
