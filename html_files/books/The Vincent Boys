<div id="review_520251642" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/520251642" />
    <a title="Beverly" class="left imgcol" href="/user/show/13095060-beverly"><img alt="Beverly" src="https://images.gr-assets.com/users/1466964369p2/13095060.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/520251642?book_show_action=true">Jan 28, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Beverly" class="user" itemprop="url" name="Beverly" href="/user/show/13095060-beverly">Beverly</a>
