      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_1917858').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1917858').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_1917858').observe('prototip:hidden', function () {
