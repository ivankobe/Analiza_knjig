<div id="review_786517143" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/786517143" />
    <a title="Lady ♥ Belleza" class="left imgcol" href="/user/show/7773122-lady-belleza"><img alt="Lady ♥ Belleza" src="https://images.gr-assets.com/users/1520522061p2/7773122.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/786517143?book_show_action=true">Dec 10, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Lady ♥ Belleza" class="user" itemprop="url" name="Lady ♥ Belleza" href="/user/show/7773122-lady-belleza">Lady ♥ Belleza</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="liked it"><span size="15x15" class="staticStar p10">liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/7773122-lady-belleza?shelf=science-fiction">science-fiction</a>, 
              <a class="actionLinkLite" href="/review/list/7773122-lady-belleza?shelf=ebook">ebook</a>, 
              <a class="actionLinkLite" href="/review/list/7773122-lady-belleza?shelf=fantasy">fantasy</a>, 
              <a class="actionLinkLite" href="/review/list/7773122-lady-belleza?shelf=fiction">fiction</a>, 
              <a class="actionLinkLite" href="/review/list/7773122-lady-belleza?shelf=series">series</a>, 
              <a class="actionLinkLite" href="/review/list/7773122-lady-belleza?shelf=library-book">library-book</a>, 
              <a class="actionLinkLite" href="/review/list/7773122-lady-belleza?shelf=reviewed">reviewed</a>
          </div>
