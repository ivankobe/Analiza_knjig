<div id="review_1984881340" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1984881340" />
    <a title="Thenia" class="left imgcol" href="/user/show/1216094-thenia"><img alt="Thenia" src="https://images.gr-assets.com/users/1274391786p2/1216094.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1984881340?book_show_action=true">Apr 28, 2017</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Thenia" class="user" itemprop="url" name="Thenia" href="/user/show/1216094-thenia">Thenia</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="liked it"><span size="15x15" class="staticStar p10">liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/1216094-thenia?shelf=reviewed">reviewed</a>, 
              <a class="actionLinkLite" href="/review/list/1216094-thenia?shelf=paranormal">paranormal</a>, 
              <a class="actionLinkLite" href="/review/list/1216094-thenia?shelf=series">series</a>, 
              <a class="actionLinkLite" href="/review/list/1216094-thenia?shelf=2018">2018</a>, 
              <a class="actionLinkLite" href="/review/list/1216094-thenia?shelf=r-ab">r-ab</a>, 
              <a class="actionLinkLite" href="/review/list/1216094-thenia?shelf=ya-na">ya-na</a>
