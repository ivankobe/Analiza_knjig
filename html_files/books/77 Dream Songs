        $('bookCover_6307034').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_6307034').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
