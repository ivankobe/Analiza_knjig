      var newTip = new Tip($('rating_details_tip'), "\n        <div id=\"moreBookData\" class=\"bookMeta\" >\n  \n\n  \n\n<table border=\"0\" id=\"rating_distribution\" style=\"width: auto; max-width: 500px;\" cellspacing=\"5\" cellpadding=\"0\">\n      <tr>\n        <th width=\"25\" class=\"right\">5&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 66%;\" title=\"34 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/1820849.As_Is?rating=5\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'yek9hfBBWyF0GKR+gkNr+J9Z5a8BTsLchKT+BnHZ2GhJMVbBezIiJmnUDL/ShvhUiDFUXG4AnB0ZnDGPwDDhng==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          25% (34)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">4&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 100%;\" title=\"51 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/1820849.As_Is?rating=4\', {asynchronous:true, evalScripts:true, method:\'get\', parameters:\'authenticity_token=\' + encodeURIComponent(\'qpoVcUgxfsrKtwX+B+4Ii+j/py768M0iZhWGeRt4z+YqQn41w0IHzdd7rT9XK5sn/5cW3ZW+k+P7LUnwqpH2EA==\')})\" data-remote=\"true\">&nbsp;<\/a>\n        <\/td>\n        <td width=\"90\">\n          38% (51)\n        <\/td>\n      <\/tr>\n      <tr>\n        <th width=\"25\" class=\"right\">3&nbsp;<a size=\"12x12\" class=\"staticStar p10\" style=\"float: right;\"><\/a><\/th>\n        <td width=\"350\">\n          <a style=\"display: block; background-color: #215625; width: 76%;\" title=\"39 ratings\" href=\"#\" onclick=\"new Ajax.Request(\'/book/reviews/1820849.As_Is?rating=3\', {asynchronous:true, evalScripts:true, me