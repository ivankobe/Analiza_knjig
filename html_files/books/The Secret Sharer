<div id="review_2581005055" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2581005055" />
    <a title="Haaze" class="left imgcol" href="/user/show/6941528-haaze"><img alt="Haaze" src="https://images.gr-assets.com/users/1591937826p2/6941528.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2581005055?book_show_action=true">Apr 29, 2020</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Haaze" class="user" itemprop="url" name="Haaze" href="/user/show/6941528-haaze">Haaze</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was amazing"><span size="15x15" class="staticStar p10">it was amazing</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span></span>

          <span class="greyText">&nbsp;&middot;&nbsp;</span>
          <a class="lightGreyText" title="Paperback 9780146001482" href="/book/show/3560468-the-secret-sharer">review of another edition</a>


        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/6941528-haaze?shelf=lit-english">lit-english</a>, 
              <a class="actionLinkLite" href="/review/list/6941528-haaze?shelf=x_conrad">x_conrad</a>, 
              <a class="actionLinkLite" href="/review/list/6941528-haaze?shelf=sea">sea</a>, 
              <a class="actionLinkLite" href="/review/list/6941528-haaze?shelf=literature-20th-century">literature-20th-century</a>, 
              <a class="actionLinkLite" href="/review/list/6941528-haaze?shelf=rec_lebr">rec_lebr</a>, 
              <a class="actionLinkLite" href="/review/list/6941528-haaze?shelf=2020">2020</a>
          </div>
