        $('bookCover_1244288').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1244288').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
