      });

      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_285438').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_285438').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_285438').observe('prototip:hidden', function () {
