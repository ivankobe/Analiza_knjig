        $('bookCover_52735921').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_52735921').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
