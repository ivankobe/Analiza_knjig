<div id="review_773362377" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/773362377" />
    <a title="Lady ♥ Belleza" class="left imgcol" href="/user/show/7773122-lady-belleza"><img alt="Lady ♥ Belleza" src="https://images.gr-assets.com/users/1520522061p2/7773122.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/773362377?book_show_action=true">Nov 24, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Lady ♥ Belleza" class="user" itemprop="url" name="Lady ♥ Belleza" href="/user/show/7773122-lady-belleza">Lady ♥ Belleza</a>
