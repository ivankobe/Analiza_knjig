<div id="review_656003278" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/656003278" />
    <a title="pocket calculator" class="left imgcol" href="/user/show/18240543-pocket-calculator"><img alt="pocket calculator" src="https://images.gr-assets.com/users/1606941090p2/18240543.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/656003278?book_show_action=true">Jul 01, 2013</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="pocket calculator" class="user" itemprop="url" name="pocket calculator" href="/user/show/18240543-pocket-calculator">pocket calculator</a>
