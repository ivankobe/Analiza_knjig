        $('bookCover_654258').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_654258').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_654258').observe('prototip:hidden', function () {
