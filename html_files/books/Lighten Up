        $('bookCover_45186376').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_45186376').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_45186376').observe('prototip:hidden', function () {
