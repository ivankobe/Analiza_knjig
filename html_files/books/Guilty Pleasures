<div id="review_236623043" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/236623043" />
    <a title="Shannon" class="left imgcol" href="/user/show/6773727-shannon"><img alt="Shannon" src="https://images.gr-assets.com/users/1579036502p2/6773727.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/236623043?book_show_action=true">Nov 17, 2011</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Shannon" class="user" itemprop="url" name="Shannon" href="/user/show/6773727-shannon">Shannon</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="it was amazing"><span size="15x15" class="staticStar p10">it was amazing</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/6773727-shannon?shelf=fantasy-scifi">fantasy-scifi</a>, 
              <a class="actionLinkLite" href="/review/list/6773727-shannon?shelf=action">action</a>, 
              <a class="actionLinkLite" href="/review/list/6773727-shannon?shelf=books-reviewed">books-reviewed</a>, 
              <a class="actionLinkLite" href="/review/list/6773727-shannon?shelf=horror">horror</a>, 
              <a class="actionLinkLite" href="/review/list/6773727-shannon?shelf=reviews-that-need-pics-added">reviews-that-need-pics-added</a>, 
              <a class="actionLinkLite" href="/review/list/6773727-shannon?shelf=my-very-best-reads">my-very-best-reads</a>, 
              <a class="actionLinkLite" href="/review/list/6773727-shannon?shelf=owned">owned</a>, 
              <a class="actionLinkLite" href="/review/list/6773727-shannon?shelf=thrillers">thrillers</a>
          </div>
