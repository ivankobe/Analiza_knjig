        $('bookCover_7292791').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_7292791').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
