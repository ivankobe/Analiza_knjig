        $('bookCover_54511226').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_54511226').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
