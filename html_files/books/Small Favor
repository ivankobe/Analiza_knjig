        $('bookCover_1773616').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1773616').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
