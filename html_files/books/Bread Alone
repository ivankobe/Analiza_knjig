        $('bookCover_45485735').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_45485735').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
