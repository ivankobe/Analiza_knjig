        $('bookCover_7844023').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_7844023').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
