        $('bookCover_44890112').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_44890112').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
