        $('bookCover_37530').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_37530').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
