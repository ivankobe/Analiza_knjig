        $('bookCover_49540').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_49540').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
