<div id="review_1912232856" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1912232856" />
    <a title="Karin" class="left imgcol" href="/user/show/20048682-karin"><img alt="Karin" src="https://images.gr-assets.com/users/1588016778p2/20048682.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1912232856?book_show_action=true">Dec 19, 2019</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Karin" class="user" itemprop="url" name="Karin" href="/user/show/20048682-karin">Karin</a>
