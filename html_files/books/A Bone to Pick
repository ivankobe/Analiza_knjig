        $('bookCover_140087').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_140087').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
