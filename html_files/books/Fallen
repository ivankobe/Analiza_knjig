        $('bookCover_6339664').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_6339664').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
