          $$('div.prototip').each(function (e) {
            if ($('bookCover_78159').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_78159').observe('prototip:hidden', function () {
