      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_54304017').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_54304017').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_54304017').observe('prototip:hidden', function () {
