        $('bookCover_49204960').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_49204960').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
