        $('bookCover_1891120').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_1891120').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
