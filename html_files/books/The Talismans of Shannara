<div id="review_2094721685" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2094721685" />
    <a title="Emma" class="left imgcol" href="/user/show/26734926-emma"><img alt="Emma" src="https://images.gr-assets.com/users/1565436013p2/26734926.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2094721685?book_show_action=true">Jun 14, 2020</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Emma" class="user" itemprop="url" name="Emma" href="/user/show/26734926-emma">Emma</a>
      </span>

