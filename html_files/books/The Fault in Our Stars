  <div class="friendReviews elementListBrown" >
    <div class="section firstReview">
      

<div id="review_289575457" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/289575457" />
    <a title="Sophia." class="left imgcol" href="/user/show/6520961-sophia"><img alt="Sophia." src="https://images.gr-assets.com/users/1322479535p2/6520961.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/289575457?book_show_action=true">Mar 06, 2012</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Sophia." class="user" itemprop="url" name="Sophia." href="/user/show/6520961-sophia">Sophia.</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="liked it"><span size="15x15" class="staticStar p10">liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/6520961-sophia?shelf=awesome-parent">awesome-parent</a>, 
              <a class="actionLinkLite" href="/review/list/6520961-sophia?shelf=unconvincing-romance">unconvincing-romance</a>, 
              <a class="actionLinkLite" href="/review/list/6520961-sophia?shelf=death">death</a>, 
              <a class="actionLinkLite" href="/review/list/6520961-sophia?shelf=pretentious-writing">pretentious-writing</a>, 
              <a class="actionLinkLite" href="/review/list/6520961-sophia?shelf=teary">teary</a>, 
              <a class="actionLinkLite" href="/review/list/6520961-sophia?shelf=depression">depression</a>, 
              <a class="actionLinkLite" href="/review/list/6520961-sophia?shelf=heart-breaking">heart-breaking</a>, 
              <a class="actionLinkLite" href="/review/list/6520961-sophia?shelf=beautiful-ending">beautiful-ending</a>, 
              <a class="actionLinkLite" href="/review/list/6520961-sophia?shelf=medical-places">medical-places</a>, 
              <a class="actionLinkLite" href="/review/list/6520961-sophia?shelf=deja-vu">deja-vu</a>
          </div>
