        $('bookCover_44099044').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_44099044').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
