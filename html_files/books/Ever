<div id="review_2583912499" class="review" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/2583912499" />
    <a title="Sha" class="left imgcol" href="/user/show/82147023-sha"><img alt="Sha" src="https://images.gr-assets.com/users/1588618096p2/82147023.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/2583912499?book_show_action=true">Nov 04, 2018</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Sha" class="user" itemprop="url" name="Sha" href="/user/show/82147023-sha">Sha</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="liked it"><span size="15x15" class="staticStar p10">liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/82147023-sha?shelf=male-mc">male-mc</a>, 
              <a class="actionLinkLite" href="/review/list/82147023-sha?shelf=romance">romance</a>, 
              <a class="actionLinkLite" href="/review/list/82147023-sha?shelf=fantasy">fantasy</a>, 
              <a class="actionLinkLite" href="/review/list/82147023-sha?shelf=young-adult">young-adult</a>, 
              <a class="actionLinkLite" href="/review/list/82147023-sha?shelf=retelling">retelling</a>
          </div>
