      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_21863539').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_21863539').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_21863539').observe('prototip:hidden', function () {
