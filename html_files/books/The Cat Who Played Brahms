<div id="review_405341640" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/405341640" />
    <a title="Joy Gerbode" class="left imgcol" href="/user/show/5049682-joy-gerbode"><img alt="Joy Gerbode" src="https://images.gr-assets.com/users/1300106835p2/5049682.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/405341640?book_show_action=true">Jun 29, 2019</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Joy Gerbode" class="user" itemprop="url" name="Joy Gerbode" href="/user/show/5049682-joy-gerbode">Joy Gerbode</a>
