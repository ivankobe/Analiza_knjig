      newTip['wrapper'].addClassName('prototipAllowOverflow');

        $('bookCover_2974804').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_2974804').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_2974804').observe('prototip:hidden', function () {
