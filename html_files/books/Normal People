        $('bookCover_34538386').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_34538386').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
