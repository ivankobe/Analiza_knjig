        $('bookCover_55883996').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_55883996').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
