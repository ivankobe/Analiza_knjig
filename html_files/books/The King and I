        $('bookCover_8524936').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_8524936').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
