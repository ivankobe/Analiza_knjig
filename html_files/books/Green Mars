<div id="review_364621346" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/364621346" />
    <a title="Wanda" class="left imgcol" href="/user/show/9246169-wanda"><img alt="Wanda" src="https://images.gr-assets.com/users/1528903039p2/9246169.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/364621346?book_show_action=true">Mar 20, 2020</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Wanda" class="user" itemprop="url" name="Wanda" href="/user/show/9246169-wanda">Wanda</a>
      </span>

        rated it
        <span class=" staticStars notranslate" title="liked it"><span size="15x15" class="staticStar p10">liked it</span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p10"></span><span size="15x15" class="staticStar p0"></span><span size="15x15" class="staticStar p0"></span></span>



        

          <div class="uitext greyText bookshelves">
            Shelves:
              <a class="actionLinkLite" href="/review/list/9246169-wanda?shelf=speculative-fiction">speculative-fiction</a>, 
              <a class="actionLinkLite" href="/review/list/9246169-wanda?shelf=used-books">used-books</a>, 
              <a class="actionLinkLite" href="/review/list/9246169-wanda?shelf=reading-project">reading-project</a>, 
              <a class="actionLinkLite" href="/review/list/9246169-wanda?shelf=read-in-2020">read-in-2020</a>
          </div>
