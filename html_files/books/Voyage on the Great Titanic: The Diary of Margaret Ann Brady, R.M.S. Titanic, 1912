          $$('div.prototip').each(function (e) {
            if ($('bookCover_1638371').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
      $('bookCover_1638371').observe('prototip:hidden', function () {
