        $('bookCover_458822').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_458822').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
