        $('bookCover_32230471').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_32230471').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
