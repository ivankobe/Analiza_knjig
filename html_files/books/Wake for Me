        $('bookCover_43801254').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_43801254').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
        });
