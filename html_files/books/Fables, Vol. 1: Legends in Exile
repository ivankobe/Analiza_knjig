        $('bookCover_15704307').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_15704307').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
