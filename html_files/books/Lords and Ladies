<div id="review_1259302164" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/1259302164" />
    <a title="Lindsay" class="left imgcol" href="/user/show/10326608-lindsay"><img alt="Lindsay" src="https://images.gr-assets.com/users/1588746288p2/10326608.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/1259302164?book_show_action=true">Apr 19, 2015</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Lindsay" class="user" itemprop="url" name="Lindsay" href="/user/show/10326608-lindsay">Lindsay</a>
