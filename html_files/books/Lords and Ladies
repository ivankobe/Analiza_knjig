        $('bookCover_46220828').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_46220828').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
