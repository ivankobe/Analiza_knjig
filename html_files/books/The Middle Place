//<![CDATA[
      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_380'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/1779929.The_Middle_Place?rating=\">all <span class=\"greyText\">(52033)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/1779929.The_Middle_Place?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;dwIjAEnGx/LO0tICAP4dmkBBmWo6S0En+N9sFc+ekviNLbJQ1CglRitTFnFz+axvypJJtZW6amKnCDCgo4ciyA==&#39;)}); return false;\">5 stars <span class=\"greyText\">(8590)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/1779929.The_Middle_Place?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;T45s8fxFtDfSxruSCqJYjP8v498eIykzha+UA5U3Lpq1of2hYatWgzdHf+F5pel5dfwzALHSAnbaeMi2+S6eqg==&#39;)}); return false;\">4 stars <span class=\"greyText\">(11901)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n           