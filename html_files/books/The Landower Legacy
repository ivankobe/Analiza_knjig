<div id="review_3029269018" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/3029269018" />
    <a title="Tannie" class="left imgcol" href="/user/show/1136543-tannie"><img alt="Tannie" src="https://images.gr-assets.com/users/1474405541p2/1136543.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/3029269018?book_show_action=true">Oct 29, 2019</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Tannie" class="user" itemprop="url" name="Tannie" href="/user/show/1136543-tannie">Tannie</a>
