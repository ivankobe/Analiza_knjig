<div id="review_3240528130" class="review nosyndicate" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
  <link itemprop="url" href="https://www.goodreads.com/review/show/3240528130" />
    <a title="Mavel" class="left imgcol" href="/user/show/5940823-mavel"><img alt="Mavel" src="https://images.gr-assets.com/users/1584846788p2/5940823.jpg" /></a>

  <div class="left bodycol">
    <div class="reviewHeader uitext stacked">
        <a class="reviewDate createdAt right" href="/review/show/3240528130?book_show_action=true">Mar 22, 2020</a>

      <span itemprop="author" itemscope itemtype="http://schema.org/Person">
        <a title="Mavel" class="user" itemprop="url" name="Mavel" href="/user/show/5940823-mavel">Mavel</a>
