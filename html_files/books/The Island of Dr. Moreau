      var newTip = new Tip($('span_class_gr-hyperlink_more_filters_span_tip_219'), "\n          <div class=\"greyText smallText\" style=\"white-space:nowrap\">\n\n            <span class=\"loadingLinkSpan\"><a class=\"actionLinkLite greyText loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"/book/reviews/29981.The_Island_of_Dr_Moreau?rating=\">all <span class=\"greyText\">(198709)<\/span><\/a><span class=\"loadingAnimation\"><\/span><span class=\"loadingError\"><\/span><\/span>\n\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/29981.The_Island_of_Dr_Moreau?rating=5&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;4KXActP4LF74R+BRl3e44hRrOhwDH+QURAVuKfc+2/hAFIptZcg+sxarCqCZOiYlsrq3hKcrP2238g2GWBppWA==&#39;)}); return false;\">5 stars <span class=\"greyText\">(23318)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/span>\n                  <span class=\"loadingError\"><\/span>\n                <\/span>\n                <span class=\"greyText\">&nbsp;|&nbsp;<\/span>\n                <span class=\"loadingLinkSpan\">\n                  <a class=\"actionLinkLite loadingLink\" rel=\"nofollow\" data-keep-on-success=\"true\" data-remote=\"true\" href=\"#\" onclick=\"new Ajax.Request(&#39;/book/reviews/29981.The_Island_of_Dr_Moreau?rating=4&#39;, {asynchronous:true, evalScripts:true, method:&#39;get&#39;, parameters:&#39;authenticity_token=&#39; + encodeURIComponent(&#39;0cibWDnoVGQs+iKFMe+O75VN+NYiQj9oaPEVR/Kf7ghxedFHj9hGicIWyHQ/ohAoM5x1ToZ25BGbBnboXbtcqA==&#39;)}); return false;\">4 stars <span class=\"greyText\">(40296)<\/span><\/a>\n                  <span class=\"loadingAnimation\"><\/s